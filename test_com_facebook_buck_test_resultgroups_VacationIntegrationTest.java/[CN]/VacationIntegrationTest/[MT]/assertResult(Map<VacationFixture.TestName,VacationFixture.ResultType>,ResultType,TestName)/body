{
  for (  TestName testName : testNames) {
    assertTrue(String.format("We don't have a test result for test '%s'!",testName),results.containsKey(testName));
    ResultType actualResultType=results.get(testName);
    assertThat(String.format("Test result for '%s'",testName),actualResultType,equalTo(expectedResultType));
  }
}
