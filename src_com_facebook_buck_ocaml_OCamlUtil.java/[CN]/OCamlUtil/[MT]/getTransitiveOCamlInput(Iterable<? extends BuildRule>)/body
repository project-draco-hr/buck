{
  final ImmutableDirectedAcyclicGraph<BuildRule> graph=BuildRuleDependencyVisitors.getBuildRuleDirectedGraphFilteredBy(inputs,Predicates.instanceOf(OCamlLibrary.class),Predicates.instanceOf(OCamlLibrary.class));
  final ImmutableList<BuildRule> sorted=TopologicalSort.sort(graph,Predicates.<BuildRule>alwaysTrue());
  return FluentIterable.from(sorted).filter(OCamlLibrary.class).toList();
}
