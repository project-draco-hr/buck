{
  return new ProjectGraphParser(){
    @Override public TargetGraph buildTargetGraphForTargetNodeSpecs(    Iterable<? extends TargetNodeSpec> targetNodeSpecs) throws IOException, InterruptedException {
      try {
        return parser.buildTargetGraphForTargetNodeSpecs(targetNodeSpecs,parserConfig,buckEventBus,console,environment,enableProfiling);
      }
 catch (      BuildTargetException|BuildFileParseException e) {
        throw new HumanReadableException(e);
      }
    }
  }
;
}
