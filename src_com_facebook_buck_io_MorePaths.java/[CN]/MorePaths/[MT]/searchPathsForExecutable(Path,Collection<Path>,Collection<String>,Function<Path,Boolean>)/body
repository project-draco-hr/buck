{
  Preconditions.checkArgument(!executableToFind.isAbsolute(),"Path %s must be relative",executableToFind);
  for (  Path pathToSearch : pathsToSearch) {
    Optional<Path> maybeResolved=resolveExecutable(pathToSearch,executableToFind,extensions,pathIsExecutableChecker);
    if (maybeResolved.isPresent()) {
      return maybeResolved;
    }
  }
  return Optional.absent();
}
