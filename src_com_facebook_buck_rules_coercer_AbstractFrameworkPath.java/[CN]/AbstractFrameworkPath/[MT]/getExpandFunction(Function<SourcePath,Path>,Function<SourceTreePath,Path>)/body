{
  return new Function<FrameworkPath,Path>(){
    @Override public Path apply(    FrameworkPath input){
switch (input.getType()) {
case SOURCE_TREE_PATH:
        return Preconditions.checkNotNull(sourceTreePathResolver.apply(input.getSourceTreePath().get()));
case SOURCE_PATH:
      return Preconditions.checkNotNull(sourcePathResolver.apply(input.getSourcePath().get()));
default :
    throw new RuntimeException("Unhandled type: " + input.getType());
}
}
}
;
}
