{
  assertEquals(TargetNodePredicateSpec.of(Predicates.alwaysTrue(),BuildFileSpec.fromRecursivePath(Paths.get("hello"))),PARSER.parse(createCellRoots(null),"//hello/..."));
  assertEquals(TargetNodePredicateSpec.of(Predicates.alwaysTrue(),BuildFileSpec.fromRecursivePath(Paths.get(""))),PARSER.parse(createCellRoots(null),"//..."));
  assertEquals(TargetNodePredicateSpec.of(Predicates.alwaysTrue(),BuildFileSpec.fromRecursivePath(Paths.get(""))),PARSER.parse(createCellRoots(null),"..."));
  assertEquals(BuildTargetSpec.from(BuildTargetFactory.newInstance("//hello:...")),PARSER.parse(createCellRoots(null),"//hello:..."));
}
