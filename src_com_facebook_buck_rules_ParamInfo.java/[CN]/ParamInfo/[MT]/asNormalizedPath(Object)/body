{
  Preconditions.checkArgument(value instanceof String,"Expected argument '%s' to be a string in build file in %s",value,pathRelativeToProjectRoot);
  String path=(String)value;
  if (path.startsWith(GENFILE_PREFIX)) {
    path=path.substring(GENFILE_PREFIX.length());
    return Paths.get(BuckConstant.GEN_DIR).resolve(pathRelativeToProjectRoot).resolve(path).normalize();
  }
  return pathRelativeToProjectRoot.resolve(path).normalize();
}
