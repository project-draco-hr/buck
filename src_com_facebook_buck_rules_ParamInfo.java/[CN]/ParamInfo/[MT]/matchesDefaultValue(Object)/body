{
  if (value instanceof String && "".equals(value)) {
    return true;
  }
  if (value instanceof Number && ((Number)value).intValue() == 0) {
    return true;
  }
  if (Boolean.FALSE.equals(value)) {
    return true;
  }
  if (value instanceof List && ((List)value).isEmpty()) {
    return true;
  }
  return false;
}
