{
  filterAllTargetsInProject(parser,cell,Predicates.<TargetNode<?>>alwaysTrue(),eventBus);
  BuildTarget foo=BuildTarget.builder(cellRoot,"//java/com/facebook","foo").build();
  parser.buildTargetGraph(eventBus,cell,false,ImmutableList.of(foo));
  assertEquals("Should have cached build rules.",1,counter.calls);
}
