{
  SettableFakeClock fakeClock=new SettableFakeClock(49152,64738);
  BuckEventBus eb=new BuckEventBus(fakeClock,MostExecutors.newSingleThreadExecutor(BuckEventBus.class.getSimpleName()),BuckEventBusFactory.BUILD_ID_FOR_TEST,timeoutMillis);
  TestEvent event=new TestEvent();
  eb.post(event);
  eb.close();
  assertEquals(event.getTimestamp(),49152);
  assertEquals(event.getNanoTime(),64738);
}
