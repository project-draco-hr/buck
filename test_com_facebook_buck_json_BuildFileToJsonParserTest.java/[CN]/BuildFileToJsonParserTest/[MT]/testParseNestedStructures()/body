{
  String json="[" + "{" + "\"foo\": [\"a\", \"b\"],"+ "\"bar\": {\"baz\": \"quox\"}"+ "}"+ "]";
  try (BuildFileToJsonParser parser=new BuildFileToJsonParser(json)){
    assertEquals(ImmutableList.of(ImmutableMap.of("foo",ImmutableList.of("a","b"),"bar",ImmutableMap.of("baz","quox"))),parser.nextRules());
  }
 }
