{
  String json="[{\"foo\": \"a:1\"}, {\"foo\": \"a:2\"}]\n" + "[{\"bar\": \"b:1\"}]";
  try (BuildFileToJsonParser parser=new BuildFileToJsonParser(json,true)){
    assertEquals(ImmutableList.of(ImmutableMap.of("foo","a:1"),ImmutableMap.of("foo","a:2")),parser.nextRules());
    assertEquals(ImmutableList.of(ImmutableMap.of("bar","b:1")),parser.nextRules());
  }
 }
