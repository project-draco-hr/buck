{
  String json="[{\"foo\": \"a:1\"}, {\"foo\": \"a:2\"}]\n" + "[{\"bar\": \"b:1\"}]";
  BuildFileToJsonParser parser=new BuildFileToJsonParser(json);
  List<Map<String,Object>> a=parser.nextRules();
  assertEquals(ImmutableList.of(ImmutableMap.of("foo","a:1"),ImmutableMap.of("foo","a:2")),a);
  List<Map<String,Object>> b=parser.nextRules();
  assertEquals(ImmutableList.of(ImmutableMap.of("bar","b:1")),b);
}
