{
  String json="{" + "\"srcs\": [\"src/com/facebook/buck/Bar.java\", \"src/com/facebook/buck/Foo.java\"]" + "}";
  BuildFileToJsonParser parser=new BuildFileToJsonParser(json);
  List<Map<String,Object>> tokens=parser.nextRules();
  assertEquals(ImmutableList.of(ImmutableMap.of("srcs",ImmutableList.of("src/com/facebook/buck/Bar.java","src/com/facebook/buck/Foo.java"))),tokens);
}
