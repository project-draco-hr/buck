{
  Set<String> testClassNames=getClassNamesForSources(executionContext);
  if (testClassNames.isEmpty()) {
    return true;
  }
  Path outputDirectory=executionContext.getProjectFilesystem().getPathForRelativePath(getPathToTestOutputDirectory());
  for (  String testClass : testClassNames) {
    Path testResultFile=outputDirectory.resolve(testClass + ".xml");
    if (!Files.isRegularFile(testResultFile)) {
      return false;
    }
  }
  return true;
}
