{
  Path outputPath;
  Path relativeOutputPath=rule.getPathToOutput();
  if (relativeOutputPath != null) {
    outputPath=context.getProjectFilesystem().getAbsolutifier().apply(relativeOutputPath);
  }
 else {
    outputPath=null;
  }
  classNamesForSources=getClassNamesForSources(rule.getJavaSrcs(),outputPath,context.getProjectFilesystem());
}
