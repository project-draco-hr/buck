{
  BuildRuleParams buildRuleParams=new FakeBuildRuleParams(new BuildTarget("//scripts/python","foo"));
  SourcePath src=new FileSourcePath("");
  ImmutableSortedSet<SourcePath> srcs=ImmutableSortedSet.of(src);
  PythonLibrary pythonLibrary=new PythonLibrary(buildRuleParams,srcs);
  assertTrue(pythonLibrary.getProperties().is(LIBRARY));
}
