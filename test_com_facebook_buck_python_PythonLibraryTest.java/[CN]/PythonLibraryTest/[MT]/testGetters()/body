{
  BuildRuleParams buildRuleParams=new BuildRuleParams(BuildTargetFactory.newInstance("//scripts/python:foo"),ImmutableSortedSet.<BuildRule>of(),ImmutableSet.<BuildTargetPattern>of(),Functions.<String>identity());
  ImmutableSortedSet<String> srcs=ImmutableSortedSet.of("");
  PythonLibrary pythonLibrary=new PythonLibrary(buildRuleParams,srcs);
  assertTrue(pythonLibrary.getProperties().is(LIBRARY));
  assertSame(srcs,pythonLibrary.getPythonSrcs());
}
