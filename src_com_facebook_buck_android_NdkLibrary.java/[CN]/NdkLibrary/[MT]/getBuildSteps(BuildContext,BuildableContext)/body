{
  Path binDirectory=buildArtifactsDirectory.resolve("libs");
  Step nkdBuildStep=new NdkBuildStep(makefileDirectory,buildArtifactsDirectory,binDirectory,flags);
  Step recordStep=new RecordArtifactsInDirectoryStep(buildableContext,binDirectory,genDirectory);
  return ImmutableList.of(nkdBuildStep,recordStep);
}
