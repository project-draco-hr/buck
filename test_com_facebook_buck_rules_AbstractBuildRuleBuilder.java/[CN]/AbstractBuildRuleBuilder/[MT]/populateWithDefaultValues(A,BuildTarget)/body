{
  ProjectFilesystem filesystem=new ProjectFilesystem(Paths.get("."));
  BuildRuleFactoryParams factoryParams=NonCheckingBuildRuleFactoryParams.createNonCheckingBuildRuleFactoryParams(new BuildTargetParser(),target);
  try {
    new ConstructorArgMarshaller().populate(filesystem,factoryParams,arg,ImmutableSet.<BuildTarget>builder(),ImmutableMap.<String,Object>of(),true);
  }
 catch (  ConstructorArgMarshalException error) {
    throw Throwables.propagate(error);
  }
}
