{
  String outputName=CxxFlavorSanitizer.sanitize(getCompileOutputName(name));
  return BuildTarget.builder(getParams().getBuildTarget()).addAllFlavors(getParams().getBuildTarget().getFlavors()).addFlavors(getCxxPlatform().getFlavor()).addFlavors(ImmutableFlavor.of(String.format("infer-capture-%s",outputName))).build();
}
