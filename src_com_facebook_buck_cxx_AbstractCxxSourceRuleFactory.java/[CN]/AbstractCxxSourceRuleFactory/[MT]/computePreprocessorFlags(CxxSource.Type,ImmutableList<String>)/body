{
  return CxxToolFlags.explicitBuilder().addAllPlatformFlags(getPicType().getFlags()).addAllPlatformFlags(CxxSourceTypes.getPlatformPreprocessFlags(getCxxPlatform(),type)).addAllRuleFlags(preprocessorFlags.getUnchecked(type)).addAllRuleFlags(getRuleCompileFlags(CxxSourceTypes.getPreprocessorOutputType(type))).addAllRuleFlags(sourceFlags).build();
}
