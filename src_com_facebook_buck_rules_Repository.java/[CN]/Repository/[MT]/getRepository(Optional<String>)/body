{
  if (!repoName.isPresent()) {
    return this;
  }
  try {
    return repos.getUnchecked(repoName.get());
  }
 catch (  UncheckedExecutionException e) {
    Throwable cause=e.getCause();
    if (cause instanceof HumanReadableException) {
      throw (HumanReadableException)cause;
    }
    throw e;
  }
}
