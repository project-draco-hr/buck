{
  Preconditions.checkArgument(!target.getRepository().isPresent(),"Target %s is not from this repository.",target);
  Path relativePath=target.getBasePath().resolve(new ParserConfig(getBuckConfig()).getBuildFileName());
  if (!getFilesystem().isFile(relativePath)) {
    throw new MissingBuildFileException(target,getBuckConfig());
  }
  return getFilesystem().resolve(relativePath);
}
