@classmethod
def get_op(cls, op):
    ops = {symbol.test: cls.test, symbol.and_test: cls.and_test, symbol.atom: cls.atom, symbol.comparison: cls.comparison, 'not in': (lambda x, y: (x not in y)), 'in': (lambda x, y: (x in y)), '==': operator.eq, '!=': operator.ne, '<': operator.lt, '>': operator.gt, '<=': operator.le, '>=': operator.ge, }
    if hasattr(symbol, 'or_test'):
        ops[symbol.or_test] = cls.test
    return ops[op]
