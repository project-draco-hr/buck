{
  if (values.isEmpty()) {
    return "java.util.Collections.<String>emptySet()";
  }
  String initFormat="java.util.Collections.unmodifiableSet(" + "new java.util.HashSet<>(java.util.Arrays.asList(%s)))";
  String commaSeparatedValues=Joiner.on(',').join(FluentIterable.from(values).transform(Escaper.JAVA_ESCAPER));
  return String.format(initFormat,commaSeparatedValues);
}
