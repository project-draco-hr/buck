{
  PoolEntry result;
  if (m_nestLevel >= BAOS_POOL_SIZE) {
    result=new PoolEntry(BAOS_INIT_SIZE,BAOS_INIT_SIZE);
  }
 else {
    result=m_bufPool[m_nestLevel];
    if (result == null) {
      result=new PoolEntry(BAOS_INIT_SIZE,BAOS_INIT_SIZE);
      m_bufPool[m_nestLevel]=result;
    }
 else {
      result.m_baos.reset();
    }
  }
  ++m_nestLevel;
  return result;
}
