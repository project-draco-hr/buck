{
  Preconditions.checkArgument(!classpathEntry.startsWith("/"),"Classpath entries should be relative rather than absolute paths: %s",classpathEntry);
  String obfuscatedName=Paths.getBasename(classpathEntry,".jar") + "-obfuscated.jar";
  String dirName=new File(classpathEntry).getParent();
  String outputJar=getPathForProGuardDirectory() + "/" + dirName+ "/"+ obfuscatedName;
  return outputJar;
}
