{
  ImmutableList.Builder<Command> commands=ImmutableList.builder();
  commands.add(new SleepingCommand(0,0));
  commands.add(new SleepingCommand(10,1));
  commands.add(new SleepingCommand(5000,1));
  ExecutionContext context=new ExecutionContext(Verbosity.SILENT,new File("."),Optional.<AndroidPlatformTarget>absent(),Optional.<File>absent(),Ansi.withoutTty(),false,false,System.out,System.err);
  ThreadFactory threadFactory=new ThreadFactoryBuilder().setDaemon(true).setNameFormat(getClass().getSimpleName() + "-%d").build();
  ListeningExecutorService executorService=MoreExecutors.listeningDecorator(Executors.newFixedThreadPool(3,threadFactory));
  DefaultCommandRunner runner=new DefaultCommandRunner(context,executorService);
  runner.runCommandsInParallelAndWait(commands.build());
}
