{
  ImmutableListMultimap<Path,SourcePath> mappedUniverse=Multimaps.index(universe,resolver.getRelativePathFunction());
  for (  Pair<RuleKey,int[]> entry : entries) {
    if (hashesMatch(fileHashCache,resolver,mappedUniverse,entry.getSecond())) {
      return Optional.of(entry.getFirst());
    }
  }
  return Optional.absent();
}
