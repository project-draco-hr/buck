{
  if (paths.size() == 1) {
    return fileHashCache.get(resolver.getAbsolutePath(paths.asList().get(0)));
  }
  Hasher hasher=Hashing.md5().newHasher();
  for (  SourcePath path : paths) {
    hasher.putBytes(fileHashCache.get(resolver.getAbsolutePath(path)).asBytes());
  }
  return hasher.hash();
}
