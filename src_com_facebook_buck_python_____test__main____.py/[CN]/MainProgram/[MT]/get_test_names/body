def get_test_names(self, test_suite):
    names = []
    for test in test_suite:
        if isinstance(test, unittest.TestSuite):
            names.extend(self.get_test_names(test))
        elif (getattr(test, '_testMethodName', None) is not None):
            name = '{0}.{1}#{2}'.format(test.__class__.__module__, test.__class__.__name__, test._testMethodName)
            names.append(name)
    return names
