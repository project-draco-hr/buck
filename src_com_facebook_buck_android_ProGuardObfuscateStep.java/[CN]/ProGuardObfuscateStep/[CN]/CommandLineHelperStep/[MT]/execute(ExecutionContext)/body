{
  String proGuardArguments=Joiner.on('\n').join(getParameters(context));
  try {
    context.getProjectFilesystem().writeContentsToPath(proGuardArguments,pathToProGuardCommandLineArgsFile);
  }
 catch (  IOException e) {
    context.logError(e,"Error writing ProGuard arguments to file: %s.",pathToProGuardCommandLineArgsFile);
    return 1;
  }
  return 0;
}
