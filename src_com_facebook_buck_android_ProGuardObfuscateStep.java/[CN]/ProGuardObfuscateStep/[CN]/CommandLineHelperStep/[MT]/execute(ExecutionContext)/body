{
  String proGuardArguments=Joiner.on('\n').join(getParameters(context));
  try {
    context.getProjectFilesystem().writeContentsToPath(proGuardArguments,Paths.get(pathToProGuardCommandLineArgsFile));
  }
 catch (  IOException e) {
    context.getBuckEventBus().post(ThrowableLogEvent.create(e,"Error writing ProGuard arguments to file: %s.",pathToProGuardCommandLineArgsFile));
    return 1;
  }
  return 0;
}
