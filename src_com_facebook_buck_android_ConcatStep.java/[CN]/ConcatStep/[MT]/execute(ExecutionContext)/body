{
  ImmutableList<Path> list=inputs.get();
  try (OutputStream out=filesystem.newFileOutputStream(output)){
    for (    Path p : list) {
      filesystem.copyToOutputStream(p,out);
    }
    out.flush();
  }
 catch (  IOException e) {
    context.logError(e,"There was an error in concat step");
    return 1;
  }
  return 0;
}
