{
  File[] files=directory.listFiles(filenameFilter);
  if (files == null) {
    return;
  }
  for (  File file : files) {
    if (filesToKeep.contains(file)) {
      LOG.warn("Skipping " + file);
      continue;
    }
    LOG.warn("Deleting " + file);
    if (!file.delete()) {
      LOG.warn("Unable to delete " + file);
    }
  }
}
