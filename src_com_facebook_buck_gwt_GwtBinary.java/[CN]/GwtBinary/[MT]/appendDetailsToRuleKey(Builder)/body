{
  return builder.setReflectively("moduleDeps",moduleDeps).setReflectively("modules",modules).setReflectively("vmArgs",vmArgs).setReflectively("style",style.name()).setReflectively("draftCompile",draftCompile).setReflectively("optimize",optimize).setReflectively("localWorkers",localWorkers).setReflectively("strict",strict).setReflectively("experimentalArgs",experimentalArgs);
