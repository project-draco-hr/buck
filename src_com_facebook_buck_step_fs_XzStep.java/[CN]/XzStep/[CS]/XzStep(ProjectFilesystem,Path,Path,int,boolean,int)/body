{
  this.filesystem=filesystem;
  this.sourceFile=sourceFile;
  this.destinationFile=destinationFile;
  Preconditions.checkArgument(compressionLevel >= LZMA2Options.PRESET_MIN && compressionLevel <= LZMA2Options.PRESET_MAX,"compressionLevel out of bounds.");
  this.compressionLevel=compressionLevel;
