{
  if (exopackageModes.isEmpty()) {
    return Optional.absent();
  }
  Optional<PreDexMerge> preDexMerge=enhancementResult.getPreDexMerge();
  ExopackageInfo exopackageInfo=ImmutableExopackageInfo.builder().dexInfo(ImmutableExopackageInfo.DexInfo.of(preDexMerge.get().getMetadataTxtPath(),preDexMerge.get().getDexDirectory())).build();
  return Optional.of(exopackageInfo);
}
