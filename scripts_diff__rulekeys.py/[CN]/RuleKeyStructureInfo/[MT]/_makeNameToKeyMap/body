@staticmethod
def _makeNameToKeyMap(entries):
    result = {}
    for e in entries:
        (top_key, structure) = e
        name = RuleKeyStructureInfo._nameFromStruct(structure)
        if (name is None):
            continue
        if ((name in result) and (result[name] != top_key)):
            print((((((u'Conflict for name ' + name) + u' ') + top_key) + u' ') + result[name]))
        result[name] = top_key
    return result
