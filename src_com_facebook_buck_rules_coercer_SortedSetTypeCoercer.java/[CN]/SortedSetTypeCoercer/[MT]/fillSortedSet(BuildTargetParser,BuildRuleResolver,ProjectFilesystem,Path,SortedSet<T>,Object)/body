{
  if (object instanceof Collection) {
    for (    Object element : (Iterable<?>)object) {
      T coercedElement=elementTypeCoercer.coerce(buildTargetParser,buildRuleResolver,filesystem,pathRelativeToProjectRoot,element);
      boolean alreadyExists=!builder.add(coercedElement);
      if (alreadyExists) {
        throw new CoerceFailedException(String.format("duplicate element \"%s\"",coercedElement));
      }
    }
  }
 else {
    throw CoerceFailedException.simple(object,getOutputClass());
  }
}
