{
  Path temp=Paths.get(System.getProperty("java.io.tmpdir"),"repack" + System.currentTimeMillis());
  return new StringBuilder("cd ").append(temp.toString()).append(" && ").append("unzip ").append(inputFile).append(" && ").append("zip -r -").append(compressionLevel).append(" ").append(outputFile).append(" *").toString();
}
