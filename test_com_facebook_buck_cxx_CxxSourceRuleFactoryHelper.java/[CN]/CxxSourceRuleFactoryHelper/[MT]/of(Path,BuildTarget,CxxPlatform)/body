{
  BuildRuleResolver resolver=new BuildRuleResolver();
  return new CxxSourceRuleFactory(new FakeBuildRuleParamsBuilder(target).setProjectFilesystem(new FakeProjectFilesystem(cellRoot.toFile())).build(),resolver,new SourcePathResolver(resolver),cxxPlatform,ImmutableList.<CxxPreprocessorInput>of(),ImmutableList.<String>of(),Optional.<SourcePath>absent());
}
