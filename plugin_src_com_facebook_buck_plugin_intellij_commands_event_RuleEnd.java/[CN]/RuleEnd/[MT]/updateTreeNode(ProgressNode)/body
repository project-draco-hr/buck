{
  if (isSuccessful()) {
    node.setType(isCached() ? ProgressNode.Type.BUILT_CACHED : ProgressNode.Type.BUILT);
  }
 else {
    node.setType(ProgressNode.Type.BUILD_ERROR);
  }
  node.setName(String.format("[%s ms] %s %s",getTimestamp() - node.getEvent().getTimestamp(),getName(),isCached() ? "(From cache)" : ""));
}
