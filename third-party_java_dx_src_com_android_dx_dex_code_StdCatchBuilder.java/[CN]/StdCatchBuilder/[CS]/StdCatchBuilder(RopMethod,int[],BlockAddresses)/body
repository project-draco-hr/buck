{
  if (method == null) {
    throw new NullPointerException("method == null");
  }
  if (order == null) {
    throw new NullPointerException("order == null");
  }
  if (addresses == null) {
    throw new NullPointerException("addresses == null");
  }
  this.method=method;
  this.order=order;
  this.addresses=addresses;
}
