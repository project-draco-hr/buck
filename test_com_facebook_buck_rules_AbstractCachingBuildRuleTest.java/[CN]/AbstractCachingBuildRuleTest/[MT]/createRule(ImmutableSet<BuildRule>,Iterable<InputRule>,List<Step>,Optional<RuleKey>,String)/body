{
  Comparator<BuildRule> comparator=RetainOrderComparator.createComparator(deps);
  ImmutableSortedSet<BuildRule> sortedDeps=ImmutableSortedSet.copyOf(comparator,deps);
  BuildRuleParams buildRuleParams=new BuildRuleParams(buildTarget,sortedDeps,ImmutableSet.<BuildTargetPattern>of(),Functions.<String>identity());
  return new BuildableAbstractCachingBuildRule(buildRuleParams,inputRules,pathToOutputFile,ruleKeyOnDisk,buildSteps);
}
