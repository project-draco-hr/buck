{
  if (code == null)   return null;
  try {
    if (code.length() > 0) {
      final String msg=lookup(namespace,code);
      if (msg == null) {
        return code;
      }
 else {
        return EMBED_ERROR_CODE ? "[" + code + "] "+ msg : msg;
      }
    }
 else {
      return "";
    }
  }
 catch (  Throwable t) {
    return code;
  }
}
