{
@TargetName(name="lollerskates") class NoName {
    public String foobar;
  }
  String definition=buckPyFunction.toPythonFunction(new BuildRuleType("noname"),new NoName());
  assertEquals(Joiner.on("\n").join("@provide_for_build","def noname(foobar, visibility=[], build_env=None):","  add_rule({","    'type' : 'noname',","    'name' : 'lollerskates',","    'foobar' : foobar,","    'visibility' : visibility,","  }, build_env)","",""),definition);
}
