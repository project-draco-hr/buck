{
  struct.validate();
  oprot.writeStructBegin(STRUCT_DESC);
  if (struct.version != null) {
    if (struct.isSetVersion()) {
      oprot.writeFieldBegin(VERSION_FIELD_DESC);
      oprot.writeString(struct.version);
      oprot.writeFieldEnd();
    }
  }
  oprot.writeFieldStop();
  oprot.writeStructEnd();
}
