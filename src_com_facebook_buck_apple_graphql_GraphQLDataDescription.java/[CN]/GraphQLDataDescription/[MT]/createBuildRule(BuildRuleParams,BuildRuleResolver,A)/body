{
  return new GraphQLData(params,new SourcePathResolver(resolver),params.getPathAbsolutifier(),modelGenerator.get(),params.getBuildTarget().getFlavors().contains(ImmutableFlavor.of(GraphQLGenerationMode.FOR_COMPILING.toString())) ? GraphQLGenerationMode.FOR_COMPILING : GraphQLGenerationMode.FOR_LINKING,args.queries,args.consistencyConfig,args.clientSchemaConfig,args.schema,args.mutations,args.modelTags,args.knownIssuesFile,args.persistIds);
}
