{
  CxxLink nativeLinkable=DDescriptionUtils.createNativeLinkable(params,args.srcs,ImmutableList.<String>of(),buildRuleResolver,cxxPlatform,dBuckConfig,targetGraph);
  buildRuleResolver.addToIndex(nativeLinkable);
  CommandTool.Builder executableBuilder=new CommandTool.Builder();
  executableBuilder.addArg(new BuildTargetSourcePath(nativeLinkable.getBuildTarget()));
  return new DBinary(params,new SourcePathResolver(buildRuleResolver),executableBuilder.build());
}
