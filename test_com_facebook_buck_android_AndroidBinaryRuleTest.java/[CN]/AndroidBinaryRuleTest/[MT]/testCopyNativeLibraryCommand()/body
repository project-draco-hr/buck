{
  createAndroidBinaryRuleAndTestCopyNativeLibraryCommand(ImmutableSet.of(TargetCpuType.ARMV7),"/path/to/source","/path/to/destination/",ImmutableList.of("[ -d /path/to/source/armeabi-v7a ] && mkdir -p /path/to/destination/armeabi-v7a " + "&& cp -R /path/to/source/armeabi-v7a/* /path/to/destination/armeabi-v7a"));
