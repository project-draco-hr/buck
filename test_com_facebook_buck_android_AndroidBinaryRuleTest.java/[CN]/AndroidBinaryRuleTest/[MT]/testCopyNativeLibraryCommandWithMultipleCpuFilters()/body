{
  createAndroidBinaryRuleAndTestCopyNativeLibraryCommand(ImmutableSet.of(TargetCpuType.ARM,TargetCpuType.X86),"/path/to/source","/path/to/destination/",ImmutableList.of("[ -d /path/to/source/armeabi ] && mkdir -p /path/to/destination/armeabi " + "&& cp -R /path/to/source/armeabi/* /path/to/destination/armeabi","[ -d /path/to/source/x86 ] && mkdir -p /path/to/destination/x86 " + "&& cp -R /path/to/source/x86/* /path/to/destination/x86"));
}
