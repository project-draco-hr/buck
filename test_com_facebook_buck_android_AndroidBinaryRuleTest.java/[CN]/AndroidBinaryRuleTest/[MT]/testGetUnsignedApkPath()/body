{
  BuildRuleResolver ruleResolver=new BuildRuleResolver();
  AndroidBinaryRule ruleInRootDirectory=ruleResolver.buildAndAddToIndex(AndroidBinaryRule.newAndroidBinaryRuleBuilder(new FakeAbstractBuildRuleBuilderParams()).setBuildTarget(BuildTargetFactory.newInstance("//:fb4a")).setManifest("AndroidManifest.xml").setKeystore(addKeystoreRule(ruleResolver)).setTarget("Google Inc.:Google APIs:16"));
  assertEquals(GEN_DIR + "/fb4a.apk",ruleInRootDirectory.getApkPath());
  AndroidBinaryRule ruleInNonRootDirectory=ruleResolver.buildAndAddToIndex(AndroidBinaryRule.newAndroidBinaryRuleBuilder(new FakeAbstractBuildRuleBuilderParams()).setBuildTarget(BuildTargetFactory.newInstance("//java/com/example:fb4a")).setManifest("AndroidManifest.xml").setKeystore(addKeystoreRule(ruleResolver)).setTarget("Google Inc.:Google APIs:16"));
  assertEquals(GEN_DIR + "/java/com/example/fb4a.apk",ruleInNonRootDirectory.getApkPath());
}
