{
  Set<Path> filesToRemove=Filters.filterByDensity(candidates,ImmutableSet.of(Density.MDPI),false);
  Predicate<Path> predicate=Filters.createImageDensityFilter(candidates,ImmutableSet.of(Density.MDPI),false);
  assertFalse(candidates.isEmpty());
  for (  Path candidate : candidates) {
    assertEquals(!filesToRemove.contains(candidate),predicate.apply(candidate));
  }
}
