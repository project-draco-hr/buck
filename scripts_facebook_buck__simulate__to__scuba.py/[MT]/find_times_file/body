def find_times_file(epoch_seconds):
    date = datetime.datetime.fromtimestamp(float(epoch_seconds))
    date = datetime.datetime.now()
    for i in range(8):
        new_date = (date + datetime.timedelta(days=i))
        path = get_times_file_for_date(new_date)
        if path:
            return path
    return None
