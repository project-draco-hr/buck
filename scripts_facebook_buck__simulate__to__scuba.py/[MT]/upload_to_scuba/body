def upload_to_scuba(client, rev_info, report_path):
    sample = {u'int': {}, u'normal': {}, }
    with open(report_path) as data_file:
        report = json.load(data_file)
    add_to_scuba_sample(report, sample)
    add_to_scuba_sample(rev_info, sample)
    printf(u'Sending sample to Scuba: {0}'.format(json.dumps(sample)))
    assert (u'time' in sample[u'int']), u'sample["int"]["time"] must be defined.'
    try:
        client.add_sample(sample)
    except:
        logging.exception(u'Failed to write to scuba')
