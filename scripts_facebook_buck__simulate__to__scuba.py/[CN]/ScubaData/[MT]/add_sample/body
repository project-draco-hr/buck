def add_sample(self, sample):
    content = CONTENT_TEMPLATE.format(app_id=APP_ID, token=TOKEN, category=self.category, sample=json.dumps(sample))
    response = urllib2.urlopen(url=SCRIBE_ENDPOINT, data=content)
    status = response.getcode()
    assert (status == 200), (u'Failed to upload to scuba with HTTP Status Code: ' + str(status))
