{
  log(IAppConstants.APP_VERBOSE_BUILD_ID,Project.MSG_VERBOSE);
  if (isEnabled()) {
    while (!m_tasks.isEmpty()) {
      final NestedTask task=(NestedTask)m_tasks.remove(0);
      final String name=getTaskName();
      try {
        setTaskName(task.getTaskName());
        task.execute();
      }
  finally {
        setTaskName(name);
      }
    }
  }
}
