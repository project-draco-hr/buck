{
  ImmutableSet<BuildTarget> targetsToPrint=shouldShowTransitiveDependencies() ? getTransitiveDependencies(ImmutableSet.of(target),graph) : getImmediateDependencies(target,graph);
  if (shouldIncludeTests()) {
    ImmutableSet.Builder<BuildTarget> builder=ImmutableSet.builder();
    targetsToPrint=builder.addAll(targetsToPrint).addAll(getTestTargetDependencies(params,target,graph)).build();
  }
  return targetsToPrint;
}
