{
  BuildRuleResolver ruleResolver=new BuildRuleResolver();
  BuildRule javaRule=JavaLibraryBuilder.createBuilder(BuildTargetFactory.newInstance("//javatests:lib")).build(ruleResolver);
  return (ProjectConfig)ProjectConfigBuilder.newProjectConfigRuleBuilder(BuildTarget.builder("//javatests","project_config").build()).setSrcRule(javaRule).build(ruleResolver);
}
