{
  BuildRuleResolver ruleResolver=new BuildRuleResolver();
  BuildRule javaRule=ruleResolver.buildAndAddToIndex(DefaultJavaLibraryRule.newJavaLibraryRuleBuilder(new FakeAbstractBuildRuleBuilderParams()).setBuildTarget(BuildTargetFactory.newInstance("//javatests:lib")));
  return ProjectConfigBuilder.newProjectConfigRuleBuilder().setBuildTarget(new BuildTarget("//javatests","project_config")).setSrcRule(javaRule).buildAsBuildable();
}
