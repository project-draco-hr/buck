{
  BuildRuleResolver ruleResolver=new BuildRuleResolver();
  BuildRule javaRule=ruleResolver.buildAndAddToIndex(DefaultJavaLibrary.newJavaLibraryRuleBuilder(new FakeBuildRuleBuilderParams()).setBuildTarget(BuildTargetFactory.newInstance("//javatests:lib")));
  return ProjectConfigBuilder.newProjectConfigRuleBuilder(new BuildTarget("//javatests","project_config")).setSrcRule(javaRule).build();
}
