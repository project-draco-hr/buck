{
  ImmutableSet<ProvisioningProfileMetadata> profiles=ImmutableSet.<ProvisioningProfileMetadata>of(makeTestMetadata("AAAAAAAAAA.*",new Date(0),"00000000-0000-0000-0000-000000000000"));
  Optional<ProvisioningProfileMetadata> actual=ProvisioningProfileCopyStep.getBestProvisioningProfile(profiles,"com.facebook.test",Optional.<String>absent(),Optional.<String>absent());
  assertThat(actual,is(equalTo(Optional.<ProvisioningProfileMetadata>absent())));
}
