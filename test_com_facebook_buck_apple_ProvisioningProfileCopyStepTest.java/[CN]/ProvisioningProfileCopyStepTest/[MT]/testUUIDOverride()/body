{
  ProvisioningProfileMetadata expected=makeTestMetadata("BBBBBBBBBB.*",new Date(Long.MAX_VALUE),"11111111-1111-1111-1111-111111111111");
  ImmutableSet<ProvisioningProfileMetadata> profiles=ImmutableSet.<ProvisioningProfileMetadata>of(expected,makeTestMetadata("BBBBBBBBBB.com.facebook.test",new Date(Long.MAX_VALUE),"00000000-0000-0000-0000-000000000000"));
  Optional<ProvisioningProfileMetadata> actual=ProvisioningProfileCopyStep.getBestProvisioningProfile(profiles,"com.facebook.test",Optional.<String>of("11111111-1111-1111-1111-111111111111"),Optional.<String>absent());
  assertThat(actual.get(),is(equalTo(expected)));
}
