{
  assumeTrue(Platform.detect() == Platform.MACOS);
  thrown.expect(HumanReadableException.class);
  thrown.expectMessage("No valid non-expired provisioning profiles match for *.com.example.TestApp");
  Path emptyDir=TestDataHelper.getTestDataDirectory(this).resolve("provisioning_profiles_empty");
  ProvisioningProfileCopyStep step=new ProvisioningProfileCopyStep(projectFilesystem,testdataDir.resolve("Info.plist"),Optional.<String>absent(),Optional.<Path>absent(),ProvisioningProfileStore.fromSearchPath(emptyDir),outputFile,xcentFile);
  step.execute(executionContext);
}
