{
  buildableContext.recordArtifact(output);
  return ImmutableList.of(new MkdirStep(getProjectFilesystem(),output.getParent()),new CxxLinkStep(getProjectFilesystem().getRootPath(),linker.getCommandPrefix(getResolver()),output,args,frameworkRoots,getLibrarySearchDirectories(),getLibraryNames()),new FileScrubberStep(getProjectFilesystem(),output,linker.getScrubbers(getProjectFilesystem().getRootPath())));
}
