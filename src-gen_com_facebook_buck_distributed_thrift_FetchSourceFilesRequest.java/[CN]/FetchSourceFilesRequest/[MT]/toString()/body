{
  StringBuilder sb=new StringBuilder("FetchSourceFilesRequest(");
  boolean first=true;
  if (isSetBuildId()) {
    sb.append("buildId:");
    if (this.buildId == null) {
      sb.append("null");
    }
 else {
      sb.append(this.buildId);
    }
    first=false;
  }
  if (isSetContentHashes()) {
    if (!first)     sb.append(", ");
    sb.append("contentHashes:");
    if (this.contentHashes == null) {
      sb.append("null");
    }
 else {
      sb.append(this.contentHashes);
    }
    first=false;
  }
  sb.append(")");
  return sb.toString();
}
