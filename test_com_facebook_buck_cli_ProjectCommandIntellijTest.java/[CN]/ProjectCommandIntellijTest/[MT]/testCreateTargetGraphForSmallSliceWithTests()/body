{
  TargetGraphAndTargets targetGraphAndTargets=ProjectCommandTests.createTargetGraph(targetGraph,ProjectCommandOptions.Ide.INTELLIJ,ImmutableSet.of(bazProjectNode.getBuildTarget()),ImmutableSet.<String>of(),true);
  assertEquals(ImmutableSortedSet.<TargetNode<?>>of(bazProjectNode,bazLibNode,bazTestNode),ImmutableSortedSet.copyOf(targetGraphAndTargets.getTargetGraph().getNodes()));
}
