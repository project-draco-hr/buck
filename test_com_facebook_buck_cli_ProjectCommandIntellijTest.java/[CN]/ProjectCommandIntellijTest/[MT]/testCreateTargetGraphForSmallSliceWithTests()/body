{
  TargetGraphAndTargets targetGraphAndTargets=ProjectCommandTests.createTargetGraph(targetGraph,ProjectCommand.Ide.INTELLIJ,ImmutableSet.of(bazProjectNode.getBuildTarget()),true);
  assertEquals(ImmutableSortedSet.<TargetNode<?>>of(dummyRootBinNode,dummyProjectNode,bazProjectNode,bazLibNode,bazTestNode),ImmutableSortedSet.copyOf(targetGraphAndTargets.getTargetGraph().getNodes()));
}
