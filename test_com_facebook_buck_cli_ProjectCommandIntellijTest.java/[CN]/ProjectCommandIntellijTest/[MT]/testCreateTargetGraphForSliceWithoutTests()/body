{
  TargetGraphAndTargets targetGraphAndTargets=ProjectCommand.createTargetGraph(targetGraph,ProjectCommandOptions.Ide.INTELLIJ,ImmutableSet.of(fooProjectNode.getBuildTarget()),ImmutableSet.<String>of(),false);
  assertEquals(ImmutableSortedSet.<TargetNode<?>>of(fooProjectNode,fooBinNode,fooLibNode,barLibNode),ImmutableSortedSet.copyOf(targetGraphAndTargets.getTargetGraph().getNodes()));
}
