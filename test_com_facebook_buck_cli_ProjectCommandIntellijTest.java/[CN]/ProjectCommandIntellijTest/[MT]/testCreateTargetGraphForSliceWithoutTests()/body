{
  TargetGraphAndTargets targetGraphAndTargets=ProjectCommandTests.createTargetGraph(targetGraph,ProjectCommandOptions.Ide.INTELLIJ,ImmutableSet.of(fooProjectNode.getBuildTarget()),false);
  assertEquals(ImmutableSortedSet.<TargetNode<?>>of(fooProjectNode,fooBinNode,fooLibNode,barLibNode),ImmutableSortedSet.copyOf(targetGraphAndTargets.getTargetGraph().getNodes()));
}
