{
  TargetGraphAndTargets targetGraphAndTargets=ProjectCommandTests.createTargetGraph(targetGraph,ProjectCommandOptions.Ide.INTELLIJ,ImmutableSet.<BuildTarget>of(),ImmutableSet.<String>of(),false);
  assertEquals(ImmutableSortedSet.<TargetNode<?>>of(fooProjectNode,fooBinNode,fooLibNode,barLibNode,bazProjectNode,bazLibNode),ImmutableSortedSet.copyOf(targetGraphAndTargets.getTargetGraph().getNodes()));
}
