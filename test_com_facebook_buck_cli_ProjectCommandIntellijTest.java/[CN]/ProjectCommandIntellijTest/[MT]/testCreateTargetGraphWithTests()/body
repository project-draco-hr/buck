{
  TargetGraphAndTargets targetGraphAndTargets=ProjectCommandTests.createTargetGraph(targetGraph,ProjectCommandOptions.Ide.INTELLIJ,ImmutableSet.<BuildTarget>of(),ImmutableSet.<String>of(),true);
  assertEquals(ImmutableSortedSet.<TargetNode<?>>of(fooProjectNode,fooBinNode,fooLibNode,fooBinTestNode,fooTestNode,barLibNode,bazProjectNode,bazLibNode,bazTestNode),ImmutableSortedSet.copyOf(targetGraphAndTargets.getTargetGraph().getNodes()));
}
