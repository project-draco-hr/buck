{
  TargetGraphAndTargets targetGraphAndTargets=ProjectCommand.createTargetGraph(targetGraph,ProjectCommandOptions.Ide.INTELLIJ,ImmutableSet.of(bazProjectNode.getBuildTarget()),ImmutableSet.<String>of(),false);
  assertEquals(ImmutableSortedSet.<TargetNode<?>>of(bazProjectNode,bazLibNode),ImmutableSortedSet.copyOf(targetGraphAndTargets.getTargetGraph().getNodes()));
}
