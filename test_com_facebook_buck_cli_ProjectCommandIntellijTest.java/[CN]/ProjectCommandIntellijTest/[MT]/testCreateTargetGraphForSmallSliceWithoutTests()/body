{
  TargetGraphAndTargets targetGraphAndTargets=ProjectCommandTests.createTargetGraph(targetGraph,ProjectCommandOptions.Ide.INTELLIJ,ImmutableSet.of(bazProjectNode.getBuildTarget()),false);
  assertEquals(ImmutableSortedSet.<TargetNode<?>>of(dummyRootBinNode,dummyProjectNode,bazProjectNode,bazLibNode),ImmutableSortedSet.copyOf(targetGraphAndTargets.getTargetGraph().getNodes()));
}
