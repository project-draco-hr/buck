{
  TargetGraphAndTargets targetGraphAndTargets=ProjectCommand.createTargetGraph(targetGraph,ProjectCommandOptions.Ide.INTELLIJ,ImmutableSet.of(fooProjectNode.getBuildTarget()),ImmutableSet.<String>of(),true);
  assertEquals(ImmutableSortedSet.<TargetNode<?>>of(fooProjectNode,fooBinNode,fooLibNode,fooBinTestNode,fooTestNode,barLibNode,bazProjectNode,bazLibNode),ImmutableSortedSet.copyOf(targetGraphAndTargets.getTargetGraph().getNodes()));
}
