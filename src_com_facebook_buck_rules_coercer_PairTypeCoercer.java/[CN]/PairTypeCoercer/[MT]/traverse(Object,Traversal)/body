{
  if (object instanceof Collection) {
    Collection<?> collection=(Collection<?>)object;
    if (collection.size() != 2) {
      return false;
    }
    Iterator<?> iterator=collection.iterator();
    return firstTypeCoercer.traverse(iterator.next(),traversal) && secondTypeCoercer.traverse(iterator.next(),traversal);
  }
  return false;
}
