{
  ExportFile rule=new ExportFile(params,Optional.<String>absent(),Optional.<String>absent());
  List<Step> steps=rule.getBuildSteps(context,new FakeBuildableContext());
  MkdirAndSymlinkFileStep expected=new MkdirAndSymlinkFileStep("example.html",BuckConstant.GEN_DIR + "/example.html");
  assertEquals(ImmutableList.of(expected),steps);
}
