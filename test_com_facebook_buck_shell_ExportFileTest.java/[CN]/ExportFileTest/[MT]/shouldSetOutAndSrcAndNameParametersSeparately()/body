{
  ExportFile exportFile=new ExportFile(params,Optional.of("chips"),Optional.of("fish"));
  List<Step> steps=exportFile.getBuildSteps(context,new FakeBuildableContext());
  MoreAsserts.assertSteps("The output directory should be created and then the file should be copied there.",ImmutableList.of("mkdir -p buck-out/gen","cp chips buck-out/gen/fish"),steps,TestExecutionContext.newInstance());
  assertEquals("buck-out/gen/fish",exportFile.getPathToOutputFile());
}
