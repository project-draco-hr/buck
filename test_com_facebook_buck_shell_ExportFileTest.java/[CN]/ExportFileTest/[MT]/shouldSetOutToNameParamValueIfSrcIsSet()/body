{
  ExportFile rule=new ExportFile(params,Optional.<String>absent(),Optional.of("fish"));
  List<Step> steps=rule.getBuildSteps(context);
  MkdirAndSymlinkFileStep expected=new MkdirAndSymlinkFileStep("example.html",BuckConstant.GEN_DIR + "/fish");
  assertEquals(ImmutableList.of(expected),steps);
}
