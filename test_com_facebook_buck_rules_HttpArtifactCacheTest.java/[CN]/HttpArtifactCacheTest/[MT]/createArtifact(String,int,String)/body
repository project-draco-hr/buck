{
  try (HashingOutputStream hasher=new HashingOutputStream(HASH_FUNCTION,ByteStreams.nullOutputStream());DataOutputStream output=new DataOutputStream(hasher)){
    output.writeUTF(key);
    output.writeLong(length);
    output.write(contents.getBytes(Charsets.UTF_8));
    return createArtifact(key,length,hasher.hash(),contents);
  }
 }
