{
  resourceFilesBuilder.putAll(FluentIterable.from(resourceDescriptions).transformAndConcat(new Function<AppleResourceDescription.Arg,Set<SourcePath>>(){
    @Override public Set<SourcePath> apply(    AppleResourceDescription.Arg arg){
      return arg.files;
    }
  }
).toMap(APPLE_BUNDLE_DESTINATION_RESOURCE));
}
