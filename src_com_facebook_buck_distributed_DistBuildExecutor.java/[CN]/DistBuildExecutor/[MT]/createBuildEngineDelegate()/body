{
  if (cachingBuildEngineDelegate != null) {
    return cachingBuildEngineDelegate;
  }
  createActionGraphAndResolver();
  cachingBuildEngineDelegate=new DistributedCachingBuildEngineDelegate(new SourcePathResolver(Preconditions.checkNotNull(actionGraphAndResolver).getResolver()),args.getState(),args.getProvider());
  return cachingBuildEngineDelegate;
}
