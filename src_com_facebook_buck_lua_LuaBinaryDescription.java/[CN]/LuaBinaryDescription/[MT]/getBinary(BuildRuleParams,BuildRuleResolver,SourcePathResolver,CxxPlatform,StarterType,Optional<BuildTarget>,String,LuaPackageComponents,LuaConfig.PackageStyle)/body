{
switch (packageStyle) {
case STANDALONE:
    return getStandaloneBinary(params,resolver,pathResolver,cxxPlatform,starterType,nativeStarterLibrary,mainModule,components);
case INPLACE:
  return getInPlaceBinary(params,resolver,pathResolver,cxxPlatform,starterType,nativeStarterLibrary,mainModule,components);
}
throw new IllegalStateException(String.format("%s: unexpected package style %s",params.getBuildTarget(),packageStyle));
}
