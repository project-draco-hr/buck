{
  SourcePathResolver pathResolver=new SourcePathResolver(resolver);
  LuaPackageComponents allComponents=LuaPackageComponents.concat(getPackageComponentsFromDeps(params.getDeps()));
  SymlinkTree linkTree=resolver.addToIndex(createSymlinkTree(params,resolver,pathResolver,allComponents));
  return new LuaInPlaceBinary(params,pathResolver,luaConfig.getLua(resolver),args.mainModule,allComponents,linkTree,luaConfig.getExtension());
}
