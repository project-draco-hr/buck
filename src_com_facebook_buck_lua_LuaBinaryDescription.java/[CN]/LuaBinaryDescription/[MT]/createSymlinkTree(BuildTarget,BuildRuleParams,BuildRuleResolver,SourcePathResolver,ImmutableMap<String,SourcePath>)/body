{
  Path linkTreeRoot=params.getProjectFilesystem().resolve(BuildTargets.getGenPath(params.getProjectFilesystem(),linkTreeTarget,"%s"));
  return resolver.addToIndex(SymlinkTree.from(params.copyWithChanges(linkTreeTarget,Suppliers.ofInstance(ImmutableSortedSet.copyOf(pathResolver.filterBuildRuleInputs(components.values()))),Suppliers.ofInstance(ImmutableSortedSet.<BuildRule>of())),pathResolver,linkTreeRoot,components));
}
