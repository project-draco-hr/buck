{
  return FluentIterable.from(lexInputs).transform(new Function<Path,Path>(){
    @Override public Path apply(    Path lexInput){
      return getGeneratedSourceDir().resolve(lexInput.getFileName().toString().replaceFirst(OCamlCompilables.OCAML_MLL_REGEX,OCamlCompilables.OCAML_ML));
    }
  }
);
}
