{
  Ordering<Path> lastModifiedOrdering=new Ordering<Path>(){
    @Override public int compare(    Path left,    Path right){
      try {
        long rightModTime=projectFilesystem.getLastModifiedTime(right);
        long leftModTime=projectFilesystem.getLastModifiedTime(left);
        int result=Long.signum(rightModTime - leftModTime);
        if (result != 0) {
          return result;
        }
 else {
          return right.compareTo(left);
        }
      }
 catch (      IOException e) {
        throw Throwables.propagate(e);
      }
    }
  }
;
  ImmutableCollection<Path> traces=projectFilesystem.getDirectoryContents(BuckConstant.BUCK_TRACE_DIR);
  if (traces != null) {
    return lastModifiedOrdering.immutableSortedCopy(traces);
  }
 else {
    return ImmutableList.<Path>of();
  }
}
