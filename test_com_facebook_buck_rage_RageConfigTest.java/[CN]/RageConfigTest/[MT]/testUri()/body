{
  BuckConfig buckConfig=FakeBuckConfig.builder().setSections(ImmutableMap.of("rage",ImmutableMap.of("report_upload_url","http://foo:123/bar/baz"))).build();
  RageConfig config=RageBuckConfig.create(buckConfig);
  assertThat(config.getReportUploadUri(),Matchers.equalTo(Optional.of(URI.create("http://foo:123/bar/baz"))));
}
