{
  CxxPlatform cxxPlatform=DefaultCxxPlatforms.build(new CxxBuckConfig(FakeBuckConfig.builder().setSections(ImmutableMap.of("cxx",ImmutableMap.of("lexflags","-lex -lex","yaccflags","-yacc -yacc"))).build()));
  assertEquals(ImmutableList.of("-lex","-lex"),cxxPlatform.getLexFlags());
  assertEquals(ImmutableList.of("-yacc","-yacc"),cxxPlatform.getYaccFlags());
}
