{
  SourcePath sourcePath=sourcePathTypeCoercer.coerce(buildTargetParser,projectFilesystem,pathRelativeToProjectRoot,":hello");
  assertEquals(new BuildTargetSourcePath(projectFilesystem,BuildTarget.of(UnflavoredBuildTarget.of(Optional.<String>absent(),"//","hello"),ImmutableSortedSet.<Flavor>of())),sourcePath);
}
