{
  TTupleProtocol oprot=(TTupleProtocol)prot;
  BitSet optionals=new BitSet();
  if (struct.isSetBuildId()) {
    optionals.set(0);
  }
  if (struct.isSetDebug()) {
    optionals.set(1);
  }
  oprot.writeBitSet(optionals,2);
  if (struct.isSetBuildId()) {
    struct.buildId.write(oprot);
  }
  if (struct.isSetDebug()) {
    struct.debug.write(oprot);
  }
}
