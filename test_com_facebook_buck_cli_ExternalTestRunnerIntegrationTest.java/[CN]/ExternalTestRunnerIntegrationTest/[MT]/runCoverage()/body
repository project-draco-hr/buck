{
  ProjectWorkspace.ProcessResult result=workspace.runBuckCommand("test","-c","test.external_runner=" + workspace.getPath("test_runner_coverage.py"),"//:python-coverage");
  result.assertSuccess();
  assertThat(result.getStdout(),Matchers.startsWith("[[0.9"));
  assertThat(result.getStdout(),Matchers.endsWith(", [u'simple.py']]]\n"));
}
