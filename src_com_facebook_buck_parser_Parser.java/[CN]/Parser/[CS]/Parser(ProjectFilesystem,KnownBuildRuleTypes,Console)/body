{
  this(projectFilesystem,buildRuleTypes,console,new Supplier<BuildFileTree>(){
    @Override public BuildFileTree get(){
      return BuildFileTree.constructBuildFileTree(projectFilesystem);
    }
  }
,new BuildTargetParser(projectFilesystem),Maps.<BuildTarget,BuildRuleBuilder<?>>newHashMap(),new ProjectBuildFileParser(projectFilesystem.getIgnorePaths()));
}
