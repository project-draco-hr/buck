{
  this(repository,new Supplier<BuildFileTree>(){
    @Override public BuildFileTree get(){
      return new FilesystemBackedBuildFileTree(repository.getFilesystem());
    }
  }
,repository.getBuildTargetParser(),new DefaultProjectBuildFileParserFactory(repository.getFilesystem(),pythonInterpreter,repository.getAllDescriptions()),tempFilePatterns,ruleKeyBuilderFactory);
}
