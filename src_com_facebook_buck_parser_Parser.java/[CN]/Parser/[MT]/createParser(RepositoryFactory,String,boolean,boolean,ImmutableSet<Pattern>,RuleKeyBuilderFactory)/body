{
  final Repository rootRepository=repositoryFactory.getRootRepository();
  return new Parser(repositoryFactory,new Supplier<BuildFileTree>(){
    @Override public BuildFileTree get(){
      return new FilesystemBackedBuildFileTree(rootRepository.getFilesystem());
    }
  }
,rootRepository.getBuildTargetParser(),new DefaultProjectBuildFileParserFactory(rootRepository.getFilesystem(),pythonInterpreter,allowEmptyGlobs,rootRepository.getAllDescriptions()),tempFilePatterns,ruleKeyBuilderFactory,enforceBuckPackageBoundary);
}
