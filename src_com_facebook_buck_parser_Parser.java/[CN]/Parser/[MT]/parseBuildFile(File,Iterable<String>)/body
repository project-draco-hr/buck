{
  if (parsedBuildFiles.contains(buildFile) || parsedAllBuildFiles()) {
    return;
  }
  logger.info(String.format("Parsing %s file: %s",BuckConstant.BUILD_RULES_FILE_NAME,buildFile));
  List<Map<String,Object>> rules=buildFileParser.getAllRules(absolutePathToProjectRoot,Optional.of(buildFile.getPath()),defaultIncludes);
  parseRawRulesInternal(rules,null,buildFile);
  parsedBuildFiles.add(buildFile);
}
