{
  logger.info(String.format("Parsing %s file: %s",BuckConstant.BUILD_RULES_FILE_NAME,buildFile));
  List<Map<String,Object>> rules=com.facebook.buck.json.BuildFileToJsonParser.getAllRules(absolutePathToProjectRoot,Optional.of(buildFile.getPath()),defaultIncludes,ansi);
  parseRawRulesInternal(rules,null,buildFile);
  parsedBuildFiles.add(buildFile);
}
