{
  try (PerBuildState state=new PerBuildState(permState,marshaller,eventBus,executor,rootCell,enableProfiling,SpeculativeParsing.of(true))){
    ImmutableSet<BuildTarget> buildTargets=resolveTargetSpecs(state,eventBus,rootCell,targetNodeSpecs);
    TargetGraph graph=buildTargetGraph(state,eventBus,buildTargets);
    return new Pair<>(buildTargets,graph);
  }
 }
