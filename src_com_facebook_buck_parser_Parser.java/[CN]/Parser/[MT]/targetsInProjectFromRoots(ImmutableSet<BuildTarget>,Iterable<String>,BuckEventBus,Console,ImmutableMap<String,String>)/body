{
  ActionGraph actionGraph=parseBuildFilesForTargets(roots,defaultIncludes,eventBus,console,environment);
  return ImmutableSet.copyOf(FluentIterable.from(actionGraph.getNodes()).transform(new Function<BuildRule,BuildTarget>(){
    @Override public BuildTarget apply(    BuildRule input){
      return input.getBuildTarget();
    }
  }
));
}
