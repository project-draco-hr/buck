{
  TargetGraph graph=null;
  try (ProjectBuildFileParser buildFileParser=buildFileParserFactory.createParser(defaultIncludes,console,environment,eventBus)){
    buildFileParser.setEnableProfiling(enableProfiling);
    ImmutableSet<BuildTarget> buildTargets=resolveTargetSpecs(targetNodeSpecs,defaultIncludes,buildFileParser,environment);
    postParseStartEvent(buildTargets,eventBus);
    try {
      graph=buildTargetGraph(buildTargets,defaultIncludes,buildFileParser,environment,eventBus);
      return graph;
    }
  finally {
      eventBus.post(ParseEvent.finished(buildTargets,Optional.fromNullable(graph)));
    }
  }
 }
