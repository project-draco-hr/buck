{
  ImmutableList.Builder<Step> steps=ImmutableList.builder();
  Path binPath=getBinPath();
  steps.add(new MkdirStep(binPath.getParent()));
  steps.add(new PexStep(pathToPex,binPath,PythonUtil.toModuleName(getBuildTarget(),main.toString()),PythonUtil.getPathMapFromSourcePaths(components.getModules()),PythonUtil.getPathMapFromSourcePaths(components.getResources())));
  buildableContext.recordArtifact(getBinPath());
  return steps.build();
}
