{
  GlobalSearchScope scope=GlobalSearchScope.allScope(mProject);
  PsiClass psiClass=JavaPsiFacade.getInstance(mProject).findClass(getImportedClassFromImportLine(importClassLine),scope);
  return psiClass == null ? Optional.<VirtualFile>absent() : Optional.fromNullable(psiClass.getContainingFile().getVirtualFile());
}
