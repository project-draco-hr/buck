{
  JavaFileParser parser=JavaFileParser.createJavaFileParser(DEFAULT_JAVAC_OPTIONS);
  ImmutableSortedSet<String> symbols=parser.getExportedSymbolsFromString(javaCodeWithManyClasses);
  assertEquals("JavaFileParser didn't find the symbols we expected.",ImmutableSortedSet.of("com.example.AnotherOuterClass","com.example.Example","com.example.Example.InnerEnum","com.example.Example.InnerEnum.InnerClass","com.example.Example.InnerInterface"),symbols);
}
