{
  String stdout="hello world";
  CxxTestStep step=new CxxTestStep(filesystem,ImmutableList.of("echo",stdout),ImmutableMap.<String,String>of(),exitCode,output,Optional.<Long>absent());
  step.execute(context);
  assertSame(0,readExitCode(exitCode));
  assertContents(output,stdout + System.lineSeparator());
}
