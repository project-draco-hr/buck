{
  stackTrace=Throwables.getStackTraceAsString(new Exception("Ouch"));
  successTest=new TestResultSummary("com.example.FooTest","successTest",true,500,null,null,"good stdout","good stderr");
  failingTest=new TestResultSummary("com.example.FooTest","failTest",false,200,"Unexpected fish found",stackTrace,"bad stdout","bad stderr");
}
