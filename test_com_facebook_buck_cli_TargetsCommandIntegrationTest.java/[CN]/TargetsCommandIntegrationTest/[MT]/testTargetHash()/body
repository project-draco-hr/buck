{
  ProjectWorkspace workspace=TestDataHelper.createProjectWorkspaceForScenario(this,"output_path",tmp);
  workspace.setUp();
  ProcessResult result=workspace.runBuckCommand("targets","--show-target-hash","//:test");
  result.assertSuccess();
  assertEquals("//:test 1ee8b3a59144222d0bb9aa886aef036950b93e6a\n",result.getStdout());
}
