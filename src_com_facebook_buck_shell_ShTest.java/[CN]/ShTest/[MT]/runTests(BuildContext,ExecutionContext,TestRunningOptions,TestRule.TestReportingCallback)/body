{
  if (options.isDryRun()) {
    return ImmutableList.of();
  }
  Step mkdirClean=new MakeCleanDirectoryStep(getProjectFilesystem(),getPathToTestOutputDirectory());
  Step runTest=new RunShTestAndRecordResultStep(getProjectFilesystem(),getResolver().getAbsolutePath(test),args,getPathToTestOutputResult());
  return ImmutableList.of(mkdirClean,runTest);
}
