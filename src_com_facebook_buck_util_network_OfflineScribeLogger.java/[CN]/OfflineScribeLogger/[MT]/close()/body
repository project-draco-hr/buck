{
  startedClosing=true;
synchronized (this) {
    try {
      deleteOldLogsIfNeeded(maxScribeOfflineLogsBytes - bytesStoredSoFar);
    }
 catch (    Exception e) {
      LOG.error(e,"Failed to cleanup logs.");
    }
    if (logFileStoreStream != null) {
      logFileStoreStream.close();
    }
    storingAvailable=false;
  }
  scribeLogger.close();
}
