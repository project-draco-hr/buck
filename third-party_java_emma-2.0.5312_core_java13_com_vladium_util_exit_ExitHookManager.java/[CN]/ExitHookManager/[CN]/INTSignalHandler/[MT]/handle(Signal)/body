{
  if (m_runnable != null) {
    try {
      m_runnable.run();
    }
 catch (    Throwable ignore) {
    }
  }
  m_runnable=null;
  if ((m_previous != null) && (m_previous != SIG_DFL) && (m_previous != SIG_IGN)) {
    try {
      m_previous.handle(signal);
    }
 catch (    Throwable ignore) {
    }
  }
 else {
    System.exit(0);
  }
}
