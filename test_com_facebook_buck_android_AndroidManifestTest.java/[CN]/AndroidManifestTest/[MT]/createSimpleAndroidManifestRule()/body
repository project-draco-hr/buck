{
  BuildRuleParams buildRuleParams=BuildRuleParamsFactory.createTrivialBuildRuleParams(new BuildTarget("//java/com/example","manifest"));
  AndroidManifest.Builder builder=AndroidManifest.newManifestMergeRuleBuilder(new FakeAbstractBuildRuleBuilderParams());
  final AndroidManifest androidManifest=builder.setSkeletonFile("java/com/example/AndroidManifestSkeleton.xml").newBuildable(buildRuleParams,new BuildRuleResolver());
  return new FakeBuildRule(BuildRuleType.ANDROID_MANIFEST,buildRuleParams){
    @Override public Buildable getBuildable(){
      return androidManifest;
    }
  }
;
}
