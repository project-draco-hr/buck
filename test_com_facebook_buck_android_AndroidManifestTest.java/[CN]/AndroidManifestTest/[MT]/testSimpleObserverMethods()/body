{
  BuildRule buildRule=createSimpleAndroidManifestRule();
  AndroidManifest androidManifestRule=(AndroidManifest)buildRule.getBuildable();
  assertEquals(ImmutableList.of("java/com/example/AndroidManifestSkeleton.xml"),androidManifestRule.getInputsToCompareToOutput());
  assertEquals(BuckConstant.GEN_DIR + "/java/com/example/AndroidManifest__manifest__.xml",androidManifestRule.getPathToOutputFile());
}
