{
  workspace.runBuckBuild("//Libraries/EXExample:EXExample#headers").assertSuccess();
  String pathToHeaders="buck-out/bin/Libraries/EXExample/__EXExample#headers_public_headers__";
  File headersDirectory=workspace.getFile(pathToHeaders);
  assertTrue("public header should be available in " + headersDirectory + ".",new File(headersDirectory,"EXExample/EXExampleModel.h").exists());
  assertFalse("non-public header should not be available in " + headersDirectory + ".",new File(headersDirectory,"EXExample/NSString+Palindrome.h").exists());
}
