{
  this.url=new URL(url);
  LinkedBlockingQueue<Runnable> workQueue=new LinkedBlockingQueue<>(maxParallelRequests);
  ExecutorService executor=new ThreadPoolExecutor(maxParallelRequests,maxParallelRequests,2L,TimeUnit.MINUTES,workQueue,new ThreadPoolExecutor.CallerRunsPolicy());
  requestService=MoreExecutors.listeningDecorator(executor);
}
