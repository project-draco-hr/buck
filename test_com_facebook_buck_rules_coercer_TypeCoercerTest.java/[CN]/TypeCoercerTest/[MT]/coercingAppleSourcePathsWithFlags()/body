{
  Type type=TestFields.class.getField("listOfAppleSources").getGenericType();
  TypeCoercer<?> coercer=typeCoercerFactory.typeCoercerForType(type);
  ImmutableList<?> input=ImmutableList.of(ImmutableList.of("foo.m","-Wall"),ImmutableList.of("bar.m","-fobjc-arc"));
  Object result=coercer.coerce(targetParser,filesystem,Paths.get(""),input);
  ImmutableList<AppleSource> expectedResult=ImmutableList.of(AppleSource.of(new TestSourcePath("foo.m"),"-Wall"),AppleSource.of(new TestSourcePath("bar.m"),"-fobjc-arc"));
  assertEquals(expectedResult,result);
}
