{
  Type type=TestFields.class.getField("listOfSourcesWithFlags").getGenericType();
  TypeCoercer<?> coercer=typeCoercerFactory.typeCoercerForType(type);
  ImmutableList<?> input=ImmutableList.of("Group1/foo.m",ImmutableList.of("Group1/bar.m",ImmutableList.of("-Wall","-Werror")),"Group2/baz.m",ImmutableList.of("Group2/blech.m",ImmutableList.of("-fobjc-arc")));
  Object result=coercer.coerce(filesystem,Paths.get(""),input);
  ImmutableList<SourceWithFlags> expectedResult=ImmutableList.of(SourceWithFlags.of(new TestSourcePath("Group1/foo.m")),SourceWithFlags.of(new TestSourcePath("Group1/bar.m"),ImmutableList.of("-Wall","-Werror")),SourceWithFlags.of(new TestSourcePath("Group2/baz.m")),SourceWithFlags.of(new TestSourcePath("Group2/blech.m"),ImmutableList.of("-fobjc-arc")));
  assertEquals(expectedResult,result);
}
