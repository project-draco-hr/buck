{
  ProjectFilesystem projectFilesystem=createMock(ProjectFilesystem.class);
  ReadKeystorePropertiesAndSignApkCommand readKeystorePropertiesAndSignCommand=new ReadKeystorePropertiesAndSignApkCommand("src/com/facebook/orca/keystore.properties",BIN_DIR + "/src/com/facebook/orca/orca_unsigned.apk",BIN_DIR + "/src/com/facebook/orca/orca_signed.apk",projectFilesystem);
  ExecutionContext context=createMock(ExecutionContext.class);
  replay(context,projectFilesystem);
  assertEquals("sign apk",readKeystorePropertiesAndSignCommand.getShortName(context));
  verify(context,projectFilesystem);
}
