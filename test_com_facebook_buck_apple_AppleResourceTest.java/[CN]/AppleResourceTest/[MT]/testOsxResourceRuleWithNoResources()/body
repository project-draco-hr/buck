{
  AppleResourceDescriptionArg args=new AppleResourceDescriptionArg();
  args.dirs=ImmutableSortedSet.of();
  args.files=ImmutableSortedSet.of();
  args.variants=Optional.absent();
  AppleResource appleResource=new AppleResource(new FakeBuildRuleParamsBuilder(BuildTarget.builder("//path/to/app","MyApp").build()).build(),new FakeDirectoryTraverser(),args,Optional.of(Paths.get("Resources")));
  List<Step> steps=appleResource.getBuildSteps(FakeBuildContext.NOOP_CONTEXT,new FakeBuildableContext());
  assertEquals(0,steps.size());
}
