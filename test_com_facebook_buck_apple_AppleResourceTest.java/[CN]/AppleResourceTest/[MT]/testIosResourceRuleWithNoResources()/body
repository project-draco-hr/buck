{
  FakeBuildRuleParams buildRuleParams=new FakeBuildRuleParams(new BuildTarget("//path/to/app","MyApp"));
  AppleResourceDescriptionArg args=new AppleResourceDescriptionArg();
  args.resources=ImmutableSortedSet.of();
  AppleResource appleResource=new AppleResource(buildRuleParams,args,Optional.<Path>absent());
  List<Step> steps=appleResource.getBuildSteps(FakeBuildContext.NOOP_CONTEXT,new FakeBuildableContext());
  assertEquals(0,steps.size());
}
