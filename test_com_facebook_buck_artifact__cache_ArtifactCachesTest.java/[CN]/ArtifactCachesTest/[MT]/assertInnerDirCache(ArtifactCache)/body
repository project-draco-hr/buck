{
  assertThat(artifactCache,Matchers.instanceOf(LoggingArtifactCacheDecorator.class));
  LoggingArtifactCacheDecorator cacheDecorator=(LoggingArtifactCacheDecorator)artifactCache;
  assertThat(cacheDecorator.getDelegate(),Matchers.instanceOf(LoggingArtifactCacheDecorator.class));
  LoggingArtifactCacheDecorator dirCacheDecorator=(LoggingArtifactCacheDecorator)cacheDecorator.getDelegate();
  assertThat(dirCacheDecorator.getDelegate(),Matchers.instanceOf(DirArtifactCache.class));
}
