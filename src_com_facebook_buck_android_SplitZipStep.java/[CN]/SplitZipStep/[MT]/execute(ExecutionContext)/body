{
  try {
    File primaryJarFile=new File(primaryJarPath);
    Collection<File> secondaryZips=ZipSplitter.splitZip(ImmutableSet.copyOf(Paths.transformPathToFile(inputPathsToSplit)),primaryJarFile,new File(secondaryJarDir),secondaryJarPattern,ZIP_SIZE_SOFT_LIMIT,ZIP_SIZE_HARD_LIMIT,requiredInPrimaryZip,dexSplitStrategy);
    BufferedWriter secondaryMetaInfoWriter=Files.newWriter(new File(secondaryJarMetaPath),Charsets.UTF_8);
    try {
      writeMetaList(secondaryMetaInfoWriter,secondaryZips);
    }
  finally {
      secondaryMetaInfoWriter.close();
    }
    return 0;
  }
 catch (  IOException e) {
    e.printStackTrace(context.getStdErr());
    return 1;
  }
}
