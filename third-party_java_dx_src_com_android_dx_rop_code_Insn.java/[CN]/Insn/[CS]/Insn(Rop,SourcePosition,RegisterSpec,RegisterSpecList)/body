{
  if (opcode == null) {
    throw new NullPointerException("opcode == null");
  }
  if (position == null) {
    throw new NullPointerException("position == null");
  }
  if (sources == null) {
    throw new NullPointerException("sources == null");
  }
  this.opcode=opcode;
  this.position=position;
  this.result=result;
  this.sources=sources;
}
