{
  if (this == other) {
    return true;
  }
  if (!(other instanceof ProjectFilesystem)) {
    return false;
  }
  ProjectFilesystem that=(ProjectFilesystem)other;
  return Objects.equals(projectRoot,that.projectRoot) && Objects.equals(whiteListedPaths,that.whiteListedPaths) && Objects.equals(blackListedPaths,that.blackListedPaths);
}
