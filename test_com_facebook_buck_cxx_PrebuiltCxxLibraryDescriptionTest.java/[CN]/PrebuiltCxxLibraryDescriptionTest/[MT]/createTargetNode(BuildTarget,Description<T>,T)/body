{
  BuildRuleFactoryParams params=new BuildRuleFactoryParams(new FakeProjectFilesystem(),new BuildTargetParser(),target,new FakeRuleKeyBuilderFactory(),new InMemoryBuildFileTree(ImmutableList.<BuildTarget>of()),true);
  try {
    return new TargetNode<>(description,arg,params,ImmutableSet.<BuildTarget>of(),ImmutableSet.<BuildTargetPattern>of());
  }
 catch (  NoSuchBuildTargetException|TargetNode.InvalidSourcePathInputException e) {
    throw Throwables.propagate(e);
  }
}
