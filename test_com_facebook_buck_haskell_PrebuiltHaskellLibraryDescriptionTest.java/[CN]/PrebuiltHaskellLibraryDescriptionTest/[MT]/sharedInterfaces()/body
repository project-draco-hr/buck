{
  BuildRuleResolver resolver=new BuildRuleResolver(TargetGraph.EMPTY,new DefaultTargetNodeToBuildRuleTransformer());
  SourcePath interfaces=new FakeSourcePath("interfaces");
  BuildTarget target=BuildTargetFactory.newInstance("//:rule");
  PrebuiltHaskellLibrary library=(PrebuiltHaskellLibrary)new PrebuiltHaskellLibraryBuilder(target).setSharedInterfaces(interfaces).build(resolver);
  HaskellCompileInput input=library.getCompileInput(CxxPlatformUtils.DEFAULT_PLATFORM,CxxSourceRuleFactory.PicType.PIC);
  assertThat(input.getIncludes(),Matchers.contains(interfaces));
}
