{
  ImmutableList.Builder<Step> steps=ImmutableList.builder();
  steps.add(new MkdirStep(getProjectFilesystem(),resultDir));
  steps.add(new StripStep(getProjectFilesystem().getRootPath(),stripTool.getCommandPrefix(getResolver()),ImmutableList.of("--strip-unneeded"),getResolver().getAbsolutePath(sourcePathToStrip),getPathToOutput()));
  buildableContext.recordArtifact(getPathToOutput());
  return steps.build();
}
