{
  try (BufferedOutputStream baseOut=new BufferedOutputStream(outputStream);CustomZipOutputStream out=ZipOutputStreams.newOutputStream(baseOut,OVERWRITE_EXISTING)){
    addFilesToArchive(out,defectReport.getIncludedPaths());
    out.putNextEntry(new CustomZipEntry(REPORT_FILE_NAME));
    objectMapper.writeValue(out,defectReport);
  }
 }
