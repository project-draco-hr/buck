{
  assertTrue("Should be empty",reportMerger.isReportEmpty("[]"));
  assertTrue("Should be empty",reportMerger.isReportEmpty("[ ]"));
  assertTrue("Should be empty",reportMerger.isReportEmpty("  [ ]   "));
  assertTrue("Should be empty",reportMerger.isReportEmpty("  \n[ \n\t \n]  \n"));
  assertFalse("Should not be empty",reportMerger.isReportEmpty("[{\"a\":2,\"b\":\"aaa\nbbb\"}]"));
  assertFalse("Should not be empty",reportMerger.isReportEmpty("[\n\n { \"a\":2,\"b\":\"aaa\nbbb\"  }\t]"));
}
