def test_str(self):
    p = self.cls('a/b/c')
    self.assertEqual(str(p), 'a\\b\\c')
    p = self.cls('c:/a/b/c')
    self.assertEqual(str(p), 'c:\\a\\b\\c')
    p = self.cls('//a/b')
    self.assertEqual(str(p), '\\\\a\\b\\')
    p = self.cls('//a/b/c')
    self.assertEqual(str(p), '\\\\a\\b\\c')
    p = self.cls('//a/b/c/d')
    self.assertEqual(str(p), '\\\\a\\b\\c\\d')
