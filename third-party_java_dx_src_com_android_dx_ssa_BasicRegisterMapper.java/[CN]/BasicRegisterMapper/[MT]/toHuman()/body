{
  StringBuilder sb=new StringBuilder();
  sb.append("Old\tNew\n");
  int sz=oldToNew.size();
  for (int i=0; i < sz; i++) {
    sb.append(i);
    sb.append('\t');
    sb.append(oldToNew.get(i));
    sb.append('\n');
  }
  sb.append("new reg count:");
  sb.append(runningCountNewRegisters);
  sb.append('\n');
  return sb.toString();
}
