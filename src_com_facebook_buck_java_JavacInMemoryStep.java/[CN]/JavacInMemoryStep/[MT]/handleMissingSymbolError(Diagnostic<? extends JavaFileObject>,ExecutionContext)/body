{
  if (!invokingRule.isPresent()) {
    return;
  }
  JavacErrorParser javacErrorParser=new JavacErrorParser(context.getProjectFilesystem(),context.getJavaPackageFinder());
  Optional<String> symbol=javacErrorParser.getMissingSymbolFromCompilerError(diagnostic.toString());
  if (!symbol.isPresent()) {
    return;
  }
  MissingSymbolEvent event=MissingSymbolEvent.create(invokingRule.get(),symbol.get(),MissingSymbolEvent.SymbolType.Java);
  context.getBuckEventBus().post(event);
}
