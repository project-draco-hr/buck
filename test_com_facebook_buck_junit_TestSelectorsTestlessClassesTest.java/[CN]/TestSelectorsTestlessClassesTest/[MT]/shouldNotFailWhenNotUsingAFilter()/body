{
  ProjectWorkspace.ProcessResult result=workspace.runBuckCommand("test","--all");
  result.assertSuccess("Testless classes should not cause NoTestsRemainException, " + "when filtering is *NOT* used!");
  assertThat(result.getStderr(),containsString("NOTESTS <100ms  0 Passed   0 Skipped   0 Failed   com.example.ClassWithoutTestsA"));
  assertThat(result.getStderr(),containsString("NOTESTS <100ms  0 Passed   0 Skipped   0 Failed   com.example.ClassWithoutTestsB"));
}
