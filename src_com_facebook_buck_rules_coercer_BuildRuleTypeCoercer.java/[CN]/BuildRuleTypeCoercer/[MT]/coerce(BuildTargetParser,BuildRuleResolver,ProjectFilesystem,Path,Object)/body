{
  try {
    BuildTarget buildTarget=buildTargetTypeCoercer.coerce(buildTargetParser,buildRuleResolver,filesystem,pathRelativeToProjectRoot,object);
    BuildRule rule=buildRuleResolver.get(buildTarget);
    if (rule == null) {
      throw CoerceFailedException.simple(object,getOutputClass(),"No build rule found");
    }
    return rule;
  }
 catch (  CoerceFailedException e) {
    throw CoerceFailedException.simple(object,getOutputClass());
  }
}
