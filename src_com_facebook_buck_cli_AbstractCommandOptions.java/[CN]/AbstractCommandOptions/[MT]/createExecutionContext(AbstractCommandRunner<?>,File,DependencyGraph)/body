{
  Optional<AndroidPlatformTarget> androidPlatformTarget=Build.findAndroidPlatformTarget(dependencyGraph,findAndroidSdkDir(),commandRunner.stdErr);
  return new ExecutionContext(getVerbosity(),projectDirectoryRoot,androidPlatformTarget,findAndroidNdkDir(),commandRunner.ansi,false,false,commandRunner.stdOut,commandRunner.stdErr);
}
