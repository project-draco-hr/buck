{
  TTupleProtocol iprot=(TTupleProtocol)prot;
  BitSet incoming=iprot.readBitSet(2);
  if (incoming.get(0)) {
    struct.contentHash=iprot.readString();
    struct.setContentHashIsSet(true);
  }
  if (incoming.get(1)) {
    struct.content=iprot.readBinary();
    struct.setContentIsSet(true);
  }
}
