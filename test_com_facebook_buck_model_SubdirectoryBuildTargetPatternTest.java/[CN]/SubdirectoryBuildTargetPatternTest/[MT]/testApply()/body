{
  SubdirectoryBuildTargetPattern pattern=new SubdirectoryBuildTargetPattern("src/com/facebook/buck/");
  assertFalse(pattern.apply(null));
  assertTrue(pattern.apply(BuildTargetFactory.newInstance("//src/com/facebook/buck:buck")));
  assertTrue(pattern.apply(BuildTargetFactory.newInstance("//src/com/facebook/buck/bar:bar")));
  assertFalse(pattern.apply(BuildTargetFactory.newInstance("//src/com/facebook/foo:foo")));
}
