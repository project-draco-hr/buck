{
  try {
    if (shouldRecurse) {
      context.getProjectFilesystem().copyFolder(source,destination);
    }
 else {
      context.getProjectFilesystem().copyFile(source,destination);
    }
    return 0;
  }
 catch (  IOException e) {
    e.printStackTrace(context.getStdErr());
    return 1;
  }
}
