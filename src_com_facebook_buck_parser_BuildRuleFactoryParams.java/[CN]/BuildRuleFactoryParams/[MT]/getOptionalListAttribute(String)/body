{
  Object value=instance.get(attributeName);
  if (value != null) {
    if (value instanceof List) {
      return (List<String>)value;
    }
 else {
      throw new RuntimeException(String.format("Expected an array for %s in %s but was %s",attributeName,target.getBuildFile().getPath(),value));
    }
  }
 else {
    return ImmutableList.of();
  }
}
