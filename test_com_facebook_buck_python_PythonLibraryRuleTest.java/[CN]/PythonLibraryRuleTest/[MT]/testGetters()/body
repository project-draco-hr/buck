{
  BuildRuleParams buildRuleParams=new BuildRuleParams(BuildTargetFactory.newInstance("//scripts/python:foo"),ImmutableSortedSet.<BuildRule>of(),ImmutableSet.<BuildTargetPattern>of());
  ImmutableSortedSet<String> srcs=ImmutableSortedSet.of("");
  PythonLibraryRule pythonLibraryRule=new PythonLibraryRule(buildRuleParams,srcs);
  assertTrue(pythonLibraryRule.isLibrary());
  assertSame(srcs,pythonLibraryRule.getPythonSrcs());
}
