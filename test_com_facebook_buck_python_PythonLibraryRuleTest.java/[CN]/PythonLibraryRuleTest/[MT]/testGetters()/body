{
  CachingBuildRuleParams cachingBuildRuleParams=new CachingBuildRuleParams(BuildTargetFactory.newInstance("//scripts/python:foo"),ImmutableSortedSet.<BuildRule>of(),ImmutableSet.<BuildTargetPattern>of(),new NoopArtifactCache());
  ImmutableSortedSet<String> srcs=ImmutableSortedSet.of("");
  PythonLibraryRule pythonLibraryRule=new PythonLibraryRule(cachingBuildRuleParams,srcs);
  assertTrue(pythonLibraryRule.isLibrary());
  assertSame(srcs,pythonLibraryRule.getPythonSrcs());
}
