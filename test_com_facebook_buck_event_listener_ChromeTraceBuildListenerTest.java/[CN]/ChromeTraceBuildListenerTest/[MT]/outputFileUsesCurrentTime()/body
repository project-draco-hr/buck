{
  ProjectFilesystem projectFilesystem=new ProjectFilesystem(tmpDir.getRoot().toPath());
  BuildId buildId=new BuildId("BUILD_ID");
  ChromeTraceBuildListener listener=new ChromeTraceBuildListener(projectFilesystem,buildId,new FakeClock(1409702151000000000L),ObjectMappers.newDefaultInstance(),Locale.US,TimeZone.getTimeZone("America/Los_Angeles"),1,false);
  listener.outputTrace(buildId);
  assertTrue(projectFilesystem.exists(Paths.get("buck-out/log/traces/build.2014-09-02.16-55-51.BUILD_ID.trace")));
}
