{
  if (format == null) {
    throw new NullPointerException("format == null");
  }
  if (!Opcodes.isValidShape(opcode)) {
    throw new IllegalArgumentException("invalid opcode");
  }
  this.format=format;
  this.opcode=opcode;
  this.index=index;
  this.indexType=indexType;
  this.target=target;
  this.literal=literal;
}
