{
  this.distBuildConfig=distBuildConfig;
  try {
    HttpLoadBalancer client=distBuildConfig.getFrontendConfig().createHttpClientSideSlb(new DefaultClock(),eventBus);
    this.frontendUrl=client.getBestServer().toString();
  }
 catch (  NoHealthyServersException ex) {
    LOG.error("No healthy distributed build frontend server found. " + "Do you have them listed in your .buckconfig or .buckconfig.local?");
    throw ex;
  }
}
