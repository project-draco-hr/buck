{
  FrontendResponse response=service.makeRequest(request);
  Preconditions.checkState(response.isSetWasSuccessful());
  if (!response.wasSuccessful) {
    throw new IOException(String.format("Distributed build frontend request of type [%s] failed with error message [%s].",request.type.toString(),response.getErrorMessage()));
  }
  Preconditions.checkState(request.getType().equals(response.getType()));
  return response;
}
