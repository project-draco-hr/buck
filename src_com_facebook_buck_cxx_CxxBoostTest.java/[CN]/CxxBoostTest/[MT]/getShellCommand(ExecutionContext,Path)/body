{
  return ImmutableList.<String>builder().addAll(executable.getCommandPrefix(getResolver())).add("--log_format=hrf").add("--log_level=test_suite").add("--report_format=xml").add("--report_level=detailed").add("--result_code=no").add("--report_sink=" + context.getProjectFilesystem().resolve(output)).build();
}
