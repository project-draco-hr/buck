{
  ImmutableList.Builder<Step> commands=ImmutableList.builder();
  commands.add(new RmStep(pathToOutputFile,true,false));
  commands.add(new MkdirStep(pathToOutputFile.getParent()));
  commands.add(new GenerateManifestStep(getResolver().getPath(skeletonFile),ImmutableSet.copyOf(getResolver().getAllPaths(manifestFiles)),getPathToOutput()));
  buildableContext.recordArtifact(pathToOutputFile);
  return commands.build();
}
