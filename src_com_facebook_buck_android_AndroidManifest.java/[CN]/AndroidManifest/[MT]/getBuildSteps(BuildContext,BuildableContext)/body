{
  ImmutableList.Builder<Step> commands=ImmutableList.builder();
  commands.add(new RmStep(getProjectFilesystem(),pathToOutputFile,true,false));
  commands.add(new MkdirStep(getProjectFilesystem(),pathToOutputFile.getParent()));
  commands.add(new GenerateManifestStep(getProjectFilesystem(),getResolver().deprecatedGetPath(skeletonFile),ImmutableSet.copyOf(getResolver().deprecatedAllPaths(manifestFiles)),getPathToOutput()));
  buildableContext.recordArtifact(pathToOutputFile);
  return commands.build();
}
