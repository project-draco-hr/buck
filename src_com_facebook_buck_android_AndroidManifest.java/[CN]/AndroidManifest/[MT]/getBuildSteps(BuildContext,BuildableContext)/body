{
  ImmutableList.Builder<Step> commands=ImmutableList.builder();
  commands.add(new RmStep(pathToOutputFile,true,false));
  commands.add(new MkdirStep(pathToOutputFile.getParent()));
  commands.add(new GenerateManifestStep(skeletonFile.resolve(context),manifestFiles,getPathToOutputFile()));
  return commands.build();
}
