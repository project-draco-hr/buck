{
  super(params,resolver);
  Preconditions.checkArgument(linkRule instanceof CxxLink || linkRule instanceof CxxStrip,"CxxBinary (%s) link rule (%s) is expected to be instance of either CxxLink or CxxStrip");
  Preconditions.checkArgument(getDeps().contains(linkRule),"CxxBinary (%s) must depend on its link rule (%s) via deps");
  this.params=params;
  this.ruleResolver=ruleResolver;
  this.linkRule=linkRule;
  this.executable=executable;
  this.tests=ImmutableSortedSet.copyOf(tests);
  this.frameworks=ImmutableSortedSet.copyOf(frameworks);
}
