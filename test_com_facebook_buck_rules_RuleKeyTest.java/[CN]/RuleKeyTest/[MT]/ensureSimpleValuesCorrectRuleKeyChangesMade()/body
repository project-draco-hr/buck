{
  SourcePathResolver resolver=new SourcePathResolver(new BuildRuleResolver(TargetGraph.EMPTY,new BuildTargetNodeToBuildRuleTransformer()));
  RuleKey reflective=createEmptyRuleKey(resolver).setReflectively("long",42L).setReflectively("boolean",true).setReflectively("path",Paths.get("location","of","the","rebel","plans")).build();
  RuleKey manual=createEmptyRuleKey(resolver).setReflectively("long",42L).setReflectively("boolean",true).setReflectively("path",Paths.get("location","of","the","rebel","plans")).build();
  assertEquals(manual,reflective);
}
