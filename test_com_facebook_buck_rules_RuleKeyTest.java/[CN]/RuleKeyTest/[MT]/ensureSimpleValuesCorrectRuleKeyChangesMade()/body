{
  RuleKey.Builder.RuleKeyPair reflective=createEmptyRuleKey().setReflectively("long",42L).setReflectively("boolean",true).setReflectively("path",Paths.get("location","of","the","rebel","plans")).build();
  RuleKey.Builder.RuleKeyPair manual=createEmptyRuleKey().set("long",42L).set("boolean",true).setInput("path",Paths.get("location","of","the","rebel","plans")).build();
  assertEquals(manual.getTotalRuleKey(),reflective.getTotalRuleKey());
}
