{
  RuleKey reflective=createEmptyRuleKey(new SourcePathResolver(new BuildRuleResolver())).setReflectively("long",42L).setReflectively("boolean",true).setReflectively("path",Paths.get("location","of","the","rebel","plans")).build();
  RuleKey manual=createEmptyRuleKey(new SourcePathResolver(new BuildRuleResolver())).setReflectively("long",42L).setReflectively("boolean",true).setReflectively("path",Paths.get("location","of","the","rebel","plans")).build();
  assertEquals(manual,reflective);
}
