{
  assertEquals(createEmptyRuleKey(new SourcePathResolver(new BuildRuleResolver())).setInput("key",new PathSourcePath(Paths.get("something"))).build(),createEmptyRuleKey(new SourcePathResolver(new BuildRuleResolver())).setInput("key",new PathSourcePath(Paths.get("something","else"))).build());
  assertNotEquals(createEmptyRuleKey(new SourcePathResolver(new BuildRuleResolver())).setInput("key",new PathSourcePath(Paths.get("something"))).build(),createEmptyRuleKey(new SourcePathResolver(new BuildRuleResolver())).setInput("different-key",new PathSourcePath(Paths.get("something"))).build());
}
