{
  assertEquals(createEmptyRuleKey().setInput("key",new PathSourcePath(Paths.get("something"))).build(),createEmptyRuleKey().setInput("key",new PathSourcePath(Paths.get("something","else"))).build());
  assertNotEquals(createEmptyRuleKey().setInput("key",new PathSourcePath(Paths.get("something"))).build(),createEmptyRuleKey().setInput("different-key",new PathSourcePath(Paths.get("something"))).build());
}
