{
  RuleKeyPair keyPair1=createEmptyRuleKey(new SourcePathResolver(new BuildRuleResolver())).setReflectively("something","foo").build();
  RuleKeyPair keyPair2=createEmptyRuleKey(new SourcePathResolver(new BuildRuleResolver())).setReflectively("something","foo").build();
  RuleKeyPair keyPair3=createEmptyRuleKey(new SourcePathResolver(new BuildRuleResolver())).setReflectively("something","bar").build();
  assertEquals(keyPair1,keyPair2);
  assertEquals(keyPair1.hashCode(),keyPair2.hashCode());
  assertNotEquals(keyPair1,keyPair3);
  assertNotEquals(keyPair1.hashCode(),keyPair3.hashCode());
  assertNotEquals(keyPair2,keyPair3);
  assertNotEquals(keyPair2.hashCode(),keyPair3.hashCode());
}
