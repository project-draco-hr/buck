{
switch (field) {
case PATH:
    return getPath();
case ARCHIVE_MEMBER_PATH:
  return getArchiveMemberPath();
case HASH_CODE:
return getHashCode();
case IS_DIRECTORY:
return isIsDirectory();
case PATH_IS_ABSOLUTE:
return isPathIsAbsolute();
case CONTENTS:
return getContents();
}
throw new IllegalStateException();
}
