{
  AndroidResource androidResource=new AndroidResource(new FakeBuildRuleParamsBuilder("//foo:bar").build(),new SourcePathResolver(new BuildRuleResolver()),ImmutableSortedSet.<BuildRule>of(),new TestSourcePath("foo/res"),ImmutableSortedSet.of(Paths.get("foo/res/values/strings.xml")),"com.example.android",null,ImmutableSortedSet.<Path>of(),null,false,Optional.<Supplier<Sha1HashCode>>absent());
  assertEquals("com.example.android",androidResource.getRDotJavaPackage());
}
