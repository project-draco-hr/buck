{
  Preconditions.checkState(operation.isPreprocess());
  ImmutableSet.Builder<SourcePath> prefixHeaders=ImmutableSet.builder();
  for (  CxxHeaders cxxHeaders : includes) {
    prefixHeaders.addAll(cxxHeaders.getPrefixHeaders());
  }
  return ImmutableList.<String>builder().addAll(rulePreprocessorFlags.get()).addAll(MoreIterables.zipAndConcat(Iterables.cycle("-include"),FluentIterable.from(prefixHeaders.build()).transform(getResolver().getPathFunction()).transform(Functions.toStringFunction()))).addAll(MoreIterables.zipAndConcat(Iterables.cycle("-I"),Iterables.transform(headerMaps,Functions.toStringFunction()))).addAll(MoreIterables.zipAndConcat(Iterables.cycle("-I"),Iterables.transform(includeRoots,Functions.toStringFunction()))).addAll(MoreIterables.zipAndConcat(Iterables.cycle("-isystem"),Iterables.transform(systemIncludeRoots,Functions.toStringFunction()))).addAll(MoreIterables.zipAndConcat(Iterables.cycle("-F"),Iterables.transform(frameworkRoots,Functions.toStringFunction()))).build();
}
