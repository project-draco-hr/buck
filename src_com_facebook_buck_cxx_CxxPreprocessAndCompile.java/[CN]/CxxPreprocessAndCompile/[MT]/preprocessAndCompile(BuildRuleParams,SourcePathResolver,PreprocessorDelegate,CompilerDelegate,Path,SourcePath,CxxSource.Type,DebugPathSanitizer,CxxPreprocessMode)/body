{
  return new CxxPreprocessAndCompile(params,resolver,(strategy == CxxPreprocessMode.PIPED ? CxxPreprocessAndCompileStep.Operation.PIPED_PREPROCESS_AND_COMPILE : CxxPreprocessAndCompileStep.Operation.COMPILE_MUNGE_DEBUGINFO),Optional.of(preprocessorDelegate),compilerDelegate,output,input,inputType,sanitizer);
}
