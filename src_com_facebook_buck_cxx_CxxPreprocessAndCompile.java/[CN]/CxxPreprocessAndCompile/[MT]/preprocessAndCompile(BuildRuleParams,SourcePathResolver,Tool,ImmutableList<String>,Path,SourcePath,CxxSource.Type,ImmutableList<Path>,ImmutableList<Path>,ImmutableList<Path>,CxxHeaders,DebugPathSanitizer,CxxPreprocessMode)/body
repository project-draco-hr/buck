{
  return new CxxPreprocessAndCompile(params,resolver,compiler,(strategy == CxxPreprocessMode.PIPED ? CxxPreprocessAndCompileStep.Operation.PIPED_PREPROCESS_AND_COMPILE : CxxPreprocessAndCompileStep.Operation.COMPILE_MUNGE_DEBUGINFO),flags,output,input,inputType,includeRoots,systemIncludeRoots,frameworkRoots,includes,sanitizer);
}
