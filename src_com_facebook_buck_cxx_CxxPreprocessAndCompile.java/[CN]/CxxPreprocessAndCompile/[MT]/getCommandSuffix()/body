{
  return ImmutableList.<String>builder().addAll(flags).addAll(MoreIterables.zipAndConcat(Iterables.cycle("-include"),FluentIterable.from(includes.getPrefixHeaders()).transform(getResolver().getPathFunction()).transform(Functions.toStringFunction()))).addAll(MoreIterables.zipAndConcat(Iterables.cycle("-I"),Iterables.transform(includeRoots,Functions.toStringFunction()))).addAll(MoreIterables.zipAndConcat(Iterables.cycle("-isystem"),Iterables.transform(systemIncludeRoots,Functions.toStringFunction()))).addAll(MoreIterables.zipAndConcat(Iterables.cycle("-F"),Iterables.transform(frameworkRoots,Functions.toStringFunction()))).build();
}
