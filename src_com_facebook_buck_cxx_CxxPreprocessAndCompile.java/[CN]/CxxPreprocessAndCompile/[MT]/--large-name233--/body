{
  return new CxxPreprocessAndCompile(params,resolver,(strategy == CxxPreprocessMode.PIPED ? CxxPreprocessAndCompileStep.Operation.PIPED_PREPROCESS_AND_COMPILE : CxxPreprocessAndCompileStep.Operation.COMPILE_MUNGE_DEBUGINFO),Optional.of(preprocessor),Optional.of(platformPreprocessorFlags),Optional.of(rulePreprocessorFlags),Optional.of(compiler),Optional.of(platformCompilerFlags),Optional.of(ruleCompilerFlags),output,input,inputType,includeRoots,systemIncludeRoots,headerMaps,frameworkRoots,prefixHeader,includes,sanitizer);
}
