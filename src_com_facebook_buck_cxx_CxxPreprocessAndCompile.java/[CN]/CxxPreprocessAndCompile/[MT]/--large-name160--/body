{
  return new CxxPreprocessAndCompile(params,resolver,(strategy == CxxPreprocessMode.PIPED ? CxxPreprocessAndCompileStep.Operation.PIPED_PREPROCESS_AND_COMPILE : CxxPreprocessAndCompileStep.Operation.COMPILE_MUNGE_DEBUGINFO),Optional.of(preprocessor),Optional.of(preprocessorFlags),Optional.of(compiler),Optional.of(compilerFlags),output,input,inputType,includeRoots,systemIncludeRoots,frameworkRoots,includes,(strategy == CxxPreprocessMode.COMBINED ? sanitizer : sanitizer.changePathSize(0)));
}
