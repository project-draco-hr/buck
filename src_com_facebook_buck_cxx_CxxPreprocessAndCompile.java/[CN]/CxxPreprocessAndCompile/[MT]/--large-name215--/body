{
  return new CxxPreprocessAndCompile(params,resolver,CxxPreprocessAndCompileStep.Operation.PREPROCESS,Optional.of(preprocessor),Optional.of(platformFlags),Optional.of(ruleFlags),Optional.<Compiler>absent(),Optional.<ImmutableList<String>>absent(),Optional.<ImmutableList<String>>absent(),output,input,inputType,includeRoots,systemIncludeRoots,headerMaps,frameworkRoots,includes,sanitizer);
