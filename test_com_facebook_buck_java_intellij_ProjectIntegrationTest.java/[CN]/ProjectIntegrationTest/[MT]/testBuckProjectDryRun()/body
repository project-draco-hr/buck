{
  ProjectWorkspace workspace=TestDataHelper.createProjectWorkspaceForScenario(this,"project1",temporaryFolder);
  workspace.setUp();
  ProcessResult result=workspace.runBuckCommand("project","--dry-run");
  result.assertSuccess("buck project should exit cleanly");
  ImmutableSortedSet<String> expectedResult=ImmutableSortedSet.of("//:project_config","//:root_module","//libs:generated","//libs:generated_jar","//libs:guava","//libs:jsr305","//libs:junit","//modules/dep1:dep1","//modules/dep1:project_config","//modules/dep1:test","//modules/tip:project_config","//modules/tip:test","//modules/tip:tip");
  ImmutableSortedSet<String> actualResult=ImmutableSortedSet.copyOf(Splitter.on('\n').omitEmptyStrings().split(result.getStdout()));
  assertEquals("`buck project --dry-run` should print the list of targets that would be included.",expectedResult,actualResult);
}
