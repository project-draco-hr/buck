{
switch (type.getBasicType()) {
case Type.BT_INT:
    return APUT_INT;
case Type.BT_LONG:
  return APUT_LONG;
case Type.BT_FLOAT:
return APUT_FLOAT;
case Type.BT_DOUBLE:
return APUT_DOUBLE;
case Type.BT_OBJECT:
return APUT_OBJECT;
case Type.BT_BOOLEAN:
return APUT_BOOLEAN;
case Type.BT_BYTE:
return APUT_BYTE;
case Type.BT_CHAR:
return APUT_CHAR;
case Type.BT_SHORT:
return APUT_SHORT;
}
return throwBadType(type);
}
