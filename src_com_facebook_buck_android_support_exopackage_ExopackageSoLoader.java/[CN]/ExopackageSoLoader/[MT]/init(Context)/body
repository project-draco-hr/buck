{
  if (initialized) {
    Log.d(TAG,"init() already called, so nothing to do.");
    return;
  }
  nativeLibsDir="/data/local/tmp/exopackage/" + context.getPackageName() + "/native-libs/";
  verifyMetadataFile();
  if (isCopyToPrivateStorage()) {
    preparePrivateDirectory(context);
    parseMetadata();
  }
}
