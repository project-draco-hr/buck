{
  List<JsonSerializableDatabaseEntry> entries=Lists.newArrayList();
  for (  SourceWithFlags sourceWithFlags : targetSources.getSourcesWithFlags()) {
    entries.add(createEntry(context,sourceWithFlags.getSourcePath(),sourceWithFlags.getFlags()));
  }
  Iterable<SourcePath> allHeaderPaths=Iterables.concat(targetSources.getPublicHeaderPaths(),targetSources.getPrivateHeaderPaths());
  for (  SourcePath headerPath : allHeaderPaths) {
    entries.add(createEntry(context,headerPath,ImmutableList.<String>of()));
  }
  return entries;
}
