{
  ImmutableList.Builder<Step> commands=ImmutableList.builder();
  if (!javaSourceFilePaths.isEmpty()) {
    Step javac=new DependencyCheckingJavacStep(outputDirectory,javaSourceFilePaths,transitiveClasspathEntries,declaredClasspathEntries,bootclasspathSupplier,annotationProcessingData,invokingRule,buildDependencies,suggestBuildRules,sourceLevel,targetLevel);
    commands.add(javac);
  }
  return commands.build();
}
