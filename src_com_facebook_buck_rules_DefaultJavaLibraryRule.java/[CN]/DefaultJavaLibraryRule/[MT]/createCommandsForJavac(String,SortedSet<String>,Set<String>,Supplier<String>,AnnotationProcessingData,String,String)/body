{
  ImmutableList.Builder<Command> commands=ImmutableList.builder();
  if (!javaSourceFilePaths.isEmpty()) {
    Command javac=new JavacInMemoryCommand(outputDirectory,javaSourceFilePaths,classpathEntries,bootclasspathSupplier,annotationProcessingData,sourceLevel,targetLevel);
    commands.add(javac);
  }
  return commands.build();
}
