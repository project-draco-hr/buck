{
  AndroidManifestReader manifestReader=DefaultAndroidManifestReader.forString("<manifest xmlns:android='http://schemas.android.com/apk/res/android'" + "          package='com.example'>" + "  <application>"+ "    <activity android:name='com.example.Activity1'>"+ "      <intent-filter>"+ "        <action android:name='android.intent.action.MAIN' />"+ "        <category android:name='android.intent.category.LAUNCHER' />"+ "      </intent-filter>"+ "    </activity>"+ "    <activity android:name='.Activity2'>"+ "        <activity/> <!-- Make sure a weird manifest doesn't ruin things! -->"+ "      <intent-filter>"+ "        <category android:name='android.intent.category.LAUNCHER' />"+ "        <action android:name='android.intent.action.MAIN' />"+ "      </intent-filter>"+ "    </activity>"+ "    <activity android:name='com.example.Activity3'>"+ "      <intent-filter>"+ "        <category android:name='android.intent.category.LAUNCHER' />"+ "      </intent-filter>"+ "    </activity>"+ "    <activity android:name='com.example.Activity4'>"+ "      <intent-filter>"+ "        <action android:name='android.intent.action.MAIN' />"+ "      </intent-filter>"+ "    </activity>"+ "  </application>"+ "</manifest>");
  List<String> found=manifestReader.getLauncherActivities();
  List<String> expected=ImmutableList.of("com.example.Activity1",".Activity2");
  assertEquals(expected,found);
}
