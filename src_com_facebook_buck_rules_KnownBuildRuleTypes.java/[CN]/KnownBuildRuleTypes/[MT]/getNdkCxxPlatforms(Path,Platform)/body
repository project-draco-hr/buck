{
  ImmutableMap.Builder<AndroidBinary.TargetCpuType,NdkCxxPlatform> ndkCxxPlatformBuilder=ImmutableMap.builder();
  NdkCxxPlatform armeabi=NdkCxxPlatforms.build(ImmutableFlavor.of("android-arm"),platform,ndkRoot,new NdkCxxPlatforms.TargetConfiguration(NdkCxxPlatforms.Toolchain.ARM_LINUX_ADNROIDEABI_4_8,NdkCxxPlatforms.ToolchainPrefix.ARM_LINUX_ANDROIDEABI,NdkCxxPlatforms.TargetArch.ARM,NdkCxxPlatforms.TargetArchAbi.ARMEABI,"android-9","4.8",ImmutableList.of("-march=armv5te","-mtune=xscale","-msoft-float","-mthumb","-Os"),ImmutableList.of("-march=armv7-a","-Wl,--fix-cortex-a8")),NdkCxxPlatforms.CxxRuntime.GNUSTL);
  ndkCxxPlatformBuilder.put(AndroidBinary.TargetCpuType.ARM,armeabi);
  NdkCxxPlatform armeabiv7=NdkCxxPlatforms.build(ImmutableFlavor.of("android-armv7"),platform,ndkRoot,new NdkCxxPlatforms.TargetConfiguration(NdkCxxPlatforms.Toolchain.ARM_LINUX_ADNROIDEABI_4_8,NdkCxxPlatforms.ToolchainPrefix.ARM_LINUX_ANDROIDEABI,NdkCxxPlatforms.TargetArch.ARM,NdkCxxPlatforms.TargetArchAbi.ARMEABI_V7A,"android-9","4.8",ImmutableList.of("-finline-limit=64","-march=armv7-a","-mfpu=vfpv3-d16","-mfloat-abi=softfp","-mthumb","-Os"),ImmutableList.<String>of()),NdkCxxPlatforms.CxxRuntime.GNUSTL);
  ndkCxxPlatformBuilder.put(AndroidBinary.TargetCpuType.ARMV7,armeabiv7);
  NdkCxxPlatform x86=NdkCxxPlatforms.build(ImmutableFlavor.of("android-x86"),platform,ndkRoot,new NdkCxxPlatforms.TargetConfiguration(NdkCxxPlatforms.Toolchain.X86_4_8,NdkCxxPlatforms.ToolchainPrefix.I686_LINUX_ANDROID,NdkCxxPlatforms.TargetArch.X86,NdkCxxPlatforms.TargetArchAbi.X86,"android-9","4.8",ImmutableList.of("-funswitch-loops","-finline-limit=300","-O2"),ImmutableList.<String>of()),NdkCxxPlatforms.CxxRuntime.GNUSTL);
  ndkCxxPlatformBuilder.put(AndroidBinary.TargetCpuType.X86,x86);
  return ndkCxxPlatformBuilder.build();
}
