{
  final int[] aggregates=m_aggregates;
  int value=aggregates[type];
  if (value < 0) {
    value=0;
    for (Iterator children=m_children.iterator(); children.hasNext(); ) {
      value+=((IItem)children.next()).getAggregate(type);
    }
    aggregates[type]=value;
    return value;
  }
  return value;
}
