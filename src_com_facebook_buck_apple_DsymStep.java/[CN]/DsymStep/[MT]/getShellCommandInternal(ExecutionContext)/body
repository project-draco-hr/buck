{
  ImmutableList.Builder<String> commandBuilder=ImmutableList.builder();
  commandBuilder.addAll(command);
  commandBuilder.add("--oso-prepend-path=" + context.getProjectFilesystem().getRootPath().toString() + "/","-o",context.getProjectFilesystem().resolve(output).toString(),context.getProjectFilesystem().resolve(input).toString());
  return commandBuilder.build();
}
