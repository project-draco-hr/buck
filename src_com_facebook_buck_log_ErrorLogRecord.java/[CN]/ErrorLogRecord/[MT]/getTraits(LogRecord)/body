{
  String commandId=COMMAND_ID_MAP.threadIdToCommandId(record.getThreadID());
  String logger=record.getLoggerName();
  ImmutableMap<String,String> traits=ImmutableMap.of("severity",record.getLevel().toString(),"commandId",commandId != null ? commandId : "None","logger",logger != null ? logger : "None");
  return traits;
}
