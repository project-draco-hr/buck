{
  NativeLinkableInput.Builder inputBuilder=NativeLinkableInput.builder();
  inputBuilder.addAllArgs(StringArg.from(linkerFlags));
  UnflavoredBuildTarget staticBuildTarget=staticLibraryTarget.getUnflavoredBuildTarget();
  inputBuilder.addArgs(new SourcePathArg(getResolver(),new BuildTargetSourcePath(ocamlLibraryBuild.getBuildTarget(),isBytecode ? OCamlBuildContext.getBytecodeOutputPath(staticBuildTarget,true) : OCamlBuildContext.getNativeOutputPath(staticBuildTarget,true))));
  for (  SourcePath objFile : objFiles) {
    inputBuilder.addArgs(new SourcePathArg(getResolver(),objFile));
  }
  return inputBuilder.build();
}
