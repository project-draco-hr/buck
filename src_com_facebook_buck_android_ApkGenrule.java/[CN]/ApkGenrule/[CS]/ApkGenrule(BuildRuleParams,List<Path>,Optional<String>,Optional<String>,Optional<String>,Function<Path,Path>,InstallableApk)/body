{
  super(buildRuleParams.getBuildTarget(),buildRuleParams.getDeps(),srcs,cmd,bash,cmdExe,buildRuleParams.getBuildTarget().getShortName() + ".apk",relativeToAbsolutePathFunction);
  this.apk=Preconditions.checkNotNull(apk);
  this.relativeToAbsolutePathFunction=Preconditions.checkNotNull(relativeToAbsolutePathFunction);
}
