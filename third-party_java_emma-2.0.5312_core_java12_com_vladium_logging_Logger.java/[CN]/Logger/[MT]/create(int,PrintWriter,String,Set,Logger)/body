{
  if (base == null) {
    return create(level,out,prefix,classMask);
  }
 else {
    final int _level=level >= NONE ? level : base.m_level;
    final PrintWriter _out=(out != null) && !out.checkError() ? out : base.m_out;
    final String _prefix=prefix;
    final Set _classMask=classMask != null ? classMask : base.m_classMask;
    return new Logger(_level,_out,_prefix,_classMask);
  }
}
