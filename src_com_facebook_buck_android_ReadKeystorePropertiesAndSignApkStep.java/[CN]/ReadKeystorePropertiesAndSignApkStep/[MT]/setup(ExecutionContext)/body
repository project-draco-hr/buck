{
  KeystoreProperties keystoreProperties=KeystoreProperties.createFromPropertiesFile(pathToKeystorePropertiesFile,context.getProjectFilesystem());
  this.signApkCommand=new SignApkStep(outputPath,unsignedApk,keystoreProperties.getKeystore(),keystoreProperties.getStorepass(),keystoreProperties.getKeypass(),keystoreProperties.getAlias());
}
