{
  this.thread=new Thread(new Runnable(){
    @Override @SuppressWarnings("PMD.EmptyCatchBlock") public void run(){
      try {
        ManagedThread.this.run();
      }
 catch (      InterruptedIOException|InterruptedException e) {
      }
catch (      Exception e) {
        exception=e;
      }
    }
  }
);
}
