{
  if (object instanceof List) {
    return SourceList.ofUnnamedSources(unnamedHeadersTypeCoercer.coerce(buildTargetParser,filesystem,pathRelativeToProjectRoot,object));
  }
 else {
    return SourceList.ofNamedSources(namedHeadersTypeCoercer.coerce(buildTargetParser,filesystem,pathRelativeToProjectRoot,object));
  }
}
