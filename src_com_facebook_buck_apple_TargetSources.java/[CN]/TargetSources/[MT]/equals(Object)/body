{
  if (other instanceof TargetSources) {
    TargetSources that=(TargetSources)other;
    return Objects.equals(this.srcs,that.srcs) && Objects.equals(this.perFileFlags,that.perFileFlags) && Objects.equals(this.srcPaths,that.srcPaths)&& Objects.equals(this.headerPaths,that.headerPaths);
  }
  return false;
}
