{
  try (FileInputStream fis=new FileInputStream(zipFile.toFile());BufferedInputStream bis=new BufferedInputStream(fis);JarInputStream jis=new JarInputStream(bis)){
    for (JarEntry entry=jis.getNextJarEntry(); entry != null; entry=jis.getNextJarEntry()) {
      if (entry.isDirectory()) {
        continue;
      }
      onFile.visit(Paths.get(entry.getName()),jis);
    }
  }
 }
