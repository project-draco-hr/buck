{
  testBuildRuleResolver=new BuildRuleResolver();
  testSourcePathResolver=new SourcePathResolver(testBuildRuleResolver);
  Collection<SourceWithFlags> sourcesWithFlags=ImmutableList.of(SourceWithFlags.of(new TestSourcePath("foo/Hello.h"),ImmutableList.of("public")),SourceWithFlags.of(new TestSourcePath("foo/Hello.m")));
  testTargetSources=TargetSources.fromSourcesWithFlags(testSourcePathResolver,sourcesWithFlags);
  testBuildTarget=BuildTargetFactory.newInstance("//foo:bar");
}
