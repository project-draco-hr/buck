  appleConfig=new AppleConfig(new FakeBuckConfig(ImmutableMap.<String,Map<String,String>>of("apple",ImmutableMap.of("xcode_developer_dir","/path/to/somewhere"))));
  return new CompilationDatabase(new FakeBuildRuleParamsBuilder(testBuildTarget).build(),testSourcePathResolver,appleConfig,CompilationDatabase.PlatformFlavor.IOS_SIMULATOR_8,testTargetSources,frameworks,includePaths,pchFile);
