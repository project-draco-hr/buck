{
  setUpTestValues();
  ImmutableSortedSet<String> frameworks=ImmutableSortedSet.of("$SDKROOT/System/Library/Frameworks/CoreLocation.framework","$SDKROOT/System/Library/Frameworks/Foundation.framework","$SDKROOT/System/Library/Frameworks/UIKit.framework");
