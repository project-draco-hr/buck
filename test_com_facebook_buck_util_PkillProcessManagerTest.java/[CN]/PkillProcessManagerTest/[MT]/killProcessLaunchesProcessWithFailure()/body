{
  ProcessExecutorParams pkillParams=ImmutableProcessExecutorParams.builder().setCommand(ImmutableList.of("pkill","processName")).build();
  FakeProcessExecutor processExecutor=new FakeProcessExecutor(ImmutableMap.<ProcessExecutorParams,FakeProcess>of(pkillParams,new FakeProcess(1)));
  PkillProcessManager processManager=new PkillProcessManager(processExecutor);
  processManager.killProcess("processName");
  assertTrue(processExecutor.isProcessLaunched(pkillParams));
}
