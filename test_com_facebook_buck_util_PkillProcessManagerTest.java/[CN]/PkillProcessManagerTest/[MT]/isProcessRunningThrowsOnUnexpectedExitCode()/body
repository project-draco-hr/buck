{
  ProcessExecutorParams pkill0Params=ImmutableProcessExecutorParams.builder().setCommand(ImmutableList.of("pkill","-0","processName")).build();
  FakeProcessExecutor processExecutor=new FakeProcessExecutor(ImmutableMap.of(pkill0Params,new FakeProcess(49152)));
  PkillProcessManager processManager=new PkillProcessManager(processExecutor);
  processManager.isProcessRunning("processName");
}
