{
  Preconditions.checkState(newInputsHash != null,"Must call checkIsCached first!");
  final DxCommand dxCommand=new DxCommand(outputPath,srcs);
  final WriteFileCommand updateHashCommand=new WriteFileCommand(newInputsHash,outputHashPath);
  return ImmutableList.<Command>of(new CompositeCommand(ImmutableList.of(dxCommand,updateHashCommand)));
}
