{
  ImmutableList<GroupedSource> groupedSources=ImmutableList.of(GroupedSource.ofSourceGroup("Group1",ImmutableList.of(GroupedSource.ofSourcePath(new TestSourcePath("foo.m")),GroupedSource.ofSourcePath(new TestSourcePath("bar.m")))),GroupedSource.ofSourceGroup("Group2",ImmutableList.of(GroupedSource.ofSourcePath(new TestSourcePath("baz.m")),GroupedSource.ofSourcePath(new TestSourcePath("blech.m")))));
  assertEquals(ImmutableList.of(new TestSourcePath("foo.m"),new TestSourcePath("bar.m"),new TestSourcePath("baz.m"),new TestSourcePath("blech.m")),GroupedSources.sourcePaths(groupedSources));
}
