{
  String invalidPath=temporaryFolder.getRoot().getAbsolutePath() + "DoesNotExist";
  FakeBuckConfig config=new FakeBuckConfig(ImmutableMap.<String,Map<String,String>>builder().put("tools",ImmutableMap.of("python",invalidPath)).build());
  config.getPythonInterpreter();
  fail("Should throw exception as python config is invalid.");
}
