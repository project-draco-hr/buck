{
  assertEquals(0L,new FakeBuckConfig().getDefaultTestTimeoutMillis());
  Reader reader=new StringReader(Joiner.on('\n').join("[test]","timeout = 54321"));
  BuckConfig config=BuckConfigTestUtils.createWithDefaultFilesystem(temporaryFolder,reader,null);
  assertEquals(54321L,config.getDefaultTestTimeoutMillis());
}
