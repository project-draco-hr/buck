{
  File python=temporaryFolder.newFile("python.exe");
  assertTrue("Should be able to set file executable",python.setExecutable(true));
  FakeBuckConfig config=new FakeBuckEnvironment(ImmutableMap.<String,Map<String,String>>of(),ImmutableMap.<String,String>builder().put("PATH",temporaryFolder.getRoot().getAbsolutePath()).put("PATHEXT",".exe").build(),ImmutableMap.<String,String>of());
  config.getPythonInterpreter();
}
