{
  Reader reader=new StringReader(Joiner.on('\n').join("[cache]","dir = /cache_dir"));
  BuckConfig config=BuckConfig.createFromReader(reader,null);
  ImmutableSet<String> ignorePaths=config.getIgnorePaths();
  assertFalse("Absolute cache directory should not be in set of ignored paths",ignorePaths.contains("/cache_dir"));
}
