{
  ProjectFilesystem projectFilesystem=new FakeProjectFilesystem(){
    @Override public Path getRootPath(){
      return MorePathsForTests.rootRelativePath("project/root");
    }
  }
;
  BuildTargetParser parser=new BuildTargetParser();
  StringReader reader=new StringReader(Joiner.on('\n').join(lines));
  return BuckConfigTestUtils.createFromReader(reader,projectFilesystem,parser,Platform.detect(),ImmutableMap.copyOf(System.getenv()));
}
