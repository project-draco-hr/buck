{
  String proguardJarName="proguard.jar";
  temporaryFolder.newFile(proguardJarName);
  Reader reader=new StringReader(Joiner.on('\n').join("[tools]","    proguard = " + proguardJarName));
  BuckConfig config=createWithDefaultFilesystem(reader,null);
  assertEquals("Should resolve to the fully qualified path",temporaryFolder.getRoot().toPath().resolve(proguardJarName).toString(),config.getProguardJarOverride().transform(Functions.toStringFunction()).orNull());
}
