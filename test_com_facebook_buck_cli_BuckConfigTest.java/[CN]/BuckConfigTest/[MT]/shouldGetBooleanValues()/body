{
  assertTrue("a.b is true when 'yes'",createFromText("[a]","  b = yes").getBooleanValue("a","b",true));
  assertTrue("a.b is true when literally 'true'",createFromText("[a]","  b = true").getBooleanValue("a","b",true));
  assertTrue("a.b is true when 'YES' (capitalized)",createFromText("[a]","  b = YES").getBooleanValue("a","b",true));
  assertFalse("a.b is false by default",createFromText("[x]","  y = COWS").getBooleanValue("a","b",false));
  assertFalse("a.b is true when 'no'",createFromText("[a]","  b = no").getBooleanValue("a","b",true));
}
