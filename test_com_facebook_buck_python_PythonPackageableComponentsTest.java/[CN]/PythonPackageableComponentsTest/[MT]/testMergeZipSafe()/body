{
  PythonPackageComponents compA=PythonPackageComponents.of(ImmutableMap.<Path,SourcePath>of(Paths.get("test"),new TestSourcePath("sourceA")),ImmutableMap.<Path,SourcePath>of(),ImmutableMap.<Path,SourcePath>of(),Optional.of(true));
  PythonPackageComponents compB=PythonPackageComponents.of(ImmutableMap.<Path,SourcePath>of(Paths.get("test2"),new TestSourcePath("sourceB")),ImmutableMap.<Path,SourcePath>of(),ImmutableMap.<Path,SourcePath>of(),Optional.of(false));
  BuildTarget me=BuildTargetFactory.newInstance("//:me");
  BuildTarget them=BuildTargetFactory.newInstance("//:them");
  PythonPackageComponents.Builder builder=new PythonPackageComponents.Builder(me);
  builder.addComponent(compB,them);
  builder.addComponent(compA,them);
  assertFalse(builder.build().isZipSafe().get());
}
