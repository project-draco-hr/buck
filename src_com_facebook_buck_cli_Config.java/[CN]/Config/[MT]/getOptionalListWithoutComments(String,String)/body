{
  Optional<String> value=getValue(sectionName,propertyName);
  if (!value.isPresent()) {
    return Optional.absent();
  }
  Iterable<String> allValues=Splitter.on(',').omitEmptyStrings().trimResults().split(value.get());
  return Optional.of(FluentIterable.from(allValues).filter(new Predicate<String>(){
    @Override public boolean apply(    String input){
      return !Pattern.compile("^\\s*[#;]").matcher(input).find();
    }
  }
).toList());
}
