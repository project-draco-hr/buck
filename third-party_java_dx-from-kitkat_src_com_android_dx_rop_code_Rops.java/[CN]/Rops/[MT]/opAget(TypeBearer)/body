{
switch (type.getBasicType()) {
case Type.BT_INT:
    return AGET_INT;
case Type.BT_LONG:
  return AGET_LONG;
case Type.BT_FLOAT:
return AGET_FLOAT;
case Type.BT_DOUBLE:
return AGET_DOUBLE;
case Type.BT_OBJECT:
return AGET_OBJECT;
case Type.BT_BOOLEAN:
return AGET_BOOLEAN;
case Type.BT_BYTE:
return AGET_BYTE;
case Type.BT_CHAR:
return AGET_CHAR;
case Type.BT_SHORT:
return AGET_SHORT;
}
return throwBadType(type);
}
