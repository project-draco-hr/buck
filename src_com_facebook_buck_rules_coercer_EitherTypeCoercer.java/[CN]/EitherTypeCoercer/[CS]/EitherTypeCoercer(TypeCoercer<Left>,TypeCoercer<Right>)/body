{
  this.leftTypeCoercer=Preconditions.checkNotNull(leftTypeCoercer);
  this.rightTypeCoercer=Preconditions.checkNotNull(rightTypeCoercer);
  Preconditions.checkState(!(leftTypeCoercer instanceof EitherTypeCoercer || rightTypeCoercer instanceof EitherTypeCoercer),"Either of Eithers is not allowed");
}
