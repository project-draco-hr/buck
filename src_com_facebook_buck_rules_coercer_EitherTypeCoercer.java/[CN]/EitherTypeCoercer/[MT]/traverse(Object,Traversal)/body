{
  if (leftTypeCoercer instanceof CollectionTypeCoercer || leftTypeCoercer instanceof MapTypeCoercer) {
    return leftTypeCoercer.traverse(object,traversal) || rightTypeCoercer.traverse(object,traversal);
  }
 else {
    return rightTypeCoercer.traverse(object,traversal) || leftTypeCoercer.traverse(object,traversal);
  }
}
