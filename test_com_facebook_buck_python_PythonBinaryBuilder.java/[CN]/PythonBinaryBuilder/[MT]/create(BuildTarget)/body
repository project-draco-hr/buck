{
  PythonBuckConfig pythonBuckConfig=new PythonBuckConfig(new FakeBuckConfig(),new ExecutableFinder());
  return new PythonBinaryBuilder(target,pythonBuckConfig,new PythonEnvironment(Paths.get("python"),PythonVersion.of("2.7")),CxxPlatformUtils.DEFAULT_PLATFORM,new FlavorDomain<>("C/C++ Platform",ImmutableMap.of(CxxPlatformUtils.DEFAULT_PLATFORM.getFlavor(),CxxPlatformUtils.DEFAULT_PLATFORM)));
}
