{
  assertEquals(lstV(),lstI(MoreIterables.zipAndConcat()));
  assertEquals(lstV("a"),lstI(MoreIterables.zipAndConcat(lstV("a"))));
  assertEquals(lstV("a","b","a","b"),lstI(MoreIterables.zipAndConcat(lstV("a","a","a"),lstV("b","b"))));
  assertEquals(lstV("a","b","c"),lstI(MoreIterables.zipAndConcat(lstV("a"),lstV("b"),lstV("c"))));
}
