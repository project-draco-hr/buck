{
  Path baseArtifactDir=ocamlContext.getOutput().getParent();
  buildableContext.recordArtifact(baseArtifactDir);
  buildableContext.recordArtifact(baseArtifactDir.resolve(OCamlBuildContext.OCAML_COMPILED_DIR));
  buildableContext.recordArtifact(baseArtifactDir.resolve(OCamlBuildContext.OCAML_COMPILED_BYTECODE_DIR));
  return ImmutableList.of(new MakeCleanDirectoryStep(getProjectFilesystem(),ocamlContext.getOutput().getParent()),new OCamlBuildStep(getResolver(),getProjectFilesystem(),ocamlContext,cCompiler.getEnvironment(getResolver()),cCompiler.getCommandPrefix(getResolver()),cxxCompiler.getEnvironment(getResolver()),cxxCompiler.getCommandPrefix(getResolver())));
}
