{
  Path baseArtifactDir=ocamlContext.getOutput().getParent();
  buildableContext.recordArtifactsInDirectory(baseArtifactDir);
  buildableContext.recordArtifactsInDirectory(baseArtifactDir.resolve(OCamlBuildContext.OCAML_COMPILED_DIR));
  buildableContext.recordArtifactsInDirectory(baseArtifactDir.resolve(OCamlBuildContext.OCAML_COMPILED_BYTECODE_DIR));
  return ImmutableList.of(new MakeCleanDirectoryStep(ocamlContext.getOutput().getParent()),new OCamlBuildStep(ocamlContext,getResolver().getPath(cCompiler),getResolver().getPath(cxxCompiler)));
}
