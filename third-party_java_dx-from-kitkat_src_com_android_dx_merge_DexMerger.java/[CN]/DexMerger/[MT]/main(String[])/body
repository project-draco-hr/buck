{
  if (args.length < 2) {
    printUsage();
    return;
  }
  Dex merged=new Dex(new File(args[1]));
  for (int i=2; i < args.length; i++) {
    Dex toMerge=new Dex(new File(args[i]));
    merged=new DexMerger(merged,toMerge,CollisionPolicy.KEEP_FIRST).merge();
  }
  merged.writeTo(new File(args[0]));
}
