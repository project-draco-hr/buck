{
  FakeProjectFilesystem projectFilesystem=new FakeProjectFilesystem();
  TargetNode<?> node=createJavaLibraryTargetNodeWithSrcs(projectFilesystem,BuildTargetFactory.newInstance("//foo:lib"),HashCode.fromLong(64738),ImmutableMap.of(Paths.get("foo/FooLib.java"),"Hello world"));
  TargetGraph targetGraph=TargetGraphFactory.newInstance(node);
  ImmutableMap<BuildTarget,HashCode> expectedTargetHashes=ImmutableMap.of(BuildTargetFactory.newInstance("//foo:lib"),HashCode.fromString("15bcb369ef1f2c6df995f0538b7d95f4798a16e7"));
  assertThat(TargetGraphHashing.hashTargetGraph(projectFilesystem,targetGraph,ImmutableList.of(node.getBuildTarget())),equalTo(expectedTargetHashes));
}
