{
  FakeProjectFilesystem projectFilesystem=new FakeProjectFilesystem();
  TargetNode<?> node=createJavaLibraryTargetNodeWithSrcs(projectFilesystem,BuildTargetFactory.newInstance("//foo:lib"),HashCode.fromLong(64738),ImmutableMap.of(Paths.get("foo/FooLib.java"),"Goodbye world"));
  TargetGraph targetGraph=TargetGraphFactory.newInstance(node);
  ImmutableMap<BuildTarget,HashCode> expectedTargetHashes=ImmutableMap.of(BuildTargetFactory.newInstance("//foo:lib"),HashCode.fromString("6cb8ac509e591d722c2902a08be8738649a620b9"));
  assertThat(TargetGraphHashing.hashTargetGraph(projectFilesystem,targetGraph,ImmutableList.of(node.getBuildTarget())),equalTo(expectedTargetHashes));
}
