{
switch (operation) {
case PIPED_PREPROCESS_AND_COMPILE:
{
      return Joiner.on(' ').join(FluentIterable.from(makePreprocessCommand(false)).transform(Escaper.SHELL_ESCAPER)) + " | " + Joiner.on(' ').join(FluentIterable.from(makeCompileCommand("-",inputType.getPreprocessedLanguage(),false,false)).transform(Escaper.SHELL_ESCAPER));
    }
default :
{
    return Joiner.on(' ').join(FluentIterable.from(getCommand()).transform(Escaper.SHELL_ESCAPER));
  }
}
}
