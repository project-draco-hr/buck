{
  ProcessBuilder builder=new ProcessBuilder();
  builder.directory(workingDirectory.toAbsolutePath().toFile());
  builder.redirectError(ProcessBuilder.Redirect.PIPE);
  builder.environment().put("PWD",operation == Operation.COMPILE_MUNGE_DEBUGINFO ? sanitizer.getExpandedPath(workingDirectory.toAbsolutePath()) : workingDirectory.toAbsolutePath().toString());
  return builder;
}
