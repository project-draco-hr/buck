{
  ProcessBuilder builder=new ProcessBuilder();
  builder.directory(context.getProjectDirectoryRoot().toAbsolutePath().toFile());
  builder.redirectError(ProcessBuilder.Redirect.PIPE);
  builder.environment().put("PWD",operation == Operation.COMPILE_MUNGE_DEBUGINFO ? sanitizer.getExpandedPath(context.getProjectDirectoryRoot().toAbsolutePath()) : context.getProjectDirectoryRoot().toAbsolutePath().toString());
  return builder;
}
