{
  boolean isTestRunRequired;
  BuildRuleSuccess.Type successType;
  if (executionContext.isDebugEnabled()) {
    isTestRunRequired=true;
  }
 else   if (((successType=test.getBuildResultType()) != null) && (successType == BuildRuleSuccess.Type.FETCHED_FROM_CACHE || successType == BuildRuleSuccess.Type.MATCHING_RULE_KEY) && test.hasTestResultFiles(executionContext)) {
    isTestRunRequired=false;
  }
 else {
    isTestRunRequired=true;
  }
  return isTestRunRequired;
}
