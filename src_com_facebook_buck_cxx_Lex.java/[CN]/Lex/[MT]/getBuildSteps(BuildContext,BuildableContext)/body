{
  buildableContext.recordArtifact(outputHeader);
  buildableContext.recordArtifact(outputSource);
  return ImmutableList.of(new MkdirStep(outputSource.getParent()),new RmStep(outputSource,true),new MkdirStep(outputHeader.getParent()),new RmStep(outputHeader,true),new LexStep(lex.resolve(),flags,outputSource,outputHeader,input.resolve()));
}
