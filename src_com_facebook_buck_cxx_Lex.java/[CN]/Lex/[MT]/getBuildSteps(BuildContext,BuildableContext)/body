{
  buildableContext.recordArtifact(outputHeader);
  buildableContext.recordArtifact(outputSource);
  return ImmutableList.of(new MkdirStep(getProjectFilesystem(),outputSource.getParent()),new RmStep(getProjectFilesystem(),outputSource,true),new MkdirStep(getProjectFilesystem(),outputHeader.getParent()),new RmStep(getProjectFilesystem(),outputHeader,true),new LexStep(getProjectFilesystem().getRootPath(),lex.getCommandPrefix(getResolver()),flags,outputSource,outputHeader,getResolver().deprecatedGetPath(input)));
}
