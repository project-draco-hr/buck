{
  buildableContext.recordArtifact(outputHeader);
  buildableContext.recordArtifact(outputSource);
  return ImmutableList.of(new MkdirStep(outputSource.getParent()),new RmStep(outputSource,true),new MkdirStep(outputHeader.getParent()),new RmStep(outputHeader,true),new LexStep(getResolver().getPath(lex),flags,outputSource,outputHeader,getResolver().getPath(input)));
}
