{
  AssumeAndroidPlatform.assumeSdkIsAvailable();
  ProjectFilesystem filesystem=new ProjectFilesystem(Paths.get("."));
  AndroidDirectoryResolver resolver=new DefaultAndroidDirectoryResolver(filesystem,Optional.<String>absent(),new DefaultPropertyFinder(filesystem,ImmutableMap.copyOf(System.getenv())));
  Path aapt=AndroidPlatformTarget.getDefaultPlatformTarget(resolver,Optional.<Path>absent()).getAaptExecutable();
  String buildToolsVersion=aapt.getParent().getFileName().toString();
  isBuildToolsNew=new VersionStringComparator().compare(buildToolsVersion,"21") >= 0;
}
