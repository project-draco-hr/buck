def mkdir_p(path):
    'Runs the equivalent of `mkdir -p`\n  Taken from http://stackoverflow.com/questions/600268/mkdir-p-functionality-in-python\n  Args:\n    path: an absolute path\n  '
    try:
        os.makedirs(path)
    except OSError as exc:
        if (exc.errno == errno.EEXIST):
            pass
        else:
            raise
