{
  ProcessExecutor.Result process=executor.launchAndExecute(ProcessExecutorParams.ofCommand(args),ImmutableSet.<ProcessExecutor.Option>of(),Optional.<String>absent(),Optional.of(TimeUnit.MINUTES.toMillis(5)),Optional.<Function<Process,Void>>absent());
  if (process.getExitCode() != 0) {
    LOG.error("Error %d executing %s",process.getExitCode(),Joiner.on(" ").join(args));
    return Optional.absent();
  }
  String stdout=process.getStdout().or("{}");
  LOG.debug("stdout of command: " + stdout);
  Map<String,String> output=OBJECT_MAPPER.readValue(stdout,new TypeReference<Map<String,String>>(){
  }
);
  return Optional.fromNullable(output);
}
