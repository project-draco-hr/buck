{
  BuildTarget buildTarget=BuildTargetFactory.newInstance(ANNOTATION_SCENARIO_TARGET);
  annotationProcessingParamsBuilder.setOwnerTarget(buildTarget);
  tmp.newFolder("android","java","src","com","facebook");
  String src="android/java/src/com/facebook/Main.java";
  tmp.newFile(src);
  AnnotationProcessingParams params=annotationProcessingParamsBuilder.build();
  JavacOptions.Builder options=JavacOptions.builder().setAnnotationProcessingData(params);
  BuildRuleParams buildRuleParams=new BuildRuleParams(buildTarget,ImmutableSortedSet.<BuildRule>of(),ImmutableSet.<BuildTargetPattern>of(),projectFilesystem,new FakeRuleKeyBuilderFactory());
  Buildable buildable=new AndroidLibrary(buildRuleParams,ImmutableSet.of(new TestSourcePath(src)),ImmutableSet.<SourcePath>of(),Optional.<Path>absent(),ImmutableList.<String>of(),ImmutableSortedSet.<BuildRule>of(),ImmutableSortedSet.<BuildRule>of(),options.build(),Optional.<Path>absent());
  return new AbstractBuildable.AnonymousBuildRule(AndroidLibraryDescription.TYPE,buildable,buildRuleParams);
}
