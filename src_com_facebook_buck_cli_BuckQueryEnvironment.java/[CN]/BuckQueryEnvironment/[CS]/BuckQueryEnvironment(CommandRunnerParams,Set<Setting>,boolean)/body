{
  this.params=params;
  this.settings=settings;
  this.enableProfiling=enableProfiling;
  this.parserConfig=new ParserConfig(params.getBuckConfig());
  this.buildFileTree=new FilesystemBackedBuildFileTree(params.getRepository().getFilesystem(),parserConfig.getBuildFileName());
  this.targetPatternEvaluator=new TargetPatternEvaluator(params,enableProfiling);
}
