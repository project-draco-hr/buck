{
  try {
    graph=params.getParser().buildTargetGraphForBuildTargets(targets,parserConfig,params.getBuckEventBus(),params.getConsole(),params.getEnvironment(),enableProfiling);
  }
 catch (  BuildTargetException|BuildFileParseException|IOException e) {
    throw new QueryException("Error in building depencency graph");
  }
}
