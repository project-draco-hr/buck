{
  assumeTrue(WatchmanWatcher.isWatchmanAvailable());
  tmp.newFile("CreatedBeforeWatch");
  EventBus eventBus=createStrictMock(EventBus.class);
  replay(eventBus);
  try (WatchmanWatch watch=new WatchmanWatch(tmp.getRoot().toPath())){
    WatchmanWatcher watcher=new WatchmanWatcher(new ProjectFilesystem(tmp.getRoot().toPath()),eventBus);
    watcher.postEvents();
  }
   verify(eventBus);
}
