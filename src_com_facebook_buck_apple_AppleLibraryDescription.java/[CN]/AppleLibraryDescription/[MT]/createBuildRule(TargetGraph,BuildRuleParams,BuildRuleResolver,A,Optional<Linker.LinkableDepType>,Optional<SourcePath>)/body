{
  SourcePathResolver pathResolver=new SourcePathResolver(resolver);
  CxxLibraryDescription.Arg delegateArg=delegate.createUnpopulatedConstructorArg();
  TypeAndPlatform typeAndPlatform=CxxLibraryDescription.getTypeAndPlatform(params.getBuildTarget(),cxxPlatformFlavorDomain);
  AppleDescriptions.populateCxxLibraryDescriptionArg(pathResolver,delegateArg,args,params.getBuildTarget(),!isSharedLibraryTarget(params.getBuildTarget()));
  return delegate.createBuildRule(targetGraph,params,resolver,delegateArg,typeAndPlatform,linkableDepType,bundleLoader);
}
