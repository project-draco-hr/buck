{
  assumeTrue(Platform.detect() == Platform.MACOS);
  ProjectWorkspace workspace=TestDataHelper.createProjectWorkspaceForScenario(this,"simple_application_bundle",tmp);
  workspace.setUp();
  workspace.runBuckCommand("build","--config","apple.default_debug_info_format=none","//:DemoApp#dwarf-and-dsym").assertSuccess();
  Path output=getGenDir().resolve("DemoApp#dwarf-and-dsym/DemoApp.app.dSYM/Contents/Resources/DWARF/DemoApp");
  assertThat(Files.exists(output),Matchers.equalTo(true));
}
