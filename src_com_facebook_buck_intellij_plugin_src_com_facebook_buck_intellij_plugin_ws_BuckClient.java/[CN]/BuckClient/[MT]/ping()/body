{
  if ((new Date()).getTime() - mLastActionTime < PING_PERIOD) {
    return;
  }
  if (mConnected) {
    try {
      mWSSocket.sendMessage("ping");
synchronized (syncObject) {
        mLastActionTime=(new Date()).getTime();
      }
    }
 catch (    Throwable t) {
      Logger.getInstance(this.getClass()).error("Could not send ping");
    }
  }
}
