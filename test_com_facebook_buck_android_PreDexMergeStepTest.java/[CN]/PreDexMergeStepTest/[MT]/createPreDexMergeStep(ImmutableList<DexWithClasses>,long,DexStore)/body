{
  DexWithClasses rDotJavaDex=new FakeDexWithClasses("buck-out/gen/app/r_classes.dex.jar",ImmutableSet.of("com.example.R","com.example.R$id"),100);
  Path primaryDexPath=Paths.get("buck-out/gen/app/__app_classes.dex");
  return new PreDexMergeStep(dexFilesToMerge,Optional.of(rDotJavaDex),primaryDexPath,DEFAULT_PRIMARY_DEX_PATTERNS_FOR_TEST,secondaryDexMetadataTxt,secondaryDexJarFilesDir,dexStore,linearAllocHardLimit,scratchDirectory){
    @Override protected StepRunner createStepRunner(    ExecutionContext context){
      return stepRunner;
    }
  }
;
}
