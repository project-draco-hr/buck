{
  NewNativeTargetProjectMutator mutator=new NewNativeTargetProjectMutator(pathRelativizer,sourcePathResolver.getPathFunction());
  mutator.setTargetName("TestTarget").setTargetGroupPath(ImmutableList.of("Grandparent","Parent")).setProduct(PBXTarget.ProductType.BUNDLE,"TestTargetProduct",Paths.get("TestTargetProduct.bundle")).buildTargetAndAddToProject(generatedProject);
  assertTargetExistsAndReturnTarget(generatedProject,"TestTarget");
  PBXGroup grandparentGroup=assertHasSubgroupAndReturnIt(generatedProject.getMainGroup(),"Grandparent");
  assertHasSubgroupAndReturnIt(grandparentGroup,"Parent");
}
