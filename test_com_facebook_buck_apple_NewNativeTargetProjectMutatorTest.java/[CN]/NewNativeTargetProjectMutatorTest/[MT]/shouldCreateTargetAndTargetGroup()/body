{
  NewNativeTargetProjectMutator mutator=new NewNativeTargetProjectMutator(pathRelativizer,sourcePathResolver);
  mutator.setTargetName("TestTarget").setProduct(PBXTarget.ProductType.BUNDLE,"TestTargetProduct",Paths.get("TestTargetProduct.bundle")).buildTargetAndAddToProject(generatedProject);
  assertTargetExistsAndReturnTarget(generatedProject,"TestTarget");
  assertHasTargetGroupWithName(generatedProject,"TestTarget");
}
