{
  String pathToOutputFile=getPathToOutputFile();
  String outputParentDirectory=new File(pathToOutputFile).getParent();
  return ImmutableList.<Step>builder().addAll(buildArchive(context)).add(new MakeCleanDirectoryStep(outputParentDirectory)).add(new ZipStep(new File(getPathToOutputFile()),getArchivePath().toFile())).build();
}
