{
  BuildRuleResolver resolver=new BuildRuleResolver(TargetGraph.EMPTY,new DefaultTargetNodeToBuildRuleTransformer());
  SourcePathResolver pathResolver=new SourcePathResolver(resolver);
  BuildRule rule=resolver.addToIndex(new FakeBuildRule("//foo:bar",pathResolver));
  SourcePath sourcePath1=new BuildTargetSourcePath(rule.getBuildTarget(),BuckConstant.GEN_PATH.resolve("foo").resolve("name1"));
  SourcePath sourcePath2=new BuildTargetSourcePath(rule.getBuildTarget(),BuckConstant.GEN_PATH.resolve("foo").resolve("name2"));
  pathResolver.getSourcePathNames(rule.getBuildTarget(),"srcs",ImmutableList.of(sourcePath1,sourcePath2));
}
