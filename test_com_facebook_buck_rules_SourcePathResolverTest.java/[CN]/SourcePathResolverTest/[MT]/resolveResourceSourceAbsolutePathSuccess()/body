{
  BuildRuleResolver resolver=new BuildRuleResolver(TargetGraph.EMPTY,new BuildTargetNodeToBuildRuleTransformer());
  SourcePathResolver pathResolver=new SourcePathResolver(resolver);
  PackagedResource packagedResource=PackagedResourceTestUtil.getPackagedResource("testdata/packaged_resource_one");
  ResourceSourcePath resourceSourcePathOne=new ResourceSourcePath(packagedResource);
  assertEquals(packagedResource.get(),pathResolver.getAbsolutePath(resourceSourcePathOne));
}
