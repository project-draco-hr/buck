{
  Path output=BuildTargets.getGenPath(getBuildTarget(),"%s");
  Path bundleRoot=output.resolve(getBuildTarget().getShortName() + "." + extension);
  return ImmutableList.of(new MkdirStep(bundleRoot),CopyStep.forFile(binary.getPathToOutputFile(),bundleRoot.resolve(getBuildTarget().getShortName())),new WriteFileStep("APPLWRUN",bundleRoot.resolve("PkgInfo")),CopyStep.forFile(getResolver().getPath(infoPlist.get()),bundleRoot.resolve("Info.plist")));
}
