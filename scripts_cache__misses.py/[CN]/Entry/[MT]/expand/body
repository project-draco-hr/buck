def expand(self, show_timestamp, show_event_name, show_result, show_info):
    r = ''
    if show_timestamp:
        r += (('\t\ttimestamp  = ' + self.timestamp) + '\n')
    if show_event_name:
        r += (('\t\tevent_name = ' + self.event_name) + '\n')
    if show_result:
        r += (('\t\tresult     = ' + self.result) + '\n')
    if show_info:
        r += (('\t\tinfo       = ' + self.info) + '\n')
    return r
