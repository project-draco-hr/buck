{
  String summary=compile("A.java",Joiner.on("\n").join("package com.facebook.buck.example;","public class A {","  public static class B {","    public class C {","      public void foo() {}","    }","  }","}"));
  assertTrue(summary,summary.contains("public class com.facebook.buck.example.A.B.C extends java.lang.Object"));
}
