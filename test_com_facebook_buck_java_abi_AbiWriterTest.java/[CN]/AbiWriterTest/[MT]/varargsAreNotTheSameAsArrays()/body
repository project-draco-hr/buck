{
  String original=compile("A.java",Joiner.on("\n").join("package com.facebook.buck.example;","public class A {","  public void doSomething(String... args) {}","}"));
  String amended=compile("A.java",Joiner.on("\n").join("package com.facebook.buck.example;","public class A {","  public void doSomething(String[] args) {}","}"));
  assertNotEquals(original,amended);
}
