{
  String summary=compile("A.java",Joiner.on("\n").join("package com.facebook.buck.example;","import java.util.Collection;","import java.util.Set;","public class A<T> {","  public Set<? extends Collection> extension;","  public Set<? super T> supered;","}"));
  assertTrue(summary,summary.contains("Set<? extends java.util.Collection> extension"));
  assertTrue(summary,summary.contains("Set<? super T> supered"));
}
