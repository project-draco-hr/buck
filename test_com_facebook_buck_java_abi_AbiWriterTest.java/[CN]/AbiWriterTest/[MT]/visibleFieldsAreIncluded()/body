{
  String summary=compile("A.java",Joiner.on("\n").join("package com.facebook.buck.example;","public class A {","  public int number = 42;","  private String magic = \"Now you see me\";","}"));
  assertTrue(summary,summary.contains("public int number"));
  assertFalse(summary,summary.contains("42"));
  assertFalse(summary,summary.contains("magic"));
}
