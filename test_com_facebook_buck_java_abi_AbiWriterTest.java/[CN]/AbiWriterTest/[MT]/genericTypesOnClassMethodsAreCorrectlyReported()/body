{
  String summary=compile("A.java",Joiner.on("\n").join("package com.facebook.buck.example;","public class A<Y> {","  public <X> X doSomething(Y foo, A bar, String baz) {","    return null;","  }","}"));
  assertTrue(summary,summary.contains("<X> X doSomething(Y, com.facebook.buck.example.A, java.lang.String)"));
}
