{
  String original=compile("A.java",Joiner.on("\n").join("package com.facebook.buck.example;","public class A {","  public int first;","  public int second;","}"));
  String abiCompatible=compile("A.java",Joiner.on("\n").join("package com.facebook.buck.example;","public class A {","  public int second;","  public int first;","}"));
  assertEquals(original,abiCompatible);
}
