{
  ProjectFilesystem filesystem=FakeProjectFilesystem.createJavaOnlyFilesystem();
  Config config=new Config(ConfigBuilder.rawFromLines());
  BuckConfig buckConfig=new BuckConfig(config,filesystem,Architecture.detect(),Platform.detect(),ImmutableMap.<String,String>builder().putAll(System.getenv()).put("envKey","envValue").build(),new DefaultCellPathResolver(filesystem.getRootPath(),config));
  Cell cell=new TestCellBuilder().setFilesystem(filesystem).setBuckConfig(buckConfig).build();
  BuildJobState dump=DistributedBuildState.dump(buckConfig,emptyActionGraph(),defaultCodec(cell,Optional.<Parser>absent()),TargetGraph.EMPTY);
  DistributedBuildState distributedBuildState=new DistributedBuildState(dump);
  assertThat(distributedBuildState.createBuckConfig(filesystem),Matchers.equalTo(buckConfig));
}
