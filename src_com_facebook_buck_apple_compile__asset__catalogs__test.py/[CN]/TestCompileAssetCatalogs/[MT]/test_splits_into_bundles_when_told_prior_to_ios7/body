def test_splits_into_bundles_when_told_prior_to_ios7(self):
    catalogs = ['A.xcassets', 'B.xcassets']
    pairs = actool_cmds('6.0', 'iphoneos', ['iphone'], 'output', catalogs, True)
    self.assertTrue((len(pairs) == 2))
    (c, d) = pairs[0]
    s = ' '.join(c)
    self.assertTrue(('--compile {0} {1}'.format(os.path.join('output', 'A.bundle'), 'A.xcassets') in s))
    (c, d) = pairs[1]
    s = ' '.join(c)
    self.assertTrue(('--compile {0} {1}'.format(os.path.join('output', 'B.bundle'), 'B.xcassets') in s))
