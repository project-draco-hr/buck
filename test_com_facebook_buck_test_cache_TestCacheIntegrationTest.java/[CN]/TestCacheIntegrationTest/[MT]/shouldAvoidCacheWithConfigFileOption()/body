{
  String newBuckProject=Joiner.on('\n').join("[test]","  use_cached_results = no");
  Files.write(workspace.getPath(".buckconfig"),newBuckProject.getBytes(UTF_8));
  assertThat("First run should not be cached",run("test","--all"),not(containsString("CACHED")));
  assertThat("Second run should also not be cached",run("test","--all","--no-results-cache"),not(containsString("CACHED")));
}
