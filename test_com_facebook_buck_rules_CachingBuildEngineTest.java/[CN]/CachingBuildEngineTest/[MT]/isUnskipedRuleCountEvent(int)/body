{
  return both(Matchers.<BuckEvent>instanceOf(BuildEvent.UnskippedRuleCountUpdated.class)).and(new FeatureMatcher<BuckEvent,Integer>(equalTo(numRules),"reported number of unskipped rules","numRules"){
    @Override protected Integer featureValueOf(    BuckEvent event){
      return ((BuildEvent.UnskippedRuleCountUpdated)event).getNumRules();
    }
  }
);
}
