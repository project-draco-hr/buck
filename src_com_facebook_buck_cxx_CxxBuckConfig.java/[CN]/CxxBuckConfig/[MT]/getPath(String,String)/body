{
  Optional<String> rawPath=delegate.getValue(cxxSection,flavor + "_" + name).or(delegate.getValue(cxxSection,name));
  if (!rawPath.isPresent()) {
    return Optional.absent();
  }
  return Optional.fromNullable(delegate.resolvePathThatMayBeOutsideTheProjectFilesystem(Paths.get(rawPath.get())));
}
