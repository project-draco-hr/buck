{
  Optional<CxxPreprocessMode> setting=delegate.getEnum("cxx","preprocess_mode",CxxPreprocessMode.class);
  if (setting.isPresent()) {
    return setting.get();
  }
  return delegate.getBooleanValue("cxx","combined_preprocess_and_compile",false) ? CxxPreprocessMode.COMBINED : CxxPreprocessMode.SEPARATE;
}
