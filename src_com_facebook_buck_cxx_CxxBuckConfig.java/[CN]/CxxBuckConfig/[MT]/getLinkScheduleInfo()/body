{
  Optional<Long> linkWeight=delegate.getLong(cxxSection,"link_weight");
  if (!linkWeight.isPresent()) {
    return Optional.absent();
  }
  return Optional.of(RuleScheduleInfo.builder().setJobsMultiplier(linkWeight.get().intValue()).setResourceAmounts(ResourceAmountsEstimator.DEFAULT_AMOUNTS.withCpu(linkWeight.get().intValue())).build());
}
