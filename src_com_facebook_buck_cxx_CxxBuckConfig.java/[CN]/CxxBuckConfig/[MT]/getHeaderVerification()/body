{
  return HeaderVerification.builder().setMode(delegate.getEnum(cxxSection,"untracked_headers",HeaderVerification.Mode.class).or(HeaderVerification.Mode.IGNORE)).addAllWhitelist(FluentIterable.from(delegate.getListWithoutComments(cxxSection,"untracked_headers_whitelist")).transform(new Function<String,Pattern>(){
    @Override public Pattern apply(    String input){
      return Pattern.compile(input);
    }
  }
)).build();
}
