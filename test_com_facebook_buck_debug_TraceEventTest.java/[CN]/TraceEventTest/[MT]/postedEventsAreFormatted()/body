{
  ThreadTrace.initCurrentTrace(0,new EventBus());
  ThreadTrace threadTrace=ThreadTrace.threadTracer.get();
  long id=42;
  threadTrace.traceStarted(TraceEvents.started(id,"test-trace","type",0));
  threadTrace.traceStopped(TraceEvents.stopped(id,"test-trace","type",0));
  assertThat(threadTrace.getFormattedTrace(),containsString("[type] test-trace"));
}
