{
  BuildRuleParams params=new BuildRuleParams(BuildTargetFactory.newInstance("//one:example"),ImmutableSortedSet.<BuildRule>of(),ImmutableSet.<BuildTargetPattern>of(),new FakeProjectFilesystem(),new FakeRuleKeyBuilderFactory());
  ExampleBuildable expected=new ExampleBuildable(params.getBuildTarget(),"nada");
  DescribedRule rule=new DescribedRule(new BuildRuleType("example"),expected,params);
  Buildable seen=rule.getBuildable();
  assertSame(expected,seen);
}
