{
  BuildRuleResolver resolver=new BuildRuleResolver(TargetGraph.EMPTY,new DefaultTargetNodeToBuildRuleTransformer());
  SourcePathResolver pathResolver=new SourcePathResolver(resolver);
  SymlinkTree symlinkTree=new SymlinkTree(new FakeBuildRuleParamsBuilder(buildTarget).build(),pathResolver,outputPath,ImmutableMap.<Path,SourcePath>of(Paths.get("../something"),new PathSourcePath(projectFilesystem,MorePaths.relativize(tmpDir.getRoot().toPath(),tmpDir.newFile().toPath()))));
  int exitCode=symlinkTree.getVerifiyStep().execute(TestExecutionContext.newInstance());
  assertThat(exitCode,Matchers.not(Matchers.equalTo(0)));
}
