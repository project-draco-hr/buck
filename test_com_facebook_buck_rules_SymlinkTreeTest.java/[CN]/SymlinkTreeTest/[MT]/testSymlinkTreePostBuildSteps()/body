{
  BuildContext buildContext=FakeBuildContext.NOOP_CONTEXT;
  FakeBuildableContext buildableContext=new FakeBuildableContext();
  ProjectFilesystem filesystem=FakeProjectFilesystem.createJavaOnlyFilesystem();
  ImmutableList<Step> expectedBuildSteps=ImmutableList.of(new MakeCleanDirectoryStep(filesystem,outputPath),new SymlinkTreeStep(filesystem,outputPath,new SourcePathResolver(new BuildRuleResolver()).getMappedPaths(links)));
  ImmutableList<Step> actualBuildSteps=symlinkTreeBuildRule.getPostBuildSteps(buildContext,buildableContext);
  assertEquals(expectedBuildSteps,actualBuildSteps);
}
