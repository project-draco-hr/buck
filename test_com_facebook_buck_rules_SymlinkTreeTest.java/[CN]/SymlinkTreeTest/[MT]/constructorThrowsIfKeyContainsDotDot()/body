{
  BuildRuleResolver resolver=new BuildRuleResolver(TargetGraph.EMPTY,new BuildTargetNodeToBuildRuleTransformer());
  SourcePathResolver pathResolver=new SourcePathResolver(resolver);
  exception.expect(SymlinkTree.InvalidSymlinkTreeException.class);
  new SymlinkTree(new FakeBuildRuleParamsBuilder(buildTarget).build(),pathResolver,outputPath,ImmutableMap.<Path,SourcePath>of(Paths.get("../something"),new PathSourcePath(projectFilesystem,MorePaths.relativize(tmpDir.getRoot().toPath(),tmpDir.newFile().toPath()))));
}
