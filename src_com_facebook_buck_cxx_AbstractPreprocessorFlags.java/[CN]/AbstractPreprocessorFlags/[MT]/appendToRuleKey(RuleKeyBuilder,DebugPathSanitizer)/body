{
  builder.setReflectively("prefixHeader",getPrefixHeader());
  builder.setReflectively("frameworkRoots",getFrameworkPaths());
  builder.setReflectively("platformPreprocessorFlags",sanitizer.sanitizeFlags(getOtherFlags().getPlatformFlags()));
  builder.setReflectively("rulePreprocessorFlags",sanitizer.sanitizeFlags(getOtherFlags().getRuleFlags()));
  return builder;
}
