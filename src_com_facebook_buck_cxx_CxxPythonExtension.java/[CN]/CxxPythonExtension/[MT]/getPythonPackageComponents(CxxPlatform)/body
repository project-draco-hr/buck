{
  BuildRule extension=CxxDescriptionEnhancer.requireBuildRule(params,ruleResolver,cxxPlatform.getFlavor(),CxxDescriptionEnhancer.SHARED_FLAVOR);
  SourcePath output=new BuildTargetSourcePath(extension.getBuildTarget());
  return ImmutablePythonPackageComponents.of(ImmutableMap.of(module,output),ImmutableMap.<Path,SourcePath>of(),ImmutableMap.<Path,SourcePath>of());
}
