{
  BuildRule extension=CxxDescriptionEnhancer.requireBuildRule(params,ruleResolver,CxxDescriptionEnhancer.SHARED_FLAVOR);
  SourcePath output=new BuildTargetSourcePath(extension.getBuildTarget());
  return new PythonPackageComponents(ImmutableMap.of(module,output),ImmutableMap.<Path,SourcePath>of(),ImmutableMap.<Path,SourcePath>of());
}
