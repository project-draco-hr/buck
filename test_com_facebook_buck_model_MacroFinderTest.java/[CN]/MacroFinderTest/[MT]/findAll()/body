{
  ImmutableList<MacroMatchResult> expectedResults=ImmutableList.of(MacroMatchResult.builder().setMacroType("macro1").setStartIndex(12).setEndIndex(21).build(),MacroMatchResult.builder().setMacroType("macro2").addMacroInput("arg").setStartIndex(26).setEndIndex(39).build(),MacroMatchResult.builder().setMacroType("macro1").addMacroInput("arg","arg2").setStartIndex(40).setEndIndex(58).build());
  ImmutableList<MacroMatchResult> actualResults=FINDER.findAll(ImmutableSet.of("macro1","macro2"),"hello world $(macro1) and $(macro2 arg) $(macro1 arg arg2)");
  assertEquals(expectedResults,actualResults);
}
