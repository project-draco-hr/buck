{
  return new Function<ImmutableSortedSet<FrameworkPath>,ImmutableList<String>>(){
    @Override public ImmutableList<String> apply(    ImmutableSortedSet<FrameworkPath> input){
      return FluentIterable.from(input).transformAndConcat(linkerFlagsForFrameworkPathFunction(resolver.deprecatedPathFunction())).toList();
    }
  }
;
}
