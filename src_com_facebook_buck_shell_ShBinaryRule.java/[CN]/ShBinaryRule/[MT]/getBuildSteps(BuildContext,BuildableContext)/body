{
  MakeCleanDirectoryStep mkdir=new MakeCleanDirectoryStep(output.getParent().toString());
  GenerateShellScriptStep generateShellScript=new GenerateShellScriptStep(Paths.get(getBuildTarget().getBasePath()),main,SourcePaths.toPaths(resources,context),output);
  return ImmutableList.of(mkdir,generateShellScript);
}
