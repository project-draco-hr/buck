{
  super(buildRuleParams);
  this.main=Preconditions.checkNotNull(main);
  this.resources=Preconditions.checkNotNull(resources);
  BuildTarget target=buildRuleParams.getBuildTarget();
  this.output=Paths.get(String.format("%s/%s%s.sh",BuckConstant.GEN_DIR,target.getBasePathWithSlash(),target.getShortName()));
}
