{
  ImmutableList.Builder<Path> builder=ImmutableList.builder();
  builder.add(binaryJar);
  Optionals.addIfPresent(sourceJar,builder);
  if (gwtJar.isPresent()) {
    SourcePath gwtJarSourcePath=gwtJar.get();
    Collection<Path> safePaths=SourcePaths.filterInputsToCompareToOutput(Collections.singleton(gwtJarSourcePath));
    builder.addAll(safePaths);
  }
  return builder.build();
}
