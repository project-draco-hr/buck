{
switch (field) {
case FILE_SYSTEM_ROOT_NAME:
    if (value == null) {
      unsetFileSystemRootName();
    }
 else {
      setFileSystemRootName((String)value);
    }
  break;
case ENTRIES:
if (value == null) {
  unsetEntries();
}
 else {
  setEntries((List<BuildJobStateFileHashEntry>)value);
}
break;
}
}
