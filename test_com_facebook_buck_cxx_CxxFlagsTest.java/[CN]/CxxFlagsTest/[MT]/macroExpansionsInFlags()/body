{
  assertThat(CxxFlags.getFlags(Optional.of(ImmutableList.of("-I$MACRO")),Optional.of(new PatternMatchedCollection.Builder<ImmutableList<String>>().add(Pattern.compile(".*"),ImmutableList.of("-F$MACRO")).build()),CxxPlatformUtils.DEFAULT_PLATFORM.withFlagMacros(ImmutableMap.of("MACRO","expansion"))),containsInAnyOrder("-Iexpansion","-Fexpansion"));
  assertThat(CxxFlags.getLanguageFlags(Optional.of(ImmutableList.of("-I$MACRO")),Optional.of(new PatternMatchedCollection.Builder<ImmutableList<String>>().add(Pattern.compile(".*"),ImmutableList.of("-F$MACRO")).build()),Optional.of(ImmutableMap.of(CxxSource.Type.C,ImmutableList.of("-isystem$MACRO"))),CxxPlatformUtils.DEFAULT_PLATFORM.withFlagMacros(ImmutableMap.of("MACRO","expansion"))).get(CxxSource.Type.C),containsInAnyOrder("-Iexpansion","-isystemexpansion","-Fexpansion"));
}
