{
  ImmutableMultimap<CxxSource.Type,String> flags=CxxPreprocessorFlags.fromArgs(Optional.of(ImmutableList.of("flag")),Optional.<ImmutableMap<CxxSource.Type,ImmutableList<String>>>absent());
  assertThat(ImmutableSet.copyOf(CxxSource.Type.values()),equalTo(flags.keySet()));
  assertThat(flags.values(),everyItem(equalTo("flag")));
}
