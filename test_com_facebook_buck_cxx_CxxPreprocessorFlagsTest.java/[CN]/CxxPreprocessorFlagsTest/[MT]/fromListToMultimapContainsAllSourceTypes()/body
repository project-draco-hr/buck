{
  ImmutableMultimap<CxxSource.Type,String> flags=CxxPreprocessorFlags.fromArgs(ImmutableList.of("flag"),ImmutableMap.<CxxSource.Type,ImmutableList<String>>of());
  assertThat(ImmutableSet.copyOf(CxxSource.Type.values()),equalTo(flags.keySet()));
  assertThat(flags.values(),everyItem(equalTo("flag")));
}
