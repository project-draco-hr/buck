{
  if (this == o) {
    return true;
  }
  if (!(o instanceof CxxHeaderSourceSpec)) {
    return false;
  }
  CxxHeaderSourceSpec that=(CxxHeaderSourceSpec)o;
  if (!cxxHeaders.equals(that.cxxHeaders)) {
    return false;
  }
  if (!cxxSources.equals(that.cxxSources)) {
    return false;
  }
  return true;
}
