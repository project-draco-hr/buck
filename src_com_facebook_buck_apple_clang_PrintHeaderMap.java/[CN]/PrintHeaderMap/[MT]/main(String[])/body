{
  if (args.length < 1) {
    System.err.println("Usage: java -jar hmaptool.jar HEADER-MAP-FILE(S)");
    System.exit(1);
  }
  try {
    for (int i=0; i < args.length; i++) {
      process(FileSystems.getDefault().getPath(args[i]).toFile());
    }
  }
 catch (  IOException e) {
    System.err.println(e.toString());
    System.exit(1);
  }
}
