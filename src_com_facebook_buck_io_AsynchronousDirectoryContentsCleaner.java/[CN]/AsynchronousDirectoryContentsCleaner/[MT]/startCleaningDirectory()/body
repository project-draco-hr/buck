{
  executor.execute(new Runnable(){
    @Override public void run(){
      LOG.debug("Starting to clean %s",pathToClean);
      try {
        MoreFiles.deleteRecursivelyWithOptions(pathToClean,EnumSet.of(MoreFiles.DeleteRecursivelyOptions.DELETE_CONTENTS_ONLY),MoreFiles.ErrorHandler.warn("I/O error cleaning trash"));
      }
 catch (      IOException e) {
        throw new RuntimeException(e);
      }
 finally {
        LOG.debug("Done cleaning %s",pathToClean);
      }
    }
  }
);
}
