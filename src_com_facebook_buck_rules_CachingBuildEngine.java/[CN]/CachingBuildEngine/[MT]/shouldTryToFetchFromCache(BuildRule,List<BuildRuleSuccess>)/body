{
  if (rule.getCacheMode() == CacheMode.DISABLED) {
    return false;
  }
  for (  BuildRuleSuccess success : ruleSuccesses) {
    if (success.getType() == BuildRuleSuccess.Type.BUILT_LOCALLY && success.getRule().getCacheMode() == CacheMode.ENABLED) {
      return false;
    }
  }
  return true;
}
