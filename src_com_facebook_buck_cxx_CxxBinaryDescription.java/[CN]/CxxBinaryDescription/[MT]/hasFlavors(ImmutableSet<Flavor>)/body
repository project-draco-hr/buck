{
  Set<Flavor> flavors=inputFlavors;
  Set<Flavor> platformFlavors=Sets.intersection(flavors,cxxPlatforms.getFlavors());
  if (platformFlavors.size() > 1) {
    return false;
  }
  flavors=Sets.difference(flavors,platformFlavors);
  Set<Flavor> typeFlavors=Sets.intersection(flavors,ImmutableSet.of(CxxDescriptionEnhancer.HEADER_SYMLINK_TREE_FLAVOR,CxxDescriptionEnhancer.HEADER_MAP_FILE_FLAVOR));
  if (typeFlavors.size() > 1) {
    return false;
  }
  flavors=Sets.difference(flavors,typeFlavors);
  return flavors.isEmpty();
}
