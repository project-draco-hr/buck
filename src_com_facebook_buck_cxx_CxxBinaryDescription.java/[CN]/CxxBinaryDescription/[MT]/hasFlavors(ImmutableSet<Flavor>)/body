{
  Set<Flavor> flavors=inputFlavors;
  Set<Flavor> platformFlavors=Sets.intersection(flavors,cxxPlatforms.getFlavors());
  if (platformFlavors.size() > 1) {
    return false;
  }
  flavors=Sets.difference(flavors,platformFlavors);
  flavors=Sets.difference(flavors,ImmutableSet.of(CxxDescriptionEnhancer.HEADER_SYMLINK_TREE_FLAVOR,CxxCompilationDatabase.COMPILATION_DATABASE,CxxCompilationDatabase.UBER_COMPILATION_DATABASE,CxxInferEnhancer.INFER,CxxInferEnhancer.INFER_ANALYZE));
  return flavors.isEmpty();
}
