{
  return ImmutableList.of(new MkdirStep(getProjectFilesystem(),output.getParent()),new GoTestMainStep(getProjectFilesystem().getRootPath(),testMainGen.getCommandPrefix(getResolver()),"",ImmutableMap.<Path,ImmutableMap<String,Path>>of(),testPackage,FluentIterable.from(testSources).transform(getResolver().getAbsolutePathFunction()).toList(),output));
}
