{
  int jobsDone=0;
  while (jobsDone < TOTAL_JOBS) {
    distBuildService.doMagic();
    DistBuildStatus status=DistBuildStatus.builder().setPercentProgress(((float)jobsDone) / TOTAL_JOBS).build();
    eventBus.post(new DistBuildStatusEvent(status));
    jobsDone+=TOTAL_JOBS / 3;
  }
  return 0;
}
