{
  this(console,repository,androidDirectoryResolver,new CachingBuildEngine(),artifactCacheFactory,eventBus,Parser.createParser(repositoryFactory,pythonInterpreter,ImmutableSet.<Pattern>of(),new RuleKeyBuilderFactory(){
