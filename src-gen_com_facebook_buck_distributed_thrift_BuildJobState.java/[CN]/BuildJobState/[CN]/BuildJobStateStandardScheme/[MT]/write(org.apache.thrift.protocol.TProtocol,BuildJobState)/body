{
  struct.validate();
  oprot.writeStructBegin(STRUCT_DESC);
  if (struct.buckConfig != null) {
    if (struct.isSetBuckConfig()) {
      oprot.writeFieldBegin(BUCK_CONFIG_FIELD_DESC);
      struct.buckConfig.write(oprot);
      oprot.writeFieldEnd();
    }
  }
  oprot.writeFieldStop();
  oprot.writeStructEnd();
}
