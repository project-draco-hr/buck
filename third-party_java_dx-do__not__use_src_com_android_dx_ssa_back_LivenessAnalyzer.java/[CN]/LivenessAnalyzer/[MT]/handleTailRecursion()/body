{
  while (nextFunction != NextFunction.DONE) {
switch (nextFunction) {
case LIVE_IN_AT_STATEMENT:
      nextFunction=NextFunction.DONE;
    liveInAtStatement();
  break;
case LIVE_OUT_AT_STATEMENT:
nextFunction=NextFunction.DONE;
liveOutAtStatement();
break;
case LIVE_OUT_AT_BLOCK:
nextFunction=NextFunction.DONE;
liveOutAtBlock();
break;
default :
}
}
}
