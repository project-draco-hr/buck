{
  if (m_indent <= 0)   return "";
 else {
    if ((m_sindent == null) || (m_sindent.length() < m_indent)) {
      final char[] ca=new char[m_indent];
      for (int i=0; i < m_indent; ++i)       ca[i]=' ';
      m_sindent=new String(ca);
      return m_sindent;
    }
 else {
      return m_sindent.substring(0,m_indent);
    }
  }
}
