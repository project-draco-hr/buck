{
  builder.setReflectively("compiler",getResolver().getPath(compiler)).setReflectively("flags",flags).setReflectively("outputDir",outputDir.toString()).setReflectively("options",ImmutableSortedSet.copyOf(options)).setReflectively("language",language);
  for (  Path path : ImmutableSortedSet.copyOf(includes.keySet())) {
    builder.setReflectively("include(" + path + ")",includes.get(path));
  }
  return builder;
}
