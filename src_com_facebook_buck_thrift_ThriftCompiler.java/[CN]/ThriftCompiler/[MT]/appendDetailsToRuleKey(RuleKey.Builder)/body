{
  builder.setInput("compiler",compiler.resolve()).set("flags",flags).set("outputDir",outputDir.toString()).set("options",ImmutableSortedSet.copyOf(options)).set("language",language);
  for (  Path path : ImmutableSortedSet.copyOf(includes.keySet())) {
    SourcePath source=includes.get(path);
    builder.setInput("include(" + path + ")",source.resolve());
  }
  return builder;
}
