{
  builder.setInput("compiler",getResolver().getPath(compiler)).set("flags",flags).set("outputDir",outputDir.toString()).set("options",ImmutableSortedSet.copyOf(options)).set("language",language);
  for (  Path path : ImmutableSortedSet.copyOf(includes.keySet())) {
    builder.setInput("include(" + path + ")",includes.get(path));
  }
  return builder;
}
