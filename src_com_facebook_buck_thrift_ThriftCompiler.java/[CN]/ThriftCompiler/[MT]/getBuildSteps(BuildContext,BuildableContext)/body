{
  for (  String source : generatedSources) {
    buildableContext.recordArtifact(outputDir.resolve(resolveLanguageDir(language,source)));
  }
  return ImmutableList.of(new MakeCleanDirectoryStep(getProjectFilesystem(),outputDir),new ThriftCompilerStep(getProjectFilesystem().getRootPath(),ImmutableList.<String>builder().addAll(compiler.getCommandPrefix(getResolver())).addAll(flags).build(),outputDir,getResolver().deprecatedGetPath(input),language,options,FluentIterable.from(headerMaps).append(includeRoots).toList()));
}
