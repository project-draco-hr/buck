{
  this.projectFilesystem=projectFilesystem;
  this.targetGraph=targetGraph;
  this.projectConfigTargets=projectConfigTargets;
  this.projectGenerators=null;
  this.projectGeneratorOptions=ImmutableSet.<ProjectGenerator.Option>builder().addAll(projectGeneratorOptions).addAll(ProjectGenerator.SEPARATED_PROJECT_OPTIONS).build();
  for (  BuildTarget target : projectConfigTargets) {
    TargetNode<?> node=this.targetGraph.get(target);
    if (node == null) {
      throw new HumanReadableException("target not found: " + target.toString());
    }
    if (!node.getType().equals(XcodeProjectConfigDescription.TYPE)) {
      throw new HumanReadableException("expected only 'xcode_project_config' rules, got a '" + node.getType().toString() + "' rule: "+ target.toString());
    }
  }
}
