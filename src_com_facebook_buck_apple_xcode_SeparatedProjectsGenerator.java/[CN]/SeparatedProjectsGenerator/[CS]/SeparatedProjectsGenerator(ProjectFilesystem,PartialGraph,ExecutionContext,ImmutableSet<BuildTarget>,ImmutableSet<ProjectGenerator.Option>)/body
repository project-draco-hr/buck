{
  this.projectFilesystem=Preconditions.checkNotNull(projectFilesystem);
  this.partialGraph=Preconditions.checkNotNull(partialGraph);
  this.executionContext=Preconditions.checkNotNull(executionContext);
  this.projectConfigTargets=Preconditions.checkNotNull(projectConfigTargets);
  this.projectGenerators=null;
  this.projectGeneratorOptions=ImmutableSet.<ProjectGenerator.Option>builder().addAll(projectGeneratorOptions).addAll(ProjectGenerator.SEPARATED_PROJECT_OPTIONS).build();
  for (  BuildTarget target : projectConfigTargets) {
    BuildRule rule=partialGraph.getActionGraph().findBuildRuleByTarget(target);
    if (rule == null) {
      throw new HumanReadableException("target not found: " + target.toString());
    }
    if (!rule.getType().equals(XcodeProjectConfigDescription.TYPE)) {
      throw new HumanReadableException("expected only xcode_project_config rules, got: " + target.toString());
    }
  }
}
