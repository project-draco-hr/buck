{
  AstyanaxContext<Keyspace> mockContext=createMock(AstyanaxContext.class);
  BuckEventBus mockEventBus=createMock(BuckEventBus.class);
  FileHashCache fileHashCache=FakeFileHashCache.createFromStrings(Maps.<String,String>newHashMap());
  mockContext.shutdown();
  replay(mockContext);
  CassandraArtifactCache cache=new CassandraArtifactCache(10,true,mockEventBus,fileHashCache,mockContext);
  cache.close();
  verify(mockContext);
}
