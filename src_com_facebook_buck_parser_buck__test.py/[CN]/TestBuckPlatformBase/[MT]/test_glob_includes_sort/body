def test_glob_includes_sort(self):
    search_base = self.fake_path('foo', glob_results={'*.java': ['A.java', 'E.java', 'D.java', 'C.java', 'B.java'], })
    self.assertGlobMatches(['A.java', 'B.java', 'C.java', 'D.java', 'E.java'], glob_internal(includes=['*.java'], excludes=[], include_dotfiles=False, allow_empty=False, search_base=search_base))
