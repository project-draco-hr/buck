def test_glob_excludes_multi(self):
    search_base = self.fake_path('foo', glob_results={'bar/*.java': ['bar/A.java', 'bar/B.java'], 'baz/*.java': ['baz/C.java', 'baz/D.java'], })
    self.assertGlobMatches(['bar/B.java', 'baz/D.java'], glob_internal(includes=['bar/*.java', 'baz/*.java'], excludes=['*/[AC].java'], include_dotfiles=False, allow_empty=False, search_base=search_base))
