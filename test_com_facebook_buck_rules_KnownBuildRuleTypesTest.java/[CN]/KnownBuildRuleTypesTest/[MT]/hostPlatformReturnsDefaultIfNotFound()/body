{
  ImmutableMap<String,ImmutableMap<String,String>> sections=ImmutableMap.of("cxx",ImmutableMap.of("host_platform","baz"));
  BuckConfig buckConfig=FakeBuckConfig.builder().setSections(sections).build();
  CxxPlatform hostPlatform=KnownBuildRuleTypes.getHostCxxPlatformFromConfig(new CxxBuckConfig(buckConfig),FAKE_CXX_PLATFORMS,CxxPlatformUtils.DEFAULT_PLATFORM);
  assertEquals(CxxPlatformUtils.DEFAULT_PLATFORM,hostPlatform);
}
