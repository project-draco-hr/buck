{
  final File javac=temporaryFolder.newFile();
  javac.setExecutable(true);
  Map<String,Map<String,String>> sections=ImmutableMap.of("tools",(Map<String,String>)ImmutableMap.of("javac",javac.toString()));
  FakeBuckConfig buckConfig=new FakeBuckConfig(sections);
  KnownBuildRuleTypes buildRuleTypes=KnownBuildRuleTypes.createBuilder(buckConfig,new FakeAndroidDirectoryResolver(),new JavaCompilerEnvironment(buckConfig.getJavac(),Optional.<JavacVersion>absent())).build();
  BuildRuleFactory<?> factory=buildRuleTypes.getFactory(BuildRuleType.JAVA_LIBRARY);
  BuildRule rule=factory.newInstance(params).build(new BuildRuleResolver());
  assertTrue("Rule is DefaultJavaLibraryRule",rule instanceof DefaultJavaLibrary);
  DefaultJavaLibrary libraryRule=(DefaultJavaLibrary)rule;
  assertEquals(javac.toPath(),libraryRule.getJavac().get());
}
