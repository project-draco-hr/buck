{
  Dot<BuildRule> dot=new Dot<BuildRule>(actionGraph,"action_graph",new Function<BuildRule,String>(){
    @Override public String apply(    BuildRule buildRule){
      return "\"" + buildRule.getFullyQualifiedName() + "\"";
    }
  }
,getStdOut());
  try {
    dot.writeOutput();
  }
 catch (  IOException e) {
    return 1;
  }
  return 0;
}
