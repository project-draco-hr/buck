{
  Dot<TargetNode<?>> dot=new Dot<>(actionGraph,"action_graph",new Function<TargetNode<?>,String>(){
    @Override public String apply(    TargetNode<?> targetNode){
      return "\"" + targetNode.getBuildTarget().getFullyQualifiedName() + "\"";
    }
  }
,getStdOut());
  try {
    dot.writeOutput();
  }
 catch (  IOException e) {
    return 1;
  }
  return 0;
}
