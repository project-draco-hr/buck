{
switch (headerVisibility) {
case PUBLIC:
    return CxxPreprocessorInput.builder().from(CxxPreprocessables.getCxxPreprocessorInput(targetGraph,params,ruleResolver,cxxPlatform.getFlavor(),headerVisibility,CxxPreprocessables.IncludeType.SYSTEM,exportedPreprocessorFlags.apply(cxxPlatform),ImmutableList.<FrameworkPath>of())).addAllSystemIncludeRoots(ImmutableSortedSet.copyOf(includeDirs)).build();
case PRIVATE:
  return CxxPreprocessorInput.EMPTY;
}
throw new RuntimeException("Invalid header visibility: " + headerVisibility);
}
