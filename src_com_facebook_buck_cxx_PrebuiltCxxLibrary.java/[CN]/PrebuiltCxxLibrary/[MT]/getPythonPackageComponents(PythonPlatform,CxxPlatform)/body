{
  String resolvedSoname=PrebuiltCxxLibraryDescription.getSoname(getBuildTarget(),cxxPlatform,soname,libName);
  ImmutableMap.Builder<Path,SourcePath> nativeLibrariesBuilder=ImmutableMap.builder();
  if (!headerOnly && !provided && linkage != Linkage.STATIC) {
    SourcePath sharedLibrary=requireSharedLibrary(cxxPlatform);
    nativeLibrariesBuilder.put(Paths.get(resolvedSoname),sharedLibrary);
  }
  ImmutableMap<Path,SourcePath> nativeLibraries=nativeLibrariesBuilder.build();
  return PythonPackageComponents.of(ImmutableMap.<Path,SourcePath>of(),ImmutableMap.<Path,SourcePath>of(),nativeLibraries,ImmutableSet.<SourcePath>of(),Optional.<Boolean>absent());
}
