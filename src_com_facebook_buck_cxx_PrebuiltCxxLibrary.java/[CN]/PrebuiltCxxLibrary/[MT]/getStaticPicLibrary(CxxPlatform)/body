{
  Path staticPicLibraryPath=PrebuiltCxxLibraryDescription.getStaticPicLibraryPath(getBuildTarget(),cxxPlatform,libDir,libName);
  if (params.getProjectFilesystem().exists(staticPicLibraryPath)) {
    return Optional.of(staticPicLibraryPath);
  }
  Path staticLibraryPath=PrebuiltCxxLibraryDescription.getStaticLibraryPath(getBuildTarget(),cxxPlatform,libDir,libName);
  if (params.getProjectFilesystem().exists(staticLibraryPath)) {
    return Optional.of(staticLibraryPath);
  }
  return Optional.absent();
}
