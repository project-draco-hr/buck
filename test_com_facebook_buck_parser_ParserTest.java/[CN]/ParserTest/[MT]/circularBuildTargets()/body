{
  return ImmutableMap.<BuildTarget,BuildRuleBuilder>builder().put(BuildTargetFactory.newInstance("//:A"),createBuildRuleBuilder("A","B","C")).put(BuildTargetFactory.newInstance("//:B"),createBuildRuleBuilder("B","D","E")).put(BuildTargetFactory.newInstance("//:C"),createBuildRuleBuilder("C","E")).put(BuildTargetFactory.newInstance("//:D"),createBuildRuleBuilder("D","F")).put(BuildTargetFactory.newInstance("//:E"),createBuildRuleBuilder("E","F")).put(BuildTargetFactory.newInstance("//:F"),createBuildRuleBuilder("F","C")).build();
}
