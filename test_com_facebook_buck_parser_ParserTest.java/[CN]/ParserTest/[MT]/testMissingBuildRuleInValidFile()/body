{
  BuildTarget fooTarget=BuildTargetFactory.newInstance("//java/com/facebook","foo",testBuildFile);
  BuildTarget razTarget=BuildTargetFactory.newInstance("//java/com/facebook","raz",testBuildFile);
  Iterable<BuildTarget> buildTargets=ImmutableList.of(fooTarget,razTarget);
  Iterable<String> defaultIncludes=ImmutableList.of();
  try {
    testParser.parseBuildFilesForTargets(buildTargets,defaultIncludes);
    fail("HumanReadableException should be thrown");
  }
 catch (  HumanReadableException e) {
    assertEquals("No rule found when resolving target //java/com/facebook:raz in build file " + "//java/com/facebook/BUCK",e.getHumanReadableErrorMessage());
  }
}
