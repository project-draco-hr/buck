{
  TestProjectBuildFileParserFactory buildFileParserFactory=new TestProjectBuildFileParserFactory(filesystem);
  Parser parser=createParser(emptyBuildTargets());
  parseBuildFile(testBuildFile,parser,buildFileParserFactory);
  WatchEvent<Path> event=createPathEvent(tempDir.newFile("SomeClass.java__backup"),StandardWatchEventKinds.ENTRY_MODIFY);
  parser.onFileSystemChange(event);
  parseBuildFile(testBuildFile,parser,buildFileParserFactory);
  assertEquals("Should have not invalidated cache.",1,buildFileParserFactory.calls);
}
