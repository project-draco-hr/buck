{
  parser.getRawTargetNodes(eventBus,cell,false,executorService,testBuildFile);
  WatchEvent<Path> event=createPathEvent(MorePaths.relativize(tempDir.getRoot().toRealPath(),includedByIncludeFile),StandardWatchEventKinds.ENTRY_MODIFY);
  parser.onFileSystemChange(event);
  parser.getRawTargetNodes(eventBus,cell,false,executorService,testBuildFile);
  assertEquals("Should have invalidated cache.",2,counter.calls);
}
