{
  return FluentIterable.from(parser.buildTargetGraphForTargetNodeSpecs(buckEventBus,cell,false,executor,ImmutableList.of(TargetNodePredicateSpec.of(filter,BuildFileSpec.fromRecursivePath(Paths.get("")))),false).getTargetGraph().getNodes()).filter(filter).transform(HasBuildTarget.TO_TARGET).toSet();
}
