{
  return FluentIterable.from(parser.buildTargetGraphForTargetNodeSpecs(ImmutableList.of(TargetNodePredicateSpec.of(filter,BuildFileSpec.fromRecursivePath(Paths.get(""),filesystem.getIgnorePaths()))),parserConfig,buckEventBus,console,environment,enableProfiling).getSecond().getNodes()).filter(filter).transform(HasBuildTarget.TO_TARGET).toSet();
}
