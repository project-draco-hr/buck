{
  Parser parser=new Parser(repository,ImmutableMap.copyOf(System.getenv()),buildFileTreeSupplier,buildTargetParser,buildFileParserFactory,tempFilePatterns,new FakeRuleKeyBuilderFactory());
  try {
    parser.parseRawRulesInternal(rules);
  }
 catch (  BuildTargetException|IOException e) {
    throw new RuntimeException(e);
  }
  return parser;
}
