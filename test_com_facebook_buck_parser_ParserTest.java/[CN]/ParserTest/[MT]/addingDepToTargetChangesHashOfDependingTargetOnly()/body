{
  Parser parser=createParser(emptyBuildTargets());
  tempDir.newFolder("foo");
  File testFooBuckFile=tempDir.newFile("foo/" + BuckConstant.BUILD_RULES_FILE_NAME);
  Files.write("java_library(name = 'lib', visibility=['PUBLIC'], deps=[':lib2'])\n" + "java_library(name = 'lib2', visibility=['PUBLIC'])\n",testFooBuckFile,Charsets.UTF_8);
  BuildTarget fooLibTarget=BuildTarget.builder("//foo","lib").build();
  BuildTarget fooLib2Target=BuildTarget.builder("//foo","lib2").build();
  assertEquals(ImmutableMap.of(fooLibTarget,HashCode.fromString("c116d53f8789387c63cb79ad1fa0a1c23bee3524"),fooLib2Target,HashCode.fromString("3fafb8b650251f4264af02227c52ae75f94e8dfd")),buildTargetGraphAndGetHashCodes(parser,fooLibTarget,fooLib2Target));
}
