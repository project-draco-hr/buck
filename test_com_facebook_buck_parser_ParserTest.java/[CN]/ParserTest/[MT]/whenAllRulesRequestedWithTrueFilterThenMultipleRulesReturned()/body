{
  ImmutableSet<BuildTarget> targets=testParser.filterAllTargetsInProject(filesystem,Lists.<String>newArrayList(),alwaysTrue(),new TestConsole(),ImmutableMap.<String,String>of());
  ImmutableSet<BuildTarget> expectedTargets=ImmutableSet.of(BuildTarget.builder("//java/com/facebook","foo").build(),BuildTarget.builder("//java/com/facebook","bar").build());
  assertEquals("Should have returned all rules.",expectedTargets,targets);
}
