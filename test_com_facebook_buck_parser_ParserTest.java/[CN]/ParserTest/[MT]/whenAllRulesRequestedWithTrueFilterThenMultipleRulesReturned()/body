{
  List<BuildTarget> targets=testParser.filterAllTargetsInProject(filesystem,Lists.<String>newArrayList(),alwaysTrue());
  List<BuildTarget> expectedTargets=ImmutableList.of(BuildTarget.builder("//java/com/facebook","foo").build(),BuildTarget.builder("//java/com/facebook","bar").build());
  assertEquals("Should have returned all rules.",expectedTargets,targets);
}
