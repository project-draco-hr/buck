{
  ImmutableSet<BuildTarget> targets=filterAllTargetsInProject(testParser,filesystem,new ParserConfig(new FakeBuckConfig()),Predicates.<TargetNode<?>>alwaysTrue(),new TestConsole(),ImmutableMap.<String,String>of(),BuckEventBusFactory.newInstance(),false);
  ImmutableSet<BuildTarget> expectedTargets=ImmutableSet.of(BuildTarget.builder("//java/com/facebook","foo").build(),BuildTarget.builder("//java/com/facebook","bar").build(),BuildTarget.builder("//java/com/facebook","baz").build());
  assertEquals("Should have returned all rules.",expectedTargets,targets);
}
