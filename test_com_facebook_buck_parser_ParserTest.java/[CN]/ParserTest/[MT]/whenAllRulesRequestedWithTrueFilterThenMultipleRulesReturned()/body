{
  List<BuildTarget> targets=testParser.filterAllTargetsInProject(filesystem,Lists.<String>newArrayList(),alwaysTrue());
  List<BuildTarget> expectedTargets=ImmutableList.of(BuildTargetFactory.newInstance("//java/com/facebook","foo",testBuildFile),BuildTargetFactory.newInstance("//java/com/facebook","bar",testBuildFile));
  assertEquals("Should have returned all rules.",expectedTargets,targets);
}
