{
  ImmutableList<BuildTarget> buildTargetsList=ImmutableList.copyOf(buildTargets);
  parser.buildTargetGraphForBuildTargets(buildTargetsList,new ParserConfig(new FakeBuckConfig()),BuckEventBusFactory.newInstance(),new TestConsole(),ImmutableMap.<String,String>of(),false);
  return parser.getBuildTargetHashCodeCache().getAll(buildTargetsList);
}
