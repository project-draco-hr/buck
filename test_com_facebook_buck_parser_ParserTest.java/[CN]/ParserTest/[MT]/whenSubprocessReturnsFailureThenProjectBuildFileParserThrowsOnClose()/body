{
  assumeTrue(Platform.detect() != Platform.WINDOWS);
  TestProjectBuildFileParserFactory buildFileParserFactory=new TestProjectBuildFileParserFactory(filesystem,buildRuleTypes);
  ProjectBuildFileParser buildFileParser=buildFileParserFactory.createNoopParserThatAlwaysReturnsError();
  buildFileParser.initIfNeeded();
  buildFileParser.close();
}
