{
  return FluentIterable.from(delegate.getListWithoutComments("project","read_only_paths")).transform(new Function<String,Path>(){
    @Override public Path apply(    String input){
      Path path=Paths.get(input);
      if (Files.notExists(path)) {
        throw new HumanReadableException("Path %s, specified under read_only_paths " + "does not exist.",path);
      }
      return path;
    }
  }
).toSet();
}
