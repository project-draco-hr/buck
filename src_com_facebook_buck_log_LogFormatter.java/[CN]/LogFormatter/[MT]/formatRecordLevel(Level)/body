{
  int l=level.intValue();
  if (l == ERROR_LEVEL) {
    return "[error]";
  }
 else   if (l == WARN_LEVEL) {
    return "[warn ]";
  }
 else   if (l == INFO_LEVEL) {
    return "[info ]";
  }
 else   if (l == DEBUG_LEVEL) {
    return "[debug]";
  }
 else {
    return String.format("[%-5d]",l);
  }
}
