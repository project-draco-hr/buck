{
  ExecutionContext executionContext=TestExecutionContext.newBuilder().setProjectFilesystem(projectFilesystem).build();
  return ImmutableBuildContext.builder().setStepRunner(new DefaultStepRunner(executionContext)).setClock(new DefaultClock()).setBuildId(new BuildId()).setArtifactCache(new NoopArtifactCache()).setEventBus(BuckEventBusFactory.newInstance());
}
