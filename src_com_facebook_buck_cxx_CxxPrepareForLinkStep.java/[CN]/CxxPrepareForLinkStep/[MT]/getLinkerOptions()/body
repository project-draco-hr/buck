{
  return ImmutableList.<String>builder().add("-o",output.toString()).addAll(MoreIterables.zipAndConcat(Iterables.cycle("-L"),Iterables.transform(librarySearchDirectories,Functions.toStringFunction()))).addAll(Iterables.transform(libraries,new Function<String,String>(){
    @Nullable @Override public String apply(    String input){
      return "-l" + input;
    }
  }
)).addAll(args).build();
}
