{
  ProjectWorkspace workspace=TestDataHelper.createProjectWorkspaceForScenario(this,name,tmp);
  workspace.setUp();
  workspace.writeContentsToPath(String.format("[cxx]\n  preprocess_mode = %s\n" + "[ndk]\n  compiler = %s\n  cxx_runtime = %s\n",mode.toString().toLowerCase(),compiler,cxxRuntime),".buckconfig");
  return workspace;
}
