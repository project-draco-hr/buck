{
  List<Object[]> data=Lists.newArrayList();
  for (  String arch : ImmutableList.of("arm","armv7","x86")) {
    data.add(new Object[]{NdkCxxPlatforms.Compiler.Type.GCC,NdkCxxPlatforms.CxxRuntime.GNUSTL,arch});
    data.add(new Object[]{NdkCxxPlatforms.Compiler.Type.CLANG,NdkCxxPlatforms.CxxRuntime.GNUSTL,arch});
    data.add(new Object[]{NdkCxxPlatforms.Compiler.Type.CLANG,NdkCxxPlatforms.CxxRuntime.LIBCXX,arch});
  }
  return data;
}
