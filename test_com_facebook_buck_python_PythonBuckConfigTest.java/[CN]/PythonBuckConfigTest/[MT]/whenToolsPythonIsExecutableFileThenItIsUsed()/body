{
  File configPythonFile=temporaryFolder.newFile("python");
  assertTrue("Should be able to set file executable",configPythonFile.setExecutable(true));
  PythonBuckConfig config=new PythonBuckConfig(new FakeBuckConfig(ImmutableMap.<String,Map<String,String>>builder().put("tools",ImmutableMap.of("python",configPythonFile.getAbsolutePath())).build()));
  assertEquals("Should return path to temp file.",configPythonFile.getAbsolutePath(),config.getPythonInterpreter());
}
