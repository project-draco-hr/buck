{
  DebuggableTemporaryFolder projectDir=new DebuggableTemporaryFolder();
  projectDir.create();
  Path pexExecuter=Paths.get("pex-executer");
  ProjectFilesystem projectFilesystem=new FakeProjectFilesystem(new FakeClock(0),projectDir.getRoot(),ImmutableSet.of(pexExecuter));
  Files.createFile(projectFilesystem.resolve(pexExecuter));
  assumeTrue("Should be able to set file non-executable",projectFilesystem.resolve(pexExecuter).toFile().setExecutable(false));
  PythonBuckConfig config=new PythonBuckConfig(new FakeBuckConfig(ImmutableMap.<String,Map<String,String>>builder().put("python",ImmutableMap.of("path_to_pex_executer",pexExecuter.toString())).build(),projectFilesystem),new ExecutableFinder());
  config.getPathToPexExecuter();
}
