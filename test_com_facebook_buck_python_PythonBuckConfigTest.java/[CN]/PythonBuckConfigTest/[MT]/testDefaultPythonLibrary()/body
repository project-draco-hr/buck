{
  BuildTarget library=BuildTargetFactory.newInstance("//:library");
  PythonBuckConfig config=new PythonBuckConfig(new FakeBuckConfig(ImmutableMap.of("python",ImmutableMap.of("library",library.toString()))),new AlwaysFoundExecutableFinder());
  assertThat(config.getDefaultPythonPlatform(new FakeProcessExecutor(Functions.constant(new FakeProcess(0,"Python 2.7.5","")),new TestConsole())).getCxxLibrary(),Matchers.equalTo(Optional.of(library)));
}
