{
  File python=temporaryFolder.newFile("python");
  assertTrue("Should be able to set file executable",python.setExecutable(true));
  PythonBuckConfig config=new PythonBuckConfig(new FakeBuckEnvironment(ImmutableMap.<String,ImmutableMap<String,String>>of(),ImmutableMap.<String,String>builder().put("PATH",temporaryFolder.getRoot().getAbsolutePath()).put("PATHEXT","").build()),new ExecutableFinder());
  config.getPythonInterpreter();
}
