{
  File python=temporaryFolder.newFile("my-py.exe");
  assertTrue("Should be able to set file executable",python.setExecutable(true));
  PythonBuckConfig config=new PythonBuckConfig(new FakeBuckEnvironment(ImmutableMap.<String,Map<String,String>>builder().put("python",ImmutableMap.of("interpreter","my-py")).build(),ImmutableMap.<String,String>builder().put("PATH",temporaryFolder.getRoot().getAbsolutePath()).put("PATHEXT",".exe").build()),new ExecutableFinder(Platform.WINDOWS));
  config.getPythonInterpreter();
}
