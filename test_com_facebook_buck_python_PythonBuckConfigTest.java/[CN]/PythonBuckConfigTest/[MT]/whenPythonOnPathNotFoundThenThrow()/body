{
  PythonBuckConfig config=new PythonBuckConfig(new FakeBuckEnvironment(ImmutableMap.<String,Map<String,String>>of(),ImmutableMap.<String,String>builder().put("PATH",temporaryFolder.getRoot().getAbsolutePath()).put("PATHEXT","").build(),ImmutableMap.<String,String>of()));
