{
  PythonBuckConfig config=new PythonBuckConfig(new FakeBuckEnvironment(ImmutableMap.of("python",ImmutableMap.of("interpreter","does-not-exist")),ImmutableMap.<String,String>builder().put("PATH",temporaryFolder.getRoot().getAbsolutePath()).put("PATHEXT","").build()),new ExecutableFinder());
  config.getPythonInterpreter();
  fail("Should throw an exception when Python isn't found.");
}
