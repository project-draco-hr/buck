{
  return new NdkLibrary(new FakeBuildRuleParamsBuilder(buildTarget).setType(NdkLibraryDescription.TYPE).build(),resolver,buildTarget.getBasePath().resolve("Android.mk"),sources.build(),flags.build(),isAsset,ndkVersion,NdkLibraryDescription.MACRO_HANDLER.getExpander(buildTarget,ruleResolver,projectFilesystem));
}
