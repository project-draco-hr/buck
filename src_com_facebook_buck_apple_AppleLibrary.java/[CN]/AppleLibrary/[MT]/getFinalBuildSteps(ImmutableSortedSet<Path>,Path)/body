{
  if (files.isEmpty()) {
    return ImmutableList.of();
  }
 else   if (linkedDynamically) {
    return ImmutableList.<Step>of(new CompilerStep(getCompiler(),true,files,outputFile,false,ImmutableSortedSet.<Path>of(),ImmutableList.<String>of()));
  }
 else {
    return ImmutableList.<Step>of(new ArchiveStep(getResolver().getPath(archiver),outputFile,ImmutableList.copyOf(files)));
  }
}
