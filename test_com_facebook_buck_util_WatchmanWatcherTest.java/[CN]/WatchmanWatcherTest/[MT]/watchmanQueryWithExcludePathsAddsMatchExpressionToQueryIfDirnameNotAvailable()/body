{
  String query=WatchmanWatcher.createQuery(new ObjectMapper(),"/path/to/repo",Optional.<String>absent(),"uuid",Lists.newArrayList(Paths.get("foo"),Paths.get("bar/baz")),Lists.<String>newArrayList(),ImmutableSet.<Watchman.Capability>of());
  assertEquals("[\"query\",\"/path/to/repo\",{\"since\":\"n:buckduuid\"," + "\"expression\":[\"not\",[\"anyof\"," + "[\"type\",\"d\"],"+ "[\"match\",\"foo/*\",\"wholename\"],"+ "[\"match\",\"bar/baz/*\",\"wholename\"]]],"+ "\"empty_on_fresh_instance\":true,\"fields\":[\"name\",\"exists\",\"new\"]}]",query);
}
