{
  List<List<Object>> queries=WatchmanWatcher.createQuery(ImmutableMap.of(Paths.get("/path/to/repo"),ProjectWatch.of("/path/to/repo",Optional.<String>absent())),"uuid",ImmutableSet.<PathOrGlobMatcher>of(new PathOrGlobMatcher(Paths.get("foo")),new PathOrGlobMatcher(Paths.get("bar/baz"))),ImmutableSet.<Watchman.Capability>of());
  assertEquals(1,queries.size());
  assertEquals(ImmutableList.of("query","/path/to/repo",ImmutableMap.of("since","n:buckduuid","expression",ImmutableList.of("not",ImmutableList.of("anyof",ImmutableList.of("type","d"),ImmutableList.of("match","foo" + File.separator + "*","wholename"),ImmutableList.of("match","bar" + File.separator + "baz"+ File.separator+ "*","wholename"))),"empty_on_fresh_instance",true,"fields",ImmutableList.of("name","exists","new"))),queries.get(0));
}
