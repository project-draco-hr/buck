{
  ImmutableMap<String,Object> watchmanOutput=ImmutableMap.<String,Object>of("files",ImmutableList.of(),"warning","message");
  EventBus eventBus=createStrictMock(EventBus.class);
  replay(eventBus);
  WatchmanWatcher watcher=createWatcher(eventBus,watchmanOutput);
  watcher.postEvents(new BuckEventBus(new FakeClock(0),new BuildId()),ImmutableSet.<String>builder());
  verify(eventBus);
}
