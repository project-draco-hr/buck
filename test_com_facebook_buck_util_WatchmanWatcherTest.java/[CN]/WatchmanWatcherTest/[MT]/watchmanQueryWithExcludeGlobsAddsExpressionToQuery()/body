{
  String query=WatchmanWatcher.createQuery(new ObjectMapper(),"/path/to/repo","uuid",Lists.<Path>newArrayList(),Lists.newArrayList("*/project.pbxproj","buck-out/*"));
  assertEquals("[\"query\",\"/path/to/repo\",{\"since\":\"n:buckduuid\"," + "\"expression\":[\"not\",[\"anyof\"," + "[\"type\",\"d\"],"+ "[\"match\",\"*/project.pbxproj\",\"wholename\"],"+ "[\"match\",\"buck-out/*\",\"wholename\"]]],"+ "\"empty_on_fresh_instance\":true,\"fields\":[\"name\",\"exists\",\"new\"]}]",query);
}
