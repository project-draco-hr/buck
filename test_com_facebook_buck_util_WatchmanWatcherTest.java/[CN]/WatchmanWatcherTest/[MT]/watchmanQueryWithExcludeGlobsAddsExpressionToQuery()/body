{
  List<Object> query=WatchmanWatcher.createQuery("/path/to/repo",Optional.<String>absent(),"uuid",Lists.<Path>newArrayList(),Lists.newArrayList("*.pbxproj"),ImmutableSet.of(Watchman.Capability.DIRNAME));
  assertEquals(ImmutableList.of("query","/path/to/repo",ImmutableMap.of("since","n:buckduuid","expression",ImmutableList.of("not",ImmutableList.of("anyof",ImmutableList.of("type","d"),ImmutableList.of("match","*.pbxproj"))),"empty_on_fresh_instance",true,"fields",ImmutableList.of("name","exists","new"))),query);
}
