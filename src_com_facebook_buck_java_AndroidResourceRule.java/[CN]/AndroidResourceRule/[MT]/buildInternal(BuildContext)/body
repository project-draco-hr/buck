{
  if (getRes() == null) {
    return ImmutableList.of();
  }
  MakeCleanDirectoryStep mkdir=new MakeCleanDirectoryStep(pathToTextSymbolsDir);
  ImmutableList<AndroidResourceRule> androidResourceDeps=getAndroidResourceDeps(this,context.getDependencyGraph());
  Set<String> resDirectories=ImmutableSet.copyOf(Iterables.transform(androidResourceDeps,GET_RES_FOR_RULE));
  GenRDotJavaStep genRDotJava=new GenRDotJavaStep(resDirectories,pathToTextSymbolsDir,rDotJavaPackage,true,ImmutableSet.<String>of());
  return ImmutableList.of(mkdir,genRDotJava);
}
