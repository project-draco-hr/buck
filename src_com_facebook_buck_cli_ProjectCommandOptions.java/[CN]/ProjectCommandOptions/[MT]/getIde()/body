{
  if (ide != null) {
    return ide;
  }
 else {
    Optional<Ide> ide=getBuckConfig().getValue("project","ide").transform(new Function<String,Ide>(){
      @Override public Ide apply(      String input){
        return Ide.fromString(input);
      }
    }
);
    return ide.or(DEFAULT_IDE_VALUE);
  }
}
