{
  final Path path=Paths.get(basePath);
  Node node=basePathToNodeIndex.get(path);
  if (node.children == null) {
    return ImmutableList.of();
  }
 else {
    return FluentIterable.from(node.children).transform(new Function<Node,Path>(){
      @Override public Path apply(      Node child){
        return path.relativize(child.basePath);
      }
    }
).toList();
  }
}
