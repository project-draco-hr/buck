{
  this.resolver=resolver;
  this.filesystem=filesystem;
  this.ocamlContext=ocamlContext;
  this.cCompiler=cCompiler;
  this.cxxCompiler=cxxCompiler;
  hasGeneratedSources=ocamlContext.getLexInput().size() > 0 || ocamlContext.getYaccInput().size() > 0;
  this.depToolStep=new OCamlDepToolStep(filesystem.getRootPath(),this.ocamlContext.getOcamlDepTool().get(),ocamlContext.getMLInput(),this.ocamlContext.getIncludeFlags(false,true));
}
