{
  ImmutableList.Builder<String> flags=ImmutableList.builder();
  flags.addAll(ocamlContext.getFlags());
  flags.addAll(ocamlContext.getCommonCLinkerFlags());
  OCamlLinkStep linkStep=new OCamlLinkStep(new OCamlLinkStep.Args(cxxCompiler,ocamlContext.getOcamlCompiler(),ocamlContext.getOutput(),ImmutableList.copyOf(ocamlContext.getLinkableInput().getArgs()),linkerInputs,flags.build(),ocamlContext.isLibrary(),false));
  return linkStep.execute(context);
}
