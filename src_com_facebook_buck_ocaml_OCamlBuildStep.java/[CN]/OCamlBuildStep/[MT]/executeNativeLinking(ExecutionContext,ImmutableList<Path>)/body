{
  ImmutableList.Builder<String> flags=ImmutableList.builder();
  flags.addAll(ocamlContext.getFlags());
  flags.addAll(ocamlContext.getCommonCLinkerFlags());
  OCamlLinkStep linkStep=new OCamlLinkStep(filesystem.getRootPath(),cxxCompilerEnvironment,cxxCompiler,ocamlContext.getOcamlCompiler().get().getCommandPrefix(resolver),flags.build(),ocamlContext.getNativeOutput(),ocamlContext.getNativeLinkableInput().getArgs(),ocamlContext.getCLinkableInput().getArgs(),linkerInputs,ocamlContext.isLibrary(),false);
  return linkStep.execute(context);
}
