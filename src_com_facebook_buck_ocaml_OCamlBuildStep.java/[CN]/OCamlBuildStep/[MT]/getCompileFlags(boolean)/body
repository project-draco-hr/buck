{
  ImmutableList.Builder<String> flagBuilder=ImmutableList.builder();
  flagBuilder.addAll(ocamlContext.getIncludeFlags(excludeDeps));
  flagBuilder.addAll(ocamlContext.getFlags());
  flagBuilder.add(OCamlCompilables.OCAML_INCLUDE_FLAG,ocamlContext.getCompileOutputDir().toString());
  return flagBuilder.build();
}
