{
  ImmutableList.Builder<String> flags=ImmutableList.builder();
  flags.addAll(ocamlContext.getFlags());
  flags.addAll(ocamlContext.getCommonCLinkerFlags());
  OCamlLinkStep linkStep=new OCamlLinkStep(new OCamlLinkStep.Args(cxxCompiler,ocamlContext.getOcamlBytecodeCompiler().get(),ocamlContext.getBytecodeOutput(),ImmutableList.copyOf(ocamlContext.getLinkableInput().getArgs()),linkerInputs,flags.build(),ocamlContext.isLibrary(),true));
  return linkStep.execute(context);
}
