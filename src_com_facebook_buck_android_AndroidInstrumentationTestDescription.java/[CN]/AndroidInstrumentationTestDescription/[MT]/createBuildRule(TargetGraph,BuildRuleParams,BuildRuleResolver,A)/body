{
  BuildRule apk=resolver.getRule(args.apk);
  if (!(apk instanceof InstallableApk)) {
    throw new HumanReadableException("In %s, instrumentation_apk='%s' must be an android_binary(), apk_genrule() or " + "android_instrumentation_apk(), but was %s().",params.getBuildTarget(),apk.getFullyQualifiedName(),apk.getType());
  }
  return new AndroidInstrumentationTest(params.appendExtraDeps(BuildRules.getExportedRules(params.getDeclaredDeps().get())),new SourcePathResolver(resolver),(InstallableApk)apk,args.labels.get(),args.contacts.get(),javaOptions.getJavaRuntimeLauncher(),resolver.getAllRules(args.sourceUnderTest.get()),args.testRuleTimeoutMs.or(defaultTestRuleTimeoutMs));
}
