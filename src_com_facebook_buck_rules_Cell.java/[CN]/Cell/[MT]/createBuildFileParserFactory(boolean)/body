{
  ParserConfig parserConfig=new ParserConfig(getBuckConfig());
  return new DefaultProjectBuildFileParserFactory(ProjectBuildFileParserOptions.builder().setProjectRoot(getFilesystem().getRootPath()).setPythonInterpreter(pythonInterpreter).setAllowEmptyGlobs(parserConfig.getAllowEmptyGlobs()).setBuildFileName(getBuildFileName()).setDefaultIncludes(parserConfig.getDefaultIncludes()).setDescriptions(getAllDescriptions()).setUseWatchmanGlob(useWatchmanGlob).setWatchman(watchman).setWatchmanQueryTimeoutMs(parserConfig.getWatchmanQueryTimeoutMs()).setRawConfig(getBuckConfig().getRawConfigForParser()).build());
}
