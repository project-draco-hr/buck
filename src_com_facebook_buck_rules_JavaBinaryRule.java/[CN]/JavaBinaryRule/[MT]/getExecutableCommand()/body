{
  Preconditions.checkState(mainClass != null,"Must specify a main class for %s in order to to run it.",getBuildTarget().getFullyQualifiedName());
  return String.format("java -classpath %s %s",Joiner.on(':').join(Iterables.transform(getClasspathEntries(),Functions.RELATIVE_TO_ABSOLUTE_PATH)),mainClass);
}
