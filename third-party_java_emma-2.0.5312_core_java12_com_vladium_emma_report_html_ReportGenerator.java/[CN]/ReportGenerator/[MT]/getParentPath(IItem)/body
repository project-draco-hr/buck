{
  final LinkedList _result=new LinkedList();
  for (; item != null; item=item.getParent()) {
    _result.add(item);
  }
  final IItem[] result=new IItem[_result.size()];
  int j=result.length - 1;
  for (Iterator i=_result.iterator(); i.hasNext(); --j) {
    result[j]=(IItem)i.next();
  }
  return result;
}
