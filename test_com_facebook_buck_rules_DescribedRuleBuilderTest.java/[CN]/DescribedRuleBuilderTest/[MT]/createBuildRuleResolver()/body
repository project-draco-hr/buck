{
  BuildRuleWithInterestingFile dep=new BuildRuleWithInterestingFile(new FakeBuildRuleParams(new BuildTarget("//my","library")),Paths.get("my/fileofinterest.txt"));
  FakeBuildRule fakeGenrule=new FakeBuildRule(new BuildRuleType("fake_genrule"),new BuildTarget("//my","sourcepath"));
  fakeGenrule.setOutputFile("some_file.txt");
  FakeBuildRule fakeRule=new FakeBuildRule(new BuildRuleType("fake_rule"),new BuildTarget("//my","unrelated"));
  return new BuildRuleResolver(ImmutableSet.of(dep,fakeGenrule,fakeRule));
}
