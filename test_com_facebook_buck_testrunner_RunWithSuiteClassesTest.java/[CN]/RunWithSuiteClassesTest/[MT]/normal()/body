{
  ProjectWorkspace workspace=TestDataHelper.createProjectWorkspaceForScenario(this,"run_with_suite_classes",temporaryFolder);
  workspace.setUp();
  ProjectWorkspace.ProcessResult result=workspace.runBuckCommand("test","//:test").assertSuccess();
  List<String> stderrLines=Splitter.on('\n').splitToList(result.getStderr());
  assertThat(stderrLines,Matchers.hasItem(Matchers.matchesPattern(Pattern.compile("^PASS .* 1 Passed   0 Skipped   0 Failed   A$"))));
  assertThat(stderrLines,Matchers.hasItem(Matchers.matchesPattern(Pattern.compile("^PASS .* 1 Passed   0 Skipped   0 Failed   B$"))));
}
