{
  ImmutableSortedSet.Builder<BuildRule> rules=ImmutableSortedSet.naturalOrder();
  for (  ImmutableMap.Entry<String,CxxSource> entry : sources.entrySet()) {
    rules.add(createCompileBuildRule(params,resolver,platform,preprocessorInput,compilerFlags,pic,entry.getKey(),entry.getValue()));
  }
  return rules.build();
}
