{
  setKey(key);
  if (val != null) {
    for (    SourcePath path : val) {
      setVal(path.toString());
      if (path instanceof BuildRuleSourcePath) {
        setVal(((BuildRuleSourcePath)path).getRule().getRuleKey());
      }
 else {
        setVal(((PathSourcePath)path).getRelativePath().toString());
      }
    }
  }
  return separate();
}
