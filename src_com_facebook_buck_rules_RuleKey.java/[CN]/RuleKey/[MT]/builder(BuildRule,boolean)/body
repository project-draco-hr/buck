{
  Builder builder=new Builder().set("name",rule.getFullyQualifiedName()).set("buck.type",rule.getType().getName());
  if (includeDeps) {
    builder.setKey("deps");
    for (    BuildRule buildRule : rule.getDeps()) {
      builder.setVal(buildRule.getRuleKey());
    }
  }
  builder.separate();
  return builder;
}
