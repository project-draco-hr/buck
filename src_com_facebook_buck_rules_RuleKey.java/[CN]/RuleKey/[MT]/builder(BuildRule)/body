{
  return new Builder(rule.getFullyQualifiedName()).set("buck.type",rule.getType().getDisplayName()).setStrings("deps",Iterables.transform(rule.getDeps(),new Function<BuildRule,String>(){
    @Override public String apply(    BuildRule input){
      return String.format("%s:%s",input.getFullyQualifiedName(),input.getType().getDisplayName());
    }
  }
));
}
