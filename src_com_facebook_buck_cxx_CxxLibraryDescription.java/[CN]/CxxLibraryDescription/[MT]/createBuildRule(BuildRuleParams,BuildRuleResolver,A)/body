{
  ImmutableList<CxxSource> srcs=CxxDescriptionEnhancer.parseCxxSources(params.getBuildTarget(),args.srcs.or(ImmutableList.<SourcePath>of()));
  ImmutableMap<Path,SourcePath> headers=CxxDescriptionEnhancer.parseHeaders(params.getBuildTarget(),args.headers.or((ImmutableList.<SourcePath>of())));
  return CxxDescriptionEnhancer.createCxxLibraryBuildRules(params,resolver,cxxBuckConfig,args.preprocessorFlags.or(ImmutableList.<String>of()),args.propagatedPpFlags.or(ImmutableList.<String>of()),headers,args.compilerFlags.or(ImmutableList.<String>of()),srcs,args.linkWhole.or(false));
}
