{
  return CxxDescriptionEnhancer.createHeaderSymlinkTree(params,resolver,new SourcePathResolver(resolver),cxxPlatform,true,CxxDescriptionEnhancer.parseLexSources(params,resolver,args),CxxDescriptionEnhancer.parseYaccSources(params,resolver,args),CxxDescriptionEnhancer.parseHeaders(params,resolver,cxxPlatform,args),HeaderVisibility.PRIVATE);
}
