{
  return cxxPlatforms.containsAnyOf(flavors) || flavors.contains(CxxCompilationDatabase.COMPILATION_DATABASE) || flavors.contains(CxxCompilationDatabase.UBER_COMPILATION_DATABASE)|| flavors.contains(CxxInferEnhancer.InferFlavors.INFER.get())|| flavors.contains(CxxInferEnhancer.InferFlavors.INFER_ANALYZE.get())|| flavors.contains(CxxInferEnhancer.InferFlavors.INFER_CAPTURE_ALL.get());
}
