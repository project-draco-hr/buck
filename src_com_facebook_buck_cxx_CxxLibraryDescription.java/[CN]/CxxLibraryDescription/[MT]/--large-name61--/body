{
  ImmutableList<SourcePath> objects=requireObjects(params,ruleResolver,pathResolver,lexSources,yaccSources,preprocessorFlags,headers,compilerFlags,sources,false);
  BuildTarget staticTarget=CxxDescriptionEnhancer.createStaticLibraryBuildTarget(params.getBuildTarget());
  Path staticLibraryPath=CxxDescriptionEnhancer.getStaticLibraryPath(params.getBuildTarget());
  Archive staticLibraryBuildRule=Archives.createArchiveRule(pathResolver,staticTarget,params,cxxPlatform.getAr(),staticLibraryPath,objects);
  return staticLibraryBuildRule;
}
