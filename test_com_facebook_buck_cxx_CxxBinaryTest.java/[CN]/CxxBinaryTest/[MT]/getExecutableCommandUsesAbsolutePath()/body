{
  FakeProjectFilesystem filesystem=new FakeProjectFilesystem();
  Path bin=Paths.get("path/to/exectuable");
  filesystem.touch(bin);
  BuildRuleParams params=new FakeBuildRuleParamsBuilder("//:target").build();
  BuildRuleResolver ruleResolver=new BuildRuleResolver();
  CxxBinary binary=new CxxBinary(params,ruleResolver,new SourcePathResolver(ruleResolver),bin,EasyMock.createMock(CxxLink.class),ImmutableList.<Path>of(),ImmutableList.<BuildTarget>of());
  ImmutableList<String> command=binary.getExecutableCommand(filesystem);
  assertTrue(Paths.get(command.get(0)).isAbsolute());
}
