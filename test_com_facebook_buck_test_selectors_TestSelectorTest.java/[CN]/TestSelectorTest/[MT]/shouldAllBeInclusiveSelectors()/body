{
  assertIsInclusive("#");
  assertIsInclusive("Car#Wheels");
  assertIsInclusive("com.example.clown.Car");
  assertIsInclusive("com.example.clown.Car#");
  assertIsInclusive("com.example.clown.Car#testWheelsAreInadequateForDayToDayUse");
  assertIsInclusive("#testWheelsAreInadequateForDayToDayUse");
}
