{
  String[] baseConfig={buildTarget.getFullyQualifiedName(),"--config","*//infer.infer_bin=" + inferBin.toString(),"--config","*//infer.clang_compiler=" + clangCompiler.toString(),"--config","*//infer.clang_plugin=" + clangPlugin.toString(),"--config","build.depfiles=cache"};
  String[] blacklistRegex={};
  if (rawBlacklistRegex.isPresent()) {
    blacklistRegex=new String[]{"--config","*//infer.blacklist_regex=" + rawBlacklistRegex.get()};
  }
  return (String[])ArrayUtils.addAll(baseConfig,blacklistRegex);
}
