{
  TestCommand command=getCommand("--exclude","c","--include","a+b");
  TestRule rule=new FakeTestRule(ImmutableSet.<Label>of(Label.of("a"),Label.of("b"),Label.of("c")),BuildTargetFactory.newInstance("//:for"),new SourcePathResolver(new BuildRuleResolver()),ImmutableSortedSet.<BuildRule>of());
  List<TestRule> testRules=ImmutableList.of(rule);
  Iterable<TestRule> result=command.filterTestRules(new FakeBuckConfig(),ImmutableSet.<BuildTarget>of(),testRules);
  assertEquals(ImmutableSet.of(),result);
}
