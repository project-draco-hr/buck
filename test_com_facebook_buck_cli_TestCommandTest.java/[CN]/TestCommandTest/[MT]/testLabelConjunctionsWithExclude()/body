{
  SourcePathResolver pathResolver=new SourcePathResolver(new BuildRuleResolver());
  TestCommandOptions options=getOptions("--exclude","windows+linux");
  TestRule rule1=new FakeTestRule(JavaTestDescription.TYPE,ImmutableSet.<Label>of(ImmutableLabel.of("windows"),ImmutableLabel.of("linux")),BuildTargetFactory.newInstance("//:for"),pathResolver,ImmutableSortedSet.<BuildRule>of());
  TestRule rule2=new FakeTestRule(JavaTestDescription.TYPE,ImmutableSet.<Label>of(ImmutableLabel.of("windows")),BuildTargetFactory.newInstance("//:lulz"),pathResolver,ImmutableSortedSet.<BuildRule>of());
  List<TestRule> testRules=ImmutableList.of(rule1,rule2);
  Iterable<TestRule> result=TestCommand.filterTestRules(options,testRules);
  assertEquals(ImmutableSet.of(rule2),result);
}
