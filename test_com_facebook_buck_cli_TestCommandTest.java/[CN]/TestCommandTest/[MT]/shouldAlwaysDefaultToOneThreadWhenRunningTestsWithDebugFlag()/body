{
  TestCommand command=getCommand("-j","15");
  assertThat(command.getNumTestThreads(FakeBuckConfig.builder().setSections(command.getConfigOverrides().getForCell(Optional.<String>absent())).build()),Matchers.equalTo(15));
  command=getCommand("-j","15","--debug");
  assertThat(command.getNumTestThreads(FakeBuckConfig.builder().setSections(command.getConfigOverrides().getForCell(Optional.<String>absent())).build()),Matchers.equalTo(1));
}
