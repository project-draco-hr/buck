{
  TestCommand command=getCommand("-j","15");
  assertThat(command.getNumTestThreads(new FakeBuckConfig(command.getConfigOverrides())),Matchers.equalTo(15));
  command=getCommand("-j","15","--debug");
  assertThat(command.getNumTestThreads(new FakeBuckConfig(command.getConfigOverrides())),Matchers.equalTo(1));
}
