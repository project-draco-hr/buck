{
  ProjectGenerator projectGenerator=createProjectGeneratorForCombinedProject(ImmutableSet.<TargetNode<?>>of(),ImmutableSet.<BuildTarget>of());
  projectGenerator.createXcodeProjects();
  Optional<String> pbxproj=projectFilesystem.readFileIfItExists(OUTPUT_PROJECT_FILE_PATH);
  assertTrue(pbxproj.isPresent());
}
