{
  Iterable<PBXShellScriptBuildPhase> shellSteps=Iterables.filter(target.getBuildPhases(),PBXShellScriptBuildPhase.class);
  assertEquals(1,Iterables.size(shellSteps));
  PBXShellScriptBuildPhase generatedScriptPhase=Iterables.get(shellSteps,0);
  assertThat(generatedScriptPhase.getShellScript(),containsString(SourceSigner.SIGNED_SOURCE_PLACEHOLDER));
}
