{
  ImmutableSet<ProjectGenerator.Option> options=ImmutableSet.<ProjectGenerator.Option>builder().addAll(projectGeneratorOptions).addAll(ProjectGenerator.COMBINED_PROJECT_OPTIONS).add(ProjectGenerator.Option.GENERATE_HEADER_MAPS_FOR_LIBRARY_TARGETS).build();
  return new ProjectGenerator(partialGraph,initialBuildTargets,projectFilesystem,executionContext,OUTPUT_DIRECTORY,PROJECT_NAME,options);
}
