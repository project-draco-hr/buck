{
  ProjectGenerator projectGenerator=createProjectGeneratorForCombinedProject(ImmutableSet.<BuildRule>of(),ImmutableSet.<BuildTarget>of());
  projectGenerator.createXcodeProjects();
  Document workspace=projectGenerator.getGeneratedWorkspace();
  assertThat(workspace,hasXPath("/Workspace[@version = \"1.0\"]"));
  assertThat(workspace,hasXPath("/Workspace/FileRef/@location",equalTo("container:" + PROJECT_CONTAINER)));
}
