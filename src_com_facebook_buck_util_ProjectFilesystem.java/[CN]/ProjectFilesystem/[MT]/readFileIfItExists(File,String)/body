{
  if (fileToRead.isFile()) {
    String contents;
    try {
      contents=Files.toString(fileToRead,Charsets.UTF_8);
    }
 catch (    IOException e) {
      throw new RuntimeException("Error reading " + pathRelativeToProjectRoot,e);
    }
    return Optional.of(contents);
  }
 else {
    return Optional.absent();
  }
}
