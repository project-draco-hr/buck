{
  BuildRuleResolver resolver=new BuildRuleResolver();
  AppleTestBuilder appleTestBuilder=new AppleTestBuilder(BuildTarget.builder("//foo","xctest").build()).setExtension(Either.<AppleBundleExtension,String>ofLeft(AppleBundleExtension.XCTEST)).setContacts(Optional.of(ImmutableSortedSet.<String>of())).setLabels(Optional.of(ImmutableSortedSet.<Label>of())).setDeps(Optional.of(ImmutableSortedSet.<BuildTarget>of()));
  BuildRule testRule=appleTestBuilder.build(resolver);
  assertTrue(AppleBuildRules.isXcodeTargetTestBuildRule(testRule));
}
