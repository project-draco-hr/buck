{
  ProjectFilesystem filesystem=new FakeProjectFilesystem();
  ShTest shTest=new ShTest(new FakeBuildRuleParamsBuilder("//test/com/example:my_sh_test").setProjectFilesystem(filesystem).build(),new SourcePathResolver(new BuildRuleResolver(TargetGraph.EMPTY,new DefaultTargetNodeToBuildRuleTransformer())),new FakeSourcePath("run_test.sh"),ImmutableList.<Arg>of(),ImmutableMap.<String,Arg>of(),ImmutableSortedSet.<SourcePath>of(),Optional.<Long>absent(),ImmutableSet.<Label>of());
  filesystem.touch(shTest.getPathToTestOutputResult());
  assertTrue("hasTestResultFiles() should return true if result.json exists.",shTest.hasTestResultFiles());
}
