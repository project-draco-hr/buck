{
  Iterable<? extends SourcePath> sourcePaths=ImmutableList.of(new FileSourcePath("java/com/facebook/Main.java"),new FileSourcePath("java/com/facebook/BuckConfig.java"),new BuildTargetSourcePath(BuildTargetFactory.newInstance("//java/com/facebook:facebook")));
  Iterable<Path> inputs=SourcePaths.filterInputsToCompareToOutput(sourcePaths);
  MoreAsserts.assertIterablesEquals("Iteration order should be preserved: results should not be alpha-sorted.",ImmutableList.of(Paths.get("java/com/facebook/Main.java"),Paths.get("java/com/facebook/BuckConfig.java")),inputs);
}
