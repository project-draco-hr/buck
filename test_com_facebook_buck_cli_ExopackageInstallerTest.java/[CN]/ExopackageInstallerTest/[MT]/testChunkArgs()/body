{
  assertEquals(ImmutableList.of(),ExopackageInstaller.chunkArgs(ImmutableList.<String>of(),8));
  assertEquals(ImmutableList.of(ImmutableList.of("abcd","efg")),ExopackageInstaller.chunkArgs(ImmutableList.of("abcd","efg"),8));
  assertEquals(ImmutableList.of(ImmutableList.of("abcd","efg"),ImmutableList.of("hijkl")),ExopackageInstaller.chunkArgs(ImmutableList.of("abcd","efg","hijkl"),8));
}
