{
  DescribedRule rule=new DescribedRule(new BuildRuleType("example"),new FakeBuildable(target).setPathToOutputFile((Path)null),new FakeBuildRuleParamsBuilder(target).build());
  BuildRuleSourcePath path=new BuildRuleSourcePath(rule);
  try {
    path.resolve();
    fail();
  }
 catch (  HumanReadableException e) {
    assertEquals("No known output for: " + target.getFullyQualifiedName(),e.getMessage());
  }
}
