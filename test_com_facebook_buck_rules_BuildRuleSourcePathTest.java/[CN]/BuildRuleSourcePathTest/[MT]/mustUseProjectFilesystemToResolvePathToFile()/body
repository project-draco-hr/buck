{
  AbstractBuildable.AnonymousBuildRule rule=new AbstractBuildable.AnonymousBuildRule(new BuildRuleType("example"),new FakeBuildable(target).setPathToOutputFile(Paths.get("cheese")),new FakeBuildRuleParams(target));
  BuildRuleSourcePath path=new BuildRuleSourcePath(rule);
  Path resolved=path.resolve();
  assertEquals(Paths.get("cheese"),resolved);
}
