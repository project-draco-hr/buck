{
  MakeCleanDirectoryStep mkdir=new MakeCleanDirectoryStep(output.getParent());
  GenerateShellScriptStep generateShellScript=new GenerateShellScriptStep(getBuildTarget().getBasePath(),getResolver().getPath(main),getResolver().getAllPaths(resources),output);
  buildableContext.recordArtifact(output);
  return ImmutableList.of(mkdir,generateShellScript);
}
