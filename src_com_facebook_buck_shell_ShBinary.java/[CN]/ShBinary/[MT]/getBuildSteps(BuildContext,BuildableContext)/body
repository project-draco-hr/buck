{
  MakeCleanDirectoryStep mkdir=new MakeCleanDirectoryStep(output.getParent());
  GenerateShellScriptStep generateShellScript=new GenerateShellScriptStep(Paths.get(target.getBasePath()),main,SourcePaths.toPaths(resources),output);
  buildableContext.recordArtifact(output);
  return ImmutableList.of(mkdir,generateShellScript);
}
