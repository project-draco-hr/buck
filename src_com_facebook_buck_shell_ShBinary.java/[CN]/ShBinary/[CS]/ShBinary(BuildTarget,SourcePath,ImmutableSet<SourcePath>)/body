{
  super(target);
  this.main=Preconditions.checkNotNull(main);
  this.resources=Preconditions.checkNotNull(resources);
  this.output=Paths.get(BuckConstant.GEN_DIR,target.getBasePath(),"__" + target.getShortName() + "__",target.getShortName() + ".sh");
  this.buildOutputInitializer=new BuildOutputInitializer<>(target,this);
}
