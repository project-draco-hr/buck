{
  if (testSelectorList.isEmpty()) {
    if (isSingleResultCausedByNoTestsRemainException(results)) {
      return new ArrayList<>();
    }
 else {
      return results;
    }
  }
  if (results.size() == 0) {
    return null;
  }
  if (isSingleResultCausedByNoTestsRemainException(results)) {
    return null;
  }
  return results;
}
