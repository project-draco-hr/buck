{
  if (!Files.exists(exe)) {
    return false;
  }
  if (Files.isDirectory(exe)) {
    LOG.debug("Found potential executable, but is a directory: %s",exe);
    return false;
  }
  if (!Files.isExecutable(exe)) {
    LOG.debug("Found potential executable, but not actually executable: %s",exe);
    return false;
  }
  return true;
}
