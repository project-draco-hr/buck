{
  Sets.SetView<Flavor> flavors=Sets.intersection(enhancers.keySet(),flavoredTarget.getFlavors());
  if (flavors.size() == 0) {
    return Optional.absent();
  }
  if (flavors.size() > 1) {
    throw new HumanReadableException(String.format("%s: more than one language flavor specified: %s",flavoredTarget,flavors));
  }
  return Optional.of(flavors.iterator().next());
}
