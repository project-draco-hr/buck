{
  InstallCommandOptions options=getOptions(InstallCommandOptions.RUN_SHORT_ARG,VerbosityParser.VERBOSE_SHORT_ARG,"10","wakizashi",InstallCommandOptions.ACTIVITY_SHORT_ARG,"com.facebook.katana.LoginActivity");
  assertTrue(options.shouldStartActivity());
  assertEquals("com.facebook.katana.LoginActivity",options.getActivityToStart());
}
