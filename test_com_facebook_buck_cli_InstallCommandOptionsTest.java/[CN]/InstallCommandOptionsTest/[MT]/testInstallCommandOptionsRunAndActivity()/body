{
  InstallCommand command=getCommand(InstallCommand.RUN_SHORT_ARG,VerbosityParser.VERBOSE_SHORT_ARG,"10","wakizashi",InstallCommand.ACTIVITY_SHORT_ARG,"com.facebook.katana.LoginActivity");
  assertTrue(command.shouldStartActivity());
  assertEquals("com.facebook.katana.LoginActivity",command.getActivityToStart());
}
