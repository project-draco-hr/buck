{
  this.projectFilesystem=projectFilesystem;
  this.targetNdkVersion=targetNdkVersion;
  this.propertyFinder=propertyFinder;
  this.sdkSupplier=Suppliers.memoize(new Supplier<Optional<Path>>(){
    @Override public Optional<Path> get(){
      return getSdkPathFromSdkDir();
    }
  }
);
  this.ndkSupplier=Suppliers.memoize(new Supplier<Optional<Path>>(){
    @Override public Optional<Path> get(){
      return getNdkPathFromNdkRepository().or(getNdkPathFromNdkDir());
    }
  }
);
}
