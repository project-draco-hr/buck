{
  ExecutionContext executionContext=TestExecutionContext.newBuilder().setDebugEnabled(true).build();
  assertTrue(executionContext.isDebugEnabled());
  assertTrue("In debug mode, test should always run regardless of any cached results since " + "the user is expecting to hook up a debugger.",TestRunning.isTestRunRequiredForTest(createMock(TestRule.class),createMock(CachingBuildEngine.class),executionContext,createMock(TestRuleKeyFileHelper.class),true,false,false));
}
