{
  if (console.getVerbosity() == Verbosity.ALL) {
    console.getStdErr().printf("ConcurrentMapFileHashCache watched event %s %s\n",event.kind(),projectFilesystem.createContextString(event));
  }
  if (projectFilesystem.isPathChangeEvent(event)) {
    Path path=(Path)event.context();
    hashCache.remove(path);
  }
 else {
    hashCache.clear();
  }
}
