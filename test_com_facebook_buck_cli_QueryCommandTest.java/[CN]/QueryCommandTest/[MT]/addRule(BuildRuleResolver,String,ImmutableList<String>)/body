{
  Builder builder=DefaultJavaLibraryRule.newJavaLibraryRuleBuilder(new FakeAbstractBuildRuleBuilderParams()).setBuildTarget(BuildTargetFactory.newInstance(target));
  for (  String dependency : dependencies) {
    builder=builder.addDep(BuildTargetFactory.newInstance(dependency));
  }
  ruleResolver.buildAndAddToIndex(builder);
}
