{
  if (!isIdempotent()) {
    if (!other.isIdempotent()) {
      return 0;
    }
    return -1;
  }
 else   if (!other.isIdempotent()) {
    return 1;
  }
  if (isSentinel()) {
    if (other.isSentinel()) {
      return 0;
    }
    return -1;
  }
 else   if (other.isSentinel()) {
    return 1;
  }
  return ByteBuffer.wrap(hashCode.asBytes()).compareTo(ByteBuffer.wrap(other.hashCode.asBytes()));
}
