{
  ThriftService<FrontendRequest,FrontendResponse> thriftService=new ThriftService<FrontendRequest,FrontendResponse>(){
    @Override public void makeRequest(    FrontendRequest frontendRequest,    FrontendResponse frontendResponse){
      request=frontendRequest;
    }
    @Override public void close() throws IOException {
    }
  }
;
  ThriftScribeLogger logger=new ThriftScribeLogger(thriftService,EXECUTOR_SERVICE);
  logger.log(CATEGORY,LINES);
  logger.close();
  assertEquals(request.getType(),FrontendRequestType.LOG);
  assertEquals(request.getLog().getType(),LogRequestType.SCRIBE_DATA);
  assertEquals(request.getLog().getScribeData().getCategory(),CATEGORY);
  assertThat(request.getLog().getScribeData().getLines(),Matchers.allOf(hasItem(LINES.get(0)),hasItem(LINES.get(1)),IsIterableWithSize.<String>iterableWithSize(2)));
}
