{
  super(ImmutableList.of(new MkdirStep(dummyAndroidManifest.getParent()),new WriteFileStep(new Supplier<String>(){
    @Override public String get(){
      return String.format("<manifest xmlns:android='http://schemas.android.com/apk/res/android' " + "package='%s' />",libraryPackage.get());
    }
  }
,dummyAndroidManifest),new GenRDotTxtStepInternal(resDirectories,genDirectoryPath,isTempRDotJava,dummyAndroidManifest)));
}
