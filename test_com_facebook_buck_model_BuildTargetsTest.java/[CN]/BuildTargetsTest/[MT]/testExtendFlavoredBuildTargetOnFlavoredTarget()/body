{
  BuildTarget fooBar=BuildTarget.builder("//foo","bar").addFlavors(ImmutableFlavor.of("hello")).build();
  BuildTarget fooBarBaz=BuildTargets.extendFlavoredBuildTarget(fooBar,ImmutableFlavor.of("baz"));
  assertTrue(fooBarBaz.isFlavored());
  assertEquals("//foo:bar#baz,hello",fooBarBaz.getFullyQualifiedName());
}
