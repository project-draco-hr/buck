{
  for (  GroupedSource groupedSource : groupedSources) {
switch (groupedSource.getType()) {
case SOURCE_PATH:
      if (sourcePathResolver.isSourcePathExtensionInSet(groupedSource.getSourcePath().get(),FileExtensions.CLANG_HEADERS)) {
        addSourcePathToHeadersBuildPhase(groupedSource.getSourcePath().get(),sourcesGroup,headersBuildPhase,sourceFlags);
      }
 else {
        addSourcePathToSourcesBuildPhase(groupedSource.getSourcePath().get(),sourcesGroup,sourcesBuildPhase,sourceFlags);
      }
    break;
case SOURCE_GROUP:
  PBXGroup newSourceGroup=sourcesGroup.getOrCreateChildGroupByName(groupedSource.getSourceGroupName().get());
newSourceGroup.setSortPolicy(PBXGroup.SortPolicy.UNSORTED);
traverseGroupsTreeAndHandleSources(newSourceGroup,sourcesBuildPhase,headersBuildPhase,groupedSource.getSourceGroup().get(),sourceFlags);
break;
default :
throw new RuntimeException("Unhandled grouped source type: " + groupedSource.getType());
}
}
}
