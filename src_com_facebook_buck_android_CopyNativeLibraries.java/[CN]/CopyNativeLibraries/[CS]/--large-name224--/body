{
  super(buildRuleParams,resolver);
  this.nativeLibDirectories=nativeLibDirectories;
  this.cpuFilters=cpuFilters;
  this.filteredNativeLibraries=filteredNativeLibraries;
  this.filteredNativeLibrariesAssets=filteredNativeLibrariesAssets;
  this.nativePlatforms=nativePlatforms;
  Preconditions.checkArgument(!nativeLibDirectories.isEmpty() || !filteredNativeLibraries.isEmpty() || !filteredNativeLibrariesAssets.isEmpty(),"There should be at least one native library to copy.");
}
