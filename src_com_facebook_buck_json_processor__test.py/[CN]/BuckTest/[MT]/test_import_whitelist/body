def test_import_whitelist(self):
    '\n        Verify that whitelisted modules can be imported with sandboxing enabled.\n        '
    self.enable_build_file_sandboxing = True
    build_file = ProjectFile(path='BUCK', contents=('import json', 'import functools', 'import re'))
    self.write_files(build_file)
    build_file_processor = self.create_build_file_processor()
    build_file_processor.process(build_file.path, set())
