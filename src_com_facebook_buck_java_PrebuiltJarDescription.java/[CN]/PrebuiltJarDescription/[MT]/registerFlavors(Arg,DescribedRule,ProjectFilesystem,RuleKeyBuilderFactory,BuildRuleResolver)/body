{
  Buildable gwtModule=createGwtModule(arg);
  BuildTarget prebuiltJarBuildTarget=describedRule.getBuildTarget();
  BuildTarget flavoredBuildTarget=BuildTargets.createFlavoredBuildTarget(prebuiltJarBuildTarget,JavaLibrary.GWT_MODULE_FLAVOR);
  BuildRule rule=new AbstractBuildable.AnonymousBuildRule(BuildRuleType.GWT_MODULE,gwtModule,new BuildRuleParams(flavoredBuildTarget,ImmutableSortedSet.<BuildRule>of(describedRule),BuildTargetPattern.PUBLIC,projectFilesystem,ruleKeyBuilderFactory));
  ruleResolver.addToIndex(rule.getBuildTarget(),rule);
}
