{
  SourcePathResolver pathResolver=new SourcePathResolver(resolver);
  BuildTarget abiJarTarget=BuildTarget.builder(params.getBuildTarget()).addFlavors(CalculateAbi.FLAVOR).build();
  resolver.addToIndex(CalculateAbi.of(abiJarTarget,pathResolver,params,args.binaryJar));
  return new PrebuiltJar(params,pathResolver,args.binaryJar,new BuildTargetSourcePath(abiJarTarget),args.sourceJar,args.gwtJar,args.javadocUrl,args.mavenCoords);
}
