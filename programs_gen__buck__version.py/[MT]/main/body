def main(argv):
    path = os.getcwd()
    while (not os.path.exists(os.path.join(path, '.buckconfig'))):
        path = os.path.dirname(path)
    version = buck_version.get_clean_buck_version(path)
    if (version is None):
        version = buck_version.get_dirty_buck_version(path)
    sys.stdout.write(version)
