{
  Path javacPath=Paths.get("javac");
  TestSourcePath javacJarPath=new TestSourcePath("javac_jar");
  JavacOptions options=createStandardBuilder().setJavacPath(javacPath).setJavacJarPath(javacJarPath).setProcessExecutor(new ProcessExecutor(new TestConsole())).build();
  assertThat(options.getInputs(new SourcePathResolver(new BuildRuleResolver())),Matchers.<SourcePath>containsInAnyOrder(javacJarPath));
}
