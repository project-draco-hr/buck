{
  try {
    final BuckRunner buckRunner=getBuckRunner();
    Task.Backgroundable task=new Task.Backgroundable(project,"Cleaning",true,BackgroundFromStartOption.getInstance()){
      public void run(      ProgressIndicator progressIndicator){
        CleanCommand.clean(buckRunner);
      }
    }
;
    task.queue();
  }
 catch (  NoBuckRunnerException e) {
    reportBuckNotPresent();
  }
}
