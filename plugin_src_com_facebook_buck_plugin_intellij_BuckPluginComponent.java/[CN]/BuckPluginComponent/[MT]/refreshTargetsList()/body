{
  if (!checkBuckRunner()) {
    return;
  }
  Task.Backgroundable task=new Task.Backgroundable(project,"Retrieving targets",true,BackgroundFromStartOption.getInstance()){
    public void run(    ProgressIndicator progressIndicator){
      ImmutableList<BuckTarget> targets=TargetsCommand.getTargets(buckRunner.get());
    }
  }
;
  task.queue();
}
