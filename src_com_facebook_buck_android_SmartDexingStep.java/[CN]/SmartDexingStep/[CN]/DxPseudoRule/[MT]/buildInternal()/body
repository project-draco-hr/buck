{
  Preconditions.checkState(newInputsHash != null,"Must call checkIsCached first!");
  final DxStep dxCommand=new DxStep(outputPath,srcs);
  final WriteFileStep updateHashCommand=new WriteFileStep(newInputsHash,outputHashPath);
  return ImmutableList.<Step>of(new CompositeStep(ImmutableList.of(dxCommand,updateHashCommand)));
}
