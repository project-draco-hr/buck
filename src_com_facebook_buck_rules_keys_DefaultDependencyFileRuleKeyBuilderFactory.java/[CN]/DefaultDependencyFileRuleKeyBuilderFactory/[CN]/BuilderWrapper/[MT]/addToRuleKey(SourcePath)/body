{
  if (sourcePath instanceof ArchiveMemberSourcePath) {
    builder.setArchiveMemberPath(pathResolver.getAbsoluteArchiveMemberPath(sourcePath),pathResolver.getRelativeArchiveMemberPath(sourcePath));
  }
 else {
    builder.setPath(pathResolver.getAbsolutePath(sourcePath),pathResolver.getRelativePath(sourcePath));
  }
}
