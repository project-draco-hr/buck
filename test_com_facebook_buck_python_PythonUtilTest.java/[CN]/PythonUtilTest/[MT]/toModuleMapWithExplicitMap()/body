{
  BuildTarget target=BuildTargetFactory.newInstance("//foo:bar");
  ImmutableMap<Path,SourcePath> srcs=PythonUtil.toModuleMap(target,new SourcePathResolver(new BuildRuleResolver(TargetGraph.EMPTY,new BuildTargetNodeToBuildRuleTransformer())),"srcs",target.getBasePath(),ImmutableList.of(SourceList.ofNamedSources(ImmutableSortedMap.<String,SourcePath>of("hello.py",new FakeSourcePath("goodbye.py")))));
  assertEquals(ImmutableMap.<Path,SourcePath>of(target.getBasePath().resolve("hello.py"),new FakeSourcePath("goodbye.py")),srcs);
}
