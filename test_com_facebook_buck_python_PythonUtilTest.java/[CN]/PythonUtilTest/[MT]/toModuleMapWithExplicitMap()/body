{
  BuildTarget target=BuildTargetFactory.newInstance("//foo:bar");
  ImmutableMap<Path,SourcePath> srcs=PythonUtil.toModuleMap(target,new SourcePathResolver(new BuildRuleResolver()),"srcs",target.getBasePath(),Optional.of(Either.<ImmutableSortedSet<SourcePath>,ImmutableMap<String,SourcePath>>ofRight(ImmutableMap.<String,SourcePath>of("hello.py",new TestSourcePath("goodbye.py")))));
  assertEquals(ImmutableMap.<Path,SourcePath>of(target.getBasePath().resolve("hello.py"),new TestSourcePath("goodbye.py")),srcs);
}
