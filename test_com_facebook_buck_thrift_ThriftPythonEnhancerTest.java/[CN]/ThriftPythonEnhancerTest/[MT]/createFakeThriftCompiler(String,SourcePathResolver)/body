{
  return new ThriftCompiler(BuildRuleParamsFactory.createTrivialBuildRuleParams(BuildTargetFactory.newInstance(target)),resolver,new CommandTool.Builder().addArg(new TestSourcePath("compiler")).build(),ImmutableList.<String>of(),Paths.get("output"),new TestSourcePath("source"),"language",ImmutableSet.<String>of(),ImmutableList.<Path>of(),ImmutableSet.<Path>of(),ImmutableMap.<Path,SourcePath>of());
