{
  return new ThriftCompiler(new FakeBuildRuleParamsBuilder(BuildTargetFactory.newInstance(target)).build(),resolver,new CommandTool.Builder().addArg(new TestSourcePath("compiler")).build(),ImmutableList.<String>of(),Paths.get("output"),new TestSourcePath("source"),"language",ImmutableSet.<String>of(),ImmutableList.<Path>of(),ImmutableSet.<Path>of(),ImmutableMap.<Path,SourcePath>of(),ImmutableSortedSet.<String>of());
}
