{
  final StringBuffer buffer=new StringBuffer();
  visit(new HeaderMapVisitor(){
    @Override public void apply(    String str,    String prefix,    String suffix){
      buffer.append("\"");
      buffer.append(str);
      buffer.append("\" -> \"");
      buffer.append(prefix);
      buffer.append(suffix);
      buffer.append("\"\n");
    }
  }
);
  return buffer.toString();
}
