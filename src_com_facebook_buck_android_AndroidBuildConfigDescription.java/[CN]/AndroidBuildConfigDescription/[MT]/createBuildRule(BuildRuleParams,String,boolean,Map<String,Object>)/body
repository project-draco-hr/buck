{
  BuildRuleParams buildConfigParams=params.copyWithChanges(GEN_JAVA_TYPE,BuildTarget.builder(params.getBuildTarget()).setFlavor(GEN_JAVA_FLAVOR).build(),params.getDeclaredDeps(),params.getExtraDeps());
  AndroidBuildConfig androidBuildConfig=new AndroidBuildConfig(buildConfigParams,javaPackage,useConstantExpressions,constants);
  BuildRuleParams javaLibraryParams=params.copyWithChanges(TYPE,params.getBuildTarget(),ImmutableSortedSet.<BuildRule>of(androidBuildConfig),ImmutableSortedSet.<BuildRule>of());
  return new AndroidBuildConfigJavaLibrary(javaLibraryParams,androidBuildConfig);
}
