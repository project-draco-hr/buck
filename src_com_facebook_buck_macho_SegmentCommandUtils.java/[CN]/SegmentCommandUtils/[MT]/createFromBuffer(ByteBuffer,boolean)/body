{
  int offset=buffer.position();
  UnsignedInteger cmd=UnsignedInteger.fromIntBits(buffer.getInt());
  UnsignedInteger cmdsize=UnsignedInteger.fromIntBits(buffer.getInt());
  String segname=null;
  try {
    segname=NulTerminatedCharsetDecoder.decodeUTF8String(buffer);
  }
 catch (  CharacterCodingException e) {
    throw new HumanReadableException(e,"Cannot read segname for SegmentCommand at %d",offset);
  }
  buffer.position(offset + LoadCommand.CMD_AND_CMDSIZE_SIZE + SegmentCommand.SEGNAME_SIZE_IN_BYTES);
  return SegmentCommand.of(LoadCommand.of(offset,cmd,cmdsize),segname,UnsignedLong.fromLongBits(is64Bit ? buffer.getLong() : buffer.getInt() & 0xFFFFFFFFL),UnsignedLong.fromLongBits(is64Bit ? buffer.getLong() : buffer.getInt() & 0xFFFFFFFFL),UnsignedLong.fromLongBits(is64Bit ? buffer.getLong() : buffer.getInt() & 0xFFFFFFFFL),UnsignedLong.fromLongBits(is64Bit ? buffer.getLong() : buffer.getInt() & 0xFFFFFFFFL),buffer.getInt(),buffer.getInt(),UnsignedInteger.fromIntBits(buffer.getInt()),UnsignedInteger.fromIntBits(buffer.getInt()));
}
