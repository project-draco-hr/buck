{
  Preconditions.checkArgument(old.getLoadCommand().getOffsetInBinary() == updated.getLoadCommand().getOffsetInBinary());
  Preconditions.checkArgument(old.getLoadCommand().getCmd().equals(updated.getLoadCommand().getCmd()));
  Preconditions.checkArgument(old.getLoadCommand().getCmdsize().equals(updated.getLoadCommand().getCmdsize()));
  buffer.position(old.getLoadCommand().getOffsetInBinary());
  writeCommandToBuffer(updated,buffer,is64Bit);
}
