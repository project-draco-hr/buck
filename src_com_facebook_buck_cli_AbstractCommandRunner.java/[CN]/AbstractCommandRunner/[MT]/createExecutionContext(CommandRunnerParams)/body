{
  return ExecutionContext.builder().setProjectFilesystem(params.getRepository().getFilesystem()).setConsole(params.getConsole()).setAndroidPlatformTargetSupplier(params.getAndroidPlatformTargetSupplier()).setEventBus(params.getBuckEventBus()).setPlatform(params.getPlatform()).setEnvironment(params.getEnvironment()).setJavaPackageFinder(params.getJavaPackageFinder()).setObjectMapper(params.getObjectMapper()).build();
}
