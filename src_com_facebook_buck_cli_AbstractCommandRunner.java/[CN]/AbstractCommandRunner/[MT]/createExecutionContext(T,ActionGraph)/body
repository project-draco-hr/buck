{
  return ExecutionContext.builder().setProjectFilesystem(getProjectFilesystem()).setConsole(console).setAndroidPlatformTarget(options.findAndroidPlatformTarget(androidDirectoryResolver,actionGraph,getBuckEventBus())).setEventBus(eventBus).setPlatform(platform).setEnvironment(environment).setJavaPackageFinder(commandRunnerParams.getJavaPackageFinder()).setObjectMapper(objectMapper).build();
}
