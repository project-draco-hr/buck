{
  return ExecutionContext.builder().setProjectFilesystem(getProjectFilesystem()).setConsole(console).setAndroidPlatformTargetSupplier(commandRunnerParams.getAndroidPlatformTargetSupplier()).setEventBus(eventBus).setPlatform(platform).setEnvironment(environment).setJavaPackageFinder(commandRunnerParams.getJavaPackageFinder()).setObjectMapper(objectMapper).build();
}
