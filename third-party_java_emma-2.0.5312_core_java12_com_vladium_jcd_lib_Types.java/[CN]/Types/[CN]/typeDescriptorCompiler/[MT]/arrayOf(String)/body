{
  char first=typedescriptor.charAt(0);
  Class component;
  if (first == '[')   component=arrayOf(typedescriptor.substring(1));
 else   if (first == 'L')   component=Class.forName(vmNameToJavaName(typedescriptor.substring(1,typedescriptor.length() - 1)));
 else {
    component=primitive(first);
  }
  Object array=Array.newInstance(component,0);
  return array.getClass();
}
