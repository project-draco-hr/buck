{
  char first=typedescriptor.charAt(0);
  String component;
  if (first == '[')   component=arrayOf(typedescriptor.substring(1));
 else   if (first == 'L')   component=vmNameToJavaName(typedescriptor.substring(1,typedescriptor.length() - 1));
 else   component=primitive(first);
  String array=component + " []";
  return array;
}
