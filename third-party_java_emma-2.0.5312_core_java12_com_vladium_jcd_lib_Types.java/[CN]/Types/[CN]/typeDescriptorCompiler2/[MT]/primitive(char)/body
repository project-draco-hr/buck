{
switch (c) {
case 'B':
    return "byte";
case 'C':
  return "char";
case 'D':
return "double";
case 'F':
return "float";
case 'I':
return "int";
case 'J':
return "long";
case 'S':
return "short";
case 'Z':
return "boolean";
default :
throw new IllegalArgumentException("unknown primitive: " + c);
}
}
