{
  final int i1=methoddescriptor.indexOf('(');
  final int i2=methoddescriptor.lastIndexOf(')');
  if ((i1 < 0) || (i2 <= 0) || (i1 >= i2)|| (i2 >= methoddescriptor.length() - 1))   throw new IllegalArgumentException("malformed method descriptor: [" + methoddescriptor + "]");
  return methoddescriptor.substring(i2 + 1);
}
