{
  final StringBuffer result=new StringBuffer();
  boolean first=true;
  if (isClass) {
    for (int f=0; f < IAccessFlags.ALL_ACC.length; ++f) {
      final int bit=IAccessFlags.ALL_ACC[f];
      if ((flags & bit) != 0) {
        if (first)         first=false;
 else         result.append(" ");
        if (bit == IAccessFlags.ACC_SUPER)         result.append("super");
 else         result.append(IAccessFlags.ALL_ACC_NAMES[f]);
      }
    }
  }
 else {
    for (int f=0; f < IAccessFlags.ALL_ACC.length; ++f) {
      final int bit=IAccessFlags.ALL_ACC[f];
      if ((flags & bit) != 0) {
        if (first)         first=false;
 else         result.append(" ");
        result.append(IAccessFlags.ALL_ACC_NAMES[f]);
      }
    }
  }
  return result.toString();
}
