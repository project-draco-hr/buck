{
  return Arrays.asList(new Object[][]{{"otherFlags (platform)",defaultFlags.withOtherFlags(CxxToolFlags.explicitBuilder().addPlatformFlags("-DFOO").build()),true},{"otherFlags (rule)",defaultFlags.withOtherFlags(CxxToolFlags.explicitBuilder().addRuleFlags("-DFOO").build()),true},{"includePaths",defaultFlags.withIncludePaths(Paths.get("different")),false},{"systemIncludePaths",defaultFlags.withSystemIncludePaths(Paths.get("different")),false},{"headerMaps",defaultFlags.withHeaderMaps(Paths.get("different")),false},{"frameworkPaths",defaultFlags.withFrameworkPaths(FrameworkPath.ofSourcePath(new FakeSourcePath("different"))),true},{"prefixHeader",defaultFlags.withPrefixHeader(new FakeSourcePath("different")),true}});
}
