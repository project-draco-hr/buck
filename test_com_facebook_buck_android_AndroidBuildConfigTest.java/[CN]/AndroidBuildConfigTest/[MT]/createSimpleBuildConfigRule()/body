{
  BuildTarget buildTarget=BuildTargetFactory.newInstance("//java/com/example:build_config");
  BuildRuleParams params=new FakeBuildRuleParamsBuilder(buildTarget).build();
  return new AndroidBuildConfig(params,new SourcePathResolver(new BuildRuleResolver(TargetGraph.EMPTY,new BuildTargetNodeToBuildRuleTransformer())),"com.example",BuildConfigFields.empty(),Optional.<SourcePath>absent(),false);
}
