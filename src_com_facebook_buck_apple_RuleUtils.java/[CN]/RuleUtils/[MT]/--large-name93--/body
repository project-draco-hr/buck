{
  for (  SourceWithFlags item : items) {
    addSourcePathToBuilders(resolver,item.getSourcePath(),item.getFlags(),outputAllSourcePaths,outputSourcePaths,outputPublicHeaderPaths,outputPrivateHeaderPaths);
    if (!item.getFlags().isEmpty()) {
      outputPerFileFlags.put(item.getSourcePath(),ImmutableList.copyOf(item.getFlags()));
    }
  }
}
