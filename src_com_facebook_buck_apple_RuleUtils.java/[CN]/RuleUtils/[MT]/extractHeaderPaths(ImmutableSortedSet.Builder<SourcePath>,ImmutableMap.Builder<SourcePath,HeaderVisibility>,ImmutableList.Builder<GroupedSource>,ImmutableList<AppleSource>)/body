{
  ImmutableMap.Builder<SourcePath,String> perFileFlagsBuilder=ImmutableMap.builder();
  extractSourcePaths(outputHeaders,perFileFlagsBuilder,outputGroupedHeaders,items);
  for (  ImmutableMap.Entry<SourcePath,String> entry : perFileFlagsBuilder.build().entrySet()) {
    outputPerFileHeaderVisibility.put(entry.getKey(),HeaderVisibility.fromString(entry.getValue()));
  }
}
