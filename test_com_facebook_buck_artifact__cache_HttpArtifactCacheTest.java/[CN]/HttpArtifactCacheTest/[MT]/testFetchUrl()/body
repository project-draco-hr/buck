{
  final RuleKey ruleKey=new RuleKey("00000000000000000000000000000000");
  final String expectedUri="http://localhost:8080/path/artifacts/key/00000000000000000000000000000000";
  HttpArtifactCache cache=new HttpArtifactCache("http",null,null,new URI("http://localhost:8080/path/"),true,new FakeProjectFilesystem(),BUCK_EVENT_BUS,DIRECT_EXECUTOR_SERVICE){
    @Override protected Response fetchCall(    Request request) throws IOException {
      assertEquals(expectedUri,request.uri().toString());
      return new Response.Builder().request(request).protocol(Protocol.HTTP_1_1).code(HttpURLConnection.HTTP_OK).body(createResponseBody(ImmutableSet.of(ruleKey),ImmutableMap.<String,String>of(),ByteSource.wrap(new byte[0]),"data")).build();
    }
  }
;
  cache.fetch(ruleKey,Paths.get("output/file"));
  cache.close();
}
