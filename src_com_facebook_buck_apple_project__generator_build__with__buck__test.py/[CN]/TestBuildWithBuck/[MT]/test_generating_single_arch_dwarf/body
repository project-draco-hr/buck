def test_generating_single_arch_dwarf(self):
    if (platform.system() != 'Darwin'):
        return
    result = self.run_with_data('some_plat', 'some_arch', 'some_arch other_arch', XCODE_DWARF, '/repo/path', '/buck/path', '--flags', '//My:Target', 'DWARF_FLAVOR', 'DSYM_FLAVOR')
    self.assertEqual(result, '/buck/path build --flags //My:Target#DWARF_FLAVOR,some_plat-some_arch')
