{
  ProjectWorkspace workspace=TestDataHelper.createProjectWorkspaceForScenario(this,"sdk-discovery",temp);
  workspace.setUp();
  Path root=workspace.getPath("");
  Path versionPlistPath=workspace.getPath("version.plist");
  ImmutableMap<String,Path> toolchainPaths=ImmutableMap.of();
  assertThat(AppleSdkDiscovery.discoverAppleSdkPaths(root,ImmutableList.<Path>of(),versionPlistPath,toolchainPaths).entrySet(),empty());
}
