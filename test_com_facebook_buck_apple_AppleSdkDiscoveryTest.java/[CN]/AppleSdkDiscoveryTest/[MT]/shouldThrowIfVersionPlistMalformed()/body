{
  ProjectWorkspace workspace=TestDataHelper.createProjectWorkspaceForScenario(this,"sdk-malformed-version-plist",temp);
  workspace.setUp();
  Path root=workspace.getPath("");
  Path versionPlistPath=workspace.getPath("version.plist");
  thrown.expect(HumanReadableException.class);
  thrown.expectMessage(containsString("Could not discover Xcode version, missing ProductBuildVersion"));
  ImmutableMap<String,Path> toolchainPaths=ImmutableMap.of("com.apple.dt.toolchain.XcodeDefault",root.resolve("Toolchains/XcodeDefault"));
  AppleSdkDiscovery.discoverAppleSdkPaths(root,versionPlistPath,toolchainPaths);
}
