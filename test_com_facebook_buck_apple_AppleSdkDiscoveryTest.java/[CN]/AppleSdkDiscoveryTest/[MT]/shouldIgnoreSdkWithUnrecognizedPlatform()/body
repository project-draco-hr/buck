{
  ProjectWorkspace workspace=TestDataHelper.createProjectWorkspaceForScenario(this,"sdk-unknown-platform-discovery",temp);
  workspace.setUp();
  Path root=workspace.getPath("");
  Path versionPlistPath=workspace.getPath("version.plist");
  ImmutableMap<String,Path> toolchainPaths=ImmutableMap.of("com.apple.dt.toolchain.XcodeDefault",root.resolve("Toolchains/XcodeDefault"));
  ImmutableMap<AppleSdk,AppleSdkPaths> sdks=AppleSdkDiscovery.discoverAppleSdkPaths(root,versionPlistPath,toolchainPaths);
  assertEquals(0,sdks.size());
}
