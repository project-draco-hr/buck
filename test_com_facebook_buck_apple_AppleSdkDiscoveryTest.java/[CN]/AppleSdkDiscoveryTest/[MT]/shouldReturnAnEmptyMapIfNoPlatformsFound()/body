{
  ProjectWorkspace workspace=TestDataHelper.createProjectWorkspaceForScenario(this,"sdk-discovery-empty",temp);
  workspace.setUp();
  Path path=workspace.getPath("");
  ImmutableMap<String,Path> toolchainPaths=ImmutableMap.of("com.apple.dt.toolchain.XcodeDefault",path.resolve("Toolchains/XcodeDefault.xctoolchain"));
  ImmutableMap<AppleSdk,AppleSdkPaths> sdks=AppleSdkDiscovery.discoverAppleSdkPaths(path,ImmutableList.<Path>of(),path.resolve("version.plist"),toolchainPaths);
  assertEquals(0,sdks.size());
}
