{
  return ImmutableList.<String>builder().add(args.ocamlCompiler.toString()).addAll(OCamlCompilables.DEFAULT_OCAML_FLAGS).add("-cc",args.cCompiler.toString()).add("-c").add("-annot").add("-bin-annot").add("-o",args.output.toString()).add("-ccopt","-Wall").add("-ccopt","-Wextra").add("-ccopt",String.format("-o %s",args.output.toString())).addAll(args.flags).add(args.input.toString()).build();
}
