{
  sink.setReflectively("cCompiler",cCompiler);
  sink.setReflectively("ocamlCompiler",ocamlCompiler);
  sink.setReflectively("stdlib",stdlib);
  sink.setReflectively("output",output.toString());
  sink.setReflectively("input",input);
