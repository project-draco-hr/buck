{
  Method testGetterMethod=FakeJUnit4Test.class.getMethod("testGetter");
  assertTrue("Has @Test annotation, so should be considered a test method.",JUnitRunner.isTestMethod(testGetterMethod));
  Method testSetterMethod=FakeJUnit4Test.class.getMethod("testSetter");
  assertFalse("Does not have @Test annotation, so should not be considered a test method.",JUnitRunner.isTestMethod(testSetterMethod));
}
