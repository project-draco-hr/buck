{
  new TestBench().addLayer("A[arch=i386]","a_i386","A[arch=x86_64]","a_x86_64","B[sdk=iphonesimulator*]","b_sim","C","$A $B").assertEquals("A[arch=i386] = a_i386","A[arch=x86_64] = a_x86_64","B[sdk=iphonesimulator*] = b_sim","C[arch=i386,sdk=iphonesimulator*] = a_i386 b_sim","C[arch=x86_64,sdk=iphonesimulator*] = a_x86_64 b_sim");
}
