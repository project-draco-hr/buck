{
  new TestBench().addLayer("A[arch=i386]","a_i386","A[arch=x86_64]","a_x86_64","B[sdk=iphonesimulator*]","b_sim","C","$A $B").assertEquals("__0_A[arch=i386] = a_i386","__0_A[arch=x86_64] = a_x86_64","__0_B[sdk=iphonesimulator*] = b_sim","__0_C = $(A) $(B)","A = $(__0_A)","B = $(__0_B)","C = $(__0_C)");
}
