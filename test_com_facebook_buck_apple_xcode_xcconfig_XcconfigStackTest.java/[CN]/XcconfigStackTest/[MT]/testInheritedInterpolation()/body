{
  new TestBench().addLayer("FOO","bottom_foo","BAR","bottom_bar","BAZ","$(FOO) $(BAR) $(BAZ)").addLayer("FOO","$(inherited) top_foo","BAR","$(BAR) top_bar").assertEquals("__0_BAR = bottom_bar","__0_BAZ = $(FOO) $(BAR) $(inherited)","__0_FOO = bottom_foo","__1_BAR = $(__0_BAR) top_bar","__1_FOO = $(__0_FOO) top_foo","BAR = $(__1_BAR)","BAZ = $(__0_BAZ)","FOO = $(__1_FOO)");
