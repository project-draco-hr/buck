{
  PBXNativeTarget target=generateBinaryTarget(project,Optional.<TargetNode<AppleBundleDescription.Arg>>absent(),targetNode,ProductType.TOOL,"%s",Optional.<Path>absent(),true,ImmutableSet.<AppleResourceDescription.Arg>of(),AppleResources.collectDirectResources(targetGraph,targetNode),ImmutableSet.<AppleAssetCatalogDescription.Arg>of(),AppleBuildRules.collectDirectAssetCatalogs(targetGraph,targetNode),Optional.<Iterable<PBXBuildPhase>>absent(),Optional.<TargetNode<AppleBundleDescription.Arg>>absent());
  LOG.debug("Generated Apple binary target %s",target);
  return target;
}
