{
  buildableContext.recordArtifact(YaccStep.getHeaderOutputPath(outputPrefix));
  buildableContext.recordArtifact(YaccStep.getSourceOutputPath(outputPrefix));
  return ImmutableList.of(new MkdirStep(outputPrefix.getParent()),new RmStep(getHeaderOutputPath(outputPrefix),true),new RmStep(getSourceOutputPath(outputPrefix),true),new YaccStep(yacc.resolve(),flags,outputPrefix,input.resolve()));
}
