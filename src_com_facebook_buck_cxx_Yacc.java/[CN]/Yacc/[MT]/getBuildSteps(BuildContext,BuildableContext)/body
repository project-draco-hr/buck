{
  buildableContext.recordArtifact(YaccStep.getHeaderOutputPath(outputPrefix));
  buildableContext.recordArtifact(YaccStep.getSourceOutputPath(outputPrefix));
  return ImmutableList.of(new MkdirStep(getProjectFilesystem(),outputPrefix.getParent()),new RmStep(getProjectFilesystem(),getHeaderOutputPath(outputPrefix),true),new RmStep(getProjectFilesystem(),getSourceOutputPath(outputPrefix),true),new YaccStep(getProjectFilesystem(),yacc.getCommandPrefix(getResolver()),flags,outputPrefix,getResolver().getPath(input)));
}
