{
  buildContext=BuildContext.builder().setDependencyGraph(dependencyGraph).setStepRunner(stepRunner).setProjectFilesystem(executionContext.getProjectFilesystem()).setBuildEngine(buildEngine).setArtifactCache(artifactCache).setJavaPackageFinder(javaPackageFinder).setEventBus(executionContext.getBuckEventBus()).setAndroidBootclasspathForAndroidPlatformTarget(executionContext.getAndroidPlatformTargetOptional()).setBuildDependencies(buildDependencies).build();
  return Builder.getInstance().buildRules(rulesToBuild,buildContext);
}
