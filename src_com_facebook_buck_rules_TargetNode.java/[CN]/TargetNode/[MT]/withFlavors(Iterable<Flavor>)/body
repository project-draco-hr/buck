{
  try {
    return new TargetNode<>(rawInputsHashCode,description,constructorArg,typeCoercerFactory,ruleFactoryParams.withFlavors(flavors),declaredDeps,visibilityPatterns,cellRoots);
  }
 catch (  InvalidSourcePathInputException|NoSuchBuildTargetException e) {
