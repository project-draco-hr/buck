{
  if (getBuildTarget().getCellPath().equals(other.getBuildTarget().getCellPath()) && getBuildTarget().getBaseName().equals(other.getBuildTarget().getBaseName())) {
    return true;
  }
  for (  VisibilityPattern pattern : visibilityPatterns) {
    if (pattern.apply(other)) {
      return true;
    }
  }
  return false;
}
