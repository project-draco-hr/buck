{
  if (optdef != null) {
    final int[] cardinality=optdef.getValueCardinality();
    if (valueCount < cardinality[1])     return false;
  }
  for (int p=0; p < OPT_PREFIXES.length; ++p) {
    if (av.startsWith(OPT_PREFIXES[p]))     return (av.length() > OPT_PREFIXES[p].length());
  }
  return false;
}
