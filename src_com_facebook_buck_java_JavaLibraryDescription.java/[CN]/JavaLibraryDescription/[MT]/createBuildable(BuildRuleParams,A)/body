{
  JavacOptions.Builder javacOptions=JavaLibraryDescription.getJavacOptions(args,javacEnv);
  AnnotationProcessingParams annotationParams=args.buildAnnotationProcessingParams(params.getBuildTarget());
  javacOptions.setAnnotationProcessingData(annotationParams);
  return new DefaultJavaLibrary(params.getBuildTarget(),args.srcs.get(),validateResources(args,params.getProjectFilesystem()),args.proguardConfig,args.postprocessClassesCommands.get(),params.getDeps(),args.exportedDeps.get(),args.providedDeps.get(),javacOptions.build());
}
