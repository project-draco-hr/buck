{
  BuildTarget originalBuildTarget=buildRule.getBuildTarget();
  Optional<GwtModule> gwtModuleOptional=tryCreateGwtModule(new SourcePathResolver(ruleResolver),originalBuildTarget,projectFilesystem,ruleKeyBuilderFactory,targetGraph,arg);
  if (!gwtModuleOptional.isPresent()) {
    return;
  }
  GwtModule gwtModule=gwtModuleOptional.get();
  ruleResolver.addToIndex(gwtModule);
}
