{
  while (filePath != null) {
    if (projectFilesystem.isFile(filePath.resolve(BuckConstant.BUILD_RULES_FILE_NAME)) && !isIgnored(filePath)) {
      return filePath;
    }
    if (filePath.equals(projectFilesystem.getRootPath())) {
      break;
    }
    filePath=filePath.getParent();
  }
  return Paths.get("");
}
