{
  BuildRule rule=requireBuildRule(cxxPlatform.getFlavor(),getLibraryType(type).getFlavor());
  return NativeLinkableInput.of(ImmutableList.<Arg>of(new SourcePathArg(getResolver(),new BuildTargetSourcePath(rule.getBuildTarget()))),ImmutableSet.<FrameworkPath>of(),ImmutableSet.<FrameworkPath>of());
}
