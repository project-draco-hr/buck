{
  if (m_reportTypes.contains(type)) {
    setFailure((BuildException)SuppressableTask.newBuildException(m_task.getTaskName() + ": duplicate configuration for report type [" + type+ "]",m_task.getLocation()).fillInStackTrace());
  }
 else {
    m_reportTypes.add(type);
    m_cfgList.add(cfg);
  }
  return cfg;
}
