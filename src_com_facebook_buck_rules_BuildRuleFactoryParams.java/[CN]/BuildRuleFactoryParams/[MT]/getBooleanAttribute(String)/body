{
  Object value=instance.get(attributeName);
  if (value == null) {
    return false;
  }
 else   if (value instanceof Boolean) {
    return (Boolean)value;
  }
 else {
    throw new RuntimeException(String.format("Value for %s in %s must be either True or False",attributeName,target.getFullyQualifiedName()));
  }
}
