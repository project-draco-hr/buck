{
  for (  Map.Entry<Path,Path> entry : getAllPaths(workingDir).entrySet()) {
    String replacement;
    if (expandPaths) {
      replacement=getExpandedPath(entry.getValue());
    }
 else {
      replacement=entry.getValue().toString();
    }
    contents=contents.replace(entry.getKey().toString(),replacement);
  }
  return contents;
}
