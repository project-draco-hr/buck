{
  TTupleProtocol oprot=(TTupleProtocol)prot;
  BitSet optionals=new BitSet();
  if (struct.isSetNodes()) {
    optionals.set(0);
  }
  oprot.writeBitSet(optionals,1);
  if (struct.isSetNodes()) {
{
      oprot.writeI32(struct.nodes.size());
      for (      BuildJobStateTargetNode _iter64 : struct.nodes) {
        _iter64.write(oprot);
      }
    }
  }
}
