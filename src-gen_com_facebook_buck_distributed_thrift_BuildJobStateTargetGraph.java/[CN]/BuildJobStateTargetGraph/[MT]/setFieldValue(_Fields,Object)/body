{
switch (field) {
case FILE_SYSTEM_ROOTS:
    if (value == null) {
      unsetFileSystemRoots();
    }
 else {
      setFileSystemRoots((Map<Integer,String>)value);
    }
  break;
case NODES:
if (value == null) {
  unsetNodes();
}
 else {
  setNodes((List<BuildJobStateTargetNode>)value);
}
break;
}
}
