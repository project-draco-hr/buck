{
{
    String name="out.txt";
    Genrule genrule=(Genrule)GenruleBuilder.newGenruleBuilder(BuildTargetFactory.newInstance("//:test")).setOut(name).build(new BuildRuleResolver(TargetGraph.EMPTY,new BuildTargetNodeToBuildRuleTransformer()));
    assertEquals(name,genrule.getOutputName());
  }
{
    String name="out/file.txt";
    Genrule genrule=(Genrule)GenruleBuilder.newGenruleBuilder(BuildTargetFactory.newInstance("//:test")).setOut(name).build(new BuildRuleResolver(TargetGraph.EMPTY,new BuildTargetNodeToBuildRuleTransformer()));
    assertEquals(name,genrule.getOutputName());
  }
}
