{
  BuildTarget target=BuildTargetFactory.newInstance("//:test");
  ImmutableList<String> names=ImmutableList.of("out.txt","out/file.txt");
  for (  String name : names) {
    Genrule genrule=GenruleBuilder.createGenrule(target).setOut(name).build();
    assertEquals(name,genrule.getOutputName());
  }
}
