{
  BuildTarget target=BuildTargetFactory.newInstance(String.format("//%s:genrule",contextBasePath));
  GenruleBuilder.Builder builder=GenruleBuilder.createGenrule(target).setRelativeToAbsolutePathFunctionForTesting(ABSOLUTIFIER).setBash(originalCmd).setOut("example-file");
  for (  BuildRule dep : deps) {
    builder.addDep(dep);
  }
  BuildRule genrule=builder.build();
  ruleResolver.addToIndex(target,genrule);
  return genrule;
}
