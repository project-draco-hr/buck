{
  BuildRuleResolver ruleResolver=new BuildRuleResolver(TargetGraph.EMPTY,new BuildTargetNodeToBuildRuleTransformer());
  BuildRule genrule=GenruleBuilder.newGenruleBuilder(BuildTargetFactory.newInstance("//:genrule_no_worker")).setCmd("echo hello >> $OUT").setOut("output.txt").build(ruleResolver,filesystem);
  assertFalse(((Genrule)genrule).isWorkerGenrule());
}
