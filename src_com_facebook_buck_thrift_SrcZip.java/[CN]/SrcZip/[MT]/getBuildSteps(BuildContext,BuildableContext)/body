{
  buildableContext.recordArtifact(sourceZip);
  return ImmutableList.of(new RmStep(getProjectFilesystem(),sourceZip,true),new MkdirStep(getProjectFilesystem(),sourceZip.getParent()),new ZipStep(getProjectFilesystem(),sourceZip,ImmutableSet.<Path>of(),false,ZipStep.MIN_COMPRESSION_LEVEL,sourceDirectory));
}
