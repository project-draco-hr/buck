{
  return getJobWithCacheLookup(buildTarget,cache.lookupTargetNode(cell,buildTarget),new Supplier<ListenableFuture<TargetNode<?>>>(){
    @Override public ListenableFuture<TargetNode<?>> get(){
      return computeTargetNode(cell,buildTarget,buildFile,Futures.immediateFuture(rawNode));
    }
  }
,targetNodeJobsCache);
}
