{
  ProjectFilesystem projectFilesystem=new FakeProjectFilesystem();
  PythonBinary binary=new PythonPackagedBinary(BuildRuleParamsFactory.createTrivialBuildRuleParams(BuildTargetFactory.newInstance("//:bin")),resolver,PATH_TO_PEX,ImmutableList.<String>of(),Paths.get("dummy_path_to_pex_runner"),".pex",new PythonEnvironment(Paths.get("fake_python"),PythonVersion.of("Python 2.7")),"main",PythonPackageComponents.of(ImmutableMap.<Path,SourcePath>of(Paths.get(main),new PathSourcePath(projectFilesystem,mainSrc),Paths.get(mod1),new PathSourcePath(projectFilesystem,src1),Paths.get(mod2),new PathSourcePath(projectFilesystem,src2)),ImmutableMap.<Path,SourcePath>of(),ImmutableMap.<Path,SourcePath>of(),ImmutableSet.<SourcePath>of(),Optional.<Boolean>absent()));
  RuleKey.Builder builder=ruleKeyBuilderFactory.newInstance(binary);
  return builder.build();
}
