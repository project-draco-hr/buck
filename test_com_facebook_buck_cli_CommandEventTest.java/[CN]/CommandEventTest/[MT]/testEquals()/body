{
  CommandEvent startedDaemon=configureTestEvent(CommandEvent.started("build",ImmutableList.of("sample-app"),true));
  CommandEvent startedDaemonTwo=configureTestEvent(CommandEvent.started("build",ImmutableList.of("sample-app"),true));
  CommandEvent startedNoDaemon=configureTestEvent(CommandEvent.started("build",ImmutableList.of("sample-app"),false));
  CommandEvent finishedDaemon=configureTestEvent(CommandEvent.finished("build",ImmutableList.of("sample-app"),true,0));
  CommandEvent finishedDaemonFailed=configureTestEvent(CommandEvent.finished("build",ImmutableList.of("sample-app"),true,1));
  CommandEvent finishedDifferentName=configureTestEvent(CommandEvent.finished("test",ImmutableList.of("sample-app"),true,0));
  assertEquals(startedDaemon,startedDaemonTwo);
  assertNotEquals(startedDaemon,startedNoDaemon);
  assertNotEquals(finishedDaemon,startedDaemon);
  assertNotEquals(finishedDaemon,finishedDaemonFailed);
  assertNotEquals(finishedDaemon,finishedDifferentName);
}
