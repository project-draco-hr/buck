{
  ImmutableMap<String,ImmutableMap<String,String>> sections=ImmutableMap.of("cxx",ImmutableMap.of("ld",Paths.get("fake_path").toString(),"linker_platform",linkerPlatform.name()));
  CxxBuckConfig buckConfig=new CxxBuckConfig(new FakeBuckConfig(sections,new FakeProjectFilesystem(ImmutableSet.of(Paths.get("fake_path")))));
  return DefaultCxxPlatforms.build(buckConfig).getLd();
}
