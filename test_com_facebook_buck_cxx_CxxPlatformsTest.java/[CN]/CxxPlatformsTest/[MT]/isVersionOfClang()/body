{
  assertTrue(CxxPlatforms.isVersionOfClang(ImmutableList.of("clang version 3.7.1 ","Target: x86_64-unknown-linux-gnu","Thread model: posix")));
  assertTrue(CxxPlatforms.isVersionOfClang(ImmutableList.of("Apple LLVM version 7.0.2 (clang-700.1.81)","Target: x86_64-apple-darwin15.3.0","Thread model: posix")));
  assertFalse(CxxPlatforms.isVersionOfClang(ImmutableList.of("gcc (GCC) 4.4.7 20120313 (Red Hat 4.4.7-16)","Copyright (C) 2010 Free Software Foundation, Inc.","This is free software; see the source for copying conditions.  There is NO","warranty; not even for MERCHANTABILITY or FITNESS FOR A PARTICULAR PURPOSE.")));
  assertFalse(CxxPlatforms.isVersionOfClang(ImmutableList.of("Blah blah blah.","I am a compiler.","I am not clang though.")));
}
