{
  assertThat("MACOS linker was not a DarwinLinker instance",getPlatformLinker(Platform.MACOS),instanceOf(DarwinLinker.class));
  assertThat("LINUX linker was not a GnuLinker instance",getPlatformLinker(Platform.LINUX),instanceOf(GnuLinker.class));
  assertThat("WINDOWS linker was not a GnuLinker instance",getPlatformLinker(Platform.WINDOWS),instanceOf(GnuLinker.class));
  assertThat("UNKNOWN linker was not a UnknownLinker instance",getPlatformLinker(Platform.UNKNOWN),instanceOf(UnknownLinker.class));
}
