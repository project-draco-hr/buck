{
  super(buildRuleParams);
  this.javaPackage=Preconditions.checkNotNull(javaPackage);
  this.useConstantExpressions=useConstantExpressions;
  this.constants=ImmutableMap.copyOf(constants);
  this.pathToOutputFile=BuildTargets.getGenPath(buildRuleParams.getBuildTarget(),"__%s__").resolve("BuildConfig.java");
}
