{
  ConstructorArgMarshaller marshaller=new ConstructorArgMarshaller(new DefaultTypeCoercerFactory(ObjectMappers.newDefaultInstance()));
  ProjectFilesystem projectFilesystem=new FakeProjectFilesystem();
  Arg constructorArg=description.createUnpopulatedConstructorArg();
  try {
    marshaller.populate(createCellRoots(projectFilesystem),projectFilesystem,buildRuleFactoryParams,constructorArg,ImmutableSet.<BuildTarget>builder(),ImmutableSet.<BuildTargetPattern>builder(),instance);
  }
 catch (  ConstructorArgMarshalException e) {
    throw new RuntimeException(e);
  }
  return constructorArg;
}
