{
  Description<Arg> description=new TestDescription();
  TargetNode<Arg> targetNode=new TargetNode<>(description,buildRuleFactoryParams(ImmutableMap.<String,Object>of("string","//example/path:one","target","//example/path:two")));
  assertTrue(targetNode.getExtraDeps().isEmpty());
  assertTrue(targetNode.getDeclaredDeps().isEmpty());
}
