{
  if (!future.isDone()) {
    return false;
  }
  try {
    future.get();
    return true;
  }
 catch (  ExecutionException e) {
    return false;
  }
catch (  CancellationException e) {
    return false;
  }
catch (  InterruptedException e) {
    throw new RuntimeException("Should not be possible to interrupt a resolved future.",e);
  }
}
