{
  if (!future.isDone()) {
    return false;
  }
  boolean wasInterrupted=false;
  try {
    while (true) {
      try {
        future.get();
        return true;
      }
 catch (      ExecutionException e) {
        return false;
      }
catch (      CancellationException e) {
        return false;
      }
catch (      InterruptedException e) {
        wasInterrupted=true;
      }
    }
  }
  finally {
    if (wasInterrupted) {
      Thread.currentThread().interrupt();
    }
  }
}
