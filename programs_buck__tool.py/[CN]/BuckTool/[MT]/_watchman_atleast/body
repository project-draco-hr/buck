def _watchman_atleast(self, major, minor):
    'Returns true if the installed watchman is greater than or equal to\n        the given version.'
    response = check_output(['watchman', 'version'])
    version = json.loads(response).get('version')
    parts = version.split('.')
    actualmajor = int(parts[0])
    actualminor = int(parts[1])
    return ((actualmajor > major) or ((actualmajor == major) and (actualminor >= minor)))
