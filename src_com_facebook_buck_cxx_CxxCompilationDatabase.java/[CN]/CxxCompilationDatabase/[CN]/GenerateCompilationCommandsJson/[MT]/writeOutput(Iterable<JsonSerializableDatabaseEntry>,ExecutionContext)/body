{
  Gson gson=new Gson();
  try {
    OutputStream outputStream=context.getProjectFilesystem().newFileOutputStream(getPathToOutputFile());
    outputStream.write(gson.toJson(entries).getBytes());
    outputStream.close();
  }
 catch (  IOException e) {
    logError(e,context);
    return 1;
  }
  return 0;
}
