{
  ArtifactCacheBuckConfig config=createFromText("[cache]","serve_local_cache = true","dir = /cache_dir","served_local_cache_mode = readwrite");
  assertThat(config.getServedLocalCache(),Matchers.equalTo(Optional.of(DirCacheEntry.builder().setMaxSizeBytes(Optional.<Long>absent()).setCacheDir(Paths.get("/cache_dir")).setCacheReadMode(ArtifactCacheBuckConfig.CacheReadMode.readwrite).build())));
}
