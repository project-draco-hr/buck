{
  if (expectedNumberOfParsedRules.get() == 0 || expectedNumberOfParsedBUCKFiles.get() == 0) {
    return Optional.<Double>absent();
  }
  return Optional.<Double>of(Double.valueOf(Math.min((numberOfParsedRules.doubleValue() / expectedNumberOfParsedRules.doubleValue() + numberOfParsedBUCKFiles.doubleValue() / expectedNumberOfParsedBUCKFiles.doubleValue()) / 2.0,1.0)));
}
