{
  Preconditions.checkArgument((getLoadCommand().getCmd().equals(LC_SEGMENT) && getLoadCommand().getCmdsize().intValue() >= SIZE_IN_BYTES_32_BIT) || (getLoadCommand().getCmd().equals(LC_SEGMENT_64) && getLoadCommand().getCmdsize().intValue() >= SIZE_IN_BYTES_64_BIT));
  Preconditions.checkArgument(getSegname().getBytes(StandardCharsets.UTF_8).length + 1 <= SEGNAME_SIZE_IN_BYTES);
}
