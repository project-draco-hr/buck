{
  return new DefaultJavaLibraryRule(new BuildRuleParams(buildTarget,ImmutableSortedSet.copyOf(deps),ImmutableSet.<BuildTargetPattern>of(),Functions.<String>identity()),srcs,ImmutableSet.<SourcePath>of(),Optional.<String>absent(),exportDeps,JavacOptions.builder().build()){
    @Override public Sha1HashCode getAbiKey(){
      if (abiHash == null) {
        return super.getAbiKey();
      }
 else {
        return new Sha1HashCode(abiHash);
      }
    }
  }
;
}
