{
  Set<String> testClassNames=getClassNamesForSources(executionContext);
  if (testClassNames.isEmpty()) {
    return true;
  }
  File outputDirectory=executionContext.getProjectFilesystem().getFileForRelativePath(getPathToTestOutputDirectory());
  for (  String testClass : testClassNames) {
    File testResultFile=new File(outputDirectory,testClass + ".xml");
    if (!testResultFile.isFile()) {
      return false;
    }
  }
  return true;
}
