{
  if (pathToDirectory == null) {
    return;
  }
  Set<String> files;
  try {
    files=DirectoryTraversers.getInstance().findFiles(ImmutableSet.of(pathToDirectory),traverser);
  }
 catch (  IOException e) {
    throw new RuntimeException("Exception while traversing " + pathToDirectory + ".",e);
  }
  inputsToConsiderForCachingPurposes.addAll(files);
}
