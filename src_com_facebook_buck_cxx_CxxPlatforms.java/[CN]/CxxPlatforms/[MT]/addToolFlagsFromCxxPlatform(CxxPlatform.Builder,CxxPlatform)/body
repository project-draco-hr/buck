{
  builder.addAllAsflags(platform.getAsflags()).addAllAsppflags(platform.getAsppflags()).addAllCflags(platform.getCflags()).addAllCxxflags(platform.getCxxflags()).addAllCppflags(platform.getCppflags()).addAllCxxppflags(platform.getCxxppflags()).addAllLdflags(platform.getLdflags()).addAllArflags(platform.getArflags()).addAllRanlibflags(platform.getRanlibflags());
}
