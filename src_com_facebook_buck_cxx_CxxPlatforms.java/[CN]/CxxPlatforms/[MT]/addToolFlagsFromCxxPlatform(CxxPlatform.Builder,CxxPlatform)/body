{
  builder.addAllAsflags(platform.getAsflags()).addAllAsppflags(platform.getAsppflags()).addAllAsppflags(platform.getAsflags()).addAllCflags(platform.getCflags()).addAllCxxflags(platform.getCxxflags()).addAllCppflags(platform.getCppflags()).addAllCppflags(platform.getCflags()).addAllCxxppflags(platform.getCxxflags()).addAllCxxppflags(platform.getCxxppflags()).addAllLdflags(platform.getLdflags()).addAllArflags(platform.getArflags());
}
