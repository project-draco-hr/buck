{
  Object rawCmd=params.getNullableRawAttribute(paramName);
  if (rawCmd == null) {
    return;
  }
  try {
    targets.addAll(Iterables.transform(macroHandler.extractTargets(params.target,(String)rawCmd),Functions.toStringFunction()));
  }
 catch (  MacroException e) {
    throw new HumanReadableException(e,"%s: %s",params.target,e.getMessage());
  }
}
