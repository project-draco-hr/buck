{
  ImmutableMap<String,SourcePath> headerMap=ImmutableMap.<String,SourcePath>of("virtual/path.h",new TestSourcePath("path/to/some_file.h"),"another/path.h",new TestSourcePath("path/to/another_file.h"),"another/file.h",new TestSourcePath("different/path/to/a_file.h"),"file.h",new TestSourcePath("file.h"));
  assertEquals(headerMap,AppleDescriptions.parseAppleHeadersForUseFromOtherTargets(new SourcePathResolver(new BuildRuleResolver()).getPathFunction(),Paths.get("prefix"),SourceList.ofNamedSources(headerMap)));
}
