{
switch (testBundleExtension) {
case XCTEST:
    return xctest;
case OCTEST:
  if (otest.isPresent()) {
    return otest.get();
  }
 else {
    throw new HumanReadableException("Cannot run non-xctest bundle type %s (otest not present)",testBundleExtension);
  }
default :
throw new IllegalStateException("should not happen, checked during construction");
}
}
