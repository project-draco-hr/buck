{
  super(params,resolver);
  this.xctoolPath=xctoolPath;
  this.sdkName=sdkName;
  this.simulatorName=simulatorName;
  this.arch=arch;
  this.testBundle=testBundle;
  this.testHostApp=testHostApp;
  this.contacts=contacts;
  this.labels=labels;
  this.testBundleDirectory=BuildTargets.getScratchPath(params.getBuildTarget(),"__test_bundle_%s__." + testBundleExtension);
  this.testHostAppDirectory=BuildTargets.getScratchPath(params.getBuildTarget(),"__test_host_app_%s__.app");
  this.testOutputPath=getPathToTestOutputDirectory().resolve("test-output.json");
}
