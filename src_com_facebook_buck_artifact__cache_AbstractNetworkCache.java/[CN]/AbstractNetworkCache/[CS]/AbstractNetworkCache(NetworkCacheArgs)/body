{
  this.name=args.getCacheName();
  this.repository=args.getRepository();
  this.scheduleType=args.getScheduleType();
  this.fetchClient=args.getFetchClient();
  this.storeClient=args.getStoreClient();
  this.doStore=args.getDoStore();
