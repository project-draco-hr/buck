{
  PBXNativeTarget target=generateBinaryTarget(project,rule,buildable,PBXTarget.ProductType.APPLICATION,"%s.app",buildable.getInfoPlist(),true);
  addPostBuildScriptPhasesForDependencies(rule,target);
  project.getTargets().add(target);
  LOG.debug("Generated iOS binary target %s",target);
  return target;
}
