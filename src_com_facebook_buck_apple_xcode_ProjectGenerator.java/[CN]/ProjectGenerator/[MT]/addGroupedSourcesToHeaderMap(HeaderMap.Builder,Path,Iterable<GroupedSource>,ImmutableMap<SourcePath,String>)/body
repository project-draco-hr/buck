{
  for (  GroupedSource groupedSource : groupedSources) {
switch (groupedSource.getType()) {
case SOURCE_PATH:
      if (isHeaderSourcePath(groupedSource.getSourcePath())) {
        addSourcePathToHeaderMap(groupedSource.getSourcePath(),prefix,headerMap,sourceFlags);
      }
    break;
case SOURCE_GROUP:
  addGroupedSourcesToHeaderMap(headerMap,prefix,groupedSource.getSourceGroup(),sourceFlags);
break;
default :
throw new RuntimeException("Unhandled grouped source type: " + groupedSource.getType());
}
}
}
