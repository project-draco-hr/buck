{
  return FluentIterable.from(AppleBuildRules.<AppleNativeTargetDescriptionArg>getRecursiveTargetNodeDependenciesOfType(targetGraph,AppleBuildRules.RecursiveDependenciesMode.BUILDING,targetNode,AppleLibraryDescription.TYPE)).filter(new Predicate<TargetNode<AppleNativeTargetDescriptionArg>>(){
    @Override public boolean apply(    TargetNode<AppleNativeTargetDescriptionArg> input){
      return !hasBuckHeaderMaps(input);
    }
  }
).transform(new Function<TargetNode<AppleNativeTargetDescriptionArg>,String>(){
    @Override public String apply(    TargetNode<AppleNativeTargetDescriptionArg> input){
      return getHeaderSearchPath(input);
    }
  }
).toSet();
}
