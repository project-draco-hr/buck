{
  PBXNativeTarget target=generateBinaryTarget(project,rule,buildable,PBXTarget.ProductType.MACOSX_BINARY,"%s.app",AppleResourceDescription.TYPE);
  ImmutableSet.Builder<String> frameworksBuilder=ImmutableSet.builder();
  frameworksBuilder.addAll(buildable.getFrameworks());
  collectRecursiveFrameworkDependencies(rule,frameworksBuilder);
  addCopyFrameworksBuildPhase(rule.getBuildTarget(),target,project.getMainGroup().getOrCreateChildGroupByName("Frameworks"),frameworksBuilder.build());
  project.getTargets().add(target);
  LOG.debug("Generated OS X binary target %s",target);
  return target;
}
