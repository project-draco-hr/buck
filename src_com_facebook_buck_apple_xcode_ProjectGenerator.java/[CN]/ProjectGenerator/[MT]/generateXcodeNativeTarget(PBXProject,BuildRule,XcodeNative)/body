{
  Path referencedProjectPath=buildable.getProjectContainerPath().resolve(partialGraph.getDependencyGraph());
  PBXFileReference referencedProject=project.getMainGroup().getOrCreateChildGroupByName("Project References").getOrCreateFileReferenceBySourceTreePath(new SourceTreePath(PBXReference.SourceTree.SOURCE_ROOT,this.outputDirectory.normalize().toAbsolutePath().relativize(referencedProjectPath.toAbsolutePath())));
  PBXContainerItemProxy proxy=new PBXContainerItemProxy(referencedProject,buildable.getTargetGid(),PBXContainerItemProxy.ProxyType.TARGET_REFERENCE);
  PBXAggregateTarget target=new PBXAggregateTarget(rule.getFullyQualifiedName());
  target.getDependencies().add(new PBXTargetDependency(proxy));
  project.getTargets().add(target);
  return target;
}
