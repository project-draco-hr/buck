{
  boolean isSourceUnderTest=libraryNode.getConstructorArg().getTests().contains(testNode.getBuildTarget());
  if (dependencyNode != libraryNode && dependencyNode.getConstructorArg() instanceof HasTests) {
    ImmutableSortedSet<BuildTarget> tests=((HasTests)dependencyNode.getConstructorArg()).getTests();
    if (tests.contains(testNode.getBuildTarget())) {
      isSourceUnderTest=true;
    }
  }
  return isSourceUnderTest;
}
