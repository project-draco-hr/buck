{
  return FluentIterable.from(getRecursiveRuleDependenciesOfType(RecursiveRuleDependenciesMode.LINKING,rule,IosLibraryDescription.TYPE,XcodeNativeDescription.TYPE)).transform(new Function<BuildRule,PBXFileReference>(){
    @Override public PBXFileReference apply(    BuildRule input){
      return getLibraryFileReferenceForRule(input);
    }
  }
).toSet();
}
