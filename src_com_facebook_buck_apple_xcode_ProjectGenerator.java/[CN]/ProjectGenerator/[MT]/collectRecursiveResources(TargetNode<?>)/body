{
  return FluentIterable.from(AppleBuildRules.<AppleResourceDescription.Arg>getRecursiveTargetNodeDependenciesOfType(targetGraph,AppleBuildRules.RecursiveDependenciesMode.COPYING,targetNode,AppleResourceDescription.TYPE)).transform(new Function<TargetNode<AppleResourceDescription.Arg>,AppleResourceDescription.Arg>(){
    @Override public AppleResourceDescription.Arg apply(    TargetNode<AppleResourceDescription.Arg> input){
      return input.getConstructorArg();
    }
  }
).toSet();
}
