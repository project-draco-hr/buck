{
  if (rule.getType().equals(AppleLibraryDescription.TYPE)) {
    return Joiner.on('/').join("$SYMROOT",BaseEncoding.base32().omitPadding().encode(rule.getFullyQualifiedName().getBytes()),"$CONFIGURATION$EFFECTIVE_PLATFORM_NAME");
  }
 else {
    throw new RuntimeException("Unexpected type: " + rule.getType());
  }
}
