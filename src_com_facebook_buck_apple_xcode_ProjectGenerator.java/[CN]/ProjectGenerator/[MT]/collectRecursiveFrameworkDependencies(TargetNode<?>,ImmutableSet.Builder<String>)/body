{
  for (  TargetNode<?> dependency : AppleBuildRules.getRecursiveTargetNodeDependenciesOfType(targetGraph,AppleBuildRules.RecursiveDependenciesMode.LINKING,targetNode,AppleLibraryDescription.TYPE)) {
    TargetNode<AppleNativeTargetDescriptionArg> libraryDependency=(TargetNode<AppleNativeTargetDescriptionArg>)dependency;
    frameworksBuilder.addAll(libraryDependency.getConstructorArg().frameworks.get());
  }
}
