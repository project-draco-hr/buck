{
  return FluentIterable.from(targetNodes).transformAndConcat(newRecursiveRuleDependencyTransformer(AppleBuildRules.RecursiveDependenciesMode.LINKING,AppleBuildRules.XCODE_TARGET_BUILD_RULE_TYPES)).append(includeInputs ? targetNodes : ImmutableList.<TargetNode<?>>of()).filter(new Predicate<TargetNode<?>>(){
    @Override public boolean apply(    TargetNode<?> input){
      return getLibraryNode(targetGraph,input).isPresent();
    }
  }
).transform(new Function<TargetNode<?>,PBXFileReference>(){
    @Override public PBXFileReference apply(    TargetNode<?> input){
      return getLibraryFileReference(input);
    }
  }
).toSet();
}
