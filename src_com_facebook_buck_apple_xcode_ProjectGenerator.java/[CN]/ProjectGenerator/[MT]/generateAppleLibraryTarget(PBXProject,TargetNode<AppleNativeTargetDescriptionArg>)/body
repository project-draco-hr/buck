{
  boolean isDynamic=targetNode.getBuildTarget().getFlavors().contains(AppleLibraryDescription.DYNAMIC_LIBRARY);
  PBXTarget.ProductType productType=isDynamic ? PBXTarget.ProductType.DYNAMIC_LIBRARY : PBXTarget.ProductType.STATIC_LIBRARY;
  PBXNativeTarget target=generateBinaryTarget(project,Optional.<TargetNode<AppleBundleDescription.Arg>>absent(),targetNode,productType,AppleLibrary.getOutputFileNameFormat(isDynamic),Optional.<Path>absent(),isDynamic,ImmutableSet.<AppleResourceDescription.Arg>of(),ImmutableSet.<AppleAssetCatalogDescription.Arg>of());
  LOG.debug("Generated iOS library target %s",target);
  return target;
}
