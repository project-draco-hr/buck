{
  return FluentIterable.from(getRecursiveRuleDependenciesOfType(RecursiveRuleDependenciesMode.LINKING,rule,AppleLibraryDescription.TYPE,XcodeNativeDescription.TYPE)).transform(new Function<BuildRule,String>(){
    @Override public String apply(    BuildRule input){
      return getObjectOutputPathForRule(input);
    }
  }
).toSet();
}
