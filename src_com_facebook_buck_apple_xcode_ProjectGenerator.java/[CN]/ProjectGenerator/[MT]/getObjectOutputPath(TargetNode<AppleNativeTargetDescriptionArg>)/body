{
  if (targetNode.getType().equals(AppleLibraryDescription.TYPE)) {
    return Joiner.on('/').join("$SYMROOT",BaseEncoding.base32().omitPadding().encode(targetNode.getBuildTarget().getFullyQualifiedName().getBytes()),"$CONFIGURATION$EFFECTIVE_PLATFORM_NAME");
  }
 else {
    throw new RuntimeException("Unexpected type: " + targetNode.getType());
  }
}
