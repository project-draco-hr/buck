{
  return FluentIterable.from(AppleBuildRules.<AppleNativeTargetDescriptionArg>getRecursiveTargetNodeDependenciesOfType(targetGraph,AppleBuildRules.RecursiveDependenciesMode.LINKING,targetNode,AppleLibraryDescription.TYPE)).transform(new Function<TargetNode<AppleNativeTargetDescriptionArg>,String>(){
    @Override public String apply(    TargetNode<AppleNativeTargetDescriptionArg> input){
      return getObjectOutputPath(input);
    }
  }
).toSet();
}
