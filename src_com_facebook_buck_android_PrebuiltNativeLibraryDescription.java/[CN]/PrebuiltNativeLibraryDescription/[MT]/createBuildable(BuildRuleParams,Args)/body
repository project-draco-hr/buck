{
  ImmutableSortedSet<Path> librarySources;
  try {
    librarySources=ImmutableSortedSet.copyOf(params.getProjectFilesystem().getFilesUnderPath(args.nativeLibs,EnumSet.of(FileVisitOption.FOLLOW_LINKS),Predicates.<Path>alwaysTrue()));
  }
 catch (  IOException e) {
    throw new HumanReadableException(e,"Error traversing directory %s.",args.nativeLibs);
  }
  return new PrebuiltNativeLibrary(args.nativeLibs,args.isAsset.or(false),librarySources);
}
