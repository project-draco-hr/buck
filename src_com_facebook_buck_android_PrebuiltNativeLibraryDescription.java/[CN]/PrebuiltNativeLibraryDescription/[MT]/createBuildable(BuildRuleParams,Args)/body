{
  ImmutableSortedSet<Path> librarySources;
  try {
    librarySources=ImmutableSortedSet.copyOf(params.getProjectFilesystem().getFilesUnderPath(args.nativeLibs));
  }
 catch (  IOException e) {
    throw new HumanReadableException(e,"Error traversing directory %s.",args.nativeLibs);
  }
  return new PrebuiltNativeLibrary(params.getBuildTarget(),args.nativeLibs,args.isAsset.or(false),librarySources);
}
