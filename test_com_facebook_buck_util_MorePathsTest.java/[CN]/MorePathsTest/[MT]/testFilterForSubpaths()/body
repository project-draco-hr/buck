{
  File root=tmp.getRoot();
  ImmutableSortedSet<Path> paths=MorePaths.asPaths(ImmutableSet.of(".buckd","foo/bar",root.getAbsolutePath() + "/buck-cache","/root/not/in/test"));
  assertEquals("Set should have been filtered down to paths contained under root.",ImmutableSet.of(Paths.get(".buckd"),Paths.get("foo/bar"),Paths.get("buck-cache")),MorePaths.filterForSubpaths(paths,root.toPath()));
}
