{
  PartialGraph graph;
  try {
    graph=PartialGraph.createFullGraph(getProjectFilesystem().getProjectRoot(),options.getDefaultIncludes(),ansi);
  }
 catch (  NoSuchBuildTargetException e) {
    console.printFailureWithoutStacktrace(e);
    return 1;
  }
  OwnersReport report=generateOwnersReport(graph.getDependencyGraph(),options);
  printReport(options,report);
  return 0;
}
