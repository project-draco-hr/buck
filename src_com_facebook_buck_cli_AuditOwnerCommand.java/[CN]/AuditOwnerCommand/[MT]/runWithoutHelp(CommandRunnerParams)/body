{
  ParserConfig parserConfig=new ParserConfig(params.getBuckConfig());
  BuildFileTree buildFileTree=new FilesystemBackedBuildFileTree(params.getCell().getFilesystem(),parserConfig.getBuildFileName());
  try {
    OwnersReport report=buildOwnersReport(params,parserConfig,buildFileTree,getArguments(),isGuessForDeletedEnabled());
    printReport(params,report);
  }
 catch (  BuildFileParseException|BuildTargetException e) {
    params.getBuckEventBus().post(ConsoleEvent.severe(MoreExceptions.getHumanReadableOrLocalizedMessage(e)));
    return BUILD_TARGET_ERROR;
  }
  return 0;
}
