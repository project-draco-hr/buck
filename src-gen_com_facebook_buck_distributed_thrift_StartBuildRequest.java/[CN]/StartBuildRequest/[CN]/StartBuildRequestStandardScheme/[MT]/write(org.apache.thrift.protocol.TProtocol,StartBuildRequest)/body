{
  struct.validate();
  oprot.writeStructBegin(STRUCT_DESC);
  if (struct.isSetStartTimestampMillis()) {
    oprot.writeFieldBegin(START_TIMESTAMP_MILLIS_FIELD_DESC);
    oprot.writeI64(struct.startTimestampMillis);
    oprot.writeFieldEnd();
  }
  oprot.writeFieldStop();
  oprot.writeStructEnd();
}
