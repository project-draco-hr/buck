{
  Path outputPath;
  Path relativeOutputPath=rule.getPathToOutput();
  if (relativeOutputPath != null) {
    outputPath=rule.getProjectFilesystem().getAbsolutifier().apply(relativeOutputPath);
  }
 else {
    outputPath=null;
  }
  classNamesForSources=getClassNamesForSources(rule.getJavaSrcs(),outputPath,rule.getProjectFilesystem());
}
