{
  Optional<SourcePath> packager=delegate.getSourcePath("react-native","packager");
  if (!packager.isPresent()) {
    throw new HumanReadableException("In order to use a 'react_native_library' rule, please " + "specify 'packager' in .buckconfig under the 'react-native' section.");
  }
  return packager.get();
}
