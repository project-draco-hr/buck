{
  if (cxxPlatform.getCpp().resolve(resolver).supportsHeaderMaps() && cxxPlatform.getCxxpp().resolve(resolver).supportsHeaderMaps()) {
    return BuckConstant.BUCK_OUTPUT_PATH;
  }
 else {
    return CxxDescriptionEnhancer.getHeaderSymlinkTreePath(target,cxxPlatform.getFlavor(),headerVisibility);
  }
}
