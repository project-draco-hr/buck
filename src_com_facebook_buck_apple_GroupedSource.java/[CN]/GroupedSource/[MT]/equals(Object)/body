{
  if (other instanceof GroupedSource) {
    GroupedSource that=(GroupedSource)other;
    return Objects.equals(this.type,that.type) && Objects.equals(this.sourcePath,that.sourcePath) && Objects.equals(this.sourceGroupName,that.sourceGroupName)&& Objects.equals(this.sourceGroup,that.sourceGroup);
  }
  return false;
}
