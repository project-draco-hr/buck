{
switch (this.getType()) {
case SOURCE_PATH:
    visitor.visitSourcePath(Preconditions.checkNotNull(sourcePath));
  break;
case SOURCE_GROUP:
visitor.visitSourceGroup(Preconditions.checkNotNull(sourceGroupName));
for (GroupedSource group : Preconditions.checkNotNull(sourceGroup)) {
group.visit(visitor);
}
break;
}
}
