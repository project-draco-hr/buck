{
  Preconditions.checkArgument(command.getLoadCommand().getOffsetInBinary() == updatedCommand.getLoadCommand().getOffsetInBinary());
  Preconditions.checkArgument(command.getLoadCommand().getCmd().equals(updatedCommand.getLoadCommand().getCmd()));
  Preconditions.checkArgument(command.getLoadCommand().getCmdsize().equals(updatedCommand.getLoadCommand().getCmdsize()));
  buffer.position(command.getLoadCommand().getOffsetInBinary());
  writeCommandToBuffer(updatedCommand,buffer);
}
