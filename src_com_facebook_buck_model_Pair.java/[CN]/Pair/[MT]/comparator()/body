{
  return new Comparator<Pair<FIRST,SECOND>>(){
    @Override public int compare(    Pair<FIRST,SECOND> o1,    Pair<FIRST,SECOND> o2){
      if (o1 == o2) {
        return 0;
      }
      return ComparisonChain.start().compare(o1.first,o2.first).compare(o1.second,o2.second).result();
    }
  }
;
}
