{
  ImmutableList.Builder<String> builder=ImmutableList.builder();
  builder.add(jsPackager.toString(),"bundle","--entry-file",absoluteEntryPath.toString(),"--platform",platform.toString(),"--dev",isDevMode ? "true" : "false","--bundle-output",absoluteBundleOutputPath,"--assets-dest",absoluteResourceOutputPath,"--sourcemap-output",absoluteSourceMapOutputPath);
  if (packagerFlags.isPresent()) {
    builder.addAll(Arrays.asList(packagerFlags.get().split(" ")));
  }
  return builder.build();
}
