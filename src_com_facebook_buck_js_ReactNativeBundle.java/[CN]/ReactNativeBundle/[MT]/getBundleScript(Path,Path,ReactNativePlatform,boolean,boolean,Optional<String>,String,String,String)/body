{
  ImmutableList.Builder<String> builder=ImmutableList.builder();
  builder.add(jsPackager.toString(),isUnbundle ? "unbundle" : "bundle","--entry-file",absoluteEntryPath.toString(),"--platform",platform.toString(),"--dev",isDevMode ? "true" : "false","--bundle-output",absoluteBundleOutputPath,"--assets-dest",absoluteResourceOutputPath);
  if (!isUnbundle) {
    builder.add("--sourcemap-output",absoluteSourceMapOutputPath);
  }
  if (packagerFlags.isPresent()) {
    builder.addAll(Arrays.asList(packagerFlags.get().split(" ")));
  }
  return builder.build();
}
