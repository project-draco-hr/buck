{
  BuildRuleResolver resolver=new BuildRuleResolver(Maps.<BuildTarget,BuildRule>newHashMap());
  BuildRuleFactoryParams factoryParams=NonCheckingBuildRuleFactoryParams.createNonCheckingBuildRuleFactoryParams(Maps.<String,Object>newHashMap(),new BuildTargetParser(new ProjectFilesystem(Paths.get("."))),target);
