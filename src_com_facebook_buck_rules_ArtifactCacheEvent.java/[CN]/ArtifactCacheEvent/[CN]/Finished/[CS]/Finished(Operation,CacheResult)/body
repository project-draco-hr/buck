{
  super(operation);
  Preconditions.checkArgument((operation.equals(Operation.FETCH) && cacheResult != null) || (!operation.equals(Operation.FETCH) && cacheResult == null),"For FETCH operations, cacheResult must be non-null. " + "For non-FETCH operations, cacheResult must be null.");
  this.cacheResult=cacheResult;
}
