{
  super(operation,ruleKey);
  Preconditions.checkArgument((operation.equals(Operation.FETCH) && cacheResult.isPresent()) || (!operation.equals(Operation.FETCH) && !cacheResult.isPresent()),"For FETCH operations, cacheResult must be non-null. " + "For non-FETCH operations, cacheResult must be null.");
  this.cacheResult=cacheResult;
}
