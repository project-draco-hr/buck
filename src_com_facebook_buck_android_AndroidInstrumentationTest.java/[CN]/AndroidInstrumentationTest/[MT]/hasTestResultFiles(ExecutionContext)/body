{
  List<IDevice> devices;
  AdbHelper adbHelper=AdbHelper.get(context,true);
  try {
    devices=adbHelper.getDevices(true);
  }
 catch (  InterruptedException e) {
    devices=Lists.newArrayList();
  }
  for (  IDevice device : devices) {
    Path testResultPath=getProjectFilesystem().resolve(getPathToTestOutputDirectory().resolve(BuckXmlTestRunListener.TEST_RESULT_FILE_PREFIX + device.getSerialNumber() + BuckXmlTestRunListener.TEST_RESULT_FILE_SUFFIX));
    if (!testResultPath.toFile().exists()) {
      return false;
    }
  }
  return true;
}
