{
class Dto {
    public Optional<Integer> number;
  }
  Dto dto=new Dto();
  marshaller.populate(createCellRoots(filesystem),filesystem,buildRuleFactoryParams(),dto,ImmutableSet.<BuildTarget>builder(),ImmutableSet.<BuildTargetPattern>builder(),ImmutableMap.<String,Object>of("number",0));
  assertTrue(dto.number.isPresent());
  assertEquals(Optional.of(0),dto.number);
}
