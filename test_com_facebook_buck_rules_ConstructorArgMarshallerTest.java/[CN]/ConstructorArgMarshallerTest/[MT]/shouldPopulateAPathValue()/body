{
class Dto implements ConstructorArg {
    @Hint(name="some_path") public Path somePath;
  }
  Dto dto=new Dto();
  marshaller.populate(ruleResolver,buildRuleFactoryParams(ImmutableMap.<String,Object>of("somePath","Fish.java")),dto);
  assertEquals(Paths.get("example/path","Fish.java"),dto.somePath);
}
