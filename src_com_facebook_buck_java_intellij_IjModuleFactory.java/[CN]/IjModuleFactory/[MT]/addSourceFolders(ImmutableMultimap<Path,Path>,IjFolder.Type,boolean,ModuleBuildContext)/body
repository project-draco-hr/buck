{
  for (  Map.Entry<Path,Collection<Path>> entry : foldersToInputsIndex.asMap().entrySet()) {
    context.addSourceFolder(IjFolder.builder().setPath(entry.getKey()).setInputs(FluentIterable.from(entry.getValue()).toSortedSet(Ordering.natural())).setType(type).setWantsPackagePrefix(wantsPackagePrefix).build());
  }
}
