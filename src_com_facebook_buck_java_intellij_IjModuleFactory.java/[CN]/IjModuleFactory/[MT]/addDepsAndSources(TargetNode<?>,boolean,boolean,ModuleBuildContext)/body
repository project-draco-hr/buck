{
  ImmutableSet<Path> folders=getSourceFolders(targetNode);
  addSourceFolders(folders,isTest ? IjFolder.Type.TEST_FOLDER : IjFolder.Type.SOURCE_FOLDER,wantsPackagePrefix,context);
  context.addDeps(folders,targetNode.getDeps(),isTest ? IjModuleGraph.DependencyType.TEST : IjModuleGraph.DependencyType.PROD);
}
