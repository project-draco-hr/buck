{
  ImmutableMultimap<Path,Path> foldersToInputsIndex=getSourceFoldersToInputsIndex(targetNode.getInputs());
  addSourceFolders(foldersToInputsIndex,isTest ? IjFolder.Type.TEST_FOLDER : IjFolder.Type.SOURCE_FOLDER,wantsPackagePrefix,context);
  context.addDeps(foldersToInputsIndex.keySet(),targetNode.getDeps(),isTest ? IjModuleGraph.DependencyType.TEST : IjModuleGraph.DependencyType.PROD);
}
