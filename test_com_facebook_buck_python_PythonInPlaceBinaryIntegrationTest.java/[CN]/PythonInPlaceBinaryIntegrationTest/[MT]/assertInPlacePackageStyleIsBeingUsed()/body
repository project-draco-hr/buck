{
  PythonBuckConfig pythonBuckConfig=new PythonBuckConfig(BuckConfig.createFromFiles(new ProjectFilesystem(tmp.getRootPath()),ImmutableList.of(workspace.getPath(".buckconfig").toFile()),Platform.detect(),ImmutableMap.<String,String>of()),new FakeExecutableFinder(ImmutableList.<Path>of()));
  assertThat(pythonBuckConfig.getPackageStyle(),Matchers.equalTo(PythonBuckConfig.PackageStyle.INPLACE));
}
