{
  Config rawConfig=Config.createDefaultConfig(tmp.getRootPath(),ImmutableMap.<String,ImmutableMap<String,String>>of());
  BuckConfig buckConfig=new BuckConfig(rawConfig,new ProjectFilesystem(tmp.getRootPath()),Platform.detect(),ImmutableMap.<String,String>of());
  PythonBuckConfig pythonBuckConfig=new PythonBuckConfig(buckConfig,new FakeExecutableFinder(ImmutableList.<Path>of()));
  assertThat(pythonBuckConfig.getPackageStyle(),Matchers.equalTo(PythonBuckConfig.PackageStyle.INPLACE));
}
