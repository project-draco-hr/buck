{
  return ExecutionContext.builder().setConsole(new TestConsole()).setEventBus(BuckEventBusFactory.newInstance()).setPlatform(Platform.detect()).setEnvironment(ImmutableMap.copyOf(System.getenv())).setJavaPackageFinder(new FakeJavaPackageFinder()).setObjectMapper(new ObjectMapper()).setClassLoaderCache(testClassLoaderCache);
