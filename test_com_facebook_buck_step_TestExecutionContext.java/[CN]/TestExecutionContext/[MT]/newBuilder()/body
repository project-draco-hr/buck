{
  Map<ExecutionContext.ExecutorPool,ListeningExecutorService> executors=new HashMap<>();
  executors.put(ExecutionContext.ExecutorPool.CPU,MoreExecutors.listeningDecorator(Executors.newCachedThreadPool()));
  return ExecutionContext.builder().setConsole(new TestConsole()).setEventBus(BuckEventBusFactory.newInstance()).setPlatform(Platform.detect()).setEnvironment(ImmutableMap.copyOf(System.getenv())).setJavaPackageFinder(new FakeJavaPackageFinder()).setObjectMapper(new ObjectMapper()).setClassLoaderCache(testClassLoaderCache).setExecutors(executors);
}
