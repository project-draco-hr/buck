{
  LOG.debug("Adding scheme %s",schemeName);
  schemeConfigsBuilder.put(schemeName,schemeArguments);
  if (schemeArguments.srcTarget.isPresent()) {
    orderedTargetNodesBuilder.putAll(schemeName,AppleBuildRules.getSchemeBuildableTargetNodes(projectGraph,Preconditions.checkNotNull(projectGraph.get(schemeArguments.srcTarget.get().getBuildTarget()))));
  }
  for (  BuildTarget extraTarget : schemeArguments.extraTargets.get()) {
    orderedTargetNodesBuilder.putAll(schemeName,AppleBuildRules.getSchemeBuildableTargetNodes(projectGraph,Preconditions.checkNotNull(projectGraph.get(extraTarget))));
  }
  extraTestNodesBuilder.putAll(schemeName,getExtraTestTargetNodes(projectGraph,schemeArguments.extraTests.get()));
}
