{
  Optional<String> productName=Optional.absent();
  Optional<TargetNode<?>> buildWithBuckTargetNode=getTargetNodeForBuildTarget(targetToBuildWithBuck,targetNodes);
  if (buildWithBuckTargetNode.isPresent()) {
    Optional<TargetNode<AppleBundleDescription.Arg>> appleBundleNode=buildWithBuckTargetNode.get().castArg(AppleBundleDescription.Arg.class);
    if (appleBundleNode.isPresent()) {
      productName=appleBundleNode.get().getConstructorArg().productName;
    }
  }
  return productName;
}
