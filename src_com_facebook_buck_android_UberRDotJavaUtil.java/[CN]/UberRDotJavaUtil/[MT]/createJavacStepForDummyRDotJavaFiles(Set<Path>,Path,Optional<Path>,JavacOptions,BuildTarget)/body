{
  return JavacStepUtil.createJavacStep(outputDirectory,javaSourceFilePaths,ImmutableSet.<String>of(),ImmutableSet.<String>of(),JavacOptions.builder(JavacOptions.DEFAULTS).setPathToJavac(javacOptions.getPathToJavac()).setJavacVersion(javacOptions.getJavacVersion()).build(),pathToOutputAbiFile,Optional.<String>absent(),BuildDependencies.FIRST_ORDER_ONLY,Optional.<JavacInMemoryStep.SuggestBuildRules>absent(),Optional.<Path>absent(),buildTarget);
}
