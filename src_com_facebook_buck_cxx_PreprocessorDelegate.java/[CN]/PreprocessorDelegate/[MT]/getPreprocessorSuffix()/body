{
  return ImmutableList.<String>builder().addAll(rulePreprocessorFlags).addAll(MoreIterables.zipAndConcat(Iterables.cycle("-include"),FluentIterable.from(prefixHeader.asSet()).transform(resolver.getAbsolutePathFunction()).transform(Functions.toStringFunction()))).addAll(MoreIterables.zipAndConcat(Iterables.cycle("-I"),Iterables.transform(headerMaps,ENSURE_ABSOLUTE_PATH))).addAll(MoreIterables.zipAndConcat(Iterables.cycle("-I"),Iterables.transform(includeRoots,ENSURE_ABSOLUTE_PATH))).addAll(MoreIterables.zipAndConcat(Iterables.cycle("-isystem"),Iterables.transform(systemIncludeRoots,Functions.toStringFunction()))).addAll(MoreIterables.zipAndConcat(Iterables.cycle("-F"),Iterables.transform(frameworkRoots,Functions.toStringFunction()))).build();
}
