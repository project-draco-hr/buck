{
  ImmutableMap.Builder<Path,Path> replacementPathsBuilder=ImmutableMap.builder();
  for (  Map.Entry<Path,SourcePath> entry : CxxHeaders.concat(includes).getFullNameToPathMap().entrySet()) {
    replacementPathsBuilder.put(Preconditions.checkNotNull(minLengthPathRepresentation.apply(entry.getKey())),resolver.getRelativePath(entry.getValue()));
  }
  return replacementPathsBuilder.build();
}
