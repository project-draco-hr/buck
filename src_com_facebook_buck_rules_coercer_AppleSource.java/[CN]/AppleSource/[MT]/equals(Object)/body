{
  if (other instanceof AppleSource) {
    AppleSource that=(AppleSource)other;
    return Objects.equals(this.type,that.type) && Objects.equals(this.sourcePath,that.sourcePath) && Objects.equals(this.sourcePathWithFlags,that.sourcePathWithFlags)&& Objects.equals(this.sourceGroup,that.sourceGroup);
  }
  return false;
}
