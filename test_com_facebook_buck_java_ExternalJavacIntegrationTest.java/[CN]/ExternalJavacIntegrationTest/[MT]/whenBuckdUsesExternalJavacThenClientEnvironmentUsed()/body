{
  final ProjectWorkspace workspace=TestDataHelper.createProjectWorkspaceForScenario(this,"external_javac",tmp);
  workspace.setUp();
  File javac=workspace.getFile("check_env.sh");
  javac.setExecutable(true);
  workspace.replaceFileContents(".buckconfig","@JAVAC@",javac.getAbsolutePath());
  workspace.runBuckdCommand(ImmutableMap.of("CHECK_THIS_VARIABLE","1"),"build","example").assertSuccess();
}
