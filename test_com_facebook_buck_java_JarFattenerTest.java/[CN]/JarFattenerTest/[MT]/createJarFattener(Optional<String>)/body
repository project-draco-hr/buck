{
  BuildTarget target=BuildTargetFactory.newInstance("//:foo");
  BuildRuleParams params=new FakeBuildRuleParamsBuilder(target).build();
  SourcePath sourcePath=new BuildTargetSourcePath(target);
  return new JarFattener(params,new SourcePathResolver(new BuildRuleResolver()),JavacOptions.builder().setSourceLevel("8").setTargetLevel("8").build(),sourcePath,ImmutableMap.<String,SourcePath>of(),javaBinOverride);
}
