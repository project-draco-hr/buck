{
  return ImmutableList.of(new MakeCleanDirectoryStep(getProjectFilesystem(),getPathToTestOutputDirectory()),new GoTestStep(getProjectFilesystem(),ImmutableList.<String>builder().addAll(testMain.getExecutableCommand().getCommandPrefix(getResolver())).add("-test.v").add("-test.timeout",executionContext.getDefaultTestTimeoutMillis() + "ms").build(),ImmutableMap.<String,String>of(),getPathToTestExitCode(),getPathToTestResults()));
}
