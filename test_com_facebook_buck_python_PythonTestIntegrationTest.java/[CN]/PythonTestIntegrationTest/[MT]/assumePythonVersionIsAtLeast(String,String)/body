{
  PythonVersion pythonVersion=new PythonBuckConfig(FakeBuckConfig.builder().build(),new ExecutableFinder()).getPythonEnvironment(new ProcessExecutor(new TestConsole())).getPythonVersion();
  String actualVersion=Splitter.on(' ').splitToList(pythonVersion.getVersionString()).get(1);
  assumeTrue(String.format("Needs at least Python-%s, but found Python-%s: %s",expectedVersion,actualVersion,message),new VersionStringComparator().compare(actualVersion,expectedVersion) >= 0);
}
