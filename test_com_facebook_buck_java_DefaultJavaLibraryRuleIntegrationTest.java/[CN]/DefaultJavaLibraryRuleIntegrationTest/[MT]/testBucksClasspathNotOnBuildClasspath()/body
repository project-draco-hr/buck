{
  workspace=TestDataHelper.createProjectWorkspaceForScenario(this,"guava_no_deps",tmp);
  workspace.setUp();
  ProcessResult buildResult=workspace.runBuckCommand("build","//:foo");
  buildResult.assertExitCode("Build should have failed since //:foo depends on Guava and " + "Args4j but does not include it in its deps.",1);
  workspace.verify();
}
