{
  Preconditions.checkState(target.getFlavors().contains(SOURCE_LINK_TREE));
  return new SymlinkTree(baseParams.copyWithChanges(target,Suppliers.ofInstance(ImmutableSortedSet.<BuildRule>of()),Suppliers.ofInstance(ImmutableSortedSet.<BuildRule>of())),pathResolver,baseParams.getProjectFilesystem().resolve(BuildTargets.getGenPath(baseParams.getBuildTarget(),"%s")),MoreMaps.transformKeys(sources.toNameMap(baseParams.getBuildTarget(),pathResolver,"srcs"),MorePaths.toPathFn(baseParams.getProjectFilesystem().getRootPath().getFileSystem())));
}
