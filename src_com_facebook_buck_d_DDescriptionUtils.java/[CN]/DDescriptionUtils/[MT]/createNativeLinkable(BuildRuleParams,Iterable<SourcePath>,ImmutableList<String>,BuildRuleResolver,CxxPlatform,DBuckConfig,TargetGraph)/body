{
  BuildTarget buildTarget=params.getBuildTarget();
  SourcePathResolver sourcePathResolver=new SourcePathResolver(buildRuleResolver);
  ImmutableList<SourcePath> sourcePaths=sourcePathsForCompiledSources(sources,compilerFlags,params,buildRuleResolver,sourcePathResolver,cxxPlatform,dBuckConfig);
  return CxxLinkableEnhancer.createCxxLinkableBuildRule(targetGraph,cxxPlatform,params,sourcePathResolver,dBuckConfig.getLinkerFlagsForBinary(),buildTarget,Linker.LinkType.EXECUTABLE,Optional.<String>absent(),BuildTargets.getGenPath(buildTarget,"%s/" + buildTarget.getShortName()),sourcePaths,ImmutableList.<SourcePath>of(),Linker.LinkableDepType.STATIC,params.getDeps(),Optional.<Linker.CxxRuntimeType>absent(),Optional.<SourcePath>absent(),ImmutableSet.<BuildRule>of());
}
