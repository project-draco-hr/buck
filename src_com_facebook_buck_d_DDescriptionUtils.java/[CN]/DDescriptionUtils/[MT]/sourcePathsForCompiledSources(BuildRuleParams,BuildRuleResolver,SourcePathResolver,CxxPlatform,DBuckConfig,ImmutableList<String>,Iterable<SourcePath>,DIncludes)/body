{
  ImmutableList.Builder<SourcePath> sourcePaths=ImmutableList.builder();
  for (  SourcePath source : sources) {
    BuildTarget compileTarget=createDCompileBuildTarget(baseParams.getBuildTarget(),sourcePathResolver.getSourcePathName(baseParams.getBuildTarget(),source),cxxPlatform);
    requireBuildRule(compileTarget,baseParams,buildRuleResolver,sourcePathResolver,dBuckConfig,compilerFlags,source,includes);
    sourcePaths.add(new BuildTargetSourcePath(compileTarget));
  }
  return sourcePaths.build();
}
