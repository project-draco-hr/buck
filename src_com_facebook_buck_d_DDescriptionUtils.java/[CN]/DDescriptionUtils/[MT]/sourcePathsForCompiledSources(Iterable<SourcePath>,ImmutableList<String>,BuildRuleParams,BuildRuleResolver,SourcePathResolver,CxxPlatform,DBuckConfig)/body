{
  ImmutableList.Builder<SourcePath> sourcePaths=ImmutableList.builder();
  for (  SourcePath source : sources) {
    BuildTarget compileTarget=createDCompileBuildTarget(params.getBuildTarget(),sourcePathResolver.getSourcePathName(params.getBuildTarget(),source),cxxPlatform);
    requireBuildRule(params,buildRuleResolver,sourcePathResolver,source,compilerFlags,compileTarget,dBuckConfig);
    sourcePaths.add(new BuildTargetSourcePath(compileTarget));
  }
  return sourcePaths.build();
}
