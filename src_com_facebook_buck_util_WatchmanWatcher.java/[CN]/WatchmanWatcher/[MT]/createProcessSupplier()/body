{
  final ProcessBuilder processBuilder=new ProcessBuilder("watchman","--server-encoding=json","--no-pretty","-j");
  return new Supplier<Process>(){
    @Override public Process get(){
      try {
        LOG.debug("Starting watchman command: %s",processBuilder.command());
        return processBuilder.start();
      }
 catch (      IOException e) {
        throw Throwables.propagate(e);
      }
    }
  }
;
}
