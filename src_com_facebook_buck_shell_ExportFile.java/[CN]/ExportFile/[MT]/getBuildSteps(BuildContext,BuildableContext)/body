{
  ImmutableList.Builder<Step> builder=ImmutableList.<Step>builder().add(new MkdirStep(getProjectFilesystem(),out.getParent())).add(CopyStep.forFile(getProjectFilesystem(),getProjectFilesystem().resolve(getResolver().getPath(src)),out));
  buildableContext.recordArtifact(out);
  return builder.build();
}
