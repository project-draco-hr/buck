{
  ProjectWorkspace.ProcessResult result=workspace.runBuckBuild("-c","cxx.untracked_headers=error","-c","cxx.untracked_headers_whitelist=something","//:untracked_header");
  result.assertFailure();
  assertThat(result.getStderr(),Matchers.containsString("untracked_header.cpp: included an untracked header \"untracked_header.h\""));
}
