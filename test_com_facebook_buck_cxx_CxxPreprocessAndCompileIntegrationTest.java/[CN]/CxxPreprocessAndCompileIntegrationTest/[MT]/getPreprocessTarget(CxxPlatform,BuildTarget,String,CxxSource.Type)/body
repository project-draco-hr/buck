{
  CxxSourceRuleFactory cxxSourceRuleFactory=CxxSourceRuleFactoryHelper.of(workspace.getDestPath(),target,cxxPlatform);
  if (mode == CxxPreprocessMode.SEPARATE) {
    return cxxSourceRuleFactory.createPreprocessBuildTarget(source,type,CxxSourceRuleFactory.PicType.PDC);
  }
 else {
    return cxxSourceRuleFactory.createCompileBuildTarget(source,CxxSourceRuleFactory.PicType.PDC);
  }
}
