{
  workspace.runBuckBuild("//:simple_assembly#default,static").assertSuccess();
  Path lib=workspace.getPath("buck-out/gen/simple_assembly#default,static/libsimple_assembly.a");
  String contents=Files.asByteSource(lib.toFile()).asCharSource(Charsets.ISO_8859_1).read();
  assertFalse(lib.toString(),contents.contains(tmp.getRootPath().toString()));
}
