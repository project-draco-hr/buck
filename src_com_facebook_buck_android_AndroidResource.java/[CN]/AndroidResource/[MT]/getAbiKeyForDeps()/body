{
  Sha1HashCode abiKey=HasAndroidResourceDeps.ABI_HASHER.apply(FluentIterable.from(getNonEmptyResourceDeps()).toSortedSet(HasBuildTarget.BUILD_TARGET_COMPARATOR));
  if (!additionalAbiKey.isPresent()) {
    return abiKey;
  }
  return Sha1HashCode.fromHashCode(Hashing.sha1().newHasher().putUnencodedChars(abiKey.getHash()).putUnencodedChars(additionalAbiKey.get().get().getHash()).hash());
}
