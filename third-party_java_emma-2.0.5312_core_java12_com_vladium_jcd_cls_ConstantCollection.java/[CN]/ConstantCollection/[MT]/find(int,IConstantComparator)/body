{
  if (comparator == null)   throw new IllegalArgumentException("null input: comparator");
  for (int i=0; i < m_constants.size(); ++i) {
    final CONSTANT_info constant=(CONSTANT_info)m_constants.get(i);
    if ((constant != null) && (constant.tag() == type) && comparator.equals(constant))     return i + 1;
  }
  return -1;
}
