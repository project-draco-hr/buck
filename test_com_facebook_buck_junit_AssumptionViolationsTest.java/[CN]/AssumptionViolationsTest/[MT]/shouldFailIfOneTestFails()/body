{
  ProjectWorkspace.ProcessResult result=workspace.runBuckCommand("test","--all","--filter","com.example.FailingTest","--filter","com.example.PassingTest");
  result.assertTestFailure();
  String output=result.getStderr();
  assertThat(output,containsString("FAIL    <100ms  0 Passed   1 Skipped   1 Failed   com.example.FailingTest"));
  assertThat(output,containsString("PASS    <100ms  1 Passed   0 Skipped   0 Failed   com.example.PassingTest"));
  assertThat(output,containsString("TESTS FAILED"));
}
