{
  ProjectWorkspace.ProcessResult result=workspace.runBuckCommand("test","--all","--filter","com.example.PassingTest");
  result.assertSuccess();
  String output=result.getStderr();
  assertThat(output,containsRegex(createBuckTestOutputLineRegex("PASS",1,0,0,"com.example.PassingTest")));
  assertThat(output,containsString("TESTS PASSED"));
}
