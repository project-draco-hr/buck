{
  ProjectWorkspace.ProcessResult result=workspace.runBuckCommand("test","--all","--filter","com.example.SomeAssumptionViolationsTest","--filter","com.example.PassingTest");
  result.assertSuccess();
  String output=result.getStderr();
  assertThat(output,containsRegex(createBuckTestOutputLineRegex("ASSUME",1,2,0,"com.example.SomeAssumptionViolationsTest")));
  assertThat(output,containsRegex(createBuckTestOutputLineRegex("PASS",1,0,0,"com.example.PassingTest")));
  assertThat(output,containsString("TESTS PASSED (with some assumption violations)"));
}
