{
  BuildTarget rootBuildTarget=BuildTarget.builder("//foo","root").build();
  SchemeGenerator schemeGenerator=new SchemeGenerator(projectFilesystem,Optional.of(rootBuildTarget),ImmutableSet.of(rootBuildTarget),ImmutableSet.<BuildTarget>of(),ImmutableSet.<BuildTarget>of(),"TestScheme",Paths.get("_gen/Foo.xcworkspace/scshareddata/xcshemes"),SchemeActionType.DEFAULT_CONFIG_NAMES,ImmutableMap.<BuildTarget,PBXTarget>of(),ImmutableMap.<PBXTarget,Path>of());
  schemeGenerator.writeScheme();
}
