{
  BuildRule rootRule=createBuildRuleWithDefaults(BuildTarget.builder("//foo","root").build(),ImmutableSortedSet.<BuildRule>of(),appleLibraryDescription);
  SchemeGenerator schemeGenerator=new SchemeGenerator(projectFilesystem,Optional.of(rootRule),ImmutableSet.<BuildRule>of(rootRule),ImmutableSet.<BuildRule>of(),ImmutableSet.<BuildRule>of(),"TestScheme",Paths.get("_gen/Foo.xcworkspace/scshareddata/xcshemes"),SchemeActionType.DEFAULT_CONFIG_NAMES,ImmutableMap.<BuildRule,PBXTarget>of(),ImmutableMap.<PBXTarget,Path>of());
  schemeGenerator.writeScheme();
}
