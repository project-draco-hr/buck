{
  IosLibraryDescription.Arg arg=description.createUnpopulatedConstructorArg();
  arg.srcs=ImmutableList.of(AppleSource.ofSourcePath(new TestSourcePath("some_source.m")),AppleSource.ofSourcePath(new TestSourcePath("some_header.h")));
  arg.configs=ImmutableMap.of();
  arg.frameworks=ImmutableSortedSet.of();
  BuildRuleParams params=new FakeBuildRuleParamsBuilder(new BuildTarget("//foo","foo")).build();
  IosLibrary buildable=(IosLibrary)description.createBuildable(params,arg);
  assertThat(buildable.getInputsToCompareToOutput(),containsInAnyOrder(Paths.get("some_header.h"),Paths.get("some_source.m")));
}
