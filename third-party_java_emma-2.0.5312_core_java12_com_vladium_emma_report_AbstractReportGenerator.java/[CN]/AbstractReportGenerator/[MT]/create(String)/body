{
  if ((type == null) || (type.length() == 0))   throw new IllegalArgumentException("null/empty input: type");
  if ("html".equals(type))   return new com.vladium.emma.report.html.ReportGenerator();
 else   if ("txt".equals(type))   return new com.vladium.emma.report.txt.ReportGenerator();
 else   if ("xml".equals(type))   return new com.vladium.emma.report.xml.ReportGenerator();
 else   throw new EMMARuntimeException("no report generator class found for type [" + type + "]");
}
