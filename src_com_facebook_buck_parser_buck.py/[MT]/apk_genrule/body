@provide_for_build
def apk_genrule(name, srcs, apk, out, cmd=None, bash=None, cmd_exe=None, deps=[], visibility=[], build_env=None):
    deps = (deps + [apk])
    add_rule({'type': 'apk_genrule', 'name': name, 'srcs': srcs, 'apk': apk, 'cmd': cmd, 'bash': bash, 'cmd_exe': cmd_exe, 'deps': deps, 'visibility': visibility, }, build_env)
