@provide_for_build
def java_library(name, srcs=[], resources=[], proguard_config=None, deps=[], visibility=[], source='6', target='6', build_env=None):
    add_rule({'type': 'java_library', 'name': name, 'srcs': srcs, 'resources': resources, 'proguard_config': proguard_config, 'source': source, 'target': target, 'deps': deps, 'visibility': visibility, }, build_env)
