@provide_for_build
def java_library(name, srcs=[], resources=[], export_deps=None, exported_deps=[], source='6', target='6', proguard_config=None, deps=[], visibility=[], build_env=None):
    add_rule({'type': 'java_library', 'name': name, 'srcs': srcs, 'resources': resources, 'exported_deps': (deps if export_deps else exported_deps), 'source': source, 'target': target, 'proguard_config': proguard_config, 'deps': (deps + exported_deps), 'visibility': visibility, }, build_env)
