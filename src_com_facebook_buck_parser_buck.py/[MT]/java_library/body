@provide_for_build
def java_library(name, srcs=[], resources=[], export_deps=False, source='6', target='6', proguard_config=None, deps=[], visibility=[], build_env=None):
    add_rule({'type': 'java_library', 'name': name, 'srcs': srcs, 'resources': resources, 'export_deps': export_deps, 'source': source, 'target': target, 'proguard_config': proguard_config, 'deps': deps, 'visibility': visibility, }, build_env)
