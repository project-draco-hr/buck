@provide_for_build
def genrule(name, out, cmd=None, bash=None, cmd_exe=None, srcs=[], deps=[], visibility=[], build_env=None):
    add_rule({'type': 'genrule', 'name': name, 'srcs': srcs, 'cmd': cmd, 'bash': bash, 'cmd_exe': cmd_exe, 'out': out, 'deps': deps, 'visibility': visibility, }, build_env)
