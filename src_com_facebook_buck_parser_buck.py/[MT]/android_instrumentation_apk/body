@provide_for_build
def android_instrumentation_apk(name, manifest, apk, deps=[], visibility=[], build_env=None):
    add_rule({'type': 'android_instrumentation_apk', 'name': name, 'manifest': manifest, 'apk': apk, 'classpath_deps': deps, 'deps': (deps + [apk]), 'visibility': visibility, }, build_env)
