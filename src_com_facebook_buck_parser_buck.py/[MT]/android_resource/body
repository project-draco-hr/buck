@provide_for_build
def android_resource(name, res=None, package=None, assets=None, manifest=None, deps=[], visibility=[], build_env=None):
    add_rule({'type': 'android_resource', 'name': name, 'res': res, 'package': package, 'assets': assets, 'manifest': manifest, 'deps': deps, 'visibility': visibility, }, build_env)
