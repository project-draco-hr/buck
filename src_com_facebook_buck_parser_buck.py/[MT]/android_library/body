@provide_for_build
def android_library(name, srcs=[], resources=[], manifest=None, proguard_config=None, deps=[], visibility=[], build_env=None):
    add_rule({'type': 'android_library', 'name': name, 'srcs': srcs, 'resources': resources, 'manifest': manifest, 'proguard_config': proguard_config, 'deps': deps, 'visibility': visibility, }, build_env)
