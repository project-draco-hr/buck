@provide_for_build
def android_library(name, srcs=[], resources=[], exported_deps=[], manifest=None, proguard_config=None, deps=[], visibility=[], build_env=None):
    add_rule({'type': 'android_library', 'name': name, 'srcs': srcs, 'exported_deps': exported_deps, 'resources': resources, 'manifest': manifest, 'proguard_config': proguard_config, 'deps': (deps + exported_deps), 'visibility': visibility, }, build_env)
