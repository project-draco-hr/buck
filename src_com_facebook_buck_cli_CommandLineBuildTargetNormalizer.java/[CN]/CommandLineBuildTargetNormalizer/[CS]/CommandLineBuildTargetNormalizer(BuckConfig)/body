{
  this.normalizer=new Function<String,String>(){
    @Override public String apply(    String arg){
      String aliasValue=buckConfig.getBuildTargetForAlias(arg);
      if (aliasValue != null) {
        return aliasValue;
      }
 else {
        return normalizeBuildTargetIdentifier(arg);
      }
    }
  }
;
}
