{
  ImmutableMap.Builder<String,ImmutableMap<String,String>> builder=ImmutableMap.builder();
  if (buildFileIncludes != null) {
    String includes=MoreStrings.stripPrefix(buildFileIncludes,BUILDFILE_INCLUDES_LONG_ARG + " ").or(buildFileIncludes);
    includes=MoreStrings.stripPrefix(includes,UnflavoredBuildTarget.BUILD_TARGET_PREFIX).or(includes);
    builder.put(ParserConfig.BUILDFILE_SECTION_NAME,ImmutableMap.of(ParserConfig.INCLUDES_PROPERTY_NAME,UnflavoredBuildTarget.BUILD_TARGET_PREFIX + includes));
  }
  return builder.build();
}
