{
  ImmutableList.Builder<TargetNodeSpec> specs=ImmutableList.builder();
  CommandLineTargetNodeSpecParser parser=new CommandLineTargetNodeSpecParser(config,new BuildTargetPatternTargetNodeParser(ignorePaths));
  for (  String arg : targetsAsArgs) {
    specs.add(parser.parse(arg));
  }
  return specs.build();
}
