{
  final String[] specs=Strings.merge(new String[]{value},COMMA_DELIMITERS,true);
  for (int i=0; i < specs.length; ++i) {
    final String spec=specs[i];
    if (spec.startsWith(IInclExclFilter.EXCLUSION_PREFIX_STRING)) {
      appendValue(spec,COMMA);
    }
 else {
      if (spec.startsWith(IInclExclFilter.INCLUSION_PREFIX_STRING))       appendValue(IInclExclFilter.EXCLUSION_PREFIX + spec.substring(1),COMMA);
 else       appendValue(IInclExclFilter.EXCLUSION_PREFIX + spec,COMMA);
    }
  }
}
