{
  ProjectWorkspace workspace=TestDataHelper.createProjectWorkspaceForScenario(this,"project_with_five_aliases",temporaryFolder);
  workspace.setUp();
  ProcessResult result=workspace.runBuckCommand("build");
  result.assertExitCode("buck build should exit with an error.",1);
  assertEquals(Joiner.on(' ').join("`buck build` should suggest the five aliases found in .buckconfig","(in the order in which they are listed in) as targets."),Joiner.on('\n').join("BUILD FAILED: Must specify at least one build target.","Try building one of the following targets:","myapp my_app mi_app mon_app mein_app") + '\n',result.getStderr());
}
