{
  ProjectWorkspace workspace=TestDataHelper.createProjectWorkspaceForScenario(this,"project_with_one_alias",temporaryFolder);
  workspace.setUp();
  ProcessResult result=workspace.runBuckCommand("build");
  result.assertFailure("buck build should exit with an error.");
  assertEquals("`buck build` should suggest the alias found in .buckconfig as a target.",Joiner.on('\n').join("BUILD FAILED: Must specify at least one build target.","Try building one of the following targets:","myapp") + '\n',result.getStderr());
}
