{
  if (file == null)   throw new IllegalArgumentException("null input: file");
  Properties result=null;
  InputStream in=null;
  try {
    in=new BufferedInputStream(new FileInputStream(file),8 * 1024);
    result=new XProperties();
    result.load(in);
  }
  finally {
    if (in != null)     try {
      in.close();
    }
 catch (    Throwable ignore) {
    }
    in=null;
  }
  return result;
}
