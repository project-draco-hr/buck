{
  Properties result=null;
  InputStream in=null;
  try {
    in=ResourceLoader.getResourceAsStream(name);
    if (in != null) {
      result=new XProperties();
      result.load(in);
    }
  }
 catch (  Throwable t) {
    result=null;
  }
 finally {
    if (in != null)     try {
      in.close();
    }
 catch (    Throwable ignore) {
    }
    in=null;
  }
  return result;
}
