{
  boolean success=false;
  File buck=new File(binDirectory,BUCK_BIN);
  if (buck.canExecute()) {
    buckPath=buck.getAbsolutePath();
    success=true;
  }
  File buckd=new File(binDirectory,BUCKD_BIN);
  if (buckd.canExecute()) {
    buckdPath=Optional.of(buckd.getAbsolutePath());
  }
  return success;
}
