{
  ImmutableList<String> command=ImmutableList.<String>builder().add(buckPath).addAll(ImmutableList.copyOf(args)).build();
  if (socket.isPresent()) {
    socket.get().start();
  }
  int exitCode=execute(command,ImmutableMap.<String,String>of());
  if (socket.isPresent()) {
    socket.get().stop();
  }
  return exitCode;
}
