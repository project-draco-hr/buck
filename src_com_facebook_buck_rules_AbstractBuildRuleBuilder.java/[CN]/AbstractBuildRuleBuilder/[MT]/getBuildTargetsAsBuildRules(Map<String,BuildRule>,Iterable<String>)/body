{
  return ImmutableSortedSet.copyOf(Iterables.transform(buildTargets,new Function<String,BuildRule>(){
    @Override public BuildRule apply(    String dep){
      BuildRule buildRule=buildRuleIndex.get(dep);
      Preconditions.checkNotNull(buildRule,"No rule for %s",dep);
      return buildRule;
    }
  }
));
}
