{
  JavaFileObject fileObject1=inMemoryFileManager.getJavaFileForOutput(locationOf("src"),"jvm.java.JavaFileParser",JavaFileObject.Kind.SOURCE,null);
  JavaFileObject fileObject2=inMemoryFileManager.getJavaFileForOutput(locationOf("src"),"jvm.java.JavaInMemoryFileManager",JavaFileObject.Kind.SOURCE,null);
  fileObject1.openOutputStream().close();
  fileObject2.openOutputStream().close();
  List<ZipEntry> zipEntries=outputStream.getZipEntries();
  assertEquals(4,zipEntries.size());
  assertEquals("jvm/",zipEntries.get(0).getName());
  assertEquals("jvm/java/",zipEntries.get(1).getName());
  assertEquals("jvm/java/JavaFileParser.java",zipEntries.get(2).getName());
  assertEquals("jvm/java/JavaInMemoryFileManager.java",zipEntries.get(3).getName());
}
