{
  ImmutableList.Builder<Step> commands=ImmutableList.builder();
  if (!javaSourceFilePaths.isEmpty()) {
    Step javac=new DependencyCheckingJavacStep(outputDirectory,javaSourceFilePaths,transitiveClasspathEntries,declaredClasspathEntries,javacOptions,invokingRule,buildDependencies,suggestBuildRules);
    commands.add(javac);
  }
  return commands.build();
}
