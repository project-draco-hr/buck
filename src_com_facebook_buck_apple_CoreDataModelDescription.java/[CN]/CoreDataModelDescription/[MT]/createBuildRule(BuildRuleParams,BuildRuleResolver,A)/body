{
  ProjectFilesystem projectFilesystem=params.getProjectFilesystem();
  Supplier<ImmutableCollection<Path>> inputPathsSupplier=RuleUtils.subpathsOfPathsSupplier(projectFilesystem,ImmutableSet.of(args.path));
  return new CoreDataModel(params,new SourcePathResolver(resolver),inputPathsSupplier,args);
}
