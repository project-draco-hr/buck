{
  ProjectFilesystem projectFilesystem=params.getProjectFilesystem();
  Supplier<ImmutableCollection<Path>> inputPathsSupplier=RuleUtils.subpathsOfPathsSupplier(projectFilesystem,ImmutableSet.of(args.path));
  String extension=Files.getFileExtension(args.path.getFileName().toString());
  Preconditions.checkArgument(CORE_DATA_MODEL_EXTENSION.equals(extension) || VERSIONED_CORE_DATA_MODEL_EXTENSION.equals(extension));
  return new CoreDataModel(params,new SourcePathResolver(resolver),inputPathsSupplier,args.path);
}
