{
  DummyRDotJava dummyRDotJava=new DummyRDotJava(new FakeBuildRuleParamsBuilder(BuildTargetFactory.newInstance("//java/com/example:library")).build(),new SourcePathResolver(new BuildRuleResolver(TargetGraph.EMPTY,new DefaultTargetNodeToBuildRuleTransformer())),ImmutableSet.<HasAndroidResourceDeps>of(),new FakeSourcePath("abi.jar"),ANDROID_JAVAC_OPTIONS,false,Optional.<String>absent());
  assertEquals(BuildTargets.getScratchPath(dummyRDotJava.getProjectFilesystem(),dummyRDotJava.getBuildTarget(),"__%s_rdotjava_bin__"),dummyRDotJava.getRDotJavaBinFolder());
}
