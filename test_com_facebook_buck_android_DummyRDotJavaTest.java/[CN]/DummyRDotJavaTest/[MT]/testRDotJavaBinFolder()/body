{
  DummyRDotJava dummyRDotJava=new DummyRDotJava(new FakeBuildRuleParamsBuilder(BuildTargetFactory.newInstance("//java/com/example:library")).build(),new SourcePathResolver(new BuildRuleResolver()),ImmutableSet.<HasAndroidResourceDeps>of(),new TestSourcePath("abi.jar"),ANDROID_JAVAC_OPTIONS,Optional.<String>absent());
  assertEquals(Paths.get("buck-out/bin/java/com/example/__library_rdotjava_bin__"),dummyRDotJava.getRDotJavaBinFolder());
}
