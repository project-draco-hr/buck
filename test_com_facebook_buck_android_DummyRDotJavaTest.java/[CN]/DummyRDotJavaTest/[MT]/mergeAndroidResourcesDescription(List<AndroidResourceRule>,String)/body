{
  List<String> sortedSymbolsFiles=FluentIterable.from(resourceRules).transform(new Function<AndroidResourceRule,Path>(){
    @Override public Path apply(    AndroidResourceRule input){
      return input.getPathToTextSymbolsFile();
    }
  }
).transform(Functions.toStringFunction()).toList();
  return "android-res-merge " + Joiner.on(' ').join(sortedSymbolsFiles) + " -o "+ rDotJavaSourceFolder;
}
