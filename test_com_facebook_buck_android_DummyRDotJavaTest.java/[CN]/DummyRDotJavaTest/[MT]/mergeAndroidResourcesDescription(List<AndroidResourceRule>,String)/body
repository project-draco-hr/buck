{
  List<String> sortedSymbolsFiles=FluentIterable.from(resourceRules).transform(new Function<AndroidResourceRule,String>(){
    @Override public String apply(    AndroidResourceRule input){
      return input.getPathToTextSymbolsFile();
    }
  }
).toList();
  return "android-res-merge " + Joiner.on(' ').join(sortedSymbolsFiles) + " -o "+ rDotJavaSourceFolder;
}
