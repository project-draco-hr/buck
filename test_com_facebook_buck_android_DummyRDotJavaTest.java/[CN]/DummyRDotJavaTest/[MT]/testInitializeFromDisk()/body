{
  DummyRDotJava dummyRDotJava=new DummyRDotJava(new FakeBuildRuleParamsBuilder(BuildTargetFactory.newInstance("//java/base:rule")).build(),new SourcePathResolver(new BuildRuleResolver()),ImmutableSet.<HasAndroidResourceDeps>of(),new TestSourcePath("abi.jar"),ANDROID_JAVAC_OPTIONS,Optional.<String>absent());
  FakeOnDiskBuildInfo onDiskBuildInfo=new FakeOnDiskBuildInfo();
  String keyHash=Strings.repeat("a",40);
  onDiskBuildInfo.putMetadata(AbiRule.ABI_KEY_ON_DISK_METADATA,keyHash);
  assertEquals(keyHash,dummyRDotJava.initializeFromDisk(onDiskBuildInfo).rDotTxtSha1.getHash());
}
