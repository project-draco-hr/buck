{
  return ImmutableList.of("-isystem",ndkRoot.resolve("platforms").resolve(targetConfiguration.targetPlatform).resolve("arch-" + targetConfiguration.targetArch).resolve("usr").resolve("include").toString(),"-isystem",ndkRoot.resolve("platforms").resolve(targetConfiguration.targetPlatform).resolve("arch-" + targetConfiguration.targetArch).resolve("usr").resolve("include").resolve("linux").toString());
}
