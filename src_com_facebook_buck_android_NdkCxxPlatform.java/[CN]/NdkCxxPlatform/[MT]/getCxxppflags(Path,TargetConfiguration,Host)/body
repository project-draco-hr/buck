{
  return ImmutableList.<String>builder().addAll(targetConfiguration.compilerFlags).addAll(getCommonCppflags()).addAll(getCommonCxxFlags()).addAll(getCommonFlags()).add("-Wno-literal-suffix").add("-isystem",ndkRoot.resolve("sources").resolve("cxx-stl").resolve("gnu-libstdc++").resolve(targetConfiguration.compilerVersion).resolve("include").toString()).add("-isystem",ndkRoot.resolve("sources").resolve("cxx-stl").resolve("gnu-libstdc++").resolve(targetConfiguration.compilerVersion).resolve("libs").resolve(targetConfiguration.targetArchAbi.toString()).resolve("include").toString()).addAll(getCommonIncludes(ndkRoot,targetConfiguration,host)).build();
}
