{
  super(params,resolver,pythonPlatform,mainModule,components,preloadLibraries,pexExtension);
  this.script=getScript(pythonPlatform,cxxPlatform,mainModule,components,getProjectFilesystem().resolve(getBinPath()).getParent().relativize(linkTree.getRoot()),preloadLibraries);
