{
  super(buildRuleParams,resolver);
  Preconditions.checkArgument(buildRuleParams.getBuildTarget().getFlavors().contains(RULE_FLAVOR),"CxxStrip rule %s should contain %s flavor",this,RULE_FLAVOR);
  this.stripStyle=stripStyle;
  this.cxxLinkSourcePath=cxxLinkSourcePath;
  this.strip=strip;
  this.output=output;
}
