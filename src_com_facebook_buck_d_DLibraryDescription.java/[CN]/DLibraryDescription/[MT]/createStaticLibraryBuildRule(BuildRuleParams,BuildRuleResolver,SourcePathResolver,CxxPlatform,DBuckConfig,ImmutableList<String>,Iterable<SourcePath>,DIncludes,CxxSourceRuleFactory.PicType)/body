{
  ImmutableList<SourcePath> compiledSources=DDescriptionUtils.sourcePathsForCompiledSources(params,ruleResolver,pathResolver,cxxPlatform,dBuckConfig,compilerFlags,sources,dIncludes);
  BuildTarget staticTarget=CxxDescriptionEnhancer.createStaticLibraryBuildTarget(params.getBuildTarget(),cxxPlatform.getFlavor(),pic);
  Path staticLibraryPath=CxxDescriptionEnhancer.getStaticLibraryPath(params.getBuildTarget(),cxxPlatform.getFlavor(),pic);
  return Archives.createArchiveRule(pathResolver,staticTarget,params.copyWithBuildTarget(BuildTarget.builder().from(params.getBuildTarget()).addFlavors(cxxPlatform.getFlavor(),CxxDescriptionEnhancer.STATIC_FLAVOR).build()),cxxPlatform.getAr(),cxxPlatform.getRanlib(),staticLibraryPath,compiledSources);
}
