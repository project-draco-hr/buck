{
  SourcePathResolver pathResolver=new SourcePathResolver(buildRuleResolver);
  if (params.getBuildTarget().getFlavors().contains(CxxDescriptionEnhancer.STATIC_FLAVOR)) {
    return createStaticLibraryBuildRule(params,buildRuleResolver,pathResolver,dBuckConfig,cxxPlatform,args.srcs,ImmutableList.<String>of(),CxxSourceRuleFactory.PicType.PDC);
  }
  return new DLibrary(params,buildRuleResolver,pathResolver);
}
