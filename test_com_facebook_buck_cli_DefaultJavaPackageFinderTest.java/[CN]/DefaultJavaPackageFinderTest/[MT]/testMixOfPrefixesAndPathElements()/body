{
  DefaultJavaPackageFinder javaPackageFinder=DefaultJavaPackageFinder.createDefaultJavaPackageFinder(ImmutableList.of("/java","src"));
  assertEquals("Prefixes take precedence over path elements","com/example/base/",javaPackageFinder.findJavaPackageFolderForPath("/java/main/src/com/example/base/Base.java"));
}
