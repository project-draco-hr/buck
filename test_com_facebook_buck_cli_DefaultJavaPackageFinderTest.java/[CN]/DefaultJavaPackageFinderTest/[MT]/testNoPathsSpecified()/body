{
  DefaultJavaPackageFinder javaPackageFinder=DefaultJavaPackageFinder.createDefaultJavaPackageFinder(ImmutableList.<String>of());
  assertEquals("",javaPackageFinder.findJavaPackageFolderForPath("Base.java"));
  assertEquals("java/com/example/base/",javaPackageFinder.findJavaPackageFolderForPath("java/com/example/base/Base.java"));
}
