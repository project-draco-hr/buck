{
  DefaultJavaPackageFinder javaPackageFinder=DefaultJavaPackageFinder.createDefaultJavaPackageFinder(ImmutableList.of("src","src-gen"));
  assertEquals("com/example/base/",javaPackageFinder.findJavaPackageFolderForPath("java/main/src/com/example/base/"));
  assertEquals("com/example/base/",javaPackageFinder.findJavaPackageFolderForPath("java/main/src-gen/other/project/src/com/example/base/Base.java"));
}
