{
  return (AndroidLibrary)params.buildAndAddToIndex(AndroidLibrary.newAndroidLibraryRuleBuilder(new FakeBuildRuleBuilderParams()).setBuildTarget(BuildTargetFactory.newInstance("//java/src/com/bar:bar")).addSrc(Paths.get("java/src/com/bar/Bar.java")).setManifestFile((Optional.<Path>absent())).addVisibilityPattern(BuildTargetPattern.MATCH_ALL));
}
