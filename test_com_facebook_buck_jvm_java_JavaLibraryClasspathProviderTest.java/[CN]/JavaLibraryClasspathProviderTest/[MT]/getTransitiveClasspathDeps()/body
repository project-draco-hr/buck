{
  JavaLibrary aLib=(JavaLibrary)a;
  assertEquals(ImmutableSet.of(c,e),JavaLibraryClasspathProvider.getTransitiveClasspathDeps(aLib,Optional.<SourcePath>absent()));
  assertEquals(ImmutableSet.of(a,c,e),JavaLibraryClasspathProvider.getTransitiveClasspathDeps(aLib,Optional.of(sourcePathFunction.apply(aLib.getPathToOutput()))));
}
