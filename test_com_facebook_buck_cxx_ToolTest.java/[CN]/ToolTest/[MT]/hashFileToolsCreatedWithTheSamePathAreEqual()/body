{
  SourcePathResolver pathResolver=new SourcePathResolver(new BuildRuleResolver());
  RuleKeyBuilderFactory ruleKeyBuilderFactory=new FakeRuleKeyBuilderFactory(FakeFileHashCache.createFromStrings(ImmutableMap.<String,String>builder().put("path",Strings.repeat("a",40)).put("other-path",Strings.repeat("b",40)).put("same",Strings.repeat("a",40)).build()),pathResolver);
  Path path=Paths.get("path");
  Path otherPath=Paths.get("other-path");
  Path same=Paths.get("same");
  Tool tool1=new HashedFileTool(path);
  RuleKeyPair tool1RuleKey=createRuleKeyBuilder(ruleKeyBuilderFactory,pathResolver).setReflectively("tool",tool1).build();
  Tool tool2=new HashedFileTool(path);
  RuleKeyPair tool2RuleKey=createRuleKeyBuilder(ruleKeyBuilderFactory,pathResolver).setReflectively("tool",tool2).build();
  assertEquals(tool1RuleKey,tool2RuleKey);
  Tool tool3=new HashedFileTool(otherPath);
  RuleKeyPair tool3RuleKey=createRuleKeyBuilder(ruleKeyBuilderFactory,pathResolver).setReflectively("tool",tool3).build();
  assertNotEquals(tool1RuleKey,tool3RuleKey);
  Tool tool4=new HashedFileTool(same);
  RuleKeyPair tool4RuleKey=createRuleKeyBuilder(ruleKeyBuilderFactory,pathResolver).setReflectively("tool",tool4).build();
  assertNotEquals(tool1RuleKey,tool4RuleKey);
}
