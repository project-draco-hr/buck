{
  struct.validate();
  oprot.writeStructBegin(STRUCT_DESC);
  if (struct.buildJob != null) {
    if (struct.isSetBuildJob()) {
      oprot.writeFieldBegin(BUILD_JOB_FIELD_DESC);
      struct.buildJob.write(oprot);
      oprot.writeFieldEnd();
    }
  }
  oprot.writeFieldStop();
  oprot.writeStructEnd();
}
