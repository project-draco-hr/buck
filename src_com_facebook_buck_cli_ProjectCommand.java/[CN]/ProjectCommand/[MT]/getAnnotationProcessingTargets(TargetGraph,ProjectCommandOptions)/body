{
  ImmutableSet<BuildTarget> buildTargets=getBuildTargets(options.getArgumentsFormattedAsBuildTargets());
  if (buildTargets.isEmpty()) {
    buildTargets=getRootsFromPredicate(fullGraph,ANNOTATION_PREDICATE);
  }
  return FluentIterable.from(buildTargets).transform(Functions.toStringFunction()).toList();
}
