{
  if (ide == ProjectCommandOptions.Ide.XCODE && !passedInBuildTargets.isEmpty()) {
    return Iterables.transform(passedInBuildTargets,BuildTargetSpec.TO_BUILD_TARGET_SPEC);
  }
 else {
    return ImmutableList.of(new TargetNodePredicateSpec(Predicates.<TargetNode<?>>alwaysTrue(),ignoreDirs));
  }
}
