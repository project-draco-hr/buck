{
  assertEquals("1MB",SizeUnit.formatBytes(1048576));
  assertEquals("-1MB",SizeUnit.formatBytes(-1048576));
  assertEquals("1.5MB",SizeUnit.formatBytes(1572864));
  assertEquals("-1.5MB",SizeUnit.formatBytes(-1572864));
  assertEquals("512MB",SizeUnit.formatBytes(536870912));
  assertEquals("-512MB",SizeUnit.formatBytes(-536870912));
  assertEquals("1023.9MB",SizeUnit.formatBytes(1073741823));
  assertEquals("-1023.9MB",SizeUnit.formatBytes(-1073741823));
}
