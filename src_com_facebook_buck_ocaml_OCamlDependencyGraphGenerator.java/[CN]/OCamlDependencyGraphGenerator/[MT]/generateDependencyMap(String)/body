{
  ImmutableMap.Builder<Path,ImmutableList<Path>> mapBuilder=ImmutableMap.builder();
  Iterable<String> lines=Splitter.on(LINE_SEPARATOR).split(depString);
  for (  String line : lines) {
    List<String> sourceAndDeps=Splitter.on(OCAML_SOURCE_AND_DEPS_SEPARATOR).trimResults().splitToList(line);
    if (sourceAndDeps.size() >= 1) {
      String source=replaceObjExtWithSourceExt(sourceAndDeps.get(0));
      if (source.endsWith(OCamlCompilables.OCAML_ML) || source.endsWith(OCamlCompilables.OCAML_MLI)) {
        FluentIterable<Path> dependencies=FluentIterable.from(Splitter.on(OCAML_DEPS_SEPARATOR).trimResults().splitToList(sourceAndDeps.get(1))).filter(new Predicate<String>(){
          @Override public boolean apply(          String input){
            return !input.isEmpty();
          }
        }
).transform(new Function<String,Path>(){
          @Override public Path apply(          String input){
            return Paths.get(replaceObjExtWithSourceExt(input));
          }
        }
);
        mapBuilder.put(Paths.get(source),ImmutableList.copyOf(dependencies));
      }
    }
  }
  return mapBuilder.build();
}
