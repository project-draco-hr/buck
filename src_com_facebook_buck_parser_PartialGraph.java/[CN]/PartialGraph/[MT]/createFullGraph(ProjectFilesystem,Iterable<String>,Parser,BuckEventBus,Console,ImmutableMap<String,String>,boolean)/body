{
  TargetGraph graph=parser.buildTargetGraph(ImmutableList.of(new TargetNodePredicateSpec(Predicates.<TargetNode<?>>alwaysTrue(),projectFilesystem.getIgnorePaths())),includes,eventBus,console,environment,enableProfiling);
  return new PartialGraph(graph.getActionGraph(eventBus),FluentIterable.from(graph.getNodes()).transform(HasBuildTarget.TO_TARGET).toSet());
}
