{
  super(outputDirectory,javaSourceFilePaths,transitiveClasspathEntries,declaredClasspathEntries,javacOptions,pathToOutputAbiFile,invokingRule,buildDependencies,suggestBuildRules,pathToSrcsList);
  this.pathToJavac=javacOptions.getJavaCompilerEnvironment().getJavacPath().get();
  this.target=Preconditions.checkNotNull(target);
}
