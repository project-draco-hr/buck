{
  super(outputDirectory,javaSourceFilePaths,transitiveClasspathEntries,declaredClasspathEntries,javacOptions,invokingRule,buildDependencies,suggestBuildRules,pathToSrcsList);
  this.pathToJavac=javacOptions.getJavaCompilerEnvironment().getJavacPath().get();
  this.target=target;
  this.workingDirectory=workingDirectory;
}
