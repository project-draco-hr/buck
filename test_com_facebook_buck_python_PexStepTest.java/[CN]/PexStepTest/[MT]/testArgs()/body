{
  PexStep step=new PexStep(WORKING_DIRECTORY,ImmutableList.<String>builder().add("build").add("--some","--args").build(),PYTHON_PATH,TEMP_PATH,DEST_PATH,ENTRY_POINT,MODULES,RESOURCES,NATIVE_LIBRARIES,PREBUILT_LIBRARIES,true);
  assertThat(step.getShellCommandInternal(TestExecutionContext.newInstance()),hasItems("--some","--args"));
}
