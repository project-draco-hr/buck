{
  PexStep step=new PexStep(new FakeProjectFilesystem(),PEX_ENVIRONMENT,ImmutableList.<String>builder().add("build").add("--some","--args").build(),PYTHON_PATH,TEMP_PATH,DEST_PATH,ENTRY_POINT,MODULES,RESOURCES,NATIVE_LIBRARIES,PREBUILT_LIBRARIES,PRELOAD_LIBRARIES,true);
  assertThat(step.getShellCommandInternal(TestExecutionContext.newInstance()),hasItems("--some","--args"));
}
