{
  PexStep step=new PexStep(new FakeProjectFilesystem(),PEX_ENVIRONMENT,PEX_COMMAND,PYTHON_PATH,TEMP_PATH,DEST_PATH,ENTRY_POINT,MODULES,RESOURCES,NATIVE_LIBRARIES,PREBUILT_LIBRARIES,PRELOAD_LIBRARIES,false);
  String command=Joiner.on(" ").join(step.getShellCommandInternal(TestExecutionContext.newInstance()));
  assertThat(command,containsString("--no-zip-safe"));
}
