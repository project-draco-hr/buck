{
  PexStep step=new PexStep(PEXPY_PATH,PYTHON_PATH,TEMP_PATH,DEST_PATH,ENTRY_POINT,ImmutableMap.<Path,Path>of(),ImmutableMap.<Path,Path>of(),ImmutableMap.<Path,Path>of(),false);
  String command=Joiner.on(" ").join(step.getShellCommandInternal(TestExecutionContext.newInstance()));
  assertThat(command,containsString("--no-zip-safe"));
}
