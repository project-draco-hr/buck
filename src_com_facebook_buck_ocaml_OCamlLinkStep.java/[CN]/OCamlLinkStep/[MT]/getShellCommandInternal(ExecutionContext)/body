{
  return ImmutableList.<String>builder().add(args.ocamlCompiler.toString()).addAll(OCamlCompilables.DEFAULT_OCAML_FLAGS).add("-cc",args.cxxCompiler.get(0)).addAll(MoreIterables.zipAndConcat(Iterables.cycle("-ccopt"),args.cxxCompiler.subList(1,args.cxxCompiler.size()))).addAll((args.isLibrary ? Optional.of("-a") : Optional.<String>absent()).asSet()).addAll((!args.isLibrary && args.isBytecode ? Optional.of("-custom") : Optional.<String>absent()).asSet()).add("-o",args.output.toString()).addAll(args.flags).addAll(ocamlInput).addAll(args.input).addAll(MoreIterables.zipAndConcat(Iterables.cycle("-cclib"),FluentIterable.from(args.nativeDepInput).transform(Arg.stringifyFunction()))).build();
}
