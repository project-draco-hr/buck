{
  return ImmutableList.<String>builder().add(args.ocamlCompiler.toString()).addAll(OCamlCompilables.DEFAULT_OCAML_FLAGS).add("-cc").add(args.cxxCompiler.toString()).addAll((args.isLibrary ? Optional.of("-a") : Optional.<String>absent()).asSet()).addAll((!args.isLibrary && args.isBytecode ? Optional.of("-custom") : Optional.<String>absent()).asSet()).add("-o",args.output.toString()).addAll(args.flags).addAll(ocamlInput).addAll(args.input).addAll(aAndOInput).build();
}
