{
  final IProperties appProperties=getAppProperties();
  if (appProperties != null) {
    final String property=appProperties.getProperty(EMMAProperties.PROPERTY_COVERAGE_DATA_OUT_MERGE,EMMAProperties.DEFAULT_COVERAGE_DATA_OUT_MERGE.toString());
    return Property.toBoolean(property);
  }
  return EMMAProperties.DEFAULT_COVERAGE_DATA_OUT_MERGE.booleanValue();
}
