{
  final IProperties appProperties=getAppProperties();
  if (appProperties != null) {
    final String property=appProperties.getProperty(EMMAProperties.PROPERTY_COVERAGE_DATA_OUT_FILE,EMMAProperties.DEFAULT_COVERAGE_DATA_OUT_FILE);
    return new File(property);
  }
  return new File(EMMAProperties.DEFAULT_COVERAGE_DATA_OUT_FILE);
}
