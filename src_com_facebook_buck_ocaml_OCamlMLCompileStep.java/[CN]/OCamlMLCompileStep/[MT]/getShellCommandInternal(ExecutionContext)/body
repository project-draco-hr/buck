{
  return ImmutableList.<String>builder().addAll(args.ocamlCompiler.getCommandPrefix(resolver)).addAll(OCamlCompilables.DEFAULT_OCAML_FLAGS).add("-cc",args.cCompiler.get(0)).addAll(MoreIterables.zipAndConcat(Iterables.cycle("-ccopt"),args.cCompiler.subList(1,args.cCompiler.size()))).add("-c").add("-annot").add("-bin-annot").add("-o",args.output.toString()).addAll(args.flags).add(args.input.toString()).build();
}
