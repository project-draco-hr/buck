{
  this.dummyRDotJavaBuildTarget=BuildTarget.builder(buildTarget).addFlavor(DUMMY_R_DOT_JAVA_FLAVOR).build();
  this.originalBuildRuleParams=buildRuleParams;
  this.javacOptions=JavacOptions.builder(javacOptions).setAnnotationProcessingParams(AnnotationProcessingParams.EMPTY).build();
  this.resourceDependencyMode=resourceDependencyMode;
}
