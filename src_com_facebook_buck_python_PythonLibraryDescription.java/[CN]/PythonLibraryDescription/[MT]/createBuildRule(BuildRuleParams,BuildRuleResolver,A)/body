{
  Path baseModule=PythonUtil.getBasePath(params.getBuildTarget(),args.baseModule);
  return new PythonLibrary(params,new SourcePathResolver(resolver),PythonUtil.toModuleMap(params.getBuildTarget(),"srcs",baseModule,args.srcs),PythonUtil.toModuleMap(params.getBuildTarget(),"resources",baseModule,args.resources));
}
