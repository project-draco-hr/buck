{
  SourcePathResolver pathResolver=new SourcePathResolver(resolver);
  Path baseModule=PythonUtil.getBasePath(params.getBuildTarget(),args.baseModule);
  return new PythonLibrary(params,pathResolver,PythonUtil.toModuleMap(params.getBuildTarget(),pathResolver,"srcs",baseModule,args.srcs),PythonUtil.toModuleMap(params.getBuildTarget(),pathResolver,"resources",baseModule,args.resources));
}
