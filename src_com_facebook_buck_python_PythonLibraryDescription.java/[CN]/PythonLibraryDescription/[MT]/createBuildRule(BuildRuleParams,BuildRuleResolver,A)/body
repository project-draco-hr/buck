{
  Path baseModule=PythonUtil.getBasePath(params.getBuildTarget(),args.baseModule);
  return new PythonLibrary(params,PythonUtil.toModuleMap(params.getBuildTarget(),"srcs",baseModule,args.srcs.or(ImmutableSortedSet.<SourcePath>of())),PythonUtil.toModuleMap(params.getBuildTarget(),"resources",baseModule,args.resources.or(ImmutableSortedSet.<SourcePath>of())));
}
