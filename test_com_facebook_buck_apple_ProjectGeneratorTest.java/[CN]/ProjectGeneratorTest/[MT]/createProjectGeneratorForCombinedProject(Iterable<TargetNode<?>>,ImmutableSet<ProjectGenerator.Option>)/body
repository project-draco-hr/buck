{
  ImmutableSet<BuildTarget> initialBuildTargets=FluentIterable.from(nodes).transform(HasBuildTarget.TO_TARGET).toSet();
  return new ProjectGenerator(TargetGraphFactory.newInstance(ImmutableSet.copyOf(nodes)),initialBuildTargets,projectFilesystem,OUTPUT_DIRECTORY,PROJECT_NAME,"BUCK",projectGeneratorOptions,Optional.<BuildTarget>absent(),Functions.<Path>constant(null));
}
