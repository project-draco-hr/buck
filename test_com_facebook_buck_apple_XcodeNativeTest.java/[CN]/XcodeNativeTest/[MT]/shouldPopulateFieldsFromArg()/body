{
  XcodeNativeDescription.Arg arg=new XcodeNativeDescription().createUnpopulatedConstructorArg();
  arg.projectContainerPath=new TestSourcePath("foo.xcodeproj");
  arg.targetName=Optional.absent();
  arg.buildableName=Optional.absent();
  XcodeNative xcodeNative=new XcodeNative(new FakeBuildRuleParamsBuilder(BuildTarget.builder("//test","test").build()).build(),arg);
  assertEquals(new TestSourcePath("foo.xcodeproj"),xcodeNative.getProjectContainerPath());
}
