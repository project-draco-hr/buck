{
  ImmutableList.Builder<SourcePath> cmoFiles=ImmutableList.builder();
  final HashMap<SourcePath,BuildRule> sourceToRule=new HashMap<>();
  for (  ImmutableMap.Entry<SourcePath,ImmutableList<SourcePath>> mlSource : mlSources.entrySet()) {
    generateSingleMLBytecodeCompilation(sourceToRule,cmoFiles,mlSource.getKey(),mlSources,ImmutableList.<SourcePath>of());
  }
  return cmoFiles.build();
}
