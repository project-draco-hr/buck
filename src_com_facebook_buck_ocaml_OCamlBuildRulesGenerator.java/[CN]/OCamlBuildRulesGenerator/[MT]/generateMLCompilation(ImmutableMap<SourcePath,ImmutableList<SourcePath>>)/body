{
  ImmutableList.Builder<SourcePath> cmxFiles=ImmutableList.builder();
  final HashMap<SourcePath,BuildRule> sourceToRule=new HashMap<>();
  for (  ImmutableMap.Entry<SourcePath,ImmutableList<SourcePath>> mlSource : mlSources.entrySet()) {
    generateSingleMLCompilation(sourceToRule,cmxFiles,mlSource.getKey(),mlSources,ImmutableList.<SourcePath>of());
  }
  return cmxFiles.build();
}
