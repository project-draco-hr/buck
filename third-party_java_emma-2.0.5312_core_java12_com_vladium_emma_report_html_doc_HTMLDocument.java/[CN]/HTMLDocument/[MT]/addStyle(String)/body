{
  if (css != null) {
    final IElement style=IElement.Factory.create(Tag.STYLE);
    style.getAttributes().set(Attribute.TYPE,"text/css");
    final StringBuffer def=new StringBuffer("<!--");
    def.append(IConstants.EOL);
    style.setText(css,false);
    def.append(IConstants.EOL);
    def.append("-->");
    m_head.add(style);
  }
}
