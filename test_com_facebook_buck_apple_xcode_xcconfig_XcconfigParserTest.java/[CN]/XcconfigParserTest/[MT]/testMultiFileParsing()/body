{
  ImmutableMap<String,String> files=new ImmutableMap.Builder<String,String>().put("file1.xcconfig",s2 + "\n" + s3).put("folder/file2.xcconfig",s1 + "\n" + "#include \"../file1.xcconfig\"").build();
  ProjectFilesystem fileSystem=new FakeReadonlyProjectFilesystem(files);
  assertFileHasValue(fileSystem,"folder/file2.xcconfig",values);
}
