{
  SingletonBuildTargetPattern singletonPattern1=new SingletonBuildTargetPattern("//src/com/facebook/buck:buck");
  SingletonBuildTargetPattern singletonPattern2=new SingletonBuildTargetPattern("//src/com/facebook/buck:buck");
  SingletonBuildTargetPattern singletonPattern3=new SingletonBuildTargetPattern("//src/com/facebook/buck/cli:cli");
  assertEquals(singletonPattern1.hashCode(),singletonPattern2.hashCode());
  assertNotSame(singletonPattern1.hashCode(),singletonPattern3.hashCode());
}
