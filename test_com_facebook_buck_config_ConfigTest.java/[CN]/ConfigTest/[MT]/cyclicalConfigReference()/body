{
  Config config=Configs.createConfig(RawConfig.builder().put("section","field1","$(config section.field2)").put("section","field2","$(config section.field3)").put("section","field3","$(config section.field1)").build());
  expectedException.expect(HumanReadableException.class);
  expectedException.expectMessage("section.field1 -> section.field2 -> section.field3 -> section.field1");
  config.get("section","field1");
}
