{
  BuckConfig config=BuckConfigTestUtils.createFromReader(new StringReader(Joiner.on('\n').join("[cache]","mode = http",String.format("http_url = http://127.0.0.1:%d/",port))),projectFilesystem,Architecture.detect(),Platform.detect(),ImmutableMap.<String,String>of());
  return new ArtifactCacheBuckConfig(config);
}
