{
  for (  Path artifact : getAllOutputs()) {
    buildableContext.recordArtifact(artifact);
  }
  return ImmutableList.of(new MkdirStep(getProjectFilesystem(),outputRelativePath.getParent()),new OCamlLinkStep(getProjectFilesystem().getRootPath(),cxxCompilerEnvironment,cxxCompiler,ocamlCompiler.getCommandPrefix(getResolver()),flags,getProjectFilesystem().resolve(outputRelativePath),depInput,nativeDepInput,FluentIterable.from(inputs).transform(getResolver().getAbsolutePathFunction()).toList(),isLibrary,isBytecode));
}
