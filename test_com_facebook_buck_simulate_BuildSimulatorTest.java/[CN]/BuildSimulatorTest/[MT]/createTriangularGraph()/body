{
  TargetNode<?> left=JavaLibraryBuilder.createBuilder(BuildTargetFactory.newInstance("//test/rule/left_leaf:target")).build();
  TargetNode<?> right=JavaLibraryBuilder.createBuilder(BuildTargetFactory.newInstance("//test/rule/right_leaf:target")).build();
  TargetNode<?> root=JavaLibraryBuilder.createBuilder(ROOT_NODE).addDep(left.getBuildTarget()).addDep(right.getBuildTarget()).build();
  return TargetGraphFactory.newInstance(root,left,right);
}
