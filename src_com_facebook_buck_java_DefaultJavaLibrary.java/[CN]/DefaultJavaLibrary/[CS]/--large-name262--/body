{
  this(params,resolver,srcs,resources,proguardConfig,postprocessClassesCommands,exportedDeps,providedDeps,abiJar,Suppliers.memoize(new Supplier<ImmutableSortedSet<SourcePath>>(){
    @Override public ImmutableSortedSet<SourcePath> get(){
      return JavaLibraryRules.getAbiInputs(params.getDeps());
    }
  }
),additionalClasspathEntries,javacOptions,resourcesRoot,mavenCoords);
}
