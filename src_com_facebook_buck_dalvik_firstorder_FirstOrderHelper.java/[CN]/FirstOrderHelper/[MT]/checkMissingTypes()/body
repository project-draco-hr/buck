{
  ImmutableSet.Builder<Type> builder=ImmutableSet.builder();
  for (  Type type : scenarioTypes) {
    if (!knownTypes.containsKey(type)) {
      builder.add(type);
    }
  }
  ImmutableSet<Type> unknownTypes=builder.build();
  if (!unknownTypes.isEmpty()) {
    throw new RuntimeException("WARNING: The specified .jar files did not include the following requested classes: " + Joiner.on(", ").join(FluentIterable.from(unknownTypes).transform(TYPE_TO_CLASS_NAME)));
  }
}
