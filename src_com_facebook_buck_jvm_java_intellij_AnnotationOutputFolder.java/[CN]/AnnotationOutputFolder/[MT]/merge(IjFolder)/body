{
  if (getPath().equals(otherFolder.getPath()) && getInputs().equals(otherFolder.getInputs())) {
    return this;
  }
  throw new IllegalArgumentException("Can not merge two AnnotationOutputFolder objects with different paths " + "(tried to merge " + getPath() + ","+ getInputs()+ " and "+ otherFolder.getPath()+ ","+ otherFolder.getInputs()+ " ).");
}
