{
  BuildRuleResolver ruleResolver=new BuildRuleResolver();
  Keystore keystore=addKeystoreRule(ruleResolver);
  AndroidBinary ruleInRootDirectory=(AndroidBinary)AndroidBinaryBuilder.createBuilder(BuildTargetFactory.newInstance("//:fb4a")).setManifest(new TestSourcePath("AndroidManifest.xml")).setKeystore(keystore.getBuildTarget()).setTarget("Google Inc.:Google APIs:16").build(ruleResolver);
  assertEquals(Paths.get(GEN_DIR + "/fb4a.apk"),ruleInRootDirectory.getApkPath());
  AndroidBinary ruleInNonRootDirectory=(AndroidBinary)AndroidBinaryBuilder.createBuilder(BuildTargetFactory.newInstance("//java/com/example:fb4a")).setManifest(new TestSourcePath("AndroidManifest.xml")).setKeystore(keystore.getBuildTarget()).setTarget("Google Inc.:Google APIs:16").build(ruleResolver);
  assertEquals(Paths.get(GEN_DIR + "/java/com/example/fb4a.apk"),ruleInNonRootDirectory.getApkPath());
}
