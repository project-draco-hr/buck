{
  ProjectFilesystem filesystem=new ProjectFilesystem(tmp.getRoot().toPath());
  Object daemon=Main.getDaemon(new TestRepositoryBuilder().setBuckConfig(new FakeBuckConfig(ImmutableMap.of("somesection",ImmutableMap.of("somename","somevalue")))).setFilesystem(filesystem).build(),ParserConfig.GlobHandler.PYTHON);
  assertEquals("Daemon should not be replaced when config equal.",daemon,Main.getDaemon(new TestRepositoryBuilder().setBuckConfig(new FakeBuckConfig(ImmutableMap.of("somesection",ImmutableMap.of("somename","somevalue")))).setFilesystem(filesystem).build(),ParserConfig.GlobHandler.PYTHON));
  assertNotEquals("Daemon should be replaced when config not equal.",daemon,Main.getDaemon(new TestRepositoryBuilder().setBuckConfig(new FakeBuckConfig(ImmutableMap.of("somesection",ImmutableMap.of("somename","someothervalue")))).setFilesystem(filesystem).build(),ParserConfig.GlobHandler.PYTHON));
}
