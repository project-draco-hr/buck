{
  ProjectFilesystem filesystem=new ProjectFilesystem(tmp.getRoot().toPath());
  Object daemon=Main.getDaemon(new TestCellBuilder().setBuckConfig(FakeBuckConfig.builder().setSections(ImmutableMap.of("somesection",ImmutableMap.of("somename","somevalue"))).build()).setFilesystem(filesystem).build(),new ObjectMapper());
  assertEquals("Daemon should not be replaced when config equal.",daemon,Main.getDaemon(new TestCellBuilder().setBuckConfig(FakeBuckConfig.builder().setSections(ImmutableMap.of("somesection",ImmutableMap.of("somename","somevalue"))).build()).setFilesystem(filesystem).build(),new ObjectMapper()));
  assertNotEquals("Daemon should be replaced when config not equal.",daemon,Main.getDaemon(new TestCellBuilder().setBuckConfig(FakeBuckConfig.builder().setSections(ImmutableMap.of("somesection",ImmutableMap.of("somename","someothervalue"))).build()).setFilesystem(filesystem).build(),new ObjectMapper()));
}
