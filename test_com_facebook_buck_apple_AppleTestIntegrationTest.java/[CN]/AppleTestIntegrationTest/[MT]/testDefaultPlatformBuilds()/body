{
  assumeTrue(Platform.detect() == Platform.MACOS);
  ProjectWorkspace workspace=TestDataHelper.createProjectWorkspaceForScenario(this,"apple_test_default_platform",tmp);
  workspace.setUp();
  BuildTarget target=workspace.newBuildTarget("//:foo");
  workspace.runBuckCommand("build",target.getFullyQualifiedName()).assertSuccess();
  workspace.verify(Paths.get("foo_output.expected"),BuildTargets.getGenPath(BuildTarget.builder(target).addFlavors(AppleTestDescription.BUNDLE_FLAVOR).build(),"%s"));
}
