{
  assumeTrue(Platform.detect() == Platform.MACOS);
  ProjectWorkspace workspace=TestDataHelper.createProjectWorkspaceForScenario(this,"apple_test_info_plist_substitution",tmp);
  workspace.setUp();
  BuildTarget target=workspace.newBuildTarget("//:foo#iphonesimulator-x86_64");
  workspace.runBuckCommand("build",target.getFullyQualifiedName()).assertSuccess();
  workspace.verify(Paths.get("foo_output.expected"),BuildTargets.getGenPath(BuildTarget.builder(target).addFlavors(AppleDebugFormat.DWARF_AND_DSYM.getFlavor()).addFlavors(AppleTestDescription.BUNDLE_FLAVOR).addFlavors(AppleDescriptions.NO_INCLUDE_FRAMEWORKS_FLAVOR).build(),"%s"));
}
