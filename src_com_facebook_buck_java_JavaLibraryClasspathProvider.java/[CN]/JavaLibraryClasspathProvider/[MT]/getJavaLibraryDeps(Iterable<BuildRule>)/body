{
  return FluentIterable.from(deps).transform(new Function<BuildRule,JavaLibrary>(){
    @Nullable @Override public JavaLibrary apply(    BuildRule input){
      if (input.getBuildable() instanceof JavaLibrary) {
        return (JavaLibrary)input.getBuildable();
      }
      if (input instanceof JavaLibrary) {
        return (JavaLibrary)input;
      }
      return null;
    }
  }
).filter(Predicates.notNull());
}
