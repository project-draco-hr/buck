{
  return FluentIterable.from(deps).transform(new Function<BuildRule,JavaLibraryRule>(){
    @Nullable @Override public JavaLibraryRule apply(    BuildRule input){
      if (input.getBuildable() instanceof JavaLibraryRule) {
        return (JavaLibraryRule)input.getBuildable();
      }
      if (input instanceof JavaLibraryRule) {
        return (JavaLibraryRule)input;
      }
      return null;
    }
  }
).filter(Predicates.notNull());
}
