{
  FakeProcess fakeSimctlInstallProcess=new FakeProcess(0);
  ProcessExecutorParams fakeSimctlInstallParams=ProcessExecutorParams.builder().setCommand(ImmutableList.of("xcrun","simctl","install","70200ED8-EEF1-4BDB-BCCF-3595B137D67D","Path/To/MyNeatApp.app")).build();
  FakeProcessExecutor fakeProcessExecutor=new FakeProcessExecutor(ImmutableMap.of(fakeSimctlInstallParams,fakeSimctlInstallProcess));
  boolean installed=AppleSimulatorRunning.installBundleInSimulator(fakeProcessExecutor,"70200ED8-EEF1-4BDB-BCCF-3595B137D67D",Paths.get("Path/To/MyNeatApp.app"));
  assertThat(installed,is(true));
}
