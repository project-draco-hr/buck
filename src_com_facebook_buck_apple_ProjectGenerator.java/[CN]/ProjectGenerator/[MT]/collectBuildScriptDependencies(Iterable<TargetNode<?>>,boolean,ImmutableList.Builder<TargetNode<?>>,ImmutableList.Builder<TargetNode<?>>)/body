{
  for (  TargetNode<?> targetNode : targetNodes) {
    BuildRuleType type=targetNode.getType();
    if (type.equals(XcodePostbuildScriptDescription.TYPE) || (type.equals(IosReactNativeLibraryDescription.TYPE) && !skipRNBundle)) {
      postRules.add(targetNode);
    }
 else     if (type.equals(XcodePrebuildScriptDescription.TYPE) || type.equals(GenruleDescription.TYPE)) {
      preRules.add(targetNode);
    }
  }
}
