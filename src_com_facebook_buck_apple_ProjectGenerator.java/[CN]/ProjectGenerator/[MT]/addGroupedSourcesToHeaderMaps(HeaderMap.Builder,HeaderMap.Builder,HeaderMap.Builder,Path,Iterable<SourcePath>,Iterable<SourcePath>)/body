{
  for (  SourcePath headerPath : publicHeaders) {
    addSourcePathToHeaderMaps(headerPath,prefix,publicHeaderMap,targetHeaderMap,targetUserHeaderMap,HeaderVisibility.PUBLIC);
  }
  for (  SourcePath headerPath : privateHeaders) {
    addSourcePathToHeaderMaps(headerPath,prefix,publicHeaderMap,targetHeaderMap,targetUserHeaderMap,HeaderVisibility.PROJECT);
  }
}
