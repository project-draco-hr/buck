{
  return FluentIterable.from(targetNodes).transformAndConcat(AppleBuildRules.newRecursiveRuleDependencyTransformer(targetGraph,AppleBuildRules.RecursiveDependenciesMode.LINKING,AppleBuildRules.XCODE_TARGET_BUILD_RULE_TYPES)).filter(new Predicate<TargetNode<?>>(){
    @Override public boolean apply(    TargetNode<?> input){
      return getLibraryNode(targetGraph,input).isPresent();
    }
  }
).transform(new Function<TargetNode<?>,PBXFileReference>(){
    @Override public PBXFileReference apply(    TargetNode<?> input){
      return getLibraryFileReference(input);
    }
  }
).toSet();
}
