{
switch (extension) {
case FRAMEWORK:
    return Optional.of(ProductType.FRAMEWORK);
case APPEX:
  return Optional.of(ProductType.APP_EXTENSION);
case BUNDLE:
return Optional.of(ProductType.BUNDLE);
case OCTEST:
return Optional.of(ProductType.BUNDLE);
case XCTEST:
return Optional.of(ProductType.UNIT_TEST);
default :
return Optional.absent();
}
}
