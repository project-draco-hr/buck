{
  return FluentIterable.from(targetNodes).transformAndConcat(newRecursiveRuleDependencyTransformer(AppleBuildRules.RecursiveDependenciesMode.LINKING,ImmutableSet.of(AppleLibraryDescription.TYPE))).append(includeInputs ? targetNodes : ImmutableList.<TargetNode<?>>of()).transform(new Function<TargetNode<?>,String>(){
    @Override public String apply(    TargetNode<?> input){
      return getTargetOutputPath(input);
    }
  }
).append(collectRecursiveSearchPathsForFrameworkPaths(targetNodes,FrameworkPath.FrameworkType.LIBRARY)).toSet();
}
