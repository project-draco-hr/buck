{
  FakeBuckConfig buckConfig=new FakeBuckConfig(ImmutableMap.of(VersionControlBuckConfig.VC_SECTION_KEY,ImmutableMap.of(VersionControlBuckConfig.HG_CMD_SETTING_KEY,HG_CMD)));
  VersionControlBuckConfig config=new VersionControlBuckConfig(buckConfig);
  assertThat(HG_CMD,is(equalTo(config.getHgCmd())));
}
