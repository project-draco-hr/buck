{
  FakeBuckConfig buckConfig=new FakeBuckConfig(ImmutableMap.of(VersionControlBuckConfig.VC_SECTION_KEY,ImmutableMap.of(VersionControlBuckConfig.GENERATE_STATISTICS_KEY,GENERATE_STATISTICS)));
  VersionControlBuckConfig config=new VersionControlBuckConfig(buckConfig);
  assertThat(GENERATE_STATISTICS_RESULT,is(equalTo(config.shouldGenerateStatistics())));
}
