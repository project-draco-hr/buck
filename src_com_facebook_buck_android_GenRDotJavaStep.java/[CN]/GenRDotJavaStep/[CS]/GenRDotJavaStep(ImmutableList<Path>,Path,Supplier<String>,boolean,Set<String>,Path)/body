{
  super(ImmutableList.<Step>of(new MkdirStep(dummyAndroidManifest.getParent()),new WriteFileStep(new Supplier<String>(){
    @Override public String get(){
      return String.format("<manifest xmlns:android='http://schemas.android.com/apk/res/android' " + "package='%s' />",libraryPackage.get());
    }
  }
,dummyAndroidManifest),new GenRDotJavaStepInternal(resDirectories,genDirectoryPath,isTempRDotJava,extraLibraryPackages,dummyAndroidManifest)));
