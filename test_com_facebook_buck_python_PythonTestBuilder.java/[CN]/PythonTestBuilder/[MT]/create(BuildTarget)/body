{
  PythonBuckConfig pythonBuckConfig=new PythonBuckConfig(new FakeBuckConfig(),new ExecutableFinder());
  return new PythonTestBuilder(target,new FakeProjectFilesystem(),pythonBuckConfig.getPathToPex(),pythonBuckConfig.getPathToPexExecuter(),pythonBuckConfig.getPexExtension(),pythonBuckConfig.getPathToTestMain(),new PythonEnvironment(Paths.get("python"),PythonVersion.of("2.7")),CxxPlatformUtils.DEFAULT_PLATFORM,new FlavorDomain<>("C/C++ Platform",ImmutableMap.of(CxxPlatformUtils.DEFAULT_PLATFORM.getFlavor(),CxxPlatformUtils.DEFAULT_PLATFORM)));
}
