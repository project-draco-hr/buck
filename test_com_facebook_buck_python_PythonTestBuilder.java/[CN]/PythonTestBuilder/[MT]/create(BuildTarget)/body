{
  PythonBuckConfig pythonBuckConfig=new PythonBuckConfig(new FakeBuckConfig(),new ExecutableFinder());
  return new PythonTestBuilder(target,pythonBuckConfig,PythonTestUtils.PYTHON_PLATFORMS,CxxPlatformUtils.DEFAULT_PLATFORM,new FlavorDomain<>("C/C++ Platform",ImmutableMap.of(CxxPlatformUtils.DEFAULT_PLATFORM.getFlavor(),CxxPlatformUtils.DEFAULT_PLATFORM)));
}
