{
  String outputName=Flavor.replaceInvalidCharacters(getPreprocessOutputName(type,name));
  return BuildTarget.builder(params.getBuildTarget()).addFlavors(cxxPlatform.getFlavor()).addFlavors(ImmutableFlavor.of(String.format(PREPROCESS_FLAVOR_PREFIX + "%s%s",picType == PicType.PIC ? "pic-" : "",outputName))).build();
}
