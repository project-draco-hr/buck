{
  return ImmutableList.<BuildRule>builder().addAll(pathResolver.filterBuildRuleInputs(ImmutableList.<SourcePath>builder().addAll(cxxPreprocessorInput.getIncludes().getPrefixHeaders()).addAll(cxxPreprocessorInput.getIncludes().getNameToPathMap().values()).build())).addAll(BuildRules.toBuildRulesFor(params.getBuildTarget(),resolver,cxxPreprocessorInput.getRules(),false)).build();
}
