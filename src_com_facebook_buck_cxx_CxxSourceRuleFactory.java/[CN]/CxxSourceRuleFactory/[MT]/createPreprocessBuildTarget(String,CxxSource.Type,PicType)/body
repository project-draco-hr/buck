{
  String outputName=Flavor.replaceInvalidCharacters(getPreprocessOutputName(type,name));
  return BuildTarget.builder(params.getBuildTarget()).addFlavors(cxxPlatform.getFlavor()).addFlavors(ImmutableFlavor.of(String.format("preprocess-%s%s",pic == PicType.PIC ? "pic-" : "",outputName))).build();
}
