{
  String outputName=Flavor.replaceInvalidCharacters(getCompileOutputName(name));
  return BuildTarget.builder(params.getBuildTarget()).addFlavors(cxxPlatform.getFlavor()).addFlavors(ImmutableFlavor.of(String.format(COMPILE_FLAVOR_PREFIX + "%s%s",pic == PicType.PIC ? "pic-" : "",outputName))).build();
}
