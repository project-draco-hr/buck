{
  return builder.setReflectively("platformPreprocessorFlags",sanitizer.sanitizeFlags(preprocessorFlags.getPlatformFlags())).setReflectively("rulePreprocessorFlags",sanitizer.sanitizeFlags(preprocessorFlags.getRuleFlags())).setReflectively("platformCompilerFlags",sanitizer.sanitizeFlags(compilerFlags.getPlatformFlags())).setReflectively("ruleCompilerFlags",sanitizer.sanitizeFlags(compilerFlags.getRuleFlags()));
}
