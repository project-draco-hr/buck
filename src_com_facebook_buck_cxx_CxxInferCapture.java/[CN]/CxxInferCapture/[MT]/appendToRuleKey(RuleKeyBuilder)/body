{
  return builder.setReflectively("platformPreprocessorFlags",sanitizer.sanitizeFlags(platformPreprocessorFlags)).setReflectively("rulePreprocessorFlags",sanitizer.sanitizeFlags(rulePreprocessorFlags)).setReflectively("platformCompilerFlags",sanitizer.sanitizeFlags(platformCompilerFlags)).setReflectively("ruleCompilerFlags",sanitizer.sanitizeFlags(ruleCompilerFlags));
}
