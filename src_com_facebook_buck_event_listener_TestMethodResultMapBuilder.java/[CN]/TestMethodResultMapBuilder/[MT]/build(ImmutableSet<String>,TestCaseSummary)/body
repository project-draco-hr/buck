{
  ImmutableList.Builder<TestRun.Result> builder=new ImmutableList.Builder<>();
  for (  TestResultSummary testResultSummary : testCaseSummary.getTestResults()) {
    TestRun.Status status=TestRun.Status.convertResultType(testResultSummary.getType());
    String name=String.format("%s#%s",testCaseSummary.getTestCaseName(),testResultSummary.getTestName());
    builder.add(new TestRun.Result(name,contacts,testResultSummary.getTime() / 1000,status,TimeUnit.MILLISECONDS.toSeconds(System.currentTimeMillis()),status.toString().toUpperCase(),buildDetails(testResultSummary)));
  }
  return builder.build();
}
