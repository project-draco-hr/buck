{
  ImmutableMap<BuildTarget,ProjectGenerator> projectGeneratorMap=generator.getProjectGenerators();
  assertNotNull("should have called SeparatedProjectsGenerator.generateProjects()",projectGeneratorMap);
  ProjectGenerator innerGenerator=projectGeneratorMap.get(target);
  assertNotNull("should have generated project from config rule: " + target,innerGenerator);
  return innerGenerator.getGeneratedProject();
}
