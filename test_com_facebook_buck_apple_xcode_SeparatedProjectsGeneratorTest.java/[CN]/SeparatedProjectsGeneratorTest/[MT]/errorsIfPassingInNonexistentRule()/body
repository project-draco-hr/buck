{
  BuildTarget target=BuildTarget.builder("//foo","thing").build();
  expectedException.expect(HumanReadableException.class);
  expectedException.expectMessage("target not found");
  SeparatedProjectsGenerator generator=new SeparatedProjectsGenerator(projectFilesystem,TargetGraphFactory.newInstance(ImmutableSet.<TargetNode<?>>of()),executionContext,ImmutableSet.of(target),ImmutableSet.<ProjectGenerator.Option>of());
  generator.generateProjects();
}
