{
  buildableContext.recordArtifact(output);
  return ImmutableList.of(new MkdirStep(getProjectFilesystem(),output.getParent()),new RmStep(getProjectFilesystem(),output,true),new ArchiveStep(getProjectFilesystem().getRootPath(),archiver.getCommandPrefix(getResolver()),output,getResolver().getAllPaths(inputs)),new FileScrubberStep(getProjectFilesystem(),output,archiver.getScrubbers()));
}
