{
  buildableContext.recordArtifact(output);
  return ImmutableList.of(new MkdirStep(output.getParent()),new RmStep(output,true),new ArchiveStep(getProjectFilesystem().getRootPath(),archiver.getCommandPrefix(getResolver()),output,getResolver().getAllPaths(inputs)),new FileScrubberStep(output,archiver.getScrubbers()));
}
