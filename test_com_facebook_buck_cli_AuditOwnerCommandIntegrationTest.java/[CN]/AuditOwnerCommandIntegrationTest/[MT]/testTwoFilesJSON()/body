{
  String expectedJson=Platform.detect() == Platform.WINDOWS ? "stdout-one-two-windows.json" : "stdout-one-two.json";
  ProjectWorkspace workspace=TestDataHelper.createProjectWorkspaceForScenario(this,"audit_owner",tmp);
  workspace.setUp();
  ProjectWorkspace.ProcessResult result=workspace.runBuckCommand("audit","owner","--json","example/1.txt","example/lib/2.txt");
  result.assertSuccess();
  assertEquals(workspace.getFileContents(expectedJson),result.getStdout());
}
