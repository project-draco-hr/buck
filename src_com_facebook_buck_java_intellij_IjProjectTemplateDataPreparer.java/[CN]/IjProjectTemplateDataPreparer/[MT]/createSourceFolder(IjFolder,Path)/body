{
  String packagePrefix=null;
  if (folder.getWantsPackagePrefix()) {
    packagePrefix=javaPackageFinder.findJavaPackage(folder.getPath().resolve("notfound"));
  }
  return IjSourceFolder.builder().setType(folder.getType().getIjName()).setUrl(toModuleDirRelativeString(folder.getPath(),moduleLocationBasePath)).setIsTestSource(folder.isTest()).setPackagePrefix(packagePrefix).build();
}
