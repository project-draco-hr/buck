{
  ExportFileRule rule=new ExportFileRule(params,Optional.<String>absent(),Optional.of("fish"));
  List<Step> steps=rule.buildInternal(context);
  MkdirAndSymlinkFileStep expected=new MkdirAndSymlinkFileStep(new File(root,"example.html"),new File(BuckConstant.GEN_DIR,"fish"));
  assertEquals(ImmutableList.of(expected),steps);
}
