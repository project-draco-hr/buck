{
  BuildTarget target=getBuildTarget();
  if (!target.getFlavor().equals(Flavor.DEFAULT)) {
    return Paths.get(String.format("%s/%s/%s/" + getOutputFileNameFormat(),BuckConstant.BIN_DIR,target.getBasePathWithSlash(),target.getFlavorPostfix(),target.getShortNameOnly()));
  }
 else {
    return BuildTargets.getBinPath(target,getOutputFileNameFormat());
  }
}
