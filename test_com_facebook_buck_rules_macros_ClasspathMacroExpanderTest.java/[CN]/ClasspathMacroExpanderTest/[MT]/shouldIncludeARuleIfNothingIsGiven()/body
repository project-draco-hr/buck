{
  final BuildRuleResolver buildRuleResolver=new BuildRuleResolver(TargetGraph.EMPTY,new BuildTargetNodeToBuildRuleTransformer());
  BuildRule rule=JavaLibraryBuilder.createBuilder(BuildTargetFactory.newInstance("//cheese:cake")).addSrc(Paths.get("Example.java")).build(buildRuleResolver);
  assertExpandsTo(rule,buildRuleResolver,ROOT + File.separator + rule.getPathToOutput());
}
