{
  this.executor=new ThreadPoolExecutor(1,1,0L,TimeUnit.MILLISECONDS,new LinkedBlockingQueue<Runnable>(),new ThreadPoolExecutor.DiscardPolicy());
  this.eventBus=new AsyncEventBus("buck-build-events",executor);
  this.clock=Preconditions.checkNotNull(clock);
  this.threadIdSupplier=Preconditions.checkNotNull(threadIdSupplier);
}
