{
  EventBus eventBus=new EventBus("buck-build-events");
  eventBus.register(object);
  executorsAndBuses.add(new Pair<ExecutorService,EventBus>(async ? MostExecutors.newSingleThreadExecutor(new CommandThreadFactory(BuckEventBus.class.getSimpleName())) : MoreExecutors.newDirectExecutorService(),eventBus));
}
