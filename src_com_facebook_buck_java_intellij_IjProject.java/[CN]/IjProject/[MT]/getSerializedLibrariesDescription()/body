{
  ImmutableSet.Builder<SerializableAndroidAar> serializableLibrariesBuilder=new ImmutableSet.Builder<>();
  for (  IjModule module : moduleGraph.getNodes()) {
    for (    IjLibrary library : module.getLibraries()) {
      Optional<Path> jarPath=resolver.getRelativePath(library.getBinaryJar());
      if (!jarPath.isPresent()) {
        continue;
      }
      SerializableAndroidAar serializableAndroidLibrary=new SerializableAndroidAar(library.getName(),null,null,jarPath.get());
      serializableLibrariesBuilder.add(serializableAndroidLibrary);
    }
  }
  return serializableLibrariesBuilder.build().asList();
}
