{
  ImmutableSet.Builder<SerializableAndroidAar> serializableLibrariesBuilder=new ImmutableSet.Builder<>();
  for (  IjModule module : moduleGraph.getModuleNodes()) {
    for (    IjLibrary library : moduleGraph.getDependentLibrariesFor(module).keySet()) {
      SerializableAndroidAar serializableAndroidLibrary=new SerializableAndroidAar(library.getName(),null,null,library.getBinaryJar());
      serializableLibrariesBuilder.add(serializableAndroidLibrary);
    }
  }
  return serializableLibrariesBuilder.build().asList();
}
