{
  ExecutionContext context=TestExecutionContext.newInstance();
  Jsr199Compiler firstOrder=createTestStep();
  Jsr199Compiler warn=createTestStep();
  Jsr199Compiler transitive=createTestStep();
  assertEquals(String.format("javac -source %s -target %s -g -d . -classpath foo.jar @%s",TARGETED_JAVA_VERSION,TARGETED_JAVA_VERSION,PATH_TO_SRCS_LIST),firstOrder.getDescription(context,getArgs().add("foo.jar").build()));
  assertEquals(String.format("javac -source %s -target %s -g -d . -classpath foo.jar @%s",TARGETED_JAVA_VERSION,TARGETED_JAVA_VERSION,PATH_TO_SRCS_LIST),warn.getDescription(context,getArgs().add("foo.jar").build()));
  assertEquals(String.format("javac -source %s -target %s -g -d . -classpath bar.jar%sfoo.jar @%s",TARGETED_JAVA_VERSION,TARGETED_JAVA_VERSION,File.pathSeparator,PATH_TO_SRCS_LIST),transitive.getDescription(context,getArgs().add("bar.jar" + File.pathSeparator + "foo.jar").build()));
}
