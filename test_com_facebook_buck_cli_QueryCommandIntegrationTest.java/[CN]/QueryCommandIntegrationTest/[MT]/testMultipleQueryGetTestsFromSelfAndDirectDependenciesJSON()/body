{
  ProjectWorkspace workspace=TestDataHelper.createProjectWorkspaceForScenario(this,"query_command",tmp);
  workspace.setUp();
  ProjectWorkspace.ProcessResult result=workspace.runBuckCommand("query","--json","testsof(deps(%s, 1))","//example:two");
  result.assertSuccess();
  assertEquals(workspace.getFileContents("stdout-two-deps-tests-map.json"),result.getStdout());
}
