{
  ProjectWorkspace workspace=TestDataHelper.createProjectWorkspaceForScenario(this,"query_command",tmp);
  workspace.setUp();
  ProjectWorkspace.ProcessResult result=workspace.runBuckCommand("query","--json","owner('%s')","example/1.txt","example/app/7.txt");
  result.assertSuccess();
  assertThat(parseJSON(result.getStdout()),is(equalTo(parseJSON(workspace.getFileContents("stdout-one-seven-owner.json")))));
}
