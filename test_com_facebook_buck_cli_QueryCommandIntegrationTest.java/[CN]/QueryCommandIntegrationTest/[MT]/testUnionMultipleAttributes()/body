{
  ProjectWorkspace workspace=TestDataHelper.createProjectWorkspaceForScenario(this,"query_command",tmp);
  workspace.setUp();
  ProjectWorkspace.ProcessResult result=workspace.runBuckCommand("query","labels('tests', '//example:four') + labels('srcs', '//example:five') - '//example:six'");
  result.assertSuccess();
  assertThat(result.getStdout(),is(equalTo(workspace.getFileContents("stdout-one-five-except-six-sources-tests"))));
}
