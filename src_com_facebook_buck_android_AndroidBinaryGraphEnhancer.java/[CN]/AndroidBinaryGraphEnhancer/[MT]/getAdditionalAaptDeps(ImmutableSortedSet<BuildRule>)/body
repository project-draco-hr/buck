{
  ImmutableSortedSet.Builder<BuildRule> builder=ImmutableSortedSet.<BuildRule>naturalOrder().addAll(resourceRules).addAll(getTargetsAsRules(androidResourceDepsFinder.getAndroidTransitiveDependencies().nativeTargetsWithAssets));
  if (manifest instanceof BuildTargetSourcePath) {
    builder.add(ruleResolver.get(((BuildTargetSourcePath)manifest).getTarget()));
  }
  return builder.build();
}
