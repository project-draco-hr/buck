{
  ImmutableSortedSet.Builder<BuildRule> builder=ImmutableSortedSet.<BuildRule>naturalOrder().addAll(resourceRules).addAll(getTargetsAsRules(FluentIterable.from(packageableCollection.resourceDetails.resourcesWithEmptyResButNonEmptyAssetsDir).transform(HasBuildTarget.TO_TARGET).toList()));
  if (manifest instanceof BuildRuleSourcePath) {
    builder.add(((BuildRuleSourcePath)manifest).getRule());
  }
  return builder.build();
}
