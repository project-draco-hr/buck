{
  return FluentIterable.from(getTargetsAsRules(targets)).transform(BuildRules.TO_BUILDABLE).transform(new Function<Buildable,HasAndroidResourceDeps>(){
    @Override public HasAndroidResourceDeps apply(    Buildable input){
      Preconditions.checkState(input instanceof HasAndroidResourceDeps);
      return (HasAndroidResourceDeps)input;
    }
  }
).toList();
}
