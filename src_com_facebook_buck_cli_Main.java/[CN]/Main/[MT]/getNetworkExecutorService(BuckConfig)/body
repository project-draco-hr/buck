{
  return listeningDecorator(new ThreadPoolExecutor(buckConfig.getNumThreadsForNetwork(),buckConfig.getNumThreadsForNetwork(),1L,TimeUnit.SECONDS,new LinkedBlockingQueue<Runnable>(buckConfig.getNumThreadsForNetwork()),new ThreadFactoryBuilder().setNameFormat("Network I/O" + "-%d").build(),new ThreadPoolExecutor.CallerRunsPolicy()));
}
