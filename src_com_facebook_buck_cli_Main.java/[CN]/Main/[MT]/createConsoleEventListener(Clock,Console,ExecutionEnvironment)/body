{
  if (console.getAnsi().isAnsiTerminal()) {
    SuperConsoleEventBusListener superConsole=new SuperConsoleEventBusListener(console,clock,executionEnvironment);
    superConsole.startRenderScheduler(SUPER_CONSOLE_REFRESH_RATE.getDuration(),SUPER_CONSOLE_REFRESH_RATE.getUnit());
    return superConsole;
  }
  return new SimpleConsoleEventBusListener(console,clock);
}
