{
  SourcePath undefinedSymbolsFile=cxxPlatform.getSymbolNameTool().createUndefinedSymbolsFile(params,ruleResolver,pathResolver,params.getBuildTarget().withAppendedFlavor(ImmutableFlavor.of("omnibus-undefined-symbols-file")),linkerInputs);
  return cxxPlatform.getLd().resolve(ruleResolver).createUndefinedSymbolsLinkerArgs(params,ruleResolver,pathResolver,params.getBuildTarget().withAppendedFlavor(ImmutableFlavor.of("omnibus-undefined-symbols-args")),ImmutableList.of(undefinedSymbolsFile));
}
