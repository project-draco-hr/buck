{
  SourcePath undefinedSymbolsFile=cxxPlatform.getSymbolNameTool().createUndefinedSymbolsFile(params,ruleResolver,pathResolver,BuildTarget.builder(params.getBuildTarget()).addFlavors(ImmutableFlavor.of("omnibus-undefined-symbols-file")).build(),linkerInputs);
  return cxxPlatform.getLd().resolve(ruleResolver).createUndefinedSymbolsLinkerArgs(params,ruleResolver,pathResolver,BuildTarget.builder(params.getBuildTarget()).addFlavors(ImmutableFlavor.of("omnibus-undefined-symbols-args")).build(),ImmutableList.of(undefinedSymbolsFile));
}
