{
  try (Fixture fixture=createSynchronousExecutionFixture("syntax_error")){
    final Cell cell=fixture.getCell();
    try {
      fixture.getParsePipeline().getTargetNode(cell,BuildTargetFactory.newInstance(cell.getFilesystem(),"//error:error"));
      Assert.fail("Expected BuildFileParseException");
    }
 catch (    BuildFileParseException e) {
      assertThat(e.getMessage(),Matchers.containsString("crash!"));
    }
    fixture.getParsePipeline().getTargetNode(cell,BuildTargetFactory.newInstance(cell.getFilesystem(),"//correct:correct"));
  }
 }
