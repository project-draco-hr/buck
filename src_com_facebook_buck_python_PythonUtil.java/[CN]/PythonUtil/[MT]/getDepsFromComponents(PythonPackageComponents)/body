{
  return ImmutableSortedSet.<BuildRule>naturalOrder().addAll(SourcePaths.filterBuildRuleInputs(components.getModules().values())).addAll(SourcePaths.filterBuildRuleInputs(components.getResources().values())).addAll(SourcePaths.filterBuildRuleInputs(components.getNativeLibraries().values())).build();
}
