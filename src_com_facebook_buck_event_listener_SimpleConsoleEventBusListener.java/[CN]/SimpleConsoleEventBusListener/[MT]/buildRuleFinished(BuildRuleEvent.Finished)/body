{
  super.buildRuleFinished(finished);
  if (finished.getStatus() != BuildRuleStatus.SUCCESS || console.getVerbosity().isSilent()) {
    return;
  }
  String line=String.format(locale,"%s %s",finished.getResultString(),finished.getBuildRule().getFullyQualifiedName());
  if (ruleCount.isPresent()) {
    line+=String.format(locale," (%d/%d JOBS)",numRulesCompleted.get(),ruleCount.get());
  }
  console.getStdErr().println(line);
}
