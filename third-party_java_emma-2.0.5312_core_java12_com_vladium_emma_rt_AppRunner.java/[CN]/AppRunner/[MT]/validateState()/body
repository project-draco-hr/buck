{
  super.validateState();
  if ((m_appClassName == null) || (m_appClassName.length() == 0))   throw new IllegalStateException("application class name not set");
  if (m_appArgs == null)   throw new IllegalStateException("application arguments not set");
  if (m_coveragePath == null)   throw new IllegalStateException("coverage path not set");
  if ((m_reportGenerators == null) || (m_reportGenerators.length == 0))   throw new IllegalStateException("report types not set");
}
