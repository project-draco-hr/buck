{
  return createBuildRuleWithDefaults(new BuildTarget(baseName,"project"),ImmutableSortedSet.<BuildRule>of(),xcodeProjectConfigDescription,new Function<XcodeProjectConfigDescription.Arg,XcodeProjectConfigDescription.Arg>(){
    @Override public XcodeProjectConfigDescription.Arg apply(    XcodeProjectConfigDescription.Arg input){
      input.projectName=projectName;
      input.rules=buildRules;
      return input;
    }
  }
);
}
