{
  ImmutableList.Builder<String> args=ImmutableList.builder();
  AndroidPlatformTarget androidPlatformTarget=context.getAndroidPlatformTarget();
  args.add(androidPlatformTarget.getZipalignExecutable().getAbsolutePath());
  args.add("-f").add("4");
  args.add(inputFile);
  args.add(outputFile);
  return args.build();
}
