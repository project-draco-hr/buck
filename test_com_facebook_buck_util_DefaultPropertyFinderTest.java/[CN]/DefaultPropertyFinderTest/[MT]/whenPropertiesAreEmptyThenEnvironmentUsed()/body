{
  DefaultPropertyFinder propertyFinder=new DefaultPropertyFinder(new ProjectFilesystem(Paths.get(".")),ImmutableMap.of("SOME_ENVIRONMENT_VARIABLE","."));
  Optional<Path> path=propertyFinder.findDirectoryByPropertiesThenEnvironmentVariable("NOT_A_PROPERTY_NAME","SOME_ENVIRONMENT_VARIABLE");
  assertTrue(path.isPresent());
  assertEquals(path.get(),Paths.get("."));
}
