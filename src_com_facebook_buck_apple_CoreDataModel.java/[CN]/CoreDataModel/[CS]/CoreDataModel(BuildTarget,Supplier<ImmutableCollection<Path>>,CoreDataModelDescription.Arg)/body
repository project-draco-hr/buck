{
  super(target);
  this.extension=Files.getFileExtension(args.path.getFileName().toString());
  Preconditions.checkArgument(CORE_DATA_MODEL_EXTENSION.equals(extension) || VERSIONED_CORE_DATA_MODEL_EXTENSION.equals(extension));
  this.inputPathsSupplier=Preconditions.checkNotNull(inputPathsSupplier);
  this.path=args.path;
}
