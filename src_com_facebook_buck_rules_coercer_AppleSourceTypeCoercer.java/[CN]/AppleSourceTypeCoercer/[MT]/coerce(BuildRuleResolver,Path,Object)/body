{
  if (object instanceof AppleSource) {
    return (AppleSource)object;
  }
 else {
    try {
      return AppleSource.ofSourcePath(sourcePathTypeCoercer.coerce(buildRuleResolver,pathRelativeToProjectRoot,object));
    }
 catch (    CoerceFailedException e) {
    }
    try {
      return AppleSource.ofSourcePathWithFlags(sourcePathWithFlagsTypeCoercer.coerce(buildRuleResolver,pathRelativeToProjectRoot,object));
    }
 catch (    CoerceFailedException e) {
    }
    try {
      return AppleSource.ofSourceGroup(sourceGroupTypeCoercer.coerce(buildRuleResolver,pathRelativeToProjectRoot,object));
    }
 catch (    CoerceFailedException e) {
    }
  }
  throw CoerceFailedException.simple(pathRelativeToProjectRoot,object,getOutputClass());
}
