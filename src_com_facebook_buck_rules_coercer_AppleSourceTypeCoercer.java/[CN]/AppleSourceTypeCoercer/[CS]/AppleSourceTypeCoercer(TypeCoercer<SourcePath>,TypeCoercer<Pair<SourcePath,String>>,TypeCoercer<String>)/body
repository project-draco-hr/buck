{
  this.sourcePathTypeCoercer=Preconditions.checkNotNull(sourcePathTypeCoercer);
  this.sourcePathWithFlagsTypeCoercer=Preconditions.checkNotNull(sourcePathWithFlagsTypeCoercer);
  this.sourceGroupTypeCoercer=new PairTypeCoercer<String,ImmutableList<AppleSource>>(Preconditions.checkNotNull(stringTypeCoercer),new ListTypeCoercer<AppleSource>(this));
