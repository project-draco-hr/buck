{
  ImmutableMap.Builder<BuildTarget,CxxPreprocessorInput> builder=ImmutableMap.builder();
  builder.put(getBuildTarget(),getCxxPreprocessorInput(targetGraph,cxxPlatform,headerVisibility));
  for (  BuildRule dep : getDeps()) {
    if (dep instanceof CxxPreprocessorDep) {
      builder.putAll(((CxxPreprocessorDep)dep).getTransitiveCxxPreprocessorInput(targetGraph,cxxPlatform,headerVisibility));
    }
  }
  return builder.build();
}
