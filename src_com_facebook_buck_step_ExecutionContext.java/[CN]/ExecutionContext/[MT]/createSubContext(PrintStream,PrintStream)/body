{
  return new ExecutionContext(getProjectFilesystem(),new Console(console.getVerbosity(),newStdout,newStderr,console.getAnsi()),getAndroidPlatformTargetOptional(),getNdkRoot(),getTargetDeviceOptional(),getDefaultTestTimeoutMillis(),isCodeCoverageEnabled(),isJacocoEnabled(),isDebugEnabled,eventBus,platform);
}
