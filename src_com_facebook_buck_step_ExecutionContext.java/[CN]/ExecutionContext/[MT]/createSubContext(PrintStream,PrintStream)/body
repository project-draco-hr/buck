{
  return new ExecutionContext(getProjectFilesystem(),new Console(console.getVerbosity(),newStdout,newStderr,console.getAnsi()),getAndroidPlatformTargetOptional(),getTargetDeviceOptional(),getDefaultTestTimeoutMillis(),isCodeCoverageEnabled(),isDebugEnabled,eventBus,platform,this.environment,this.javaPackageFinder);
}
