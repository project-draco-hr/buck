{
  File androidSdkDir=findAndroidSdkDir();
  Build build=new Build(graph,androidSdkDir,findAndroidNdkDir(),projectDirectoryRoot,getVerbosity(),getListeningExecutorService(),getBuckConfig().createDefaultJavaPackageFinder(),console.getAnsi(),console.getStdOut(),console.getStdErr(),isCodeCoverageEnabled(),isDebugEnabled());
  return build;
}
