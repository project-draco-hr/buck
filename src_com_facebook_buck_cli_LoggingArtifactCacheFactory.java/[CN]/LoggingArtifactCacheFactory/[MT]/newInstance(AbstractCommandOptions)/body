{
  if (options.isNoCache()) {
    return new NoopArtifactCache();
  }
 else {
    buckEventBus.post(ArtifactCacheConnectEvent.started());
    ArtifactCache artifactCache=new LoggingArtifactCacheDecorator(buckEventBus).decorate(options.getBuckConfig().createArtifactCache(executionEnvironment.getWifiSsid(),buckEventBus));
    buckEventBus.post(ArtifactCacheConnectEvent.finished());
    createdArtifactCaches.add(artifactCache);
    return artifactCache;
  }
}
