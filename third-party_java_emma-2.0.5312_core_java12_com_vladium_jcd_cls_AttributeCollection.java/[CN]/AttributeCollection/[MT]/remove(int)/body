{
  final Attribute_info result=(Attribute_info)m_attributes.remove(offset);
  if (result instanceof SyntheticAttribute_info)   --m_syntheticRefCount;
 else   if (result instanceof InnerClassesAttribute_info)   m_innerClassesAttributeOffset=-1;
 else   if (result instanceof BridgeAttribute_info)   --m_bridgeRefCount;
  if (DISALLOW_MULTIPLE_SYNTHETIC_ATTRIBUTES && $assert.ENABLED)   $assert.ASSERT(m_syntheticRefCount >= 0 && m_syntheticRefCount <= 1,"bad synthetic attribute count: " + m_syntheticRefCount);
  return result;
}
