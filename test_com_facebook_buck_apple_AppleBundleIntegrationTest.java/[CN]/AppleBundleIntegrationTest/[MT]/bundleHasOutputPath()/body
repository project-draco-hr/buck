{
  assumeTrue(Platform.detect() == Platform.MACOS);
  ProjectWorkspace workspace=TestDataHelper.createProjectWorkspaceForScenario(this,"simple_application_bundle",tmp);
  workspace.setUp();
  ProjectWorkspace.ProcessResult result=workspace.runBuckCommand("targets","--show-output","//:DemoApp");
  result.assertSuccess();
  assertEquals("//:DemoApp buck-out/gen/DemoApp.zip",result.getStdout().trim());
}
