{
  assumeTrue(Platform.detect() == Platform.MACOS);
  ProjectWorkspace workspace=TestDataHelper.createProjectWorkspaceForScenario(this,"simple_application_bundle_no_debug",tmp);
  workspace.setUp();
  ProjectWorkspace.ProcessResult result=workspace.runBuckCommand("targets","--show-output","//:DemoApp#no-debug");
  result.assertSuccess();
  assertEquals("//:DemoApp#no-debug buck-out/gen/DemoApp#no-debug/DemoApp.app",result.getStdout().trim());
}
