{
  assumeTrue(Platform.detect() == Platform.MACOS);
  assumeTrue(FakeAppleDeveloperEnvironment.supportsCodeSigning());
  ProjectWorkspace workspace=TestDataHelper.createProjectWorkspaceForScenario(this,"simple_application_bundle_with_codesigning",tmp);
  workspace.setUp();
  workspace.runBuckCommand("build","//:DemoApp#iphoneos-arm64").assertSuccess();
  workspace.verify();
  assertTrue(Files.exists(tmp.getRootPath().resolve(BuckConstant.GEN_DIR).resolve("DemoApp#iphoneos-arm64/DemoApp.app/DemoApp")));
  assertTrue(checkCodeSigning("DemoApp#iphoneos-arm64/DemoApp.app"));
}
