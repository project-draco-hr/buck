{
  CommandTool.Builder builder=new CommandTool.Builder().addArg("java");
  if (javaBinOverride.isPresent()) {
    builder.addArg(String.format("-D%s=%s",FatJarMain.BUCK_JAVA_BIN_KEY,javaBinOverride.get()));
  }
  return builder.addArg("-jar").addArg(new BuildTargetSourcePath(getBuildTarget())).build();
}
