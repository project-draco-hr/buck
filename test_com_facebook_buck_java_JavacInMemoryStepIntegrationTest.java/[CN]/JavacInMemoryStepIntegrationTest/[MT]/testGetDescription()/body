{
  JavacInMemoryStep javac=createJavac(false);
  ExecutionContext executionContext=createExecutionContext();
  String pathToOutputDir=new File(tmp.getRoot(),"out").getAbsolutePath();
  assertEquals(String.format("javac -source %s -target %s -g " + "-d %s " + "-classpath '' "+ "@"+ pathToSrcsList.toString(),JavaCompilerEnvironment.TARGETED_JAVA_VERSION,JavaCompilerEnvironment.TARGETED_JAVA_VERSION,pathToOutputDir),javac.getDescription(executionContext));
}
