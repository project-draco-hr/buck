{
  String fileName=path.getFileName().toString();
  return ".svn".equalsIgnoreCase(fileName) || ".git".equalsIgnoreCase(fileName) || ".ds_store".equalsIgnoreCase(fileName)|| MoreStrings.endsWithIgnoreCase(fileName,".scc")|| "cvs".equalsIgnoreCase(fileName)|| "thumbs.db".equalsIgnoreCase(fileName)|| "picasa.ini".equalsIgnoreCase(fileName)|| fileName.endsWith("~");
}
