{
  if (object instanceof List) {
    return SourceWithFlagsList.ofUnnamedSources(unnamedSourcesTypeCoercer.coerce(filesystem,pathRelativeToProjectRoot,object));
  }
 else {
    return SourceWithFlagsList.ofNamedSources(namedSourcesTypeCoercer.coerce(filesystem,pathRelativeToProjectRoot,object));
  }
}
