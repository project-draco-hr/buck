{
  BuildRuleResolver resolver=new BuildRuleResolver(TargetGraph.EMPTY,new BuildTargetNodeToBuildRuleTransformer());
  LuaBinary binary=(LuaBinary)new LuaBinaryBuilder(BuildTargetFactory.newInstance("//:rule"),FakeLuaConfig.DEFAULT.withExtension(".override")).build(resolver);
  assertThat(binary.getBinPath().toString(),Matchers.endsWith(".override"));
}
