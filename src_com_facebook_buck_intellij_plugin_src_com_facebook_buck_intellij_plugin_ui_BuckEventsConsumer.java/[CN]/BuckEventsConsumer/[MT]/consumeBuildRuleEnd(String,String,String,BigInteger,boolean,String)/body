{
  if (mRules.containsKey(target)) {
    BuildRuleItem bri=mRules.get(target);
    bri.endTimestamp=timestamp;
    if (bri.status == BuildRuleItem.Status.RUNNING) {
      bri.status=BuildRuleItem.Status.FINISHED;
      mRules.put(target,bri);
    }
    if (!mBatchMode) {
      refreshTasksStatus();
    }
  }
}
