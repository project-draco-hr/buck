  return compileToJar(EMPTY_CLASSPATH,"Foo.java",Joiner.on("\n").join(ImmutableList.of("package com.example.buck;","import java.lang.annotation.*;","import static java.lang.annotation.ElementType.*;","@Retention(RetentionPolicy.RUNTIME)","@Target(value={CONSTRUCTOR, FIELD, METHOD, PARAMETER, TYPE})","public @interface Foo {","  int primitiveValue() default 0;","  String[] stringArrayValue() default {\"Hello\"};","  Retention annotationValue() default @Retention(RetentionPolicy.SOURCE);","  Retention[] annotationArrayValue() default {};","  RetentionPolicy enumValue () default RetentionPolicy.CLASS;","}")));
