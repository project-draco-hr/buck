{
  BuildTarget target=BuildTargetFactory.newInstance("//:target");
  CxxPythonExtensionDescription desc=(CxxPythonExtensionDescription)getBuilder(target).build().getDescription();
  CxxPythonExtensionDescription.Arg constructorArg=desc.createUnpopulatedConstructorArg();
  constructorArg.lexSrcs=Optional.of(ImmutableList.<SourcePath>of());
  Iterable<BuildTarget> res=desc.findDepsForTargetFromConstructorArgs(BuildTargetFactory.newInstance("//foo:bar"),constructorArg);
  assertTrue(Iterables.contains(res,PYTHON_DEP_TARGET));
}
