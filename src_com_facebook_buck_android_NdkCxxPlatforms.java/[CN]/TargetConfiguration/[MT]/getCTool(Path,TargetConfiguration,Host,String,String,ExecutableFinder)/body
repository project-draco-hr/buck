{
  return new VersionedTool(getToolPath(ndkRoot,targetConfiguration,host,tool,executableFinder),ImmutableList.of("-B" + getLibexecGccToolPath(ndkRoot,targetConfiguration,host),"-B" + getToolchainBinPath(ndkRoot,targetConfiguration,host)),tool,targetConfiguration.toolchain.toString() + " " + version);
}
