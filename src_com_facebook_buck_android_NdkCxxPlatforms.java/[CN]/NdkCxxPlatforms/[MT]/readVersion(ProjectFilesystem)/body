{
  try (InputStream input=ndkRoot.newFileInputStream(Paths.get("RELEASE.TXT"));BufferedReader reader=new BufferedReader(new InputStreamReader(input,Charsets.UTF_8))){
    return reader.readLine().trim();
  }
 catch (  IOException e) {
    throw new HumanReadableException(e,"could not extract version from NDK repository at %s: %s",ndkRoot,e.getMessage());
  }
}
