{
  return ImmutableList.of("-isystem",toolchainPaths.getNdkToolRoot().resolve("include").toString(),"-isystem",toolchainPaths.getLibPath().resolve("include").toString(),"-isystem",toolchainPaths.getSysroot().resolve("usr").resolve("include").toString(),"-isystem",toolchainPaths.getSysroot().resolve("usr").resolve("include").resolve("linux").toString());
}
