{
  TargetGraphAndTargets targetGraphAndTargets=ProjectCommand.createTargetGraph(targetGraph,ProjectCommandOptions.Ide.XCODE,ImmutableSet.of(smallWorkspaceNode.getBuildTarget()),ImmutableSet.<String>of(),false);
  assertEquals(ImmutableSortedSet.<TargetNode<?>>of(smallWorkspaceNode,bazProjectNode,bazLibNode),ImmutableSortedSet.copyOf(targetGraphAndTargets.getTargetGraph().getNodes()));
}
