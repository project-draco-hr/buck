{
  TargetGraphAndTargets targetGraphAndTargets=ProjectCommandTests.createTargetGraph(targetGraph,ProjectCommandOptions.Ide.XCODE,ImmutableSet.of(smallWorkspaceNode.getBuildTarget()),false);
  assertEquals(ImmutableSortedSet.<TargetNode<?>>of(smallWorkspaceNode,bazLibNode),ImmutableSortedSet.copyOf(targetGraphAndTargets.getTargetGraph().getNodes()));
}
