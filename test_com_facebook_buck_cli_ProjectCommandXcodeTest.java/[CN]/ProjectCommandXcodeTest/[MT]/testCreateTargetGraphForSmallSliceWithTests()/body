{
  TargetGraphAndTargets targetGraphAndTargets=ProjectCommandTests.createTargetGraph(targetGraph,ProjectCommandOptions.Ide.XCODE,ImmutableSet.of(smallWorkspaceNode.getBuildTarget()),true);
  assertEquals(ImmutableSortedSet.<TargetNode<?>>of(smallWorkspaceNode,bazLibNode,bazTestNode),ImmutableSortedSet.copyOf(targetGraphAndTargets.getTargetGraph().getNodes()));
}
