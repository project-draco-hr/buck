{
  TargetGraphAndTargets targetGraphAndTargets=ProjectCommandTests.createTargetGraph(targetGraph,ProjectCommand.Ide.XCODE,ImmutableSet.of(workspaceNode.getBuildTarget()),true);
  assertEquals(ImmutableSortedSet.<TargetNode<?>>of(workspaceNode,fooBinNode,fooBinBinaryNode,fooLibNode,fooBinTestNode,fooTestNode,barLibNode,bazLibNode),ImmutableSortedSet.copyOf(targetGraphAndTargets.getTargetGraph().getNodes()));
}
