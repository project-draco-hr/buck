{
  TargetGraphAndTargets targetGraphAndTargets=ProjectCommandTests.createTargetGraph(targetGraph,ProjectCommand.Ide.XCODE,ImmutableSet.of(workspaceNode.getBuildTarget()),false);
  assertEquals(ImmutableSortedSet.<TargetNode<?>>of(workspaceNode,fooBinNode,fooBinBinaryNode,fooLibNode,barLibNode),ImmutableSortedSet.copyOf(targetGraphAndTargets.getTargetGraph().getNodes()));
}
