{
  super(hashCache,pathResolver);
  cache=CacheBuilder.newBuilder().weakKeys().build(new CacheLoader<RuleKeyAppendable,RuleKey>(){
    @Override public RuleKey load(    @Nonnull RuleKeyAppendable appendable) throws Exception {
      RuleKey.Builder subKeyBuilder=newBuilder(pathResolver,hashCache);
      appendable.appendToRuleKey(subKeyBuilder);
      return subKeyBuilder.build();
    }
  }
);
}
