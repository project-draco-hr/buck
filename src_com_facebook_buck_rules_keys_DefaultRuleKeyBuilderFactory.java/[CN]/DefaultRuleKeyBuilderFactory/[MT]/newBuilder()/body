{
  return new RuleKeyBuilder(pathResolver,hashCache,getDefaultRuleKeyBuilderFactory()){
    @Override public RuleKeyBuilder setAppendableRuleKey(    String key,    RuleKeyAppendable appendable){
      RuleKey subKey=ruleKeyCache.getUnchecked(appendable);
      return setAppendableRuleKey(key,subKey);
    }
  }
;
}
