{
  Optional<Float> sampleRate=config.getFloat("log","remote_log_sample_rate");
  if (sampleRate.isPresent()) {
    if (sampleRate.get() > 1.0f) {
      throw new HumanReadableException(".buckconfig: remote_log_sample_rate should be less than or equal to 1.0.");
    }
    if (sampleRate.get() < 0.0f) {
      throw new HumanReadableException(".buckconfig: remote_log_sample_rate should be greater than or equal to 0.");
    }
  }
  return sampleRate;
}
