{
  console=new TestConsole();
  Repository repository=new TestRepositoryBuilder().build();
  AndroidDirectoryResolver androidDirectoryResolver=new FakeAndroidDirectoryResolver();
  ArtifactCache artifactCache=new NoopArtifactCache();
  BuckEventBus eventBus=BuckEventBusFactory.newInstance();
  targetsCommand=new TargetsCommand(new CommandRunnerParams(console,repository,androidDirectoryResolver,new InstanceArtifactCacheFactory(artifactCache),eventBus,BuckTestConstant.PYTHON_INTERPRETER,Platform.detect(),ImmutableMap.copyOf(System.getenv()),new FakeJavaPackageFinder()));
}
