@classmethod
def minimum_sys_modules(cls, site_libs):
    new_modules = {}
    for (module_name, module) in sys.modules.items():
        if any((path.startswith(site_lib) for path in getattr(module, '__path__', ()) for site_lib in site_libs)):
            TRACER.log(('Scrubbing %s from sys.modules' % module))
        else:
            new_modules[module_name] = module
    return new_modules
