{
  this.cacheHitCounter=new IntegerCounter(COUNTER_CATEGORY,CACHE_HIT_COUNTER_NAME,ImmutableMap.<String,String>of());
  this.cacheMissCounter=new IntegerCounter(COUNTER_CATEGORY,CACHE_MISS_COUNTER_NAME,ImmutableMap.<String,String>of());
