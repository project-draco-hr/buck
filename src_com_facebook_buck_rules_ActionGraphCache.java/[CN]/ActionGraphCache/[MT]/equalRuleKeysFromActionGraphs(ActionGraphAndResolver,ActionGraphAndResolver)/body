{
  Map<BuildRule,RuleKey> graph1RuleKeys=getRuleKeysFromBuildRules(graph1.getActionGraph().getNodes(),graph2.getResolver());
  Map<BuildRule,RuleKey> graph2RuleKeys=getRuleKeysFromBuildRules(graph2.getActionGraph().getNodes(),graph2.getResolver());
  return graph1RuleKeys.equals(graph2RuleKeys);
}
