{
  final Function<String,String> makeOutputArgsAbsolute=new Function<String,String>(){
    @Override public String apply(    String input){
      return input.startsWith(BuckConstant.BUCK_OUTPUT_DIRECTORY) ? params.getProjectFilesystem().resolve(input).toString() : input;
    }
  }
;
  return FluentIterable.from(args).transform(makeOutputArgsAbsolute).transform(Escaper.BASH_ESCAPER).transform(Escaper.MAKEFILE_VALUE_ESCAPER).transform(Escaper.MAKEFILE_VALUE_ESCAPER).transform(Escaper.MAKEFILE_VALUE_ESCAPER).transform(Escaper.MAKEFILE_VALUE_ESCAPER);
}
