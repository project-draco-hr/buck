{
  Pair<String,Iterable<BuildRule>> makefilePair=generateMakefile(params,resolver);
  return new NdkLibrary(params.appendExtraDeps(ImmutableSortedSet.<BuildRule>naturalOrder().addAll(makefilePair.getSecond()).build()),new SourcePathResolver(resolver),getGeneratedMakefilePath(params.getBuildTarget(),params.getProjectFilesystem()),makefilePair.getFirst(),findSources(params.getProjectFilesystem(),params.getBuildTarget().getBasePath()),args.flags.get(),args.isAsset.or(false),ndkVersion,MACRO_HANDLER.getExpander(params.getBuildTarget(),params.getCellRoots(),resolver));
}
