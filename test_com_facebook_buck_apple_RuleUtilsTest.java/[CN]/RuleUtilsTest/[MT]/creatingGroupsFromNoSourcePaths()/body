{
  ImmutableList<GroupedSource> expected=ImmutableList.of();
  SourcePathResolver resolver=new SourcePathResolver(new BuildRuleResolver());
  ImmutableList<GroupedSource> actual=RuleUtils.createGroupsFromSourcePaths(resolver.getPathFunction(),ImmutableList.<SourceWithFlags>of(),ImmutableList.<SourcePath>of(),ImmutableList.<SourcePath>of(),ImmutableList.<SourcePath>of());
  assertEquals(expected,actual);
}
