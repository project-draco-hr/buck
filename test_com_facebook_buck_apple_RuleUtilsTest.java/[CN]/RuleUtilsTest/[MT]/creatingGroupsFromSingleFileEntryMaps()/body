{
  ImmutableMultimap<Path,String> subgroups=ImmutableMultimap.of();
  ImmutableMultimap<Path,SourcePath> entries=ImmutableMultimap.<Path,SourcePath>of(Paths.get("root"),new TestSourcePath("File1.h"));
  ImmutableList<GroupedSource> expected=ImmutableList.of(ImmutableGroupedSource.ofSourcePath(new TestSourcePath("File1.h")));
  ImmutableList<GroupedSource> actual=RuleUtils.createGroupsFromEntryMaps(subgroups,entries,Paths.get("root"));
  assertEquals(expected,actual);
}
