{
  workspace=TestDataHelper.createProjectWorkspaceForScenario(this,"warn_on_transitive",tmp);
  workspace.setUp();
  ProcessResult buildResult=workspace.runBuckCommand("build","//:raz","-b","WARN_ON_TRANSITIVE");
  String expectedWarning=Joiner.on("\n").join("Rule //:raz builds with its transitive dependencies but not with its first order " + "dependencies.","The following packages were missing:","Blargh","Meh","Try adding the following deps:","//:foo","//:blargh");
  buildResult.assertSuccess("Build should have succeeded with warnings.");
  assertThat(buildResult.getStderr(),containsString(expectedWarning));
  workspace.verify();
}
