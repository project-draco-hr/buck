def test_subparser2_help(self):
    self._test_subparser_help('5.0 2 -h', textwrap.dedent('            usage: PROG bar 2 [-h] [-y {1,2,3}] [z [z ...]]\n\n            2 description\n\n            positional arguments:\n              z           z help\n\n            optional arguments:\n              -h, --help  show this help message and exit\n              -y {1,2,3}  y help\n            '))
