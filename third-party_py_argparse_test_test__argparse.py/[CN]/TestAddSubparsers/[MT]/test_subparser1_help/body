def test_subparser1_help(self):
    self._test_subparser_help('5.0 1 -h', textwrap.dedent('            usage: PROG bar 1 [-h] [-w W] {a,b,c}\n\n            1 description\n\n            positional arguments:\n              {a,b,c}     x help\n\n            optional arguments:\n              -h, --help  show this help message and exit\n              -w W        w help\n            '))
