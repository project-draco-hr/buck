{
  sink.setReflectively("sourceLevel",getSourceLevel()).setReflectively("targetLevel",getTargetLevel()).setReflectively("extraArguments",Joiner.on(',').join(getExtraArguments())).setReflectively("debug",isDebug()).setReflectively("bootclasspath",getBootclasspath()).setReflectively("javac",getJavac()).setReflectively("annotationProcessingParams",getAnnotationProcessingParams()).setReflectively("spoolMode",getSpoolMode()).setReflectively("trackClassUsage",trackClassUsage());
}
