{
  PythonBinary binary=new PythonBinary(BuildRuleParamsFactory.createTrivialBuildRuleParams(BuildTargetFactory.newInstance("//:bin")),Paths.get("dummy_path_to_pex"),Paths.get("main.py"),new PythonPackageComponents(ImmutableMap.<Path,SourcePath>of(Paths.get(main),new PathSourcePath(mainSrc),Paths.get(mod1),new PathSourcePath(src1),Paths.get(mod2),new PathSourcePath(src2)),ImmutableMap.<Path,SourcePath>of(),ImmutableMap.<Path,SourcePath>of()));
  RuleKey.Builder builder=ruleKeyBuilderFactory.newInstance(binary);
  binary.appendToRuleKey(builder);
  return builder.build();
}
