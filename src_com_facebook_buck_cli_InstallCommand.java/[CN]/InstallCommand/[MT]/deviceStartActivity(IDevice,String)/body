{
  try {
    ErrorParsingReceiver receiver=new ErrorParsingReceiver(){
      @Override protected String matchForError(      String line){
        return (Pattern.matches("^([\\w_$.])*(Exception|Error|error).*$",line) || line.contains("am: not found")) ? line : null;
      }
    }
;
    device.executeShellCommand(String.format("am start -n %s",activityToRun),receiver,INSTALL_TIMEOUT);
    return receiver.getErrorMessage();
  }
 catch (  Exception e) {
    return e.toString();
  }
}
