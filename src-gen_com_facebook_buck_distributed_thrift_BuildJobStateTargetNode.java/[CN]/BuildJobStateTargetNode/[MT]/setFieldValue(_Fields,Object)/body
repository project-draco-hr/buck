{
switch (field) {
case FILE_SYSTEM_ROOT_INDEX:
    if (value == null) {
      unsetFileSystemRootIndex();
    }
 else {
      setFileSystemRootIndex((Integer)value);
    }
  break;
case RAW_NODE:
if (value == null) {
  unsetRawNode();
}
 else {
  setRawNode((String)value);
}
break;
case BUILD_TARGET:
if (value == null) {
unsetBuildTarget();
}
 else {
setBuildTarget((BuildJobStateBuildTarget)value);
}
break;
}
}
