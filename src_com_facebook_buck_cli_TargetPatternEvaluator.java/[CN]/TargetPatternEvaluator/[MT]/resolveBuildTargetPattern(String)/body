{
  Set<BuildTarget> buildTargets=params.getParser().resolveTargetSpec(params.getBuckEventBus(),params.getCell(),enableProfiling,targetNodeSpecParser.parse(params.getCell().getCellRoots(),pattern));
  ImmutableSet.Builder<QueryTarget> builder=ImmutableSortedSet.naturalOrder();
  for (  BuildTarget target : buildTargets) {
    builder.add(QueryBuildTarget.of(target));
  }
  return builder.build();
}
