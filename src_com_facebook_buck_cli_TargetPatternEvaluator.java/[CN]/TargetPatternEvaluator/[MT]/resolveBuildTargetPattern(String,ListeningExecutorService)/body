{
  Set<BuildTarget> buildTargets=params.getParser().resolveTargetSpecs(params.getBuckEventBus(),params.getCell(),enableProfiling,executor,ImmutableSet.of(targetNodeSpecParser.parse(params.getCell().getCellRoots(),pattern)),SpeculativeParsing.of(false),ParserConfig.ApplyDefaultFlavorsMode.DISABLED);
  LOG.debug("Build target pattern %s -> targets %s",pattern,buildTargets);
  ImmutableSet.Builder<QueryTarget> builder=ImmutableSortedSet.naturalOrder();
  for (  BuildTarget target : buildTargets) {
    builder.add(QueryBuildTarget.of(target));
  }
  return builder.build();
}
