{
  this.enableProfiling=enableProfiling;
  this.params=params;
  this.parserConfig=new ParserConfig(params.getBuckConfig());
  this.projectRoot=params.getCell().getFilesystem().getRootPath();
  this.targetNodeSpecParser=new CommandLineTargetNodeSpecParser(params.getBuckConfig(),new BuildTargetPatternTargetNodeParser());
}
