{
  Description<FakeDescription.FakeArg> description=new FakeDescription();
  FakeDescription.FakeArg arg=description.createUnpopulatedConstructorArg();
  arg.inputs=inputs;
  BuildRuleFactoryParams params=NonCheckingBuildRuleFactoryParams.createNonCheckingBuildRuleFactoryParams(buildTarget);
  try {
    return new TargetNode<>(Hashing.sha1().hashString(params.target.getFullyQualifiedName(),UTF_8),description,arg,params,ImmutableSet.<BuildTarget>of(),ImmutableSet.<BuildTargetPattern>of(),createCellRoots(params.getProjectFilesystem()),UNALIASED);
  }
 catch (  NoSuchBuildTargetException|TargetNode.InvalidSourcePathInputException e) {
    throw new RuntimeException(e);
  }
}
