{
  if (androidFacet.isAndroidLibrary()) {
    androidProperties.put(APK_PATH_TEMPLATE_PARAMETER,EMPTY_STRING);
    return;
  }
  Path apkPath=moduleBasePath.relativize(Paths.get("")).resolve(Project.ANDROID_APK_DIR).resolve(Paths.get("").relativize(moduleBasePath)).resolve(module.getName() + ".apk");
  androidProperties.put(APK_PATH_TEMPLATE_PARAMETER,apkPath);
}
