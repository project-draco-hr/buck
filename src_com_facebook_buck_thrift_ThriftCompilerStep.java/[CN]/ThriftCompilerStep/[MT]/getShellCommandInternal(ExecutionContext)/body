{
  return ImmutableList.<String>builder().add(compiler.toString()).addAll(flags).add("--gen",String.format("%s:%s",language,Joiner.on(',').join(options))).addAll(MoreIterables.zipAndConcat(Iterables.cycle("-I"),Iterables.transform(includes,Functions.toStringFunction()))).add("-o",outputDir.toString()).add(input.toString()).build();
}
