{
  if (mkall) {
    if (!dir.mkdirs() && !dir.exists())     throw new EMMARuntimeException(IAppErrorCodes.OUT_MKDIR_FAILURE,new Object[]{dir.getAbsolutePath()});
  }
 else {
    if (!dir.mkdir() && !dir.exists())     throw new EMMARuntimeException(IAppErrorCodes.OUT_MKDIR_FAILURE,new Object[]{dir.getAbsolutePath()});
  }
}
