{
  for (  Path root : getIncludeRoots()) {
    Preconditions.checkState(root.isAbsolute(),"Expected include root to be absolute: %s",root);
  }
  for (  Path root : getSystemIncludeRoots()) {
    Preconditions.checkState(root.isAbsolute(),"Expected system include root to be absolute: %s",root);
  }
  for (  Path map : getHeaderMaps()) {
    Preconditions.checkState(map.isAbsolute(),"Expected header map path to be absolute: %s",map);
  }
}
