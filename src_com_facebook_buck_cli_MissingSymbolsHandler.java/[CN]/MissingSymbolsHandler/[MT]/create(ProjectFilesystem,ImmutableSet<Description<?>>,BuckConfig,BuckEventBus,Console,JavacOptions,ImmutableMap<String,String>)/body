{
  SrcRootsFinder srcRootsFinder=new SrcRootsFinder(projectFilesystem);
  ParserConfig parserConfig=new ParserConfig(config);
  ProjectBuildFileParserFactory projectBuildFileParserFactory=new DefaultProjectBuildFileParserFactory(projectFilesystem.getRootPath(),parserConfig.getPythonInterpreter(),parserConfig.getAllowEmptyGlobs(),parserConfig.getBuildFileName(),parserConfig.getDefaultIncludes(),descriptions);
  JavaSymbolFinder javaSymbolFinder=new JavaSymbolFinder(projectFilesystem,srcRootsFinder,javacOptions,projectBuildFileParserFactory,config,buckEventBus,console,environment);
  return new MissingSymbolsHandler(console,javaSymbolFinder,parserConfig.getBuildFileName());
}
