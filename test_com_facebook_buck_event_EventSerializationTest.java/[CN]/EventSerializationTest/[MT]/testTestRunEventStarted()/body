{
  TestRunEvent.Started event=TestRunEvent.started(true,Optional.<TestSelectorList>absent(),false,ImmutableList.<String>of());
  event.configure(timestamp,nanoTime,threadId,buildId);
  String message=new ObjectMapper().writeValueAsString(event);
  assertJsonEquals("{\"timestamp\":%d,\"nanoTime\":%d,\"threadId\":%d,\"buildId\":\"%s\"," + "\"runAllTests\":true,\"testSelectorListOptional\":{\"present\":false}," + "\"targetNames\":[],\"type\":\"RunStarted\"}",message);
}
