{
  ParseEvent.Finished event=ParseEvent.finished(ImmutableList.<BuildTarget>of(new BuildTarget("//base","short")),Optional.<DependencyGraph>absent());
  event.configure(timestamp,nanoTime,threadId,buildId);
  String message=new ObjectMapper().writeValueAsString(event);
  assertJsonEquals("{\"timestamp\":%d,\"nanoTime\":%d,\"threadId\":%d,\"buildId\":\"%s\"," + "\"buildTargets\":[{\"baseName\":\"//base\",\"shortName\":\"short\"}]," + "\"type\":\"ParseFinished\"}",message);
}
