{
  ParseEvent.Finished event=ParseEvent.finished(ImmutableList.<BuildTarget>of(BuildTarget.builder("//base","short").addFlavors(ImmutableFlavor.of("flv")).build()),Optional.<TargetGraph>absent());
  event.configure(timestamp,nanoTime,threadId,buildId);
  String message=new ObjectMapper().writeValueAsString(event);
  assertJsonEquals("{\"timestamp\":%d,\"nanoTime\":%d,\"threadId\":%d,\"buildId\":\"%s\"," + "\"buildTargets\":[{\"repository\":{\"present\":false},\"baseName\":\"//base\"," + "\"shortName\":\"short\",\"flavor\":\"flv\"}],\"type\":\"ParseFinished\"}",message);
}
