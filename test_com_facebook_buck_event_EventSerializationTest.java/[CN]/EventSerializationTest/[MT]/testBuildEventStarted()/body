{
  BuildEvent.Started event=BuildEvent.started(ImmutableList.<BuildTarget>of(new BuildTarget("//base","short")));
  event.configure(timestamp,nanoTime,threadId,buildId);
  String message=new ObjectMapper().writeValueAsString(event);
  assertJsonEquals("{\"timestamp\":%d,\"nanoTime\":%d,\"threadId\":%d,\"buildId\":\"%s\"," + "\"buildTargets\":[{\"baseName\":\"//base\",\"shortName\":\"short\",\"flavor\":\"\"}]," + "\"type\":\"BuildStarted\"}",message);
}
