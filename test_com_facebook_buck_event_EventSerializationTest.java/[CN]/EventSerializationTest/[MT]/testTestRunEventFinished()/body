{
  TestRunEvent.Finished event=TestRunEvent.finished(ImmutableSet.<String>of("target"),ImmutableList.<TestResults>of(generateFakeTestResults()));
  event.configure(timestamp,nanoTime,threadId,buildId);
  String message=new ObjectMapper().writeValueAsString(event);
  assertJsonEquals("{\"timestamp\":%d,\"nanoTime\":%d,\"threadId\":%d,\"buildId\":\"%s\",\"" + "results\":[{\"testCases\":[{\"testCaseName\":\"Test1\",\"testResults\":[{\"testName\":" + "null,\"type\":\"FAILURE\",\"time\":0,\"message\":null,"+ "\"stacktrace\":null,\"stdOut\":null,"+ "\"stdErr\":null}],\"failureCount\":1,\"totalTime\":0,\"success\":false}],"+ "\"failureCount\":1,\"contacts\":[],"+ "\"dependenciesPassTheirTests\":true,\"success\":false}],"+ "\"type\":\"RunComplete\"}",message);
}
