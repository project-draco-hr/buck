{
  TestRunEvent.Finished event=TestRunEvent.finished(ImmutableList.<String>of("target"),ImmutableList.<TestResults>of(generateFakeTestResults()));
  event.configure(timestamp,nanoTime,threadId,buildId);
  String message=new ObjectMapper().writeValueAsString(event);
  assertJsonEquals("{\"timestamp\":%d,\"nanoTime\":%d,\"threadId\":%d,\"buildId\":\"%s\"," + "\"results\":[{\"testCases\":[{\"testCaseName\":\"Test1\",\"testResults\":[{\"success\"" + ":false,\"time\":0}],\"failureCount\":1,\"totalTime\":0,\"success\":false}],"+ "\"failureCount\":1,\"success\":false}],\"type\":\"RunComplete\"}",message);
}
