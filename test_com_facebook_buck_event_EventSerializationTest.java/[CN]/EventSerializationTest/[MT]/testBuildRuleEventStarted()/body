{
  BuildRule rule=generateFakeBuildRule();
  BuildRuleEvent.Started event=BuildRuleEvent.started(rule);
  event.configure(timestamp,nanoTime,threadId,buildId);
  String message=new ObjectMapper().writeValueAsString(event);
  assertJsonEquals("{\"timestamp\":%d,\"nanoTime\":%d,\"threadId\":%d,\"buildId\":\"%s\"," + "\"buildRule\":{\"type\":\"fake_build_rule\",\"name\":\"//fake:rule\"}," + "\"type\":\"BuildRuleStarted\","+ "\"eventKey\":{\"value\":1024186770}}",message);
}
