{
  BuildRuleEvent.Started event=BuildRuleEvent.started(generateFakeBuildRule());
  event.configure(timestamp,nanoTime,threadId,buildId);
  String message=new ObjectMapper().writeValueAsString(event);
  assertJsonEquals("{\"timestamp\":%d,\"nanoTime\":%d,\"threadId\":%d,\"buildId\":\"%s\"," + "\"buildRule\":{\"type\":{\"name\":\"java_library\",\"testRule\":false}," + "\"name\":\"//fake:rule\"},\"type\":\"BuildRuleStarted\"}",message);
}
