{
  BuildEvent.Finished event=BuildEvent.finished(ImmutableList.<BuildTarget>of(new BuildTarget("//base","short")),0);
  event.configure(timestamp,nanoTime,threadId,buildId);
  String message=new ObjectMapper().writeValueAsString(event);
  assertJsonEquals("{\"timestamp\":%d,\"nanoTime\":%d,\"threadId\":%d,\"buildId\":\"%s\"," + "\"buildTargets\":[{\"baseName\":\"//base\",\"shortName\":\"short\",\"flavor\":\"\"}]" + ",\"exitCode\":0,\"type\":\"BuildFinished\"}",message);
}
