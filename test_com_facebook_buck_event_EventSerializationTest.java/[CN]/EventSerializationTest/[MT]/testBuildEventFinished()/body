{
  BuildEvent.Finished event=BuildEvent.finished(BuildEvent.started(ImmutableSet.of("//base:short")),0);
  event.configure(timestamp,nanoTime,threadId,buildId);
  String message=MAPPER.writeValueAsString(event);
  assertJsonEquals("{\"timestamp\":%d,\"nanoTime\":%d,\"threadId\":%d,\"buildId\":\"%s\"," + "\"eventKey\":{\"value\":4242}," + "\"buildArgs\":[\"//base:short\"], \"exitCode\":0,\"type\":\"BuildFinished\"}",message);
}
