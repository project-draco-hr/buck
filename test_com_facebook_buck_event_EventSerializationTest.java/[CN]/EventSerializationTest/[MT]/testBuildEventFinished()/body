{
  BuildEvent.Finished event=BuildEvent.finished(ImmutableSet.<BuildTarget>of(BuildTarget.builder("//base","short").build()),0);
  event.configure(timestamp,nanoTime,threadId,buildId);
  String message=new ObjectMapper().writeValueAsString(event);
  assertJsonEquals("{\"timestamp\":%d,\"nanoTime\":%d,\"threadId\":%d,\"buildId\":\"%s\"," + "\"buildTargets\":[{\"repository\":{\"present\":false},\"baseName\":\"//base\"," + "\"shortName\":\"short\",\"flavor\":\"\"}],\"exitCode\":0,\"type\":\"BuildFinished\"}",message);
}
