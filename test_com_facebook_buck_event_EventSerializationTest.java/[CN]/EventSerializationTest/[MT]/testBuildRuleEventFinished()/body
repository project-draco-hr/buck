{
  BuildRuleEvent.Finished event=BuildRuleEvent.finished(generateFakeBuildRule(),BuildRuleStatus.SUCCESS,CacheResult.miss(),Optional.<BuildRuleSuccessType>absent());
  event.configure(timestamp,nanoTime,threadId,buildId);
  String message=new ObjectMapper().writeValueAsString(event);
  assertJsonEquals("{\"timestamp\":%d,\"nanoTime\":%d,\"threadId\":%d,\"buildId\":\"%s\"," + "\"status\":\"SUCCESS\",\"cacheResult\":{\"type\":\"MISS\",\"cacheSource\":{" + "\"present\":false},\"cacheError\":{\"present\":false}},\"buildRule\":{\"type\":"+ "\"fake_build_rule\",\"name\":\"//fake:rule\"},\"ruleKeySafe\":\"aaaa\","+ "\"type\":\"BuildRuleFinished\"}",message);
}
