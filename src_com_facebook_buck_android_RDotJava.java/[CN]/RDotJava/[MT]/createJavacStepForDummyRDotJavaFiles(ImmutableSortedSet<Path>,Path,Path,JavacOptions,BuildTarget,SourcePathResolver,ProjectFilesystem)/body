{
  return new JavacStep(outputDirectory,Optional.<Path>absent(),Optional.<StandardJavaFileManagerFactory>absent(),Optional.<Path>absent(),javaSourceFilePaths,pathToSrcsList,ImmutableSortedSet.<Path>of(),javacOptions.getJavac(),JavacOptions.builder(javacOptions).setAnnotationProcessingParams(AnnotationProcessingParams.EMPTY).build(),buildTarget,Optional.<SuggestBuildRules>absent(),resolver,filesystem);
}
