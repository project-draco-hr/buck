{
  if ((-1 <= value) && (value <= 5)) {
    out.write(_iconst_0 + value);
  }
 else   if ((-128 <= value) && (value <= 127)) {
    out.write2(_bipush,value);
  }
 else   if ((-32768 <= value) && (value <= 32767)) {
    out.write3(_sipush,value >>> 8,value);
  }
 else {
    final int index=cls.getConstants().add(new CONSTANT_Integer_info(value));
    if (index <= 0xFF) {
      out.write2(_ldc,index);
    }
 else {
      out.write3(_ldc_w,index >>> 8,index);
    }
  }
}
