{
  for (  Map.Entry<String,BuildRule> target : matchingBuildRules.entrySet()) {
    String output=target.getKey();
    if (showOutput) {
      String outputPath=target.getValue().getPathToOutputFile();
      if (outputPath != null) {
        output+=" " + outputPath;
      }
    }
    getStdOut().println(output);
  }
}
