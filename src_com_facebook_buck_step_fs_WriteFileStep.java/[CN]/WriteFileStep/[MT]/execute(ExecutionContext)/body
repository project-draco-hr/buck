{
  try (InputStream sourceStream=source.openStream()){
    context.getProjectFilesystem().copyToPath(sourceStream,outputPath,StandardCopyOption.REPLACE_EXISTING);
    if (executable) {
      Path resolvedPath=context.getProjectFilesystem().resolve(outputPath);
      MoreFiles.makeExecutable(resolvedPath);
    }
    return 0;
  }
 catch (  IOException e) {
    LOG.error(e,"Couldn't copy bytes to %s",outputPath);
    e.printStackTrace(context.getStdErr());
    return 1;
  }
}
