{
  for (  String tag : tagList) {
    if (tag.startsWith("-")) {
      excludedTags.add(tag.substring(1));
    }
 else     if (tag.startsWith("+")) {
      requiredTags.add(tag.substring(1));
    }
 else     if (tag.equals("manual")) {
      continue;
    }
 else {
      requiredTags.add(tag);
    }
  }
}
