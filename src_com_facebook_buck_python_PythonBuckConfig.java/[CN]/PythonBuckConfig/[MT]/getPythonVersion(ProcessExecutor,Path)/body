{
  try {
    ProcessExecutor.Result versionResult=processExecutor.execute(Runtime.getRuntime().exec(pythonPath + " --version"),EnumSet.of(ProcessExecutor.Option.EXPECTING_STD_ERR),Optional.<String>absent());
    return extractPythonVersion(pythonPath,versionResult);
  }
 catch (  IOException e) {
    throw new HumanReadableException("Could not run " + pythonPath + " --version");
  }
}
