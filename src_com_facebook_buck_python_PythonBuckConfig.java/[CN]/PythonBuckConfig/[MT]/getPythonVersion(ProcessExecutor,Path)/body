{
  try {
    ProcessExecutor.Result versionResult=processExecutor.launchAndExecute(ProcessExecutorParams.builder().addCommand(pythonPath.toString(),"-V").build(),EnumSet.of(ProcessExecutor.Option.EXPECTING_STD_ERR),Optional.<String>absent(),Optional.<Long>absent());
    return extractPythonVersion(pythonPath,versionResult);
  }
 catch (  IOException e) {
    throw new HumanReadableException(e,"Could not run \"%s --version\": %s",pythonPath,e.getMessage());
  }
}
