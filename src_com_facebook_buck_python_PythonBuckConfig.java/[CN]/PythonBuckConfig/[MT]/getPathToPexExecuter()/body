{
  Optional<Path> path=delegate.getPath(SECTION,"path_to_pex_executer");
  if (!path.isPresent()) {
    return Paths.get(getPythonInterpreter());
  }
  if (!isExecutableFile(path.get().toFile())) {
    throw new HumanReadableException("%s is not executable (set in python.path_to_pex_executer in your config",path.get().toString());
  }
  return path.get();
}
