{
  BuildTargetPatternParser<BuildTargetPattern> buildTargetPatternParser=BuildTargetPatternParser.forVisibilityArgument();
  assertEquals(new ImmediateDirectoryBuildTargetPattern("test/com/facebook/buck/parser/"),buildTargetPatternParser.parse(createCellRoots(filesystem),"//test/com/facebook/buck/parser:"));
  assertEquals(new SingletonBuildTargetPattern(filesystem.getRootPath(),"//test/com/facebook/buck/parser:parser"),buildTargetPatternParser.parse(createCellRoots(filesystem),"//test/com/facebook/buck/parser:parser"));
  assertEquals(new SubdirectoryBuildTargetPattern("test/com/facebook/buck/parser/"),buildTargetPatternParser.parse(createCellRoots(filesystem),"//test/com/facebook/buck/parser/..."));
}
