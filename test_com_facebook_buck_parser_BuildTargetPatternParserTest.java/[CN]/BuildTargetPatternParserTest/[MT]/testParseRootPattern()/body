{
  BuildTargetPatternParser buildTargetPatternParser=BuildTargetPatternParser.forVisibilityArgument(targetParser);
  assertEquals(new ImmediateDirectoryBuildTargetPattern(""),buildTargetPatternParser.parse("//:"));
  assertEquals(new SingletonBuildTargetPattern("//:parser"),buildTargetPatternParser.parse("//:parser"));
  assertEquals(new SubdirectoryBuildTargetPattern(""),buildTargetPatternParser.parse("//..."));
}
