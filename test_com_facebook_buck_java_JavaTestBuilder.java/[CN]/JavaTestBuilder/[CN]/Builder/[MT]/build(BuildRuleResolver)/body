{
  BuildRuleParams params=new FakeBuildRuleParamsBuilder(target).setType(JavaTestDescription.TYPE).setDeps(deps.build()).build();
  JavaTest test=new JavaTest(params,srcs.build(),resources.build(),labels.build(),contacts.build(),proguardConfig,ImmutableSet.<Path>of(),JavacOptions.DEFAULTS,vmArgs,sourcesUnderTest.build(),Optional.<Path>absent());
  resolver.addToIndex(test);
  return test;
}
