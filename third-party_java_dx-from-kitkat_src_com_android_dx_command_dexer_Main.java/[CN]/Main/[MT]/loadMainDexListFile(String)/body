{
  Set<String> mainDexList=new HashSet<String>();
  BufferedReader bfr=null;
  try {
    FileReader fr=new FileReader(mainDexListFile);
    bfr=new BufferedReader(fr);
    String line;
    while (null != (line=bfr.readLine())) {
      mainDexList.add(fixPath(line));
    }
  }
  finally {
    if (bfr != null) {
      bfr.close();
    }
  }
  return mainDexList;
}
