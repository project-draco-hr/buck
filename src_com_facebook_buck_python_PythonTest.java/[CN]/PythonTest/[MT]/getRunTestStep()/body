{
  return new ShellStep(){
    @Override protected ImmutableList<String> getShellCommandInternal(    ExecutionContext context){
      ProjectFilesystem fs=context.getProjectFilesystem();
      return ImmutableList.of(fs.resolve(getResolver().getPath(binary)).toString(),"-o",fs.resolve(getPathToTestOutputResult()).toString());
    }
    @Override public String getShortName(){
      return "pyunit";
    }
  }
;
}
