{
  throwIfNotPrepared();
  int sz=typeIds.size();
  int offset=(sz == 0) ? 0 : getFileOffset();
  if (sz > DexFormat.MAX_TYPE_IDX + 1) {
    throw new DexException("Too many type references: " + sz + "; max is "+ (DexFormat.MAX_TYPE_IDX + 1)+ ".\n"+ Main.getTooManyIdsErrorMessage());
  }
  if (out.annotates()) {
    out.annotate(4,"type_ids_size:   " + Hex.u4(sz));
    out.annotate(4,"type_ids_off:    " + Hex.u4(offset));
  }
  out.writeInt(sz);
  out.writeInt(offset);
}
