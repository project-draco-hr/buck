{
  BuildRule buildRule=buildRuleResolver.requireRule(getBuildTarget().withFlavors(cxxPlatform.getFlavor(),CxxDescriptionEnhancer.STATIC_FLAVOR));
  return NativeLinkableInput.of(ImmutableList.of(new SourcePathArg(getResolver(),new BuildTargetSourcePath(buildRule.getBuildTarget()))),ImmutableSet.<FrameworkPath>of(),ImmutableSet.<FrameworkPath>of());
}
