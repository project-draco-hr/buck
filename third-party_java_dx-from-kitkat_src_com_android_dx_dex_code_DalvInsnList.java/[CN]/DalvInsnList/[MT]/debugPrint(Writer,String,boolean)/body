{
  @SuppressWarnings("resource") IndentingWriter iw=new IndentingWriter(out,0,prefix);
  int sz=size();
  try {
    for (int i=0; i < sz; i++) {
      DalvInsn insn=(DalvInsn)get0(i);
      String s;
      if ((insn.codeSize() != 0) || verbose) {
        s=insn.listingString("",0,verbose);
      }
 else {
        s=null;
      }
      if (s != null) {
        iw.write(s);
      }
    }
    iw.flush();
  }
 catch (  IOException ex) {
    throw new RuntimeException(ex);
  }
}
