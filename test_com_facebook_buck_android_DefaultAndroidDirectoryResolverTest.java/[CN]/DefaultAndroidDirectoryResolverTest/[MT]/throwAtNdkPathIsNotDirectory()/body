{
  Path file=tmpDir.getRoot().resolve(tmpDir.newFile("file"));
  DefaultAndroidDirectoryResolver resolver=createResolver(Optional.<String>absent(),Optional.<Path>absent(),Optional.of(file),Optional.<Path>absent());
  expectedException.expect(HumanReadableException.class);
  expectedException.expectMessage(file + " does not contain a valid properties file for " + "Android NDK.");
  resolver.getNdkOrThrow();
}
