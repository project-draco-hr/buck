{
  super(params,resolver,ImmutableList.of(bundle),Optional.of(packageConfigAndPlatformInfo.getExpandedArg()),Optional.<Arg>absent(),Optional.<Arg>absent(),params.getBuildTarget().getShortName() + "." + packageConfigAndPlatformInfo.getConfig().getExtension());
  this.packageConfigAndPlatformInfo=packageConfigAndPlatformInfo;
}
