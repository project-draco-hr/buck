{
  super(params,resolver,ImmutableList.of(bundle),Optional.<Arg>of(new StringArg(packageConfigAndPlatformInfo.getConfig().getCommand())),Optional.<Arg>absent(),Optional.<Arg>absent(),params.getBuildTarget().getShortName() + "." + packageConfigAndPlatformInfo.getConfig().getExtension(),ImmutableSortedSet.<BuildTarget>of());
  this.packageConfigAndPlatformInfo=packageConfigAndPlatformInfo;
}
