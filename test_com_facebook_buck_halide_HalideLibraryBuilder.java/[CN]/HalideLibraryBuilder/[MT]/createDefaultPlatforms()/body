{
  Flavor defaultFlavor=ImmutableFlavor.of("default");
  CxxPlatform cxxPlatform=CxxPlatformUtils.DEFAULT_PLATFORM;
  return new FlavorDomain<CxxPlatform>("C/C++ Platform",ImmutableMap.<Flavor,CxxPlatform>builder().put(defaultFlavor,cxxPlatform).put(cxxPlatform.getFlavor(),cxxPlatform).build());
}
