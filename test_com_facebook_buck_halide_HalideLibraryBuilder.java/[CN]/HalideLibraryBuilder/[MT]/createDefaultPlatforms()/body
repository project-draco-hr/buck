{
  Flavor hostFlavor=CxxPlatforms.getHostFlavor();
  CxxPlatform hostCxxPlatform=CxxPlatform.builder().from(CxxPlatformUtils.DEFAULT_PLATFORM).setFlavor(hostFlavor).build();
  return new FlavorDomain<>("C/C++ Platform",ImmutableMap.<Flavor,CxxPlatform>builder().put(DefaultCxxPlatforms.FLAVOR,CxxPlatformUtils.DEFAULT_PLATFORM).put(hostCxxPlatform.getFlavor(),hostCxxPlatform).build());
}
