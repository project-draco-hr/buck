{
  Path parent=tmp.newFolder("zipstep");
  Path out=parent.resolve("output.zip");
  try (Zip zip=new Zip(out,true)){
    zip.add("file1.txt","");
  }
   ZipStep step=new ZipStep(filesystem,Paths.get("zipstep"),ImmutableSet.<Path>of(),false,ZipStep.DEFAULT_COMPRESSION_LEVEL,Paths.get("zipdir"));
