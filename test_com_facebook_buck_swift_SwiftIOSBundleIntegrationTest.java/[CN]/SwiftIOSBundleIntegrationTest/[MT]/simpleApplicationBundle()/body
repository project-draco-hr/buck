{
  assumeThat(AppleNativeIntegrationTestUtils.isSwiftAvailable(ApplePlatform.IPHONESIMULATOR),is(true));
  ProjectWorkspace workspace=TestDataHelper.createProjectWorkspaceForScenario(this,"simple_swift_application_bundle",tmp);
  workspace.setUp();
  workspace.runBuckCommand("build","//:DemoApp#iphonesimulator-x86_64,no-debug").assertSuccess();
  workspace.verify();
  assertTrue(Files.exists(tmp.getRootPath().resolve(BuckConstant.GEN_DIR).resolve("DemoApp#iphonesimulator-x86_64,no-debug,no-include-frameworks/DemoApp.app/" + "DemoApp")));
}
