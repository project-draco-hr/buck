{
  File classFileOuter=new File(outputDir,"test/TestClassWithInner.class");
  InputStream inputStreamOuter=new FileInputStream(classFileOuter);
  DalvikStatsTool.Stats statsOuter=DalvikStatsTool.getEstimate(inputStreamOuter);
  assertMethodReferences(statsOuter.methodReferences,"test/TestClassWithInner.<init>:()V","java/lang/Object.<init>:()V","test/TestClassWithInner.get:()Ljava/lang/Object;","test/TestClassWithInner$1.<init>:(Ltest/TestClassWithInner;)V","test/TestClassWithInner.access$000:(Ltest/TestClassWithInner;)J","java/lang/System.currentTimeMillis:()J");
  assertEquals(224,statsOuter.estimatedLinearAllocSize);
  File classFileInner=new File(outputDir,"test/TestClassWithInner$1.class");
  InputStream inputStreamInner=new FileInputStream(classFileInner);
  DalvikStatsTool.Stats statsInner=DalvikStatsTool.getEstimate(inputStreamInner);
  assertMethodReferences(statsInner.methodReferences,"test/TestClassWithInner$1.toString:()Ljava/lang/String;","test/TestClassWithInner$1.<init>:(Ltest/TestClassWithInner;)V","test/TestClassWithInner$1.get:()Ljava/lang/Object;","java/lang/Object.<init>:()V","java/lang/Long.toString:(J)Ljava/lang/String;","test/TestClassWithInner.access$000:(Ltest/TestClassWithInner;)J");
  assertEquals(172,statsInner.estimatedLinearAllocSize);
}
