{
  if (!maxCacheSizeBytes.isPresent()) {
    return;
  }
  try {
    for (    File fileAccessedEntry : findFilesToDelete()) {
      try {
        Files.deleteIfExists(fileAccessedEntry.toPath());
      }
 catch (      IOException e) {
        continue;
      }
    }
  }
 catch (  IOException e) {
    LOG.error(e,"Failed to delete old files from cache");
  }
}
