{
  try {
    HashCode hash=rawHashCode == null ? Hashing.sha1().hashString(factoryParams.target.getFullyQualifiedName(),UTF_8) : rawHashCode;
    return new TargetNode<>(hash,description,arg,factoryParams,getDepsFromArg(),ImmutableSet.<BuildTargetPattern>of(),cellRoots,new CellFilesystemResolver(factoryParams.getProjectFilesystem(),new Function<Optional<String>,ProjectFilesystem>(){
      @Override public ProjectFilesystem apply(      Optional<String> input){
        if (input.isPresent()) {
          throw new RuntimeException("No cross repo tests to be created this way");
        }
        return factoryParams.getProjectFilesystem();
      }
    }
));
  }
 catch (  NoSuchBuildTargetException|TargetNode.InvalidSourcePathInputException e) {
    throw Throwables.propagate(e);
  }
}
