{
  CommandRunnerParams params=CommandRunnerParamsForTesting.builder().build();
  for (  Command command : Command.values()) {
    try {
      command.getCommandRunnerClass().getDeclaredConstructor(CommandRunnerParams.class).newInstance(params);
    }
 catch (    Exception e) {
      fail(String.format("%s: %s %s",command.getDeclaringClass(),e.getClass(),e.getMessage()));
    }
  }
}
