def test_glob_excludes_double_star(self):
    search_base = self.fake_path('foo', glob_results={'**/*.java': ['A.java', 'B.java', 'Test.java'], })
    self.assertGlobMatches(['A.java', 'B.java'], glob_internal(includes=['**/*.java'], excludes=['**/*Test.java'], include_dotfiles=False, search_base=search_base))
