def test_watchman_query_params_includes_and_excludes(self):
    query_params = format_watchman_query_params(['**/*.java'], ['**/*Test.java'], False, '/path/to/glob')
    self.assertEquals({'relative_root': '/path/to/glob', 'path': [''], 'fields': ['name'], 'expression': ['allof', 'exists', ['type', 'f'], ['anyof', ['match', '**/*.java', 'wholename', {}]], ['not', ['anyof', ['match', '**/*Test.java', 'wholename', {}]]]], }, query_params)
