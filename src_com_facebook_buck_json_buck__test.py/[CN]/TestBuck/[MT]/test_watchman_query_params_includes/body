def test_watchman_query_params_includes(self):
    query_params = format_watchman_query_params(['**/*.java'], [], False, '/path/to/glob')
    self.assertEquals({'relative_root': '/path/to/glob', 'path': [''], 'fields': ['name'], 'expression': ['allof', 'exists', ['anyof', ['type', 'f'], ['type', 'l']], ['anyof', ['match', '**/*.java', 'wholename', {}]]], }, query_params)
