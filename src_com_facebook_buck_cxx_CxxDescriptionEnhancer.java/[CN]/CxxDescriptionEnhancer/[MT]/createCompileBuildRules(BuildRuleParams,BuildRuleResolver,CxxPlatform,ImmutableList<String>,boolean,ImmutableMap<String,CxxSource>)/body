{
  ImmutableSortedSet<BuildRule> objectRules=CxxSourceRuleFactory.createCompileBuildRules(params,resolver,config,compilerFlags,pic,sources);
  resolver.addAllToIndex(objectRules);
  return FluentIterable.from(objectRules).transform(SourcePaths.getToBuildTargetSourcePath(params.getProjectFilesystem())).toList();
}
