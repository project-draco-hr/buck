{
  ImmutableSortedSet<BuildRule> objectRules=CxxCompilableEnhancer.createCompileBuildRules(params,resolver,config.getCompiler().or(CxxCompilables.DEFAULT_CXX_COMPILER),cxxPreprocessorInput,compilerFlags,pic,sources);
  resolver.addAllToIndex(objectRules);
  return FluentIterable.from(objectRules).transform(SourcePaths.TO_BUILD_RULE_SOURCE_PATH).toList();
}
