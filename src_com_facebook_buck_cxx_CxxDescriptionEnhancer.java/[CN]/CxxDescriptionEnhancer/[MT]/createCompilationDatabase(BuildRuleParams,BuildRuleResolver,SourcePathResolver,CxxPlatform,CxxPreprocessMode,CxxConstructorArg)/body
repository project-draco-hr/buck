{
  ImmutableMap<CxxPreprocessAndCompile,SourcePath> objects=requireObjects(params.withoutFlavor(CxxCompilationDatabase.COMPILATION_DATABASE),ruleResolver,pathResolver,cxxPlatform,preprocessMode,CxxSourceRuleFactory.PicType.PIC,arg);
  return CxxCompilationDatabase.createCompilationDatabase(params,pathResolver,preprocessMode,objects.keySet());
}
