{
  ImmutableMap<CxxPreprocessAndCompile,SourcePath> objects=requireObjects(params.withoutFlavor(CxxCompilationDatabase.COMPILATION_DATABASE),ruleResolver,pathResolver,cxxBuckConfig,cxxPlatform,CxxSourceRuleFactory.PicType.PIC,arg);
  return CxxCompilationDatabase.createCompilationDatabase(params,pathResolver,cxxBuckConfig.getPreprocessMode(),objects.keySet());
}
