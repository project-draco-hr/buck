{
switch (sourceWithFlagsList.getType()) {
case NAMED:
    sources.putAll(sourceWithFlagsList.getNamedSources().get());
  break;
case UNNAMED:
sources.putAll(pathResolver.getSourcePathNames(buildTarget,"srcs",sourceWithFlagsList.getUnnamedSources().get(),SourceWithFlags.TO_SOURCE_PATH));
break;
}
}
