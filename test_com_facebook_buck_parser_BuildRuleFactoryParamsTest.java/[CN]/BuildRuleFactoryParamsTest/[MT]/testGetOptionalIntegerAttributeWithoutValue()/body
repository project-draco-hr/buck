{
  BuildTarget target=BuildTargetFactory.newInstance("//src/com/facebook:Main");
  Map<String,Object> instance=ImmutableMap.<String,Object>of("another_value","yolo");
  BuildRuleFactoryParams params=new BuildRuleFactoryParams(instance,filesystem,tree,parser,target,new FakeRuleKeyBuilderFactory());
  assertEquals(Optional.absent(),params.getOptionalIntegerAttribute("some_value"));
}
