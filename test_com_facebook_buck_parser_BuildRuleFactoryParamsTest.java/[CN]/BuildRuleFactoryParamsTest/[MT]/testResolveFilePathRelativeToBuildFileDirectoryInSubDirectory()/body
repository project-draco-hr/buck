{
  BuildTarget buildTarget=BuildTargetFactory.newInstance("//src/com/facebook:Main");
  BuildRuleFactoryParams params=new BuildRuleFactoryParams(filesystem,parser,buildTarget,new FakeRuleKeyBuilderFactory());
  assertEquals(Paths.get("src/com/facebook/A.java"),params.resolveFilePathRelativeToBuildFileDirectory("A.java"));
}
