{
  BuildTarget buildTarget=BuildTargetFactory.newInstance("//src/com/facebook:Main");
  BuildRuleFactoryParams params=new BuildRuleFactoryParams(null,filesystem,tree,parser,buildTarget,new FakeRuleKeyBuilderFactory());
  assertEquals("src/com/facebook/A.java",params.resolveFilePathRelativeToBuildFileDirectory("A.java"));
}
