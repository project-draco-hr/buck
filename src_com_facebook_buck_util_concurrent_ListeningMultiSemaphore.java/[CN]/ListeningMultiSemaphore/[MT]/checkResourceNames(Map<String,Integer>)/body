{
  Sets.SetView<String> unknownResourceNames=Sets.difference(resources.keySet(),actualCounters.keySet());
  if (unknownResourceNames.size() != 0) {
    throw new IllegalArgumentException(String.format("ListeningMultiSemaphore (%s) was not initialized with " + "the following resource names: %s",actualCounters.keySet(),unknownResourceNames));
  }
}
