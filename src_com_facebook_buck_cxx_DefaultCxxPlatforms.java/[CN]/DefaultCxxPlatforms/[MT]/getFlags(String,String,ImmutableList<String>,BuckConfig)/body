{
  Optional<String> value=delegate.getValue(section,field);
  if (!value.isPresent()) {
    return def;
  }
  ImmutableList.Builder<String> split=ImmutableList.builder();
  if (!value.get().trim().isEmpty()) {
    split.addAll(Splitter.on(" ").split(value.get().trim()));
  }
  return split.build();
}
