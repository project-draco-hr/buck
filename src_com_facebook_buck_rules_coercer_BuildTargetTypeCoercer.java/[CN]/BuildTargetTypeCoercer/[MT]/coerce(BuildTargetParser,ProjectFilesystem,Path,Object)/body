{
  if (!(object instanceof String)) {
    throw new IllegalArgumentException("BuildTargetTypeCoercer called for non-string object");
  }
  String param=(String)object;
  try {
    String baseName=BuildTarget.BUILD_TARGET_PREFIX + MorePaths.pathWithUnixSeparators(pathRelativeToProjectRoot);
    return buildTargetParser.parse(param,BuildTargetPatternParser.forBaseName(buildTargetParser,baseName));
  }
 catch (  BuildTargetParseException e) {
    throw CoerceFailedException.simple(object,getOutputClass());
  }
}
